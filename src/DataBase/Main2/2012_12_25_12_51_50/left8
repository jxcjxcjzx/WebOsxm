                   $('#gallery').css('background-image','url('+imgsrc+')');  
                }
                $('#slideshow').fadeTo('fast', 0.0, callback);
            },
            onTransitionIn:         function() {
                $('#slideshow').fadeTo(1200, 1.0);
            }
        });
    });
</script>   


