<script type="text/javascript">
var VideoIDS = "122786725";
var VERSION  = "/v1.0.0826";
var PLAYERVERSION="/v1.0.0295";

function play(){
 	if(!window.SWFObject||!document.getElementById("pageover")){
		setTimeout('play();', 200); 
		return;
	}

	var fo = new SWFObject("http://static.youku.com/v1.0.0295/v/swf/player.swf", "index_player_swf", "100%", "100%", 7, "#FFFFFF");
	fo.addParam("wmode","transparent");
	fo.addParam("quality","high");
	fo.addParam("allowscriptaccess","always");
	fo.addParam("allowfullscreen",'true');
	fo.addVariable("isShowRelatedVideo","false");
	fo.addVariable("VideoIDS",VideoIDS);
	fo.addVariable("isAutoPlay",false);
	fo.addVariable("UserID",0);
	fo.addVariable("isLoop",false);
	fo.addVariable("imglogo","http://g4.ykimg.com/2100401F4650D44747B056000000013594CF30-5570-356F-A6A4-3AF7FFE55429?u=1356089159");
	fo.addVariable("winType",'index');

	if(!fo.write("focus_player")){
		document.getElementById("focus_player").innerHTML='<span align="center" valign="middle"><a href="http://static.youku.com/pub/youku/help/install_flash_player_active_x.exe" target=_blank><img src="/v/img/download.jpg" alt="下载播放器" width="312" height="234" border="0" /></a></span>';
	}
}

</script>

		
