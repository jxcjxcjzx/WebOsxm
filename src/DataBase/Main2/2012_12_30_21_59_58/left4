<script type="text/javascript" >
  function initCommon(){
    var menuBar ;
    if  (navigator.appVersion.indexOf("MSIE")>0)
      menuBar = new YAHOO.widget.MenuBar("hlmenus", {autosubmenudisplay: true,hidedelay: 750,lazyload: true,shadow:false,iframe:true});
    else
      menuBar = new YAHOO.widget.MenuBar("hlmenus", {autosubmenudisplay: true,hidedelay: 750,lazyload: true,shadow:false});

    menuBar.render();

    document.getElementById("hl-search-q1").focus();
  }
  YAHOO.util.Event.onContentReady("hlmenus",initCommon);
</script>
<div id="hl-div-threecol-body">

        <div id="hl-div-threecol-lc">
        <script src="http://yui.yahooapis.com/2.7.0/build/animation/animation-min.js" type="text/javascript"></script>
<script>
  YAHOO.util.Event.onDOMReady(function () {
    var   $E = YAHOO.util.Event
        , $D = YAHOO.util.Dom;

    var stepTime = 6000;
    var jumbotrons = $D.getElementsByClassName('hl-jumbotron', 'div');

    $D.batch(jumbotrons, function (el) {
      var   slideAnim = []
          , navAnim = []

          , currentSlide = 0

          , slide = $D.getElementsByClassName('hl-jumbotron-slides', 'ul', el)[0]

          , slides = $D.getChildrenBy(slide, function (el) {
              var   fadeIn = new YAHOO.util.Anim(el, { opacity: { to: 1 } }, .7, YAHOO.util.Easing.easeIn)
                  , fadeOut = new YAHOO.util.Anim(el, { opacity: { to: 0 } }, .7, YAHOO.util.Easing.easeOut);

              fadeIn.onStart.subscribe(function () {
                el.style.display = 'block';
              });

              fadeOut.onComplete.subscribe(function () {
                el.style.display = 'none';
              });

              slideAnim.push({
                fadeOut : fadeOut,
                fadeIn : fadeIn
              });
              return true;
            })

          , nav = $D.getElementsByClassName('hl-jumbotron-nav', 'ul', el)[0]

          , navEls = $D.getChildrenBy(nav, function (el) {
              var   expand = new YAHOO.util.ColorAnim(el, { /*width: { from: 278, to: 300 }*//*, height: { to: 110, from: 45 }*//*, backgroundColor: { to: '#53C199', from: '#E6F1F3' } */}, .7, YAHOO.util.Easing.easeOut)
                  , collapse = new YAHOO.util.ColorAnim(el, { /*width: { to: 278, from: 300 }*//*height: { to: 45, from: 110 }*//*, backgroundColor: { from: '#53C199', to: '#E6F1F3' } */}, .7, YAHOO.util.Easing.easeOut);

              navAnim.push({
                expand: expand,
                collapse: collapse
              });

              return true;
            })

          , fader = function (newIndex) {
              slideAnim[currentSlide]['fadeOut'].animate();
              //navAnim[currentSlide]['collapse'].animate();
              $D.removeClass(navEls[currentSlide], 'active');
              currentSlide = newIndex || (++currentSlide == slideAnim.length ? 0 : currentSlide);
              slideAnim[currentSlide]['fadeIn'].animate();
              //navAnim[currentSlide]['expand'].animate();
              $D.addClass(navEls[currentSlide], 'active');
            }

          , timerFn = function () { fader(); }

          , timer = setInterval(timerFn, stepTime);

      $E.addListener(nav, 'click', function (e) {
        var   target = $E.getTarget(e)
            , index;

        while (target.nodeName.toLowerCase() !== 'li') target = target.parentNode;
        //if (target.nodeName.toLowerCase() !== 'a' || (index = target.innerHTML - 1) === currentSlide) return false;
        index = $D.getAttribute(target, 'data-billboard-index');
        if (index == currentSlide) return;

        clearInterval(timer);
        fader(index);
        $E.stopPropagation(e);
        $E.preventDefault(e);
        return false;
      });

      $E.addListener(slide, 'mouseover', function (e) {
        clearInterval(timer);
      });
      $E.addListener(slide, 'mouseout', function (e) {
        clearInterval(timer);
        timer = setInterval(timerFn, stepTime);
      })
    });
  });
</script>

