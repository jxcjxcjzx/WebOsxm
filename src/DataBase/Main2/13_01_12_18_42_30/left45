<script>
//广告初始化函数
function init_ad(ad_div_id){
	if ( $("#" + ad_div_id + " .child").length < 2){
		return;
	}
	
	$("#" + ad_div_id + " .child:not(:first)").hide();
	
	var change_time = parseInt($("#" + ad_div_id ).css('top'));
	if(isNaN(change_time) || change_time == 0){
		return ;
	}
	
	var B=$("#" + ad_div_id + " .child:last");
	var C=$("#" + ad_div_id + " .child:first");
	setInterval(function(){
		if(B.is(":visible")){
			B.hide();
			C.show();
		}else{
			$("#" + ad_div_id + " .child:visible").next().addClass("in");
			$("#" + ad_div_id + " .child:visible").hide(); 
			$("#" + ad_div_id + " .child.in").show().removeClass("in");
		}
	},change_time * 1000);
}
</script>
