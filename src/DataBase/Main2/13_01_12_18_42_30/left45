<script type="text/javascript">
var t = n = 0, count=$("#play_list span").size();
$().ready(function(){
	$("#play_list span:not(:first)").hide();
	$("#play_text a:first").addClass("hover");
	$("#play_text a").click(function() {
		var i = $(this).attr("a_id") - 1;
		n = i;
		if (i >= count) return;
		$("#play_list span").hide();
		$("#play_list span").eq(i).show();
		//$("#play_list span").filter(":visible").fadeOut(500).parent().children().eq(i).fadeIn(1000);
		$("#play_text a").removeClass("hover");
		$(this).addClass("hover");
	});
	$("#play_list span").each(function(){
		$(this).mouseover(function(){
			clearInterval(t);
		}).mouseout(function(){
			t = setInterval("showAuto()", 3000);
		});
	});
	$("#play_text a").each(function(){
		$(this).mouseover(function(){
			clearInterval(t);
		}).mouseout(function(){
			t = setInterval("showAuto()", 3000);
		});
	});
	t = setInterval("showAuto()", 3000);
})

function showAuto()
{
	n = n >= (count - 1) ? 0 : ++n;
	$("#play_text a").eq(n).trigger('click');
}
</script>
