<script>

UI.ready(function() {
    MI.api.type = 7;
    
	// 瀑布流
	    if( new Date -_timeStart > 1000 ){
		MI.bannerTips({
			newTips: { 
				dir     :'up',
				type    :1,
				xy      :[ UI.width( $$('.C_top_nav')[0]) - 278, 85],
				dirStyle:'left:230px',
				style   :"width:279px",
				target  :".top_title .left",
				content :'您的网络状况不佳，建议您切换到阅读模式浏览内容'
			}
		});
		MI.Bos({ id:1220, name:'btnSlowTips'});
	}
    	MI.Crs.disabled = 1;
	var autoUpdate  = true; 
	var param = {
		cid : MI.ChannelInfo.id,
		n   : 10
	};
	var url = UI.parseUrl();    
	for (var urlHash in url) {
		if ('p' == urlHash && url['p'] > 1) {
			autoUpdate = false; 
			break;
		}
		if ('time' == urlHash) {
			continue;
		}
		if ('id' == urlHash) {
			continue;
		}
		if ('filter' == urlHash) {
			continue;
		}
		param[urlHash] = url[urlHash];
	}
	var rel = newlyUrl;
	for (var pr in param){
		rel += pr + "=" + param[pr] + "&";
	}
	rel += 'turn=1';
	
	
    MI.ChannelInfo.personalOrder   = '0';
    MI.ChannelInfo.relationMap     = {};
    MI.ChannelInfo.opeCacheContent = [];
    MI.ChannelInfo.searchPage      = 1+1;
		
	var slideConfig = {
		searchPage     :MI.ChannelInfo.searchPage,              
		listNum        :1000, 
		arynUrl        :rel,
		moreNode       :$('moreList'),
		pageNode       :$('pageNav'),
		cid            :MI.ChannelInfo.id,
		// col            :col,
        haslike        :false,
		version        :version,
		personalOrder  :MI.ChannelInfo.personalOrder,
		relationMap    :MI.ChannelInfo.relationMap,
		opeCacheContent:MI.ChannelInfo.opeCacheContent
	};
    var mcslide = {};
    MI.app({
        'c/mi.cSlide.waterfall' : function(){
            mcslide = new MI.cSlide($('pic-list'), slideConfig);
        }
    });
	
	
	
		

}); //end ui.ready





MI.user.account = 10000;//快速登录
function mb_quick_reg(el,from){
	if(!MI.user.account){		
		var frame = $('QuickLoginFrame');
		if (frame){
			UI.remove(frame);
		}
		MI.dialog.show({title:'<h3 class="DmainTit">微博登录</h3>',end:function(){
        UI.cookie('reg_quick',1,0.42);        },html:'<iframe src="http://mini.t.qq.com/mblogin_quick.htm" frameborder="0" scrolling="no" width="571" height="187" id="QuickLoginFrame"></iframe>',width:620});		
	}
	MI.Bos('btnPortalQuickLogin',from);
}
function mb_quick_reg_call(isReg){
	MI.dialog.hide();
	if (isReg){
        document.location.reload();
	}
	else {
        document.location.href = 'http://t.qq.com/regfriend.php?reg';
	}
}
if(!MI.user.account){
	MI.app({
		Base:function(){
			                if(UI.cookie('mb_reg_quick') == 1){
                    return;
                }if (!UI.cookie('reg_quick')){
                            mb_quick_reg(document.body,'');
                          }                    //cookie:mb_reg_quick弹出之后1小时内不再弹出
                    UI.cookie('mb_reg_quick', 1, 1/24);		}
	});
}
function ptlogin2_onResize(w,h){var F=$('QuickLoginFrame');if(F) F.style.height=h+'px'}
function ptui_notifyClose(){MI.dialog.hide()}
function mb_cbRegister(){MI.dialog.hide()}MI.user.account = 0;

</script>




