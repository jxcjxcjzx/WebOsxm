			return document.getElementById(v);
		};
		this.init = function(){
			var url = window.location.toString();
			var urlif = url.indexOf('?webQQoUt');
			if(urlif != -1){
				var dis = url.substring(url.indexOf('?webQQoUt=')+10).split(",");
				if(dis!=''){
					for(var i=0;i<dis.length;i++){
						this.$(dis[i]).style.display = 'none';
					}
				}
				uedCommonWebqq.fn.LoadJs("http://web.qstatic.com/jsapi/alloy.api.js",function(){
					alloy.onReady(function(){
						alloy.window.setBodySize({
							width: 1000
						});
					});
				})
			}
		}
	}
})(window);
