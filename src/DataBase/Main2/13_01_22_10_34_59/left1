  name : 'financeHQ',
  callback : function(){
    var $ = function(i){return document.getElementById(i);};
    var amtHQ = parseFloat( hq_str_s_sh000001.split(",")[1] ) , rateHQ = parseFloat( hq_str_s_sh000001.split(",")[3] );
    $("text_sh600001_hq").innerHTML = "沪 " + amtHQ.toFixed( 1 ) + "(" + ( rateHQ > 0 ? "+" : ( rateHQ == 0 ? "" : "" ) ) + rateHQ.toFixed(1) + "%)";
    $("text_sh600011_hq").innerHTML = "上基 " + hq_str_s_sh000011.split(",")[1];
  }
});
