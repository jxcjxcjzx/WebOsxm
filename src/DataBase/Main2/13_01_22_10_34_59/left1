  name : 'iplookup',
  callback : function(){
    try {
      if (remote_ip_info.ret==1) {
        var yNav01w3 = document.getElementById('imp_city_gd');
        switch (remote_ip_info.province) {
          case '四川' : yNav01w3.innerHTML = '<a href="http://sc.sina.com.cn/">四川</a>';
            break;
          case '河南' : yNav01w3.innerHTML = '<a href="http://henan.sina.com.cn/">河南</a>';
            break;
          case '云南' : yNav01w3.innerHTML = '<a href="http://qcyn.sina.com.cn/">云南</a>';
            break;
          case '福建' : yNav01w3.innerHTML = '<a href="http://fj.sina.com.cn/">福建</a>';
            break;
          case '陕西' : yNav01w3.innerHTML = '<a href="http://sx.sina.com.cn/">陕西</a>';
            break;
          case '上海' : yNav01w3.innerHTML = '<a href="http://sh.sina.com.cn/">上海</a>';
            break;
            default : break;
        }
      }
    } catch (e) {};
  }
});
