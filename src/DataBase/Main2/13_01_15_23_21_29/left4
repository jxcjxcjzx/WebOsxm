function openkeyword(obj) {
var loc = location.href;
var searchUrl = '';
searchUrl='http://so.cntv.cn/index.php?qtext='+encodeURIComponent(obj)+'&sid=0000&pid=0002';
window.open(searchUrl);
}
function getOnkeyDown(e){
var ev = e;
ev = ev || event;
if(ev.keyCode == 13){
submitSearch();
}
}
function submitSearch(){
