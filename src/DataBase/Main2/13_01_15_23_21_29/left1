<span id="videoWindowWrap"></span>
<script language="javascript" type="text/javascript">
//<![CDATA[
function videoWindowOpenWin(){
  var o = new SinaDotAdJs();//加载通用类
  this.initAd = function(){
	try{var c = document.getElementsByTagName("meta")['publishid'].content.split(',')[0];}catch(e){var c=0;}
	document.write('<script language="javascript" type="text/javascript" src="http://dcads.sina.com.cn/html.ng/c='+c+'&site=sina&type=richmedia&location1='+encodeURI(location1)+'&location2='+encodeURI(location2)+'&loca='+local_index+'&'+Math.random()+'" charset="utf-8"></scr'+'ipt>');
	document.write('<script language="javascript" type="text/javascript" src="http://d2.sina.com.cn/d1images/cpm/vwin.js"></scr'+'ipt>');
  }
  var cookie = o.getAdCookie("sinaVideoAd");
  if(cookie==""){o.setAdCookie("sinaVideoAd","played",10);this.initAd();}
}
videoWindowOpenWin();
//]]>
</script>
