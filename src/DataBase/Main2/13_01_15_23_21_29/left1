                  for(var i=arrWeb1218.length-1;i>=0;i--){
                      if(arrWeb1218[i].pos==li.getAttribute("pos") && arrWeb1218[i].area.substr(local_index,1)=="1")
                      {
                      sp[0].innerHTML='<img width="103" height="68"  border="0" alt="'+arrWeb1218[i].title+'"  src="'+arrWeb1218[i].pic+'"/>';
                      sp[1].innerHTML=WSCSubstr(arrWeb1218[i].title, 0, 16);
                      li.href=arrWeb1218[i].url;
                      if(arrWeb1218[i].type=="S"&&typeof arrWeb1221!="undefined"){
                        for(var j=0;j<arrWeb1221.length;j++){
                          if(arrWeb1221[j].area.substr(local_index,1)=="1"){
                             sp[0].innerHTML='<img width="103" height="68" border="0" alt="'+arrWeb1221[j].title+'" src="'+arrWeb1221[j].picpath+'"/>';
                                 sp[1].innerHTML=WSCSubstr(arrWeb1221[j].title, 0, 16);
                             li.href=arrWeb1221[j].url;
                             break;
                          };
                        };
                      };
                      break;
                      };
                  };
        webShow({'resid': '1231','webs': arrWeb1231,'area': local_index,'rearrange': false,'titlelen': 4});
        webShow({'resid': '1216','webs': arrWeb1216,'area': local_index,'rearrange': false,'titlelen': 16});
        webShow({'resid': '1217','webs': arrWeb1217,'area': local_index,'rearrange': false,'titlelen': 22});
                    if(typeof arrWeb1219 != "undefined"){
                  var arrPad=webShow({'resid':'1','webs':arrWeb1219,'area':local_index,'rearrange':true,'cols':2,'rows':4,'titlelen':16,'action':'data'});
                  if(typeof arrPad.grid!="object"){arrPad.grid=new Array();};
                  var numArr=[0,2,4,6,1,3,5,7];
                  var li=document.getElementsByName("res1216");for(var i=0,j=0;i<li.length;i++){j=numArr[i];if(li[j].getAttribute("webtype")=="S"){while(arrPad.grid.length>0&&(web=arrPad.grid.shift())){if(typeof web.title!='undefined'){li[j].href=web.url;li[j].innerHTML=WSCSubstr(web.title,0,16);break;};};};};
                  };
                  if(typeof arrWeb1220 != "undefined"){
                  var arrPad=webShow({'resid':'1','webs':arrWeb1220,'area':local_index,'rearrange':true,'cols':2,'rows':5,'titlelen':24,'action':'data'});
                  if(typeof arrPad.grid!="object"){arrPad.grid=new Array();};
                  var numArr2=[0,2,4,6,8,1,3,5,7,9];
                  var li=document.getElementsByName("res1217");for(var i=0,j=0;i<li.length;i++){j=numArr2[i];if(li[j].getAttribute("webtype")=="S"){while(arrPad.grid.length>0&&(web=arrPad.grid.shift())){if(typeof web.title!='undefined'){li[j].href=web.url;li[j].innerHTML=WSCSubstr(web.title,0,22);break;};};};};
                  };
                }
              });
            }
          });
          </script>
