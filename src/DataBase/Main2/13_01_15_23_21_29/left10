   /*去左右空格函数*/
function jtrim(sstr){ 
var astr=""; 
var dstr=""; 
var flag=0; 
for (i=0;i<sstr.length;i++){
  if ((sstr.charAt(i)!=' ')||(flag!=0)){
  dstr+=sstr.charAt(i); 
  flag=1; 
   } 
} 

flag=0; 
for (i=dstr.length-1;i>=0;i--){
  if ((dstr.charAt(i)!=' ')||(flag!=0)){
astr+=dstr.charAt(i); 
flag=1; 
   } 
} 

dstr=""; 

for (i=astr.length-1;i>=0;i--)
dstr+=astr.charAt(i); 
    return dstr; 
 }  

  
