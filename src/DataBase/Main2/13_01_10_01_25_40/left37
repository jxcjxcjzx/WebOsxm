					(function(){
						var pthis = this;
						//获取对象
						this.$ = function(id){if(document.getElementById){return eval('document.getElementById("'+id+'")')}else{return eval('document.all.'+id)}};
						//获取cookie
						this.getAdCookie = function(N){
							var c=document.cookie.split("; ");
							for(var i=0;i<c.length;i++){var d=c[i].split("=");if(d[0]==N)return unescape(d[1]);}
							return "";
						};
						//设置cookie
						this.setAdCookie = function(N,V,Q){
							var L=new Date();
							var z=new Date(L.getTime()+Q*60000);	
							//var z=new Date(L.getTime()+Q*1000);	
							document.cookie=N+"="+escape(V)+";path=/;domain=sina.com.cn;expires="+z.toGMTString()+";";		
						};
						this.init=function(S){
							try{
								var curD,curS,rand,styleP;
								curD = new Date();
								curS = curD.getTime();
								rand = Math.random()*10;
								if(!S){
									styleP = rand<5?'A':'B';
								}else{
									styleP = S;
								}

if(styleP=='A'){
	_S_uaTrack("index_sports_AB_test", "0");
	pthis.$('styleA').style.display = '';
	pthis.$('styleB').style.display = 'none';
}else{
	_S_uaTrack("index_sports_AB_test", "1");
	pthis.$('styleA').style.display = 'none';
	pthis.$('styleB').style.display = '';
}

									pthis.setAdCookie("udvABTestCookie",styleP,43200);
							}catch(e){

							}
						}
						 var cookie = pthis.getAdCookie("udvABTestCookie");
						 cookie = cookie==""?0:cookie;
						 pthis.init(cookie);
					})()
