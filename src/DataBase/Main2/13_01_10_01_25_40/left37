<script language="javascript" type="text/javascript">
<!--//--><![CDATA[//><!--
//默认登录状态
if(sina.readCookie("loginType") != ""){
  loginManager.LoginSelect(sina.readCookie("loginType"));
}else{
  setTimeout('document.showLogin.entry.value = "null"',500);
