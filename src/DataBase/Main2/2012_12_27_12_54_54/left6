<script type="text/javascript">
	function isEmail(email){ 
		return /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test(email);
	}
	function isEmpty(value){
		return value == null || value == "";
	}
	function validatePromotionFormElem() {
		var emailAddr = document.whitepaperPromotionForm.emailAddr;
		if(isEmpty(emailAddr.value)){
			alert("邮箱不能为空！");
			emailAddr.focus();
			return false;
		}
		if(!isEmail(emailAddr.value)){
			alert("邮箱格式不正确！");
			emailAddr.focus();
			return false;
		}
		return true;
	} 
	
	function initWhitePaperPromotionBox(){
		if(document.whitepaperPromotionForm && document.whitepaperPromotionForm.whitePaperId && document.whitepaperPromotionForm.emailAddr) {
			document.whitepaperPromotionForm.onsubmit = validatePromotionFormElem;
			document.whitepaperPromotionForm.emailAddr.value = "";
		}
	}
	initWhitePaperPromotionBox();
</script></div>
