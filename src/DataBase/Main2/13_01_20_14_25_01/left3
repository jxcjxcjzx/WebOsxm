  <div class="partAd"><!--950x90轮播通栏 开始 -->
<div id="ad_3468"></div>

<script language="javascript" type="text/javascript">
//<![CDATA[
var rad = new Array();
var nad = new Array();
rad.width = 950; //广告宽度
rad.height = 90; //广告高度
rad.num = 2; //轮播数量

