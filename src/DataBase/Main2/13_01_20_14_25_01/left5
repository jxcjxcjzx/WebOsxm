function show_pongo_1(ad){
	var div = document.getElementById('marqueebox0');
	div.innerHTML = ad;
}
function get_pongo_1(){
	if (!document.body){
		setTimeout('get_pongo_1()',500);
		return;
	}	
	var url = encodeURIComponent(location.href);
	var id = 1;
	var cb = 'show_pongo_1'
	var src = 'http://a.pongo.cn/Job/GetAdForCallBack?id='+id+'&UrlAdParam='+url+'&CallBack='+cb;
	var sc = document.createElement('script');
	sc.type = 'text/javascript';
	sc.src = src;
	document.body.appendChild(sc);
}
setTimeout('get_pongo_1()',1000);
