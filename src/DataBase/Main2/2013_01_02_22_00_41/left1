  ipt.onfocus=function () {
    if (this.value=='������ؼ��') {
      this.value='';
    }
  }
  ipt.onblur = function() {
    if (this.value=='') {
      this.value='������ؼ��'
    }else {
      current_kw = this.value;
    }
  }
  var fm=$('searchbox_con').getElementsByTagName('form')[0];
  sina.addEvent(fm,'submit',function () {
    if (ipt.value=='������ؼ��') {
      ipt.value='';
    }
  })
  if (current_kw) {
    ipt.value = current_kw
  } else {
    current_kw = ipt.value
  };
  if(typeof(scrHotKey) == 'object'){
    scrHotKey.init();
  };
}
function loadAJAXTab(f, e) {
  var h = $(e);
  if (ajccache[f] == null) {
    var g = getXmlhttp();
    g.onreadystatechange = function() {
      if (g.readyState == 4 && (g.status == 200 || window.location.href.indexOf("http") == -1)) {
        h.innerHTML = g.responseText;
        ajccache[f] = h.innerHTML;
        set_kw()
      }
    }
    set_kw();
    g.open("GET", f + "?r=" + urlRdm, true);
    g.send(null)
  } else {
    h.innerHTML = ajccache[f];
    set_kw()
  }
}
window.onerror = function() {
  return true
}
})();
</script>
