                <div class="part-j clearfix">
                    <div class="part-j-l">
                        <!-- mod27 -->
                        <div class="mod-27 mod-02">
                            <div class="tit02">
                                <h3><a href="http://history.sina.com.cn/news/list/2.shtml" target="_blank">历史专栏</a></h3>
                            </div>
                            <div class="mod27-cont" data-sudaclick="blk_history_zl">
