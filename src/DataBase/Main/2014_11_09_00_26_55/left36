document.write('<script type="text/javascript" charset="utf-8" src="http://www.csdn.net/ui/scripts/Csdn/counter.js"></'+'script>');
</script>
<script type="text/javascript">
document.write('<script id="noticeScript" type="text/javascript" charset="utf-8" btnId="header_notice_num" wrapId="note1" count="5" subCount="5"  src="http://csdnimg.cn/rabbit/notev2/js/notify.js?' + (new Date()/120000|0) + '="></'+'script>');
document.write('<script src="http://csdnimg.cn/pubfooter/js/tracking.js?'+(new Date()/120000|0)+'="></'+'script>');
$( function () {
	// swapImg_openLayer.js
    csdn.SwapImage.swap( {
        swapRoot: $( '.hot' )
    } );

    csdn.openLayer.layer( {
        layer: $( '.nav_more' ).find( '.nav_more_list' ),
        openBtn: $( '.nav_more' )
    } );
    
    //搜索
    $( "#srch1" ).keydown( function( event ) {
        if( event.keyCode == 13 )
        {
            var val = $( this ).val();
            val
                ? window.open( 'http://so.csdn.net/Default.aspx?q=' + encodeURIComponent( val ) )
                : alert( '请输入搜索内容' );
        }
    } );
});
</script>
