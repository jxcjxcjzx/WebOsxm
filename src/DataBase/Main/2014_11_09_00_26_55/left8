                        <!-- mod46 -->
                        <div class="mod-02">
                            <div class="tit02"><h3><a target="_blank" href="http://fashion.sina.com.cn/">时尚热点</a></h3></div>
                            <div style="height:8px;" class="blank-cont"></div>
