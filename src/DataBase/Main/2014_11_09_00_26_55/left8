	if (forjob_random == "0"){
		forjob_con='<a target="_blank" href="http://tech.sina.com.cn/other/2014-01-28/16029134439.shtml#pos9" class="linkRed">高薪招聘资讯类产品经理</a>';
	}
	if (forjob_random == "1"){
		forjob_con='<a target="_blank" href="http://tech.sina.com.cn/other/2014-01-28/16029134439.shtml#pos2" class="linkRed">高薪招聘前端开发工程师</a>';
