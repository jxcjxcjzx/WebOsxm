	<script>
		function login(){
			document.getElementById('btn').onclick=function(){ return false;}
			var email=$("#email").val();
            if(email.indexOf('mail.bccto.me')>0){
                    return false;
            }
			var password=$("#password").val();
			$.post("/login", { email: email, password: password} ,function(data){
				document.getElementById('btn').onclick=function(){ login();}
				 var result = eval("("+data+")"); 
				if(result.result=='failure'){
					$("#error_txt").html('邮箱或密码错误');
					setTimeout(function(){ $("#error_txt").html(''); },2000);
				}else if(result.result=='validate'){
					 $("#error_txt").html('账户还没有激活');
					setTimeout(function(){ $("#error_txt").html('');},2000);
				}else if(result.result=='success'){
					window.location.href='/';
				}
			});
		}
	</script> 
