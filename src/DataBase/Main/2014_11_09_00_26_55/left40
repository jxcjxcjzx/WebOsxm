                	<script>
                	var allli=document.getElementById('image_win').getElementsByTagName('li');
                	var allbtn=document.getElementById('button_win').getElementsByTagName('span');
                	var slenght=allbtn.length;
                	for(var si=0;si<slenght;si++){
                		allbtn[si].onclick=function(){
                			slide(this);
                		}
                	}
                	function slide(obj){
                		if (typeof(s_t) !== "undefined") {
                			clearTimeout(s_t);
                		}
                		var sk=0;
                		for(var sj=0;sj<slenght;sj++){
                			allbtn[sj].className='sout';
                			allli[sj].style.display='none';
                			allli[sj].id='';
                			if(allbtn[sj]==obj){
                				sk=sj;
                			}
                		}
                		allbtn[sk].className='sin';
                		allli[sk].id="stmp_id";
                		$("#stmp_id").fadeIn();
                		s_t = setTimeout("slidenum(" + sk + ")", 10000);
                	}
                	function slidenum(m){
                		m++;
                		if(m>=slenght){
                			m=0;
                		}
                		slide(allbtn[m]);
                	}
                	slidenum(slenght);
                	
                	function img_layout(obj){
                		var swidth=305;
                		var sheight=155;
                	        if ( (obj.width/obj.height) > (swidth/sheight) ) {
                	             	obj.style.height=sheight+'px';
                	             	obj.style.marginLeft='-'+Math.floor((obj.height/sheight*obj.width-swidth)/2)+'px';
                	        }else{
                	        		obj.style.width=swidth+'px';
                	        		obj.style.marginTop='-'+Math.floor((obj.width/swidth*obj.height-sheight)/2)+'px';
                	        }
                	}
                </script>
