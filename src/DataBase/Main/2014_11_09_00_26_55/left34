<script type="text/javascript">

cms_follow_videoupdate = cms_follow_videoupdate || {};
cms_follow_videoupdate.show_followed_callback = function(res){
	try {
		res = (typeof res == 'object') ? res : res.stripScripts().evalJSON(1);
	} catch(e) {
		return false;
	}
	if ((res.error != 1) || (typeof res.target === 'undefined')) {
		return false;
	}
	var objectUser		= null;
	for (var index in res.target) {
		objectUser	= res.target[index];
		if ((typeof objectUser.id !== 'undefined') && objectUser.followed) {
			$$('[_cms_subsc_div="' + objectUser.id + '"]').each(function(o){
				$(o).innerHTML = '<span class="v-subactioned">已订阅</span>'
			});
		}
	}
	return true;
}
cms_follow_videoupdate.follow_callback = function(res){
	try {
		res = (typeof res == 'object') ? res : res.stripScripts().evalJSON(1);
	} catch(e) {
		cms_follow_videoupdate.showError('操作失败, 请稍候再试!');
		return false;
	}
	if (res.error != 1) {
		cms_follow_videoupdate.showError(res.zh);
		return false;
	}
	$$('[_cms_subsc_div="' + cms_follow_videoupdate.friendId + '"]').each(function(o){
		$(o).innerHTML = '<span class="v-subactioned">已订阅</span>'
	});
	return true;
}

</script>


