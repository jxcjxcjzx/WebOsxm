function preloader()
{
	var i = 0,
	imageObj = new Image();
	images = new Array();
	urls = new Array();
	text = new Array();
	
	images.push("http://www.gter.net/attachment/slide/2014114/141104161101_xzQum.png");
	urls.push("http://bbs.gter.net/thread-1782960-1-1.html");
	text.push("");
	images.push("http://www.gter.net/attachment/slide/20141031/141031171035_NHyFh.jpg");
	urls.push("http://bbs.gter.net/thread-1712121-1-1.html");
	text.push("");
	images.push("http://www.gter.net/attachment/slide/20141031/141031171005_voTAY.jpg");
	urls.push("http://bbs.gter.net/thread-1727679-1-1.html");
	text.push("");
	images.push("http://www.gter.net/attachment/slide/20141031/141031171035_FZyTG.jpg");
	urls.push("http://bbs.gter.net/thread-1746423-1-1.html");
	text.push("");
	images.push("http://www.gter.net/attachment/slide/20141031/141031171032_aMhVz.jpg");
	urls.push("http://bbs.gter.net/thread-1477840-1-1.html");
	text.push("");

	for(i=0; i<images.length; i++)
	{
		imageObj.src=images[i];
	}
}
preloader()
var autoslide;
function selit(el,val)
{
	var c = el.parentNode.getElementsByTagName(el.tagName);
	for (var i = c.length - 1; i >= 0; i--)
	{
		c[i].className = "";
	};
	el.className = "active"
	document.getElementById("bigimg").setAttribute("href",urls[val]);
	document.getElementById("bigimg").setAttribute("target","_blank");
	document.getElementById("slideshowimage").setAttribute("src",images[val]);
    document.getElementById("slideshowdesc").innerHTML=text[val];
	clearTimeout(autoslide);
	autoslide = setTimeout(function(){
		if(c.length == 1+val)
		{
			selit(c[0],0);
		}
		else
		{
			selit(c[1+val],1+val);
		}
	},5000)
}
</script>

