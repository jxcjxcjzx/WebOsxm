<script>
;(function() {
   var now = new Date();
   //var timestr = now.getFullYear() + prefixInteger(now.getMonth() + 1) + prefixInteger(now.getDate()) + prefixInteger(now.getHours()) + prefixInteger(now.getMinutes()) + prefixInteger(now.getSeconds()); 
   var _dstat = (typeof _dstat === 'object')? _dstat : {}
