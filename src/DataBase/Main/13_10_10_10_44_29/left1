evtclick:function(t){var tmpstr=t.innerHTML;var key=encodeURIComponent(encodeURIComponent(tmpstr));this.input.value=tmpstr;if(this.cache.curVal!=undefined&&tmpstr!=trim(this.cache.curVal)){if(this.cache.listType==='sug'){document.hform_10.Refer.value='sina_sug';}else{document.hform_10.Refer.value = 'sina_hot_sug';}}else{document.hform_10.Refer.value = 'sina_index';}document.hform_10.setAttribute("action",globalConfig.searchURL.replace("@@key@@",key));document.hform_10.submit()},keydownEvt:function(){var me=this;var e=arguments[0]||w.event;if(!!me.cache.keyDownOff){return false}switch(e.keyCode){case 38:var idx,curMaxLen=me.curMaxLen||me.maxLen;me.cache.curindex===-1?idx=curMaxLen-1:idx=me.cache.curindex-1;me.select(idx,true);break;case 40:var idx,curMaxLen=me.curMaxLen||me.maxLen;if(me.cache.curindex===-1){idx=0}else{if(me.cache.curindex===curMaxLen-1){idx=-1}else{idx=me.cache.curindex+1}}me.select(idx,true);break;case 13:break;default:break}},select:function(obj,valchange){var me=this,ele,index,sugitemovercn=sugProFix+"mover",curitem=me.cache.curitem;typeof(obj)==="object"?(function(){ele=obj;index=+ele.getAttribute("index")})():(function(){if(obj===-1){ele=null;index=-1}else{ele=byAttr(me.sugWrap,"index",obj+"")[0];index=+obj}})();!!curitem&&removeClass(curitem,sugitemovercn);if(!!ele){addClass(ele,sugitemovercn);me.cache.curitem=ele;me.cache.curindex=index;if(!!valchange){me.input.value=ele.innerHTML}}else{me.input.value=me.cache.curVal;me.cache.curitem=null;me.cache.curindex=-1}setTimeout(function(){me.setCursorPos(me.input.value.length)},-1)},setCursorPos:function(pos){var ipt=this.input;if(ipt.setSelectionRange){ipt.setSelectionRange(pos,pos);ipt.focus()}else{var range=ipt.createTextRange();range.collapse(true);range.moveEnd("character",pos);range.moveStart("character",pos);range.select()}},createSugList:function(){var me=this,ipt=me.input,uniqueKey=$.util.getUniqueKey();if(!ipt){throw"input is undefined!"}me.sugWrap=document.createElement("DIV");me.sugWrap.cl