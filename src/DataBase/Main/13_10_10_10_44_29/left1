;(function(){
	var API = 'http://ip.house.sina.com.cn/sina_sanshou_2010.php';
	var render = function() {
		for (var i = 0, len = SI_IP_House_.length; i < len; i++) {
			var item = SI_IP_House_[i];
			var node = document.getElementById('SI_IP_House_'+i);
			if (item&&node) {
				node.innerHTML = item;
			}
		}
	};
	jsLoader({
	    name: 'ipHouse',
	    url:API,
	    callback: function() {
	    	render();
	    }
	});
})();

