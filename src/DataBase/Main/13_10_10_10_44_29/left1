				name: 'res_PGLS000022',
				url: "http://pfp.sina.com.cn/pfpnew/merge/res_PGLS000022.js",
				callback: function() {
					jsLoader({
						name: 'res_render',
						callback: function() {
							if (typeof arrWeb1348 != "undefined") {
								ginaWebShow.show({
									resid: 1348,
									rows: 1,
									data: arrWeb1348,
									supData: dupDataResPDPS1348
								});
							}
						}
					});
				}
			});
		}

	}
	/**gina-end**/
	new RotatorAD(rad,nad,'gina_res_show1348');
	}
	});
	}
