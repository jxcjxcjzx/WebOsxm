jsLoader({
    name: 'libweb',
    callback: function() {
        jsLoader({
            name: 'res_PGLS000022',
            callback: function() {
                //for 1327
                if (typeof local_index == "undefined") {
                    local_index = 1;
                }
                var li = document.getElementById("res1327");
                var sp = li.getElementsByTagName("span");
                for (var i = 0, il = arrWeb1327.length; i < il; i++) {
                    if (arrWeb1327[i].area.substr(local_index, 1) == "1")
                    {
                        sp[0].innerHTML = '<img width="103" height="68" border="0" alt="' + arrWeb1327[i].title + '" src="' + arrWeb1327[i].picpath + '"/>';
                        sp[1].innerHTML = WSCSubstr(arrWeb1327[i].title, 0, 16);
                        li.href = arrWeb1327[i].url;
                        break;
                    }
                };
                //for 1326, 1328, 1329
                webShow({
                    'resid': '1326',
                    'webs': arrWeb1326,
                    'area': local_index,
                    'rearrange': false,
                    'titlelen': 4
                });
                webShow({
                    'resid': '1328',
                    'webs': arrWeb1328,
                    'area': local_index,
                    'rearrange': true,
                    'cols': 2,
                    'rows': 4,
                    'titlelen': 16
