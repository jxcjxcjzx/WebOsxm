<div class="f"><script type="text/javascript" src="http://js.jrj.com.cn/common/foot/v2.js" charset="gb2312"></script></div>
<script src="http://js.jrjimg.cn/lib/jquery-1.4.4.min.js" type="text/javascript"></script>
<script  src="http://js.jrjimg.cn/lib/ui/ImageScroll-min.js"></script>	
<script>
	window.onload = function(){
	new JRJ.ui.ImageScroll('viewCon', {delay:2, scrollItemCount:6, speed:10, distance:420,leftBtn:"view_left",rightBtn:"view_right"});	
     var counter = 0;
	  function onScroll(){
	  	$("#logger").val($("#logger").val()+'滚动次数：' + ++counter + '\r\n');
	  }
	}
	
	
/*=====滚动行情=====*/
var url='http://data.share.jrj.com.cn/stocks/pubdata/jrjIndex.js?callback=?';
var indexhtml='<b><a href="{url}" target="_blank">{name} {index}</a></b><em class="{updowncss}">{updownindex}</em>';
indexhtml='<b><a href="{url}" target="_blank">{name}</a>：</b><i class="{updowncss}">{index} {updownindex} {percent}%</i>';
//上值
var shz="000001";
//深值
var szz='399001';
//恒指
var hz='HSI';
//道指
var dz='indu';
//显示指数代码
var allHtml='';
$.ajax({
  type: "GET",
  url: url,
  dataType: "script",
  success:function(){
	//alert('success');
	var shzhi,szhi,hzhi,dzhi;
	if(typeof(jrjIndexData) == "undefined") return;
	var json=eval(jrjIndexData);
	$.each(json,function(i,item){
		//alert("item.code="+item.code)
		if(item.code==shz){
			shzhi="<li>"+assembleIndexHtml_1(item,'沪指','http://stock.jrj.com.cn/share/index/sh000001/index.shtml')+"</li>";	
		}else if(item.code==szz){
			szhi="<li>"+assembleIndexHtml_1(item,'深指','http://stock.jrj.com.cn/share/index/sz399001/index.shtml')+"</li>";	
		}else if(item.code==hz){
	 		hzhi="<li class=\"mss\">"+assembleIndexHtml_2(item,'恒指','http://hk.jrj.com.cn/share/I5001/')+"</li>";	
		}else if(item.code==dz){
	 		dzhi="<li class=\"mss\">"+assembleIndexHtml_2(item,'道指','http://usstock.jrj.com.cn/stocklist/indux.shtml')+"</li>";	
		}
	})
	//alert(allHtml);
	allHtml=shzhi+szhi+hzhi+dzhi;
	//alert(allHtml);
	$("#jrjindex").html($(allHtml));
	var argsScr = {
	     	 	speed:5,
	     	 	timeOut:3000,
	     	 	lineHeight:29,
	     	  	scrollObjId:"jrjindex"
    }
    var scroll = new JRJ.ui.ScrollText(argsScr);
  },
  error:function(){
	alert('error');
  }
});
function assembleIndexHtml_1(item,name,url){
	var returnHtml;
	returnHtml=indexhtml.replace("{url}",url);
	returnHtml=returnHtml.replace("{name}",name);
	returnHtml=returnHtml.replace("{index}",item.price);
	returnHtml=returnHtml.replace("{percent}",item.percent);
	var percent=parseFloat(item.percent);
	//alert(percent);
	if(percent<0)
	   returnHtml=returnHtml.replace("{updowncss}","gre");
	else if(percent>=0)
	   returnHtml=returnHtml.replace("{updowncss}","red");
	
	var diff=parseFloat(item.diff);
	//alert(diff);
	returnHtml=returnHtml.replace("{updownindex}",Math.abs(diff));

        return returnHtml;
}
function assembleIndexHtml_2(item,name,url){
	var returnHtml;
	returnHtml=indexhtml.replace("{url}",url);
	returnHtml=returnHtml.replace("{name}",name);
	returnHtml=returnHtml.replace("{index}",item.price);
	returnHtml=returnHtml.replace("{percent}",item.percent);
	var percent=parseFloat(item.percent);
	//alert(percent);
	  if(percent<0)
           returnHtml=returnHtml.replace("{updowncss}","red");
        else if(percent>=0)
           returnHtml=returnHtml.replace("{updowncss}","gre");
	
	var diff=parseFloat(item.diff);
	//alert(diff);
	returnHtml=returnHtml.replace("{updownindex}",Math.abs(diff));

    return returnHtml;
}
</script>
