<script language="javascript" type="text/javascript" src="http://static.youku.com/v1.0.0844/v/js/swfobject.js"></script>


<script type="text/javascript">
var VideoIDS = "132227637";
var VERSION  = "/v1.0.0844";
var PLAYERVERSION="/v1.0.0310";

function play(){
 	if(!window.SWFObject||!document.getElementById("pageover")){
		setTimeout('play();', 200); 
		return;
	}

	var fo = new SWFObject("http://static.youku.com/v1.0.0310/v/swf/player.swf", "index_player_swf", "100%", "100%", 7, "#FFFFFF");
	fo.addParam("wmode","transparent");
	fo.addParam("quality","high");
	fo.addParam("allowscriptaccess","always");
	fo.addParam("allowfullscreen",'true');
	fo.addVariable("isShowRelatedVideo","false");
	fo.addVariable("VideoIDS",VideoIDS);
	fo.addVariable("isAutoPlay",false);
	fo.addVariable("UserID",0);
	fo.addVariable("isLoop",false);
