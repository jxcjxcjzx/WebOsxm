							<section id="game" class="mods_crosslastsubject_list_cross answer clearfix">
<div class="header">小强填字</div>
<div id="crossCore" onclick="location.href='http://cross.infzm.com/cross/148'" >
<span id="subject_id" style="display:none">148</span>
</div>
<div id="relArticle" class="tab-panel right">
		<div id="answerList" class="mods_crosslastsubject_list_cross answer clearfix">
		</div>
</div>
<div class="clearfix"></div>
<div class="title"><em>最新游戏：</em>韩寒的一部代表作。</div>
</section>
<script>
function mouseMove(ev)
{
	ev= ev || window.event;
	var mousePos = mouseCoords(ev);
	var left = document.getElementById("game").offsetLeft;
	var top = document.getElementById("game").offsetTop + 190 ;
	var obj = document.getElementById('answerList');

	if (mousePos.y > top && mousePos.x>left && mousePos.x - 275<left ){
		obj.style.top = "50px";
	}
	if(mousePos.x - 140 >left && top-140 < mousePos.y &&  mousePos.y< top + 140  ){
		obj.style.left = "23px";
	}
	if(mousePos.y < top){
		obj.style.top = "188px";
	}
	if(mousePos.x - 140 <left && top-140 < mousePos.y&&  mousePos.y < top + 140 ){
		obj.style.left = "153px";
	}

}
function mouseCoords(ev){
	if(ev.pageX || ev.pageY){
		return {x:ev.pageX, y:ev.pageY};
}
	return {
		x:ev.clientX + document.body.scrollLeft - document.body.clientLeft,
		y:ev.clientY + document.body.scrollTop - document.body.clientTop
	};
}
document.onmousemove = mouseMove;
</script>
