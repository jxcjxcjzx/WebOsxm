jQuery(document).ready(function(){
	jQuery(".newslist li:not(:first)").css("display","none");
		var B=jQuery(".newslist li:last");
		var C=jQuery(".newslist li:first");
		setInterval(function(){
			if(B.is(":visible")){
				C.fadeIn(500).addClass("in");
				B.hide(400)
			}else{
				jQuery(".newslist li:visible").addClass("in");
				jQuery(".newslist li.in").next().fadeIn(500);
				jQuery("li.in").hide(400).removeClass("in")
			}
		},3000)
});
