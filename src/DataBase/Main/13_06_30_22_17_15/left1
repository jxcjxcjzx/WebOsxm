	var data_tag = 1;

	var date = new Date();
	var adArr = [];
	var strToDate = function(str,ext){
		var arys = new Array();
		arys = str.split('-');
		var newDate = new Date(arys[0],arys[1]-1,arys[2],9,0,0);
		if(ext){
			newDate = new Date(newDate.getTime()+1000*60*60*24);
		}
		return newDate;
	}
	for(var k=0; k<rad.length; k++){
		var start = strToDate(rad[k][2].replace('<startdate>','').replace('</startdate>',''));
		var end = strToDate(rad[k][3].replace('<enddate>','').replace('</enddate>',''),true);
		if(date>start && date<end){
			adArr.push([rad[k][0], rad[k][1], rad[k][4], rad[k][5]?rad[k][5]:'0']);
		}
	}
	if(adArr.length==0){
		data_tag = 0;
	}
	/**gina-start**/
   if(!(typeof rad!="undefined" && data_tag)){
		if (typeof arrWeb1 != "undefined") {
			var dupDataResPDPS1348 = [];
