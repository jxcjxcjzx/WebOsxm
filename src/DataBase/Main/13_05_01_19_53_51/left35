	<script type="text/javascript">
function tmplfun(o) {var p=[],print=function(){p.push.apply(p,arguments);};p.push('<div id="',o.id,'" token="',o.token,'" class="blog_item">'); if( o.image&&o.image.length > 0){ p.push('<div class="blog_pic" style="height:',parseInt(o.imageInfo[0]["height"]*220/o.imageInfo[0]["width"]),'px"><a target="_blank" href="http://c.t.qq.com/p/33/',o.id,'"><img src="',o.image[0],'/',o.imageInfo[0].type == 2 ? 460 : 220,'"></a></div>'); }else if( o.videos&&o.videos[0].miniPicUrl){ p.push('<div class="blog_pic"><a target="_blank" href="http://c.t.qq.com/p/33/',o.id,'"><img src="',o.videos[0].miniPicUrl,'"><span class="ico_vedio"></span></a></div>'); } p.push('<div class="blog_user clear"><a target="_blank" title="',o.nick,'(@',o.chName||o.name,')" href="http://t.qq.com/',o.name,'" class="user_pic"><img src="',o.pic,'"></a><a target="_blank" title="',o.nick,'(@',o.chName||o.name,')" href="http://t.qq.com/',o.name,'" class="user_name '); if(o.miniUserName){ p.push('mini_user_name'); }p.push('">',o.nick,'</a>',o.icon,''); if( !o.isme){ if( o.isfollow){ p.push('<span class="add_attention added_state"><span class="ico_added"></span>已收听</span>'); }else{ p.push('<a edata="',o.name,'" href="#" class="add_attention add_state js_fol">收听</a>'); }} p.push('</div>'); if( o.content){p.push('<div class="blog_info"><p>',o.content,'</p>'); if(o.chsrc || o.hotSpot) {p.push('<p class="hot_channel">'); if(o.chsrc) {p.push('<a href="/i/',o.chsrc.categoryId,'?pgv_ref=web.c.watrefall.src" title="',o.chsrc.categoryName,'"><span class="ico_bookmark"></span>',o.chsrc.categoryName,'</a>'); } p.push(''); if(o.hotSpot) p.push('<a target="_blank" href="',o.hotSpot.spotUrl,'?pgv_ref=web.c.hotspot" title="',o.hotSpot.spotName,'"><span class="ico_hdot"></span>',o.hotSpot.spotName,'</a></p>'); } p.push(''); if(false && o.commodity && o.commodity.UrlInfos[0].Price > 0) {p.push('<div class="book_shop"><a title="点击购买:',o.commodity.Name,'" target="_blank" href="',o.commodity.UrlInfos[0].Url,'&pgv_ref=c_waterfall2"><i class="trolley"></i><span>',o.commodity.UrlInfos[0].From,'(',o.commodity.UrlInfos[0].Price,'元');if( o.commodity.UrlInfos[0].Discount > 0) {p.push(', ',o.commodity.UrlInfos[0].Discount,'折');}p.push(')</span></a></div>'); } p.push('</div>'); } p.push('<div class="blog_option"><a class="option_item js_comt">评论</a><span class="split"></span><a class="option_item js_relay"><span class="ico_relay"></span>转播<em class="num">',o.counts[0],'</em></a></div></div>');return p.join('');}	
	function formatData( o, k){
		o.isme     = account == o.name;
		o.isfollow = k.relationMap[ o.name];
		o.chsrc    = k.categoryMap[ o.id];
		o.token    = k.token[ o.id];
		o.hotSpot  = k.hotSpots[o.id] && k.hotSpots[o.id].status == 0 ? k.hotSpots[o.id] : 0;
		o.miniUserName = !o.icon ? false : ( o.icon.match(/title/g).length >= 2 ? true : false );
        if(k.commodity && k.commodity[ o.id])
        {
            o.commodity = k.commodity[ o.id];
        }
	}

	for( var i = 0; i < 3; i++){
		var o = messageInfo.info.talk[i];
		if( !o)
			break;
		formatData( o, messageInfo.info);
		document.getElementById( 'col' + parseInt(i +1)).innerHTML = tmplfun( o);
	}


