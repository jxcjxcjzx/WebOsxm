var MYYOUKU = {
   
    // Config
    MAXHEIGHT : 320,
    ANONYMOUSMAXHEIGHT : 200,
    SHOWNUMBER :7,    
    ANONYMOUSRECOMMENDNUMBER : 4,
    MORECOUNT : 1,
    ANONYMOUSMORECOUNT : 2,
    MAXSHOWNUMBER : 2,
    GETANONYMOUSDATANUMBER : 0,
    TRYTIMES : 3,
    CURRENTRECORDSTATUS : 0,
    CURRENTRECOMMENDSTATUS : 0,
    CURRENTSTAT : 0,
    TOKEN : 0,
    RECOMMENDCHANCE : 7,
    CURRENTINDEX : 0,

    // Timer
    timer : null,
    timerData : null,
    timerAnonymous : null,
    timerTabCheck : null,
    timeSpan : 1000,

    // Data
    dataList : [],
    dataRecommend : [], 
    dataShow : [], 
    dataShowInsert : [],
    dataRecords : [],
    dataSubscribe : [],
    dataCollect : [],
    dataAnonymous : [],

    // Dom
    dom : null,
    domList : null,
    domSubscribe : null,
    domCollect : null,
    domRecords : null,
    domShowMore : null,
    domUser : null,

    // Cookies
    cookieYsuid : '',
    cookieUid : '',
    cookieYktk : '',

    // Lock
    lockVideo : true,
    lockSubscribe : true,
    lockCollect : true,
    lockLogin : true,
    lockData : true,
    lockRecords : true,
    lockAnonymousRecommend : true,

    // Data interface
    getRecordJson : 'http://nc.youku.com/index_cookielist/s/jsonp?callback=MYYOUKU.getRecordsData&timestamp=',
    getRecommendJson : 'http://nc.youku.com/index_videocommend?callback=MYYOUKU.getRecommendData&pz=60&timestamp=',
    getCollectJson : 'http://nc.youku.com/index/fav?pz=20&pn=1&callback=MYYOUKU.getCollectData&timestamp=',
    getSubJson : 'http://nc.youku.com/index/subscribe?pz=20&pn=1&callback=MYYOUKU.getSubscribeData&timestamp=',
    getAnonymousRecommendJson : 'http://nc.youku.com/index_videocommend?callback=MYYOUKU.getAnonymousRecommendData&pz=30&timestamp=',
   
    // URL
    urlVideo : 'http://v.youku.com/v_show/id_',
    urlUser : 'http://i.youku.com/u/home',
    urlHz : 'http://hz.youku.com/red/click.php?tp=1&cp=',
    urlOfficial : 'http://i.youku.com/u/official',

    // Tab Temporary Solution
    theTabs : ['tab720911','tab720912','tab720913'],
    tabTypeSub : 0, 
    tabTypeColl : 0,

    // Statistics
    hzCode : {
        hzUsername : '4007879',
        hzAvatar : '4008071',
        hzChange : '4007092',
        hzMore : '4006771',

        hzShowThumb : ['4007880','4007882','4007884','4007886'],
        hzShowTitle : ['4007881','4007883','4007885','4007887'],
        
        hzRecommendThumb : ['4006772' ,'4006774' ,'4006776' ,'4006778' ,'4006780' ,'4006782' ,'4006784' ,'4006786' ,'4007888', '4007890','4007892', '4007894'],
        hzRecommendTitle : ['4006773' ,'4006775' ,'4006777' ,'4006779' ,'4006781' ,'4006783' ,'4006785' ,'4006787' ,'4007889', '4007891','4007893', '4007895'],

        hzSubscribeThumb : ['4007897','4007900','4007903','4007906','4007909'],
        hzSubscribeTitle : ['4007898','4007901','4007904','4007907','4007910'],
        hzSubscribeOwnerName : ['4007899','4007902','4007905','4007908','4007911'],
        hzSubscribeHallOfFame : '4007912',

        hzCollectThumb : ['4007914','4007917','4007920','4007923','4007926'],
        hzCollectTitle : ['4007915','4007918','4007921','4007924','4007927'],
        hzCollectOwnerName : ['4007916','4007919','4007922','4007925','4007928'],
        hzCollectHallOfFame : '4007929',

        hzRecordsThumb : ['4007930','4007931','4007932','4007933','4007934'],
        hzRecordsTitle : ['4006806' ,'4006810' ,'4006814' ,'4006818'],
        hzRecordsDel : ['4006807' ,'4006811' ,'4006815' ,'4006819'],
        hzRecordsGo : ['4006808' ,'4006812' ,'4006816' ,'4006820'],
        hzRecordsNext : ['4006809' ,'4006813' ,'4006817' ,'4006821'],

        hzAnonymousThumb : ['4007104' ,'4007106' ,'4007108' ,'4007110'],
        hzAnonymousTitle : ['4007105' ,'4007107' ,'4007109' ,'4007111']
    },

    returnHzUrl : function (index, url) {
        var hz = '';
        if(!!index) { hz += this.urlHz + index  + '&cpp=1000648&url='; } else { hz = '';};
        if(!url) { url = ''; };
        return  hz + url; 
    },

    // IE Hack
    isIE : document.all,

    init : function() {
        var _this = this;
        // init dom
        _this.dom = document.getElementById('MYYOUKU');
        _this.domMore = document.getElementById('MYYOUKU-ShowMore');
        _this.domList = document.getElementById('MYYOUKU-ShowVideo');
        _this.domCaption = document.getElementById('MYYOUKU-Caption');
        // get cookies
        _this.cookieYsuid = Nova.Cookie.get('__ysuid');
        _this.cookieUid = Nova.Cookie.get('_l_lgi');
        _this.cookieYktk = Nova.Cookie.get('yktk') == null?0:1;
        // logon
        if(_this.cookieYktk) {
            _this.domSubscribe = document.getElementById('MYYOUKU-Subscribe');
            _this.domCollect = document.getElementById('MYYOUKU-Collect');
            _this.domRecords = document.getElementById('MYYOUKU-Records');
            _this.getJson(_this.getRecordJson);
            _this.getJson(_this.getCollectJson);
            _this.getJson(_this.getSubJson);
            try {
                _this.timerData = setInterval(function(){
                    if(!_this.lockRecords && !_this.lockRecommend) {
                        _this.initShowVideoDom(_this.dataList, 0, _this.SHOWNUMBER);
                        _this.initRecordsDom(_this.dataRecords);
                        clearInterval(_this.timerData);
                        _this.lockData = false;
                    }
                }, _this.timeSpan);
                _this.timer = setInterval(function(){
                    if(!_this.lockData && !_this.lockVideo) {
                        _this.showContent(_this.MAXHEIGHT);
                        clearInterval(_this.timer);
                    }
                }, _this.timeSpan);
                // hide Tab and Content
                _this.timerTabCheck = setInterval(function(){
                    if(!_this.tabTypeSub) {
                        if(!_this.tabTypeColl) {
                            QIndex.switchTab(_this.theTabs['2'],_this.theTabs['0']);
                        }
                        else {
                            QIndex.switchTab(_this.theTabs['1'],_this.theTabs['0']);
                        }
                        clearInterval(_this.timerTabCheck);
                    }
                }, _this.timeSpan);
            }
            catch(e) {}
        } else {
            if(_this.cookieYsuid.substring(12,13) <= _this.RECOMMENDCHANCE) {
                _this.tabsDOM = document.getElementById('MYYOUKU_tabs');
                _this.getJson(_this.getAnonymousRecommendJson);
                _this.timerAnonymous = setInterval(function(){
                    if(!_this.lockAnonymousRecommend && !_this.lockLogin) {
                        _this.showContent(_this.ANONYMOUSMAXHEIGHT);
                        clearInterval(_this.timerAnonymous);
                    }
                }, _this.timeSpan);
           }
        }
        _this.bind();
    },

    // Bind
    bind : function() {
        var _this = this;
        this.domMore.onclick = function() {
            _this.changeMore(this);
        }       
    },

    bindDelete : function(obj) {
        var lists = obj.getElementsByTagName('UL');
        for(var i = 0; i < lists.length; i++) {
            if(lists[i].className.indexOf('playrecord') >=0 ) {
                lists[i].onmouseover = function() {
                    var cl = this.getElementsByTagName('LI');
                    for(var j = 0; j < cl.length; j++) {
                        if(cl[j].className.indexOf('rec_close') >= 0) {
                            cl[j].style.display = 'block';
                        }
                    }
                }
                lists[i].onmouseout = function() {
                    var cl = this.getElementsByTagName('LI');
                    for(var j = 0; j < cl.length; j++) {
                        if(cl[j].className.indexOf('rec_close') >= 0) {
                            cl[j].style.display = 'none';
                        }
                    }
                }
            }
        }
    },

    // Get Data
    getRecordsData : function(data) {
        if(!data || data.playtag.length == 0 && data.playtag_show.length == 0) {
            if(this.CURRENTRECORDSTATUS < this.TRYTIMES) {
                this.tryRequest(this.getRecordJson);
                this.CURRENTRECORDSTATUS++;
            }
            else {
                this.initData(data.playtag_show, this.dataShow);
                this.checkShowData(this.dataShow);
                this.initData(this.dataShowInsert, this.dataList, this.MAXSHOWNUMBER);
                this.initData(data.playtag, this.dataRecords);
                this.lockRecords = false;
                this.getJson(this.getRecommendJson);      
            }
        }
        else {
            this.initData(data.playtag_show, this.dataShow);
            this.checkShowData(this.dataShow);
            this.initData(this.dataShowInsert, this.dataList, this.MAXSHOWNUMBER);
            this.initData(data.playtag, this.dataRecords);
            this.TOKEN = data.token;
            this.lockRecords = false;
            this.getJson(this.getRecommendJson);      
        }
   
    },

    getRecommendData : function(data) {
        if(!data || data.length < this.SHOWNUMBER) {
            if(this.CURRENTRECOMMENDSTATUS < this.TRYTIMES) {
                this.tryRequest(this.getRecommendJson);
                this.CURRENTRECORDSTATUS++;
            }
            else {
                this.hideDom(this.dom);     
                return false;
            }
        }
        else {
            this.initData(data, this.dataList);
            this.lockRecommend = false;
        }
    },

    getSubscribeData : function(data) {
        if(!data.results) {
            this.initUserInfo(data.user.name, data.user.avatar);
            this.initSubscribeDom(this.dataSubscribe);
        }
        else {
            this.initData(data.results, this.dataSubscribe);
            this.initSubscribeDom(this.dataSubscribe);
            this.initUserInfo(data.user.name, data.user.avatar);
        }
    },
    
    getCollectData : function(data) {
        if(!data.results) {
            this.initCollectDom(this.dataCollect);
        }
        else {
            this.initData(data.results, this.dataCollect);
            this.initCollectDom(this.dataCollect);
        }
    },

    getAnonymousRecommendData : function(data) {
        var len = data.length;
        if(len > this.ANONYMOUSRECOMMENDNUMBER) { 
            this.initData(data, this.dataAnonymous);
            this.initAnonymousRecommendDom(this.dataAnonymous);
        }
        else {
            if(this.GETANONYMOUSDATANUMBER < this.TRYTIMES) {
                this.getJson(this.getAnonymousComJson + this.randomChar(10));   
                this.GETANONYMOUSDATANUMBER++;
            }
        }
    },

    // 3 Times
    tryRequest : function(url) {
       this.getJson(url); 
    },

    // Init Data
    initData : function(data, o, num) {
        var dataLen = data.length;
        if(!!num && num < dataLen ) { len = this.MAXSHOWNUMBER; } else { len = dataLen; }
        for(var i = 0; i < len; i++) { o.push(data[i]); };
    },

    // Check Data
    checkShowData : function(data) {
        for(var i = 0; i < data.length; i++) {
            if(data[i].showcategory == '电影') {
                if(parseInt(data[i].percent) < 95) {
                    this.dataShowInsert.push(data[i]);     
                }       
            } 
            else {
                this.dataShowInsert.push(data[i]);     
            }
        }
       
    },

    // Hide Dom
    hideDom : function(dom) {
       dom.style.display = 'none'; 
    },

    // Init Dom
    initUserInfo : function(name, avatar) {
        name =  this.removeHTMLTag(name);
        var len = name.length;
        if(len < 8 && this.GetChineseCount(name) <= 5) {
            sname = name + ', ';
        } 
        else {
            if(this.GetChineseCount(name) > 5) {
                sname = name.substring(0,5) + '... ';     
            }
            else {
                sname = name.substring(0,8) + '... ';     
            }
        }
        this.domCaption.innerHTML = '<div class="avatar"><a href="' + this.urlHz + this.hzCode.hzAvatar +'&cpp=1000648&url=' + this.urlUser + '" target="_blank"><img src="' + avatar + '" title="' + name + '" /></a></div><h3 class="title title_my"><a href="' + this.urlHz + this.hzCode.hzUsername + '&cpp=1000648&url=' + this.urlUser + '" target="_blank">' + sname + '为你推荐</a></h3><div class="clear"></div>';
    },

    initShowVideoDom : function(data, start, end) {
        var _this = this;
        var str = '';
        var hzcount = 0;
        var lastThumb = '';
        var folderid = '';
        var tModule = 1;
        var showCount = 0;
        var update = '';
        // For IE
        var imglistid = [];
        var imglistsrc = [];

        for(var i = start ; i < end ; i++) {
            var index = 0;
            var stat = '';
            var vtitle = _this.removeHTMLTag(data[i].title);
            var hzThumb = '';
            var hzTitle = '';
            if(_this.cookieYktk) {
               hzcount = i%_this.SHOWNUMBER;
               lastThumb = _this.hzCode.hzRecommendThumb[_this.SHOWNUMBER];
               hzThumb = _this.hzCode.hzRecommendThumb[hzcount];
               hzTitle = _this.hzCode.hzRecommendTitle[hzcount];
            } 
            else {
                tModule = 3
                _this.cookieUid = '';
                index = i%4;
                hzThumb = _this.hzCode.hzAnonymousThumb[index];
                hzTitle = _this.hzCode.hzAnonymousTitle[index];
            }
            if(!!data[i].ver) {
                var logdatastr = '{"uid":"' + _this.cookieUid  + '","cookie_id":"' + _this.cookieYsuid + '","apptype":"1","page":"8","module":"' + tModule + '","dvid":"' + data[i].id + '","abver":"' + data[i].ver + '","dma":"' + data[i].dma + '","pos":"' + i + '","ord":"0","dct":"' + data[i].dct + '"}';
                logdatastr = encodeURIComponent(logdatastr);
                str += '<ul class="v" logdata="' + logdatastr + '">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(hzThumb, data[i].videolink) + '" target="video" title="' + vtitle + '" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>'; 
                if(_this.isIE){
                    imglistid.push(data[i].videoid);
                    str += '<li class="v_thumb" _src="' + data[i].thumburl + '" id="' + data[i].videoid + '"></li>';
                } else {
                    str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" title="' + vtitle + '" alt="' + vtitle + '" /></li>';
                }
                if(data[i].ishd) {
                    str += '<li class="v_ishd"><span class="ico__HD" title="高清"></span></li>';
                }
                else if(data[i].issd) {
                    str += '<li class="v_ishd"><span class="ico__SD" title="超清"></span></li>';
                }
                str += '<li class="v_menu" id="PlayListFlag_' + data[i].videoid + '" style="display: none; "><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '<li class="v_title"><a href="' + _this.returnHzUrl(hzTitle ,data[i].videolink) + '" target="video" title="' + vtitle + '" onclick="MYYOUKU.submitVideoCommend(this)">' + data[i].title + '</a></li>';
                str += '<li class="v_stat"><span class="ico__statplay" title="播放"></span><span class="num">' + this.formatnum(data[i].total_vv) + '</span> <span title="评论" class="ico__statcomment"></span><span class="num">' + this.formatnum(data[i].total_comment) + '</span></li>';
                str += '</ul>';
                if(_this.cookieYktk) {            
                    if(i+1 == end) {
                        var lastLogdatastr = encodeURIComponent('{"abver":"' + data[i].ver + '","dma":"' + data[i].dma + '","ord":"0","dct":"' + data[i].dct + '"}');
                        var uReUrl = _this.urlUser + '?videoid=' + data[i+1].videoid + '&logdata=' + lastLogdatastr;
                        str += '<ul class="p">';
                        str += '<li class="p_link"><a title="查看更多推荐" href="' + _this.returnHzUrl(lastThumb ,uReUrl) + '" target="_blank" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>';
                        if(_this.isIE){
                            str += '<li class="p_thumb" id="' + data[i+1].videoid + '" _src="' + data[i+1].thumburl + '"><img alt="查看更多推荐"></li>';
                            imglistid.push(data[i+1].videoid);
                        } else {
                            str += '<li class="p_thumb"><img src="' + data[i+1].thumburl + '" title="' + vtitle + '" alt="' + vtitle + '" /></li>';
                        }
                        str += '<li class="p_status"><span class="status">查看更多推荐</span><span class="bg"></span></li>';
                        str += '</ul>';
                    }
                }

            } else {
                str += '<ul class="p">';
                str += '<li class="p_link"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzShowThumb[i] , _this.urlVideo + data[i].vidEncoded) + '?firsttime=' + data[i].sec + folderid +'"></a></li>';
                str += '<li class="p_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '"></li>';	
                if(!!data[i].nextid || !!data[i].watchStage) { 
                    if(!!data[i].updateStage) {
                       update = data[i].updateStage;
                    } 
                    else {
                       update = data[i].watchStage;
                    }
                    str += '<li class="p_status"><span class="status">' + update + '</span><span class="bg"></span></li>';
                } else { 
                    str += '<li class="p_status"><span class="status">' + data[i].watchStage + '</span></li>';
                }
                str += '<li class="p_ishd">' + data[i].definition + '</li>';                
                str += '<li class="p_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzShowTitle[i] , _this.urlVideo + data[i].vidEncoded)  + '?firsttime=' + data[i].sec + folderid +'">' + data[i].showname + '</a></li>';	
                switch (parseInt(data[i].percent)){
                    case 100 :
                        str += '<li class="p_record"><span>已在' + data[i].devicename + '上看完</span></li>';
                        break;
                    case 0 :
                        if(data[i].nextid) {
                            str += '<li class="p_record"><span>已看过' + data[i].watchStage + '</span></li>';
                        }
                        else {
                            str += '<li class="p_record"><span>已在' + data[i].devicename + '上看过</span></li>';
                        }
                        break;
                    default :
                        var tmpStr = '';
                        if(!!data[i].watchStage) {
                            tmpStr = '已看到' + data[i].watchStage + '的' + data[i].percent;
                        }
                        else {
                            tmpStr = '已看到' + data[i].percent;
                        }
                        str += '<li class="p_record"><span>' + tmpStr + '</span></li>';
                }
                str += '</ul>';
                showCount++;
            }
        } 
        _this.domList.innerHTML = str;
        if(_this.isIE){
            _this.getImages(imglistid);
        }
        _this.lockVideo = false; 
    },

    initSubscribeDom : function(data) {
        var _this = this;
        var len = data.length;
        var str = '';
        
        if(!len) {
           str += '<div class="recordnull"><div class="recordinfo"><p>你还没有订阅过任何内容</p><p>来看看这些精彩的视频官网吧</p></div><div class="form_btn form_btn_s form_btnsub_s"><span class="form_btn_text"><a href="' + _this.returnHzUrl(_this.hzCode.hzSubscribeHallOfFame, _this.urlOfficial) + '" target="_blank">立即查看</a></span></div></div>';
        } 
