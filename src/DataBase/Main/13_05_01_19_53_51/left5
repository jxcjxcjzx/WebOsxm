                str += '</ul></li>';
                str += '<li class="rec_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzSubscribeTitle[i], _this.urlVideo + data[i].videoid) + '">' + vtitle + '</a></li>';
                str += '<li class="rec_username">' + data[i].usercate + ' <a href="' + _this.returnHzUrl(_this.hzCode.hzSubscribeOwnerName[i], data[i].userlink) + '" target="_blank" title="' + _this.removeHTMLTag(data[i].username) + '">' + _this.removeHTMLTag(data[i].username) + '</a></li>';
