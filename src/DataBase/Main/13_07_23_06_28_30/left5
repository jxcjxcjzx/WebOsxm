<script type="text/javascript">
$(document).ready(function() {
	if ($.cookie(COOKIE_PRE+'auth')) {
		$('#login_status').show();
		var username = $.cookie(COOKIE_PRE+'username');
		if(!username) username = $.cookie(COOKIE_PRE+'rememberusername');
		$('#login_status em').html(username);
	} else {
		$('#logout_status').show();
	}
	// 获取登录并且绑定了新浪微博用户的未读消息
	function sina_unread() {
    	$.getJSON(
        		"http://app.cankaoxiaoxi.com/?app=member&controller=index&action=sina_unread&jsoncallback=?",
        		{dowhat: "unread"},
        		function(json) {
        			if(json.state) {
        				$(".CP_w").show();
        				if(json.data.followers != 0) {
        					$(".wp_fans").show().find("a").html(json.data.followers+'个微博新粉丝');
        				};
        				if(json.data.comments != 0) {
        					$(".wp_comments").show().find("a").html(json.data.comments+'条微博新评论');
        				};
        				if(json.data.mentions != 0) {
        					$(".wp_atme").show().find("a").html(json.data.mentions+'条微博提到你');
        				};
        			}
        		}	
        	);   		
	}
	sina_unread();
	$(".close").click(function(){
		$(".CP_w").hide();
		return false;
	});	
});
</script> 
