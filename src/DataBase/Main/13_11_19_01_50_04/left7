var arrWeb1 = new Array(),fps = new Array();arrWeb1.push({"pos":"1" ,"url":"http://sina.allyes.com/main/adfclick?db=sina&bid=601707,669863,675126&cid=0,0,0&sid=680006&advid=18500&camid=106886&show=ignore&url=http://www.70good.com/ad/xjx/","pic":"http://d2.sina.com.cn/201310/17/516726_shouyeanniu06-chongqing.gif","area":"0010000000000000000000000000000000"});var fps = webShow({'resid':'PDPS000000000001','webs':arrWeb1,'area':local_index,'city':location2,'rearrange':false,'fixedinc':true,'type':'pic','action':'data'});try {var content = fps.fixed['1'];if(content.pic != '') {rad.push([content.pic, content.url, "<startdate>2013-10-16</startdate>", "<enddate>2013-12-15</enddate>", ""]);}}catch(e){}<!--$$ 蔡贝贝/2013-10-16 ~ 2013-11-15/B $-->

	//垫底广告
	<!--垫底广告开始-->

	var data_tag = 1;

	var date = new Date();
	var adArr = [];
	var strToDate = function(str,ext){
		var arys = new Array();
		arys = str.split('-');
		var newDate = new Date(arys[0],arys[1]-1,arys[2],9,0,0);
		if(ext){
			newDate = new Date(newDate.getTime()+1000*60*60*24);
		}
		return newDate;
	}
	for(var k=0; k<rad.length; k++){
		var start = strToDate(rad[k][2].replace('<startdate>','').replace('</startdate>',''));
		var end = strToDate(rad[k][3].replace('<enddate>','').replace('</enddate>',''),true);
		if(date>start && date<end){
			adArr.push([rad[k][0], rad[k][1], rad[k][4], rad[k][5]?rad[k][5]:'0']);
		}
	}
	if(adArr.length==0){
		data_tag = 0;
	}
	/**gina-start**/
   if(!(typeof rad!="undefined" && data_tag)){
		if (typeof arrWeb1 != "undefined") {
			var dupDataResPDPS1348 = [];
			function pushResPDSRes(arr, pos) {
				 if (arr) {
					 for (var i = 0, len = arr.length; i < len; i++) {
						 if (arr[i]) {
							 arr[i].pos = pos;
							 dupDataResPDPS1348.push(arr[i]);
						 }
					 }
				 }
			}
			pushResPDSRes(arrWeb1, 1);
			arrWeb1 = [];
			jsLoader({
				name: 'res_PGLS000022',
				url: "http://pfp.sina.com.cn/pfpnew/merge/res_PGLS000022.js",
				callback: function() {
					jsLoader({
						name: 'res_render',
						callback: function() {
							if (typeof arrWeb1348 != "undefined") {
								ginaWebShow.show({
									resid: 1348,
									rows: 1,
									data: arrWeb1348,
									supData: dupDataResPDPS1348
								});
							}
						}
					});
				}
			});
		}

	}
	/**gina-end**/
	new RotatorAD(rad,nad,'gina_res_show1348');
	}
	});
	}
});
                        </script>

