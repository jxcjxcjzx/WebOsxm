var BDECPager=function(a){this.data=[];this.pagenow=-1;this.pagetotal=0;this.perpage=parseInt(a.perpage);(this.pagehost=document.getElementById(a.domid))&&this.init()};
BDECPager.prototype={init:function(){this.dom={};for(var a=this.pagehost.getElementsByTagName("div"),c=0,d=a.length;c<d;c++){var b=a[c];if(b.className&&"items"==b.className){this.dom.items=b;break}}if(this.dom.items){a=this.dom.items.getElementsByTagName("ul");c=0;for(d=a.length;c<d;c++)b=a[c],b.className&&"item"==b.className&&this.data.push(b);this.data.length&&(this.dom.pager=document.createElement("div"),this.dom.pager.className="qPage",this.dom.pager_stat=document.createElement("div"),this.dom.pager_stat.className=
"stat",this.dom.pager_turn=document.createElement("div"),this.dom.pager_turn.className="turn",this.dom.pager_prev=document.createElement("div"),this.dom.pager_prev.className="pre pre_disabled",this.dom.pager_next=document.createElement("div"),this.dom.pager_next.className="next next_disabled",this.dom.pager_turn.appendChild(this.dom.pager_prev),this.dom.pager_turn.appendChild(this.dom.pager_next),this.dom.pager.appendChild(this.dom.pager_stat),this.dom.pager.appendChild(this.dom.pager_turn),this.pagehost.appendChild(this.dom.pager),
this.pagetotal=Math.ceil(this.data.length/this.perpage),this.isde=0==this.data.length%this.perpage?!0:!1,this.bind())}},bind:function(){function a(b,c){if(1==b)return 1;var d=parseInt(Math.random()*b)+1;return c?d:d!=b?d:a(b,c)}var c=this;if(0!=this.pagetotal){1<this.pagetotal&&(this.dom.pager_prev.className="pre",this.dom.pager_prev.innerHTML='<a href="#;"></a>',this.dom.pager_next.className="next",this.dom.pager_next.innerHTML='<a href="#"></a>',this.dom.pager_prev.getElementsByTagName("A")[0].onclick=
function(){c.show(c.pagenow-1);return!1},this.dom.pager_next.getElementsByTagName("A")[0].onclick=function(){c.show(c.pagenow+1);return!1});var d=a(this.pagetotal,this.isde);this.show(d)}},show:function(a){if(a!=this.pagenow){a>this.pagetotal?a=1:1>a&&(a=this.pagetotal);for(;this.dom.items.firstChild;)this.dom.items.firstChild.parentNode.removeChild(this.dom.items.firstChild);for(var c=a*this.perpage,d=(a-1)*this.perpage;d<c;d++)this.data[d]&&(this.data[d].style.display="block",this.dom.items.appendChild(this.data[d]));
this.dom.pager_stat.innerHTML=a+"/"+this.pagetotal;this.pagenow=a;return!1}}};window.nova_init_hook_adbanner=function(){if(-1!=navigator.userAgent.indexOf("iPod")||-1!=navigator.userAgent.indexOf("iPad")||-1!=navigator.userAgent.indexOf("iPhone")||navigator.userAgent.match(/Android/i)){document.getElementById("ab_173").innerHTML='<a style="display:block;width:300px;height:245px;margin:0 auto;" href="http://www.soku.com" target="_blank"><img src="http://res.mfs.ykimg.com/051000004F16874E97927318530CEC09.jpg" width="300" height="245" /></a>';var a=document.getElementById("m_52769");
""==a.innerHTML&&(a.innerHTML='<a style="display:block;width:600px;height:90px;margin:0 auto;" href="http://mobile.youku.com/index/wireless" target="_blank"><img src="http://res.mfs.ykimg.com/051000005175F5BB6714C049D603D092.png" width="600" height="90" /></a>')}};(function() {var count = 0; function focusSearch() {s = document.getElementById('headq'); while(count++ < 3 && (!s || s.nodeType != 1)) {return setTimeout(focusSearch, 100);} s.focus();} focusSearch();})();


