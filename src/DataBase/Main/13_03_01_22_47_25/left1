				jsLoader({url:"http://auto.sina.com.cn/867/2013/0105/20.js"});
              break;

            default : break;
          }
          // bj
        /*
          if (remote_ip_info.province!='北京'){
            jsLoader({url:"http://www.sina.com.cn/js/67/2009/1014/8.js"});
          }
        */
        }
      }
    });
  }
});
//--><!]]>
</script>
<!-- yocc 检查地域 end -->

