}, 0);
}
function cmplu() {
var a=location.hash;
if (!a) {
if (location.href.search(/\/[0-9][0-9][0-9][0-9][0-9][0-9]\/p[0-9]*$/) != -1) {
var pa = location.href.split('/');
var di = pa.length - 2;
var na = location.href + '#a' + pa[di] + pa[di+1];
window.location.replace(na);
}
}
}
