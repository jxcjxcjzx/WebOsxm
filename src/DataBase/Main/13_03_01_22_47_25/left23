<script>
	$(function(){
		var $startStopSpan = $(document.createElement('div'));
		$('#billboard').anythingSlider({
		 	mode            : "fade",
			autoPlay		: true,
			pauseOnHover    : false,
			enableArrows	: false,
			hashTags		: false,
			delay           : 9000,
			startText       : "",   // Start button text 
  			stopText        : "",    // Stop button text 
  			buildStartStop      : true,
		 	appendStartStopTo: $startStopSpan,
    		
    		navigationFormatter: function(index, panel) {
    			if(index == 1){
    				$('.anythingControls').prepend($startStopSpan);
    			}
        		return "<img src='"+ $("#sliderThumb" + (index-1)).children().attr('src') + "'>";
    		}
		});
	});
</script>
