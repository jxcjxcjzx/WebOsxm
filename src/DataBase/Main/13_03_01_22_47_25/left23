						<div class="div-pad3b">
						<h4>2013 IEEE/CPMT 29th Semiconductor Thermal Measurement &amp; Management Symposium (SEMI-THERM)                                                                                                                                                                                                                   </h4>
							<p class="note">
			
				    			17 Mar - 21 Mar 2013
				    			 | 
				    			San Jose, CA, USA
							</p>
							<p class="arrow-link"><a href="/conferences_events/conferences/conferencedetails/index.html?Conf_ID=30533">Learn more about this conference</a></p>	
						<script language="javascript">		
		
		var jsonLink = '[]';
	var colCount = '1';
	var rowNum = 0;
	var j = 0;
	var i = 0;
	var externalimg = '';
	
	link = eval("(" + (jsonLink) + ")");
	rowNum = Math.ceil(link.length/colCount);
	<!--@commentText='Number of items to go in the count'-->
	var rowLnkCount = rowNum;
	var colLnkCount = colCount;
	document.write('<div class="content-1col">');
	while (i<link.length)
	{
	    <!--@commentText='If statement added for defect 231'-->
	    if (colCount>1) { document.write('\n<div class="content-3col">\n');}	
		else { document.write('\n<div>\n');}		
		
		document.write('<ul class="arrow-link">\n');
		while (i<link.length && (j<rowLnkCount))
		{	
            if (link[i].linkwtid == undefined || link[i].linkwtid == '')
			{
				document.write('<li>\n');
				document.write('<a href="' + link[i].linkurl + '" target="' + link[i].linktarget + '">');
				document.write(link[i].linktext);
				document.write('</a>\n');
				document.write('</li>\n');
			}
			else
			{
				if (!link[i].linkexternal)
				{
					document.write('<li>\n');
					document.write('<a href="' + link[i].linkurl + '?WT.mc_id=' + link[i].linkwtid + '" target="' + link[i].linktarget + '">');
					document.write(link[i].linktext);
					document.write('</a>\n');
					document.write('</li>\n');
				}
				else
				{
					document.write('<li>\n');
					document.write('<a onClick="trackEvent(\'' + link[i].linkwtid + '\')" href="' + link[i].linkurl + '" target="' + link[i].linktarget + '">');
					document.write(link[i].linktext);
					document.write('</a>\n');
					document.write('</li>\n');
				}
			}
			i++;
			j++;
		} 
		
		
		if(colLnkCount > 1) {
			rowLnkCount = Math.ceil((link.length - i)/ (colLnkCount-1));
			colLnkCount--;	
		}
		j=0;
		document.write('\n</ul>\n');
		document.write('\n</div>\n');
	}
	document.write('</div>');
</script>	
						</div>
							</div>
							<!-- <div class="clearit">&#160;</div>
							<p class="h-divider">&#160;</p> -->
						        	
									
					
					
						
					
					
					
			
					

