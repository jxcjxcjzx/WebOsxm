<script language=javascript>
function secBoard(n)
{for(i=0;i<secTable.cells.length;i++)
secTable.cells[i].className="sec1";secTable.cells[n].className="sec2";for(i=0;i<mainTable.tBodies.length;i++)
mainTable.tBodies[i].style.display="none";mainTable.tBodies[n].style.display="block";}
function sedBoard(n)
{for(i=0;i<sedTable.cells.length;i++)
sedTable.cells[i].className="sec1";sedTable.cells[n].className="sec2";for(i=0;i<mainTable1.tBodies.length;i++)
mainTable1.tBodies[i].style.display="none";mainTable1.tBodies[n].style.display="block";}
