<script type="text/javascript">
(function(index){var guide=document.getElementById('move_guide'),move_tipBox,move_samplePic,move_guide_try,move_guide_known,move_guide_btnBox,move_intoGuide_Box,move_intoGuide_sample,move_intoGuide_try,move_intoGuide_known,move_intoGuide_btnBox;guide.onclick=function(){if(move_intoGuide_Box)_showIntoGuide(guide);else _initIntoGuide();}
function _savePics(urls){for(var i=0;i<urls.length;i++){new Image().src=urls[i];}}
function move_checkGuide(){guide.style.display='block';_savePics(['http://i3.sinaimg.cn/book/deco/2012/0723/tips0.png','http://i3.sinaimg.cn/book/deco/2012/0723/tips.png','http://i3.sinaimg.cn/book/deco/2012/0723/move_arrs02.png']);if(!_getStorage('blks_book_guided')&&!/\((iPhone|iPad|iPod)/i.test(navigator.userAgent)){_setStorage();_initIntoGuide();}}
function _initIntoGuide(){var intoGuideBox=document.createElement('div'),intoSample=document.createElement('div'),intoBtnBox=document.createElement('div');intoGuideBox.className='move_intoguide_box';document.body.appendChild(intoGuideBox);intoSample.className='mobe_intoguide_sample';intoSample.innerHTML='<div class="move_guide">定制功能介绍</div>';document.body.appendChild(intoSample);intoBtnBox.className='move_intoguide_btnBox';intoBtnBox.innerHTML='<a href="javascript:void(0)" class="move_intoguide_try" onfocus="this.blur();"></a><a href="javascript:void(0)" class="move_intoguide_known" onfocus="this.blur();"></a>';move_intoGuide_Box=intoGuideBox;move_intoGuide_sample=intoSample;move_intoGuide_btnBox=intoBtnBox;document.body.appendChild(move_intoGuide_btnBox);move_intoGuide_btnBox.onclick=function(e){var evt=e||window.event,eTarget=evt.target||evt.srcElement,theClass=eTarget.className;switch(theClass){case'move_intoguide_try':_hideIntoGuide();if(move_tipBox)_showGuide(index);else setTimeout(function(){_initGuide();},50);break;case'move_intoguide_known':_hideIntoGuide();window.scrollTo(0,0);break;default:return;}}
_showIntoGuide(guide);}
function _showIntoGuide(related){var b=_getBoxData(related);move_HideArrs();coverLayer.on();window.scrollTo(0,0);move_intoGuide_Box.style.display=move_intoGuide_sample.style.display=move_intoGuide_btnBox.style.display='block';move_intoGuide_Box.style.left=b.x+100+'px';move_intoGuide_Box.style.top=b.y+0+'px';move_intoGuide_sample.style.left=b.x-20+'px';move_intoGuide_sample.style.top=b.y+'px';move_intoGuide_btnBox.style.left=b.x+135+'px';move_intoGuide_btnBox.style.top=b.y+150+'px';}
function _hideIntoGuide(){coverLayer.off();move_intoGuide_Box.style.display=move_intoGuide_sample.style.display=move_intoGuide_btnBox.style.display='none';}
function _initGuide(){var tipBox=document.createElement('div'),samplePic=document.createElement('div'),btnBox=document.createElement('div'),ss;tipBox.className='move_guide_box';document.body.appendChild(tipBox);btnBox.className='move_guide_btnBox';btnBox.innerHTML='<a href="javascript:void(0)" class="move_guide_try" onfocus="this.blur();"></a><a href="javascript:void(0)" class="move_guide_known" onfocus="this.blur();"></a>';document.body.appendChild(btnBox);btnBox.onclick=function(e){var evt=e||window.event,eTarget=evt.target||evt.srcElement,theClass=eTarget.className;switch(theClass){case'move_guide_try':move_HideArrs();_hideGuide();window.scrollTo(0,_getBoxData(move_DomStates[index][0]).y-5);for(var i=0;i<move_PosList.length;i++){if(move_PosList[i]==index){break;}}
move_setActIndex(i);move_ShowArrs(move_DomStates[index][0],i);break;case'move_guide_known':_hideGuide();break;default:return;}}
samplePic.className='move_guide_sample';samplePic.innerHTML='自定义板块';document.body.appendChild(samplePic);move_tipBox=tipBox;move_guide_btnBox=btnBox;move_samplePic=samplePic;_showGuide(index);}
function _showGuide(index){var blk=move_DomStates[index][0],b=_getBoxData(blk);move_HideArrs();coverLayer.on();move_tipBox.style.display=move_samplePic.style.display=move_guide_btnBox.style.display='block';move_tipBox.style.left=b.x+440+'px';move_tipBox.style.top=b.y+10+'px';move_guide_btnBox.style.left=b.x+490+'px';move_guide_btnBox.style.top=b.y+190+'px';move_samplePic.style.left=b.x+830+'px';move_samplePic.style.top=b.y-5+'px';move_scrollTo(document.body.scrollTop+document.documentElement.scrollTop,b.y-5,300,null,'');}
function _hideGuide(){coverLayer.off();move_tipBox.style.display=move_samplePic.style.display=move_guide_btnBox.style.display='none';}
function _getBoxData(o){var b={x:0,y:0,width:o.offsetWidth,height:o.offsetHeight};while(o.offsetParent){b.x+=o.offsetLeft;b.y+=o.offsetTop;o=o.offsetParent;}
return b;}
function _getStorage(name){var ret=window.localStorage?_getLocalStorage(name):_getCookie(name);return ret;}
function _setStorage(){window.localStorage?_setLocalStorage('blks_book_guided','true'):_setCookie('blks_book_guided','true');}
function _getCookie(name){var start=document.cookie.indexOf(name+"=");var len=start+name.length+1;if((!start)&&(name!=document.cookie.substring(0,name.length))){return null;}
if(start==-1)return null;var end=document.cookie.indexOf(';',len);if(end==-1)end=document.cookie.length;return unescape(document.cookie.substring(len,end));}
function _setCookie(name,value,expires,path,domain,secure){var today=new Date();today.setTime(today.getTime());if(expires){expires=expires*1000*60*60*24;}
var expires_date=new Date(today.getTime()+(expires));document.cookie=name+'='+escape(value)+
((expires)?';expires='+expires_date.toGMTString():'')+
((path)?';path='+path:'')+
((domain)?';domain='+domain:'')+
((secure)?';secure':'');}
function _getLocalStorage(name){return localStorage[name];}
function _setLocalStorage(name,value){localStorage.setItem(name,value);}
function _removeStorage(){localStorage.removeItem('blks_book_guided');window.location=window.location;}
window.move_checkGuide=move_checkGuide;window.__rs=_removeStorage;})(0);
</script>
