function _checkPos(x,y,mX,mY){var ul=doms.nav_c_ul,ulPos=fn.getPosition(ul),ulw=85*11,ulh=38;mX=mX-ulPos.x;mY=mY-ulPos.y;return[Math.floor(mX/85),Math.floor(mY/38)];}
function _preview(ele,pos,ed){var ul=doms.nav_c_ul,liList=ul.getElementsByTagName('li'),liListLen=liList.length,index,li,thisLi;index=pos[0]+pos[1]*11;thisLi=ele.parentNode;if(pos[0]<0||pos[1]<0||pos[0]>10||pos[1]>1||index>21){if(_preview.box&&_preview.box.parentNode){ul.removeChild(_preview.box);liList[liListLen-1].style.display='inline';}
return;};li=liList[index];for(var i=0;i<liListLen;i++){if(fn.hasClass(liList[i],'custom_items_dragging_over')){fn.removeClass(liList[i],'custom_items_dragging_over');}}
if(!fn.hasClass(li,'custom_items_dragging_over')){li.className+=' custom_items_dragging_over';}
if(!fn.hasClass(thisLi,'custom_items_dragging')){thisLi.className+=' custom_items_dragging';}
if(!_preview.box){_preview.box=document.createElement('div');_preview.box.className='custom_items_coverred';}
if(thisLi!=li){if(ed){fn.swapNode(thisLi,li);if(fn.hasClass(li,'custom_item_blank')){var rIndex=_getIndex(li);var row=(rIndex<11)?1:2;var bItem=document.createElement("li");var lineLast;bItem.className="custom_item_blank";bItem.innerHTML='<a href="#"></a>';if(row==1){lineLast=ul.getElementsByTagName('li')[11];ul.removeChild(li);ul.insertBefore(bItem,lineLast);}else{ul.removeChild(li);ul.appendChild(bItem);}}
var tIndex=_getIndex(thisLi);var tR=tIndex<11?1:2;var nLi=ul.getElementsByTagName('li');if(tR==1){for(var i=0;i<11;i++){if(fn.hasClass(nLi[i],'custom_item_blank')&&tIndex>_getIndex(nLi[i])){fn.swapNode(thisLi,nLi[i]);break;}}}else{for(var i=11;i<22;i++){if(fn.hasClass(nLi[i],'custom_item_blank')&&tIndex>_getIndex(nLi[i])){fn.swapNode(thisLi,nLi[i]);break;}}}}}
else{if(_preview.box.parentNode){ul.removeChild(_preview.box);liList[liListLen-1].style.display='inline';}}}
function _previewFromS(ele,pos){var index,li,ul;index=pos[0]+pos[1]*11,ul=doms.nav_c_ul;if(pos[0]<0||pos[1]<0||pos[0]>10||pos[1]>1||index>21){return;};if(!_previewFromS.box){_previewFromS.box=document.createElement('div');_previewFromS.box.className='custom_items_coverred';}
li=ul.getElementsByTagName('li')[index];if(_previewFromS.cur!=li){if(_previewFromS.cur){_previewFromS.cur.style.display='inline';}
li.parentNode.insertBefore(_previewFromS.box,li)
li.style.display='none';}
_previewFromS.cur=li;}
function _moveTo(ele,pos,cX,cY){var ul=doms.nav_c_ul,liList=ul.getElementsByTagName('li'),liListLen=liList.length;var index=pos[0]+pos[1]*11,li,thisLi;for(var i=0;i<liListLen;i++){if(fn.hasClass(liList[i],'custom_items_dragging_over')){fn.removeClass(liList[i],'custom_items_dragging_over');}}
if(pos[0]<0||pos[1]<0||pos[0]>10||pos[1]>1||index>21){removeNav(ele.getAttribute('data-ci'));}
else{li=doms.nav_c.getElementsByTagName('li')[index];thisLi=ele.parentNode;fn.removeClass(thisLi,'custom_items_dragging');ele.style.left="auto";ele.style.top="auto";if(_preview&&_preview.box&&_preview.box.parentNode){doms.nav_c_ul.removeChild(_preview.box);}
_upDateStorage();}
ele.style.position='static';_upDateStorage();}
function _moveToFromS(ele,pos){var index=pos[0]+pos[1]*11,ul=doms.nav_c_ul,cI,oI,aLi,oLi;if(pos[0]<0||pos[1]<0||pos[0]>10||pos[1]>1||index>21){fn.removeClass(ele.parentNode,'drag');ele.style.position='static';ele.style.left='auto';ele.style.top='auto';if(_previewFromS.cur){ul.removeChild(_previewFromS.box);_previewFromS.cur.style.display='block';}}
else{cI=ele.getAttribute('data-ci');aLi=document.createElement('li');var text=ele.innerHTML;aLi.innerHTML='<a href="#" data-ci="'+cI+'" title="'+ele.innerHTML+'">'+text+'</a>';if(_previewFromS.cur.getElementsByTagName('a')[0].getAttribute('data-ci')){oI=_previewFromS.cur.getElementsByTagName('a')[0].getAttribute('data-ci');replaceNav(oI,aLi);}else{replaceBlank(cI,aLi);customNavData.setData('filled',customNavData.getData('filled')+1);}
ul.removeChild(_previewFromS.box);fn.removeClass(ele.parentNode,'drag');ele.style.position='static';ele.style.left='auto';ele.style.top='auto';}
_previewFromS.cur=_previewFromS.box=null;}
function _animateTo(){}
function _getIndex(ele){var ul=ele.parentNode,counter=0,liList=ul.getElementsByTagName('li'),liListLen=liList.length;for(var i=0;i<liListLen;i++){if(ele==liList[i]){return counter;break;}
counter++;};return false;}
function _getIndexFromCI(cI){var ul=doms.nav_c_ul,liList=ul.getElementsByTagName('li'),liListLen=liList.length;for(var i=0;i<liListLen;i++){if(cI==liList[i].getElementsByTagName('a')[0].getAttribute('data-ci')){return i;}};return false;}
function _createTips(){var objW=document.createElement('div'),obj=document.createElement('div'),insertRefer=document.body.getElementsByTagName('div')[0];objW.id=objW.className='custom_item_tip_w';obj.className='custom_item_tip';objW.style.display='none';objW.appendChild(obj);document.body.insertBefore(objW,insertRefer);}
function _createCover(){var objW=document.createElement('div'),obj=document.createElement('a'),insertRefer=document.body.getElementsByTagName('div')[0];objW.id=objW.className='custom_item_cover';objW.style.display='none';obj.id=obj.className='custom_item_delete';objW.appendChild(obj);document.body.insertBefore(objW,insertRefer);}
function fixSwitch(){if(!sData.read('custom_switchClosed')){fn.addClass(doms.btn_switch_w,'custom_btn_switch_w_hanging');doms.btn_switch.className='custom_btn_switch2';var span=document.createElement('span');span.className='custom_btn_switch2_span';span.id='custom_btn_switch2_span';doms.btn_switch_w.appendChild(span);doms.btn_switch_w.style.display='block';}}
function hideSwitch(){if(customing||!fn.hasClass(doms.btn_switch_w,'custom_btn_switch_w_hanging')){doms.btn_switch_w.style.display='none';}}
function showSwitch(){doms.btn_switch_w.style.display='block';}
function _createPlus(){var span=document.createElement('span');fn.addClass(span,'custom_plus');doms.plus=span;document.body.insertBefore(span,document.body.getElementsByTagName('div')[0]);}
function _showTips(relatedElement,type){var pos=fn.getPosition(relatedElement);doms.tips.related=relatedElement;switch(type){case'toAbort':doms.tips.type='toAbort';doms.tips.getElementsByTagName('div')[0].innerHTML='点击取消添加';break;}
doms.tips.style.left=pos.x+'px';doms.tips.style.top=pos.y-33+'px';doms.tips.style.display='block';}
function _hideTips(type){doms.tips.style.display='none';}
function _showItemCover(relatedElement){var pos=fn.getPosition(relatedElement);var relatedLi=relatedElement.parentNode;if(!fn.hasClass(relatedElement,'hover')){fn.addClass(relatedElement,'hover');}
doms.item_cover.related=relatedElement;doms.item_cover.style.left=pos.x+1+'px';doms.item_cover.style.top=pos.y+'px';doms.item_cover.style.display='block';}
function _hideItemCover(target){var rel=doms.item_cover.related;doms.item_cover.style.display='none';if(fn.hasClass(rel,'hover')){fn.removeClass(rel,'hover');}}
function _fullFilled(){var items=doms.nav_c.getElementsByTagName('li'),itemsLen=items.length,counter=0;for(var i=0;i<itemsLen;i++){if(items[i].getElementsByTagName('a')[0].getAttribute('data-ci')!=null){counter++;}}
if(counter==itemsLen)return true;return false;}
function _sortItems(str,boo){var ul=doms.nav_c_ul,aLi,ciList=str.split(','),ciListLen=22,lastLi,navItem,nName;ul.innerHTML='';if(boo){for(var i=0;i<11;i++){aLi=document.createElement('li');if(ciList[i]=='blank'||typeof(ciList[i])=='undefined'){aLi.className='custom_item_blank';aLi.innerHTML='<a href="#"></a>';}else{navItem=customNavData.getDataBase().navItems[ciList[i]];if(typeof(navItem)!='undefined'){nName=navItem['nName'];aLi.innerHTML='<a href="'+navItem['url']+'" data-ci="'+ciList[i]+'">'+nName+'</a>';}else{aLi.className='custom_item_blank';aLi.innerHTML='<a href="#"></a>';}}
ul.appendChild(aLi);}
if(!(ciList[11]=='blank'||typeof(ciList[i])=='undefined'||typeof(customNavData.getDataBase().navItems[ciList[11]])=='undefined')){for(var i=11;i<22;i++){aLi=document.createElement('li');if(ciList[i]=='blank'||typeof(ciList[i])=='undefined'){aLi.className='custom_item_blank';aLi.innerHTML='<a href="#"></a>';}else{navItem=customNavData.getDataBase().navItems[ciList[i]];if(typeof(navItem)!='undefined'){nName=navItem['nName'];aLi.innerHTML='<a href="'+navItem['url']+'" data-ci="'+ciList[i]+'">'+nName+'</a>';}else{aLi.className='custom_item_blank';aLi.innerHTML='<a href="#"></a>';}}
ul.appendChild(aLi);}}
fn.getById('custom_nav').style.display='block';showPlus();}else{for(var i=0;i<ciListLen;i++){aLi=document.createElement('li');if(ciList[i]=='blank'||typeof(ciList[i])=='undefined'){aLi.className='custom_item_blank';aLi.innerHTML='<a href="#"></a>';}else{navItem=customNavData.getDataBase().navItems[ciList[i]];if(typeof(navItem)!='undefined'){nName=navItem['nName'];var text=nName;aLi.innerHTML='<a href="#" data-ci="'+ciList[i]+'" title="'+nName+'">'+text+'</a>';}else{aLi.className='custom_item_blank';aLi.innerHTML='<a href="#"></a>';}}
ul.appendChild(aLi);}
fn.getById('custom_nav').style.display='block';}}
function showPlus(){var plusRelated;var lis=doms.nav_c_ul.getElementsByTagName('li');var lislen=lis.length;for(var i=0;i<lislen;i++){if(fn.hasClass(lis[i],'custom_item_blank')){plusRelated=lis[i];break;}}
if(plusRelated&&sData.read('customNav_customed')){setTimeout(function(){var p=fn.getPosition(plusRelated);doms.plus.style.left=(p.x+13)+'px';doms.plus.style.top=(p.y-1)+'px';},200);}else{doms.plus.style.left='-2999px';}}
function _updateInactive(cI,toActive){var inactive_c=doms.inactive_c,itemList=fn.getElementsByAttribute('data-ci',cI,inactive_c),itemListLen=itemList.length;for(var i=0;i<itemListLen;i++){toActive?fn.removeClass(itemList[i].parentNode):fn.addClass(itemList[i].parentNode,'selected');}}
function _resetInactive(str){var inactive_c=doms.inactive_c,spans=inactive_c.getElementsByTagName('span'),spansLen=spans.length,ciList=str.split(','),ciListLen=ciList.length,aItem;for(var i=0;i<spansLen;i++){if(fn.hasClass(spans[i],'selected')){fn.removeClass(spans[i]);}}
for(var i=0;i<ciListLen;i++){_updateInactive(ciList[i],false);}}
function _upDateStorage(){var updated=[],liList=doms.nav_c_ul.getElementsByTagName('li'),liListLen=liList.length,tLi,cId;for(var i=0;i<22;i++){cId=liList[i].getElementsByTagName('a')[0].getAttribute('data-ci');cId?updated.push(cId):updated.push('blank');}
customNavData.getDataBase().navIndex=updated;sData.write('customNavIndex',customNavData.getDataBase().navIndex.join(','),24*365);firstGuide=false;}
function addNav(cI){var ul,itemStrW,itemStr,navName,domInactive,deletedBlank;ul=doms.nav_c_ul;domInactive=fn.getElementsByAttribute('data-ci',cI,doms.inactive_c)[0];var text=domInactive.innerHTML;itemStr='<a href="#" data-ci="'+cI+'" title="'+domInactive.innerHTML+'">'+text+'</a>';itemStrW=document.createElement('li');itemStrW.innerHTML=itemStr;deletedBlank=fn.getElementsByClass('custom_item_blank')[0];ul.replaceChild(itemStrW,deletedBlank);_updateInactive(cI,false);customNavData.setData('filled',customNavData.getData('filled')+1);_upDateStorage();}
function replaceNav(cI,ele_li){var ul=doms.nav_c_ul,oLi=fn.getElementsByAttribute('data-ci',cI,ul)[0].parentNode,nCI=ele_li.getElementsByTagName('a')[0].getAttribute('data-ci');ul.insertBefore(ele_li,oLi);ul.removeChild(oLi);_updateInactive(cI,true);_updateInactive(nCI,false);_upDateStorage();}
function replaceBlank(cI,ele){var ul=doms.nav_c_ul,previousLi,index;previousLi=_previewFromS.box.previousSibling;if(_previewFromS.box.previousSibling){previousLi=_previewFromS.box.previousSibling;if(previousLi.tagName!='li'){if(previousLi.previousSibling){previousLi=previousLi.previousSibling;index=_getIndex(previousLi)+2;}
else{index=0;}}}
else{index=0;}
ul.insertBefore(ele,_previewFromS.box);ul.removeChild(_previewFromS.cur);var eIndex=_getIndex(ele);var eLi=ul.getElementsByTagName('li');var teBlank,start,max;start=(eIndex<11)?0:11;max=start+11;for(var i=start;i<max;i++){teBlank=eLi[i];if(fn.hasClass(teBlank,'custom_item_blank')&&_getIndex(teBlank)<_getIndex(ele)){fn.swapNode(teBlank,ele);break;}}
_updateInactive(cI,false);_upDateStorage();}
function removeNav(cI){var ul=doms.nav_c_ul,row,lineLast,removedItem=fn.getElementsByAttribute('data-ci',cI,doms.nav_c)[0],removedLi=removedItem.parentNode,rIndex=_getIndex(removedLi);var bItem=document.createElement("li");bItem.className="custom_item_blank";bItem.innerHTML='<a href="#"></a>';row=(rIndex<11)?1:2;if(row==1){lineLast=ul.getElementsByTagName('li')[11];ul.removeChild(removedLi);ul.insertBefore(bItem,lineLast);}else{ul.removeChild(removedLi);ul.appendChild(bItem);}
_updateInactive(cI,true);customNavData.setData('filled',customNavData.getData('filled')-1);_upDateStorage();}
function setDatabase(){var inEle=doms.inactive_c;var spans=inEle.getElementsByTagName('span');var spansLen=spans.length;var a,ci,nName,url,icon;var ret=[];for(var i=0;i<spansLen;i++){a=spans[i].getElementsByTagName('a')[0];ci=a.getAttribute('data-ci');nName=a.innerHTML;url=a.href;icon=a.getAttribute('data-icon')||'';ret[ci]={nName:nName,url:url,icon:icon}}
customNavData.setData('navItems',ret);}
function doGuide(){var steps=[],fragments=[],stepIndex=0,sI=0,ss,nstep={guideStr:'<div class="custom_nav_guide0_nar" id="custom_nav_guide0"></div><div class="custom_nav_guide0_cancel_nar" id="custom_nav_guide0_cancel"></div><div class="custom_nav_guide0_continue_nar" id="custom_nav_guide0_continue"></div>',guidePos:{relatedDom:fn.getById(standardHeaderChannel),x:617,y:20},bgStr:'<div class="custom_nav_guide0_bg_nar"></div>',bgPos:{relatedDom:fn.getById(standardHeaderChannel),x:0,y:32}};guiding=true;doms.nav_w.style.display='none';fn.getById(logonav).style.display='block';_initStep();_initGuide();_initGuideEvent();showGuide(0);function _initStep(){steps.push({guideStr:'<div class="custom_nav_guide0" id="custom_nav_guide0"></div><div class="custom_nav_guide0_cancel" id="custom_nav_guide0_cancel"></div><div class="custom_nav_guide0_continue" id="custom_nav_guide0_continue"></div>',guidePos:{relatedDom:fn.getById(standardHeaderChannel),x:617,y:20},bgStr:'<div class="custom_nav_guide0_bg"></div>',bgPos:{relatedDom:fn.getById(standardHeaderChannel),x:0,y:32}});steps.push({guideStr:'<div class="custom_nav_guide1" id="custom_nav_guide1"></div><div class="custom_nav_guide1_cancel" id="custom_nav_guide1_cancel"></div><div class="custom_nav_guide1_continue" id="custom_nav_guide1_continue"></div>',guidePos:{relatedDom:fn.getById(standardHeaderChannel),x:157,y:24},bgStr:'<div class="custom_nav_guide1_bg"></div>',bgPos:{relatedDom:fn.getById(standardHeaderChannel),x:0,y:36}});steps.push({guideStr:'<div class="custom_nav_guide2" id="custom_nav_guide2"></div><div class="custom_nav_guide2_cancel" id="custom_nav_guide2_cancel"></div><div class="custom_nav_guide2_continue" id="custom_nav_guide2_continue"></div>',guidePos:{relatedDom:fn.getById(standardHeaderChannel),x:468,y:260},bgStr:'<div class="custom_nav_guide2_bg"></div>',bgPos:{relatedDom:fn.getById(standardHeaderChannel),x:0,y:36}});steps.push({guideStr:'<div class="custom_nav_guide3" id="custom_nav_guide3"></div><div class="custom_nav_guide3_cancel" id="custom_nav_guide3_cancel"></div><div class="custom_nav_guide3_continue" id="custom_nav_guide3_continue"></div>',guidePos:{relatedDom:fn.getById(standardHeaderChannel),x:200,y:24},bgStr:'<div class="custom_nav_guide3_bg"></div>',bgPos:{relatedDom:fn.getById(standardHeaderChannel),x:0,y:36}});}
function _initGuide(){var stepsLen=steps.length,fragmentItem;if(fragments.length)return;for(var i=0;i<stepsLen;i++){fragmentItem={fGuide:null,fBg:null}
fragmentItem.fGuide=document.createElement('div');fragmentItem.fGuide.className='guide'+i+'_w';fragmentItem.fBg=document.createElement('div');fragmentItem.fBg.className='guide'+i+'_bg_w';fragmentItem.fGuide.style.display=fragmentItem.fBg.style.display='none';fragments.push(fragmentItem);}}
function _clearGuide(sI){var cf;if(sI<0)return;cf=fragments[sI];document.body.removeChild(cf.fGuide);document.body.removeChild(cf.fBg);}
function _initGuideEvent(){fn.addEvent(document,'click',doStep);fn.addEvent(window,'resize',doFixGuide);}
function doStep(e){var e=e||window.event,target=e.target||e.srcElement,tCN=target.className,index;if(target.tagName=='DIV'&&tCN.indexOf('custom_nav_guide')>-1){if(e.stopPropagation){e.stopPropagation();}else{e.cancelBubble=true;}
if(tCN.indexOf('cancel')>-1){hideGuide();}else{tCN=tCN.replace('custom_nav_guide','');index=parseInt(tCN.match(/\d/)[0],10);showGuide(index+1);}}}
function doFixGuide(){if(document.body.offsetWidth<=1024){narrowMode=true;}else{narrowMode=false;}
if(guiding){_clearGuide(stepIndex);fixGuide(stepIndex);}}
function showGuide(sI){var cf=fragments[sI],cs=steps[sI],ele_guide,ele_bg,relPos_guide,relPos_bg,fPosY,tPosY;if(!cs){hideGuide();switchMoving=false;switchShow=false;_sortItems(customNavData.getDataBase().navIndex.join(','),false);doms.nav_w.style.display='block';fn.getById(logonav).style.display='none';sData.write('customNav_customed','true',24*365);dragging=false;dragging2=false;customing=true;hideSwitch();fn.addClass(doms.nav_w,'custom_nav_customing');doms.inactive_w.style.display='block';var h=fn.getById('custom_inactive_w').offsetHeight;fn.getById('custom_inactive_w').style.overflow='hidden';fn.actTo(fn.getById('custom_inactive_w').style,'height',0,h,100,function(){fn.getById('custom_inactive_w').style.overflow='visible';fn.getById('custom_inactive_w').style.height='auto';},'px');try{custom_subshow.select(0);}catch(e){}
return;}
_clearGuide(sI-1);fixGuide(sI);coverLayer2.on();stepIndex=sI;}
function fixGuide(sI){var cf=fragments[sI],cs=steps[sI];var relPos_guide=fn.getPosition(cs.guidePos.relatedDom),relPos_bg=fn.getPosition(cs.bgPos.relatedDom),fPosY=document.body.scrollTop+document.documentElement.scrollTop,tPosY=relPos_guide.y+cs.guidePos.y-100;cf.fGuide.innerHTML=cs.guideStr;cf.fBg.innerHTML=cs.bgStr;cf.fGuide.style.top=relPos_guide.y+cs.guidePos.y+'px';cf.fGuide.style.left=relPos_guide.x+cs.guidePos.x+'px';var brows=fn.getBrowserInfo();if(sI==0&&brows.browser=='msie'&&brows.version=='6'){cf.fGuide.style.left=relPos_guide.x+cs.guidePos.x-30+'px';}
cf.fBg.style.top=relPos_bg.y+cs.bgPos.y+'px';cf.fBg.style.left=relPos_bg.x+cs.bgPos.x+'px';document.body.insertBefore(cf.fGuide,document.body.getElementsByTagName('div')[0]);document.body.insertBefore(cf.fBg,document.body.getElementsByTagName('div')[0]);if(narrowMode&&sI==0){if(!fn.hasClass(cf.fGuide,'guide0_w_nar')){fn.addClass(cf.fGuide,'guide0_w_nar');fn.addClass(cf.fBg,'guide0_bg_w_nar');}}else{if(fn.hasClass(cf.fGuide,'guide0_w_nar')){fn.removeClass(cf.fGuide,'guide0_w_nar');fn.removeClass(cf.fBg,'guide0_bg_w_nar');}}
cf.fGuide.style.display=cf.fBg.style.display='block';}
function hideGuide(){var fPosY=document.body.scrollTop+document.documentElement.scrollTop;_clearGuide(stepIndex);coverLayer2.off();if(sData.read('customNav_customed')){doms.nav_w.style.display='block';fn.getById(logonav).style.display='none';}else{doms.nav_w.style.display='none';fn.getById(logonav).style.display='block';}
window.scrollTo(0,0);sData.write(storageName,'done',24*365);guiding=false;fn.removeEvent(window,'resize',doFixGuide);fn.removeEvent(document,'click',doStep);}}};
</script>
