            <a title="日期" class="ic ico_rili" href="#"><input id="date_cale" type="button" class="btn_cale" onmouseover="this.setAttribute('hideFocus','ture')" readonly="" onclick="dateFilter();return false;" value="" hidefocus="ture"></a>
    </div>
</div>
<script type="text/javascript">
window.dateFilter = function() {
	UI.DatePicker.show(event,{
		lang:'zh_CN',
		format:'yyyy年MM月dd日',
		startDay:'2010年03月30日',
		endDay:'2014年06月30日',
		markDays:{'':{title:'您的微薄',className:'datebg'}},
		call:function(date){
			document.location.href='?'+'filter='+0+'&date='+UI.formatDate(date,'yyyyMMdd')
		}
	});
};
</script>

