                                                     <div class="voteOut">
							<script type="text/javascript">
var vc_questions = [];
function vc_checkVoteForm ()
{
  var vote_id = 483;
  try {
    var current_id = null;
    var limit = null;
    vc_questions.each(function(vc_question, index){
      var checked = 0;
      $$( '#vc_question_'+vc_question.id+' .answer' ).each(function(input,index){
        var tag  = input.tagName.toLowerCase();
        if( tag == 'input' && input.checked == true ) {
          checked ++;
        } else if ( tag == 'textarea' && input.value != '' ) {
          checked = 1;
        }
      });

      current_id = vc_question.id;
      limit = vc_question.limit;
      if( ! checked && vc_question.type == 3 && vc_question.fill == 1 ) {
        throw '对不起，请填写完整。';
      } else if( ! checked || (vc_question.type == 2 && (checked < vc_question.min || checked > vc_question.max)) ) {
        throw '对不起，请按要求选择。';
      }
      if( vc_question.type == 3  ){
      	var text = $('voteanswertext'+vc_question.id).value.trim();
      	if( text.length == 0 )
      		throw '请填写投票的内容';
      	else if( text.length > limit )
      		throw '投票内容字符数不能超过'+ limit +'，请精简字数！';
      
      $('voteanswertext_'+vc_question.id).setProperty('value', $('voteanswertext'+vc_question.id).value );
      $('voteanswertext'+vc_question.id).value = '';
      }
    });
  } catch( e ) {
    $('vc_question_'+current_id).focus();
    alert(e);
    return false;
  }
	$('voteForm').submit();
}
</script>

