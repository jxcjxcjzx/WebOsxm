		<script type="text/javascript">			
			function dksub(){
					var rdo = document.getElementsByName("dkxx");
					var rdon = 0;
					var xxv = 0;
					for(var i = 0;i<rdo.length;i++){
						if(rdo[i].checked){
							xxv = rdo[i].value;
							rdon++;
							break;
						}
					}
					if(rdon==0){
						alert("请选择一个选项！");
						return;
					}else{
						var sid = document.getElementById("dksid").value;
						$.getJSON("http://vote.stcn.com/newhomepagevote/vote.action?xx="+xxv+"&sid="+sid+"&format=json&jsoncallback=?",function(data){
							if(data.res=='-3'){
								alert("每个IP一天只能投票两次，请勿重复投票!");
							}else{
								alert("投票成功！");
							}
						});
					} 
			}	
			function ccsub(){
					var rdo = document.getElementsByName("ccxx");
					var rdon = 0;
					var xxv = 0;
					for(var i = 0;i<rdo.length;i++){
						if(rdo[i].checked){
							xxv = rdo[i].value;
							rdon++;
							break;
						}
