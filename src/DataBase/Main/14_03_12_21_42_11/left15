<script type="text/javascript">
(function(){
	var href='http://news.youku.com/lianghui2014', url="http://r4.ykimg.com/05100000531D974B6714C02DB704BC9D",
		x=1117, y=165, height=40;
		
	function addStyle(content){  
	    var style;  
	    if(document.all)    //IE  
	    {  
	        style = document.createStyleSheet();  
	        style.cssText = content;  
	    }  
	    else
	    {  
	        style = document.createElement("style");   
	        style.type = "text/css";
	        //style.innerHTML = content;//Safari¡¢Chrome ÏÂinnerHTMLÖ»¶Á  
	        style.textContent = content;  
	    }  
	    try{document.getElementsByTagName("head")[0].appendChild(style);}catch(e){}
	}
	var nav=document.getElementById('qheader_nav');
	var parent=nav.parentElement;
	var skin=document.createElement('div');
	var time = +(new Date());

	skin.className = "yk-skin";
	skin.id = "yk-skin"+time;
	skin.innerHTML = '<div class="yk-skin-links">'+
		(href==""?'<div class="yk-skin-top"></div>':'<a class="yk-skin-top" href="'+ href +'" target="_blank"></a>')+
		'<div class="yk-skin-close" style="cursor:pointer;background:url('+ url +') no-repeat -'+ x +'px -'+ y +'px;width:46px;height:20px;text-align:center;line-height:15px;text-indent:-9999em;" id="close'+time+'">关闭</div></div>';
	parent.insertBefore( skin, nav );
	var close = document.getElementById('close'+time);
	//debugger;
	close.onclick=function(){
		//debugger;
		var skin = document.getElementById('yk-skin'+time);
		skin.parentElement.removeChild(skin);
		document.getElementsByTagName("body")[0].style.background="none";
	}
	var style = '.yk-skin-links{position:relative;height:' + height + 'px;margin:0 auto;width:990px;}.yk-w1190 .yk-skin-links{width:1210px;}.yk-skin-top{display:block;width:100%;height:100%;}.yk-skin-close{position:absolute;bottom:0;right:0;}body{background:url('+url+') no-repeat 50% 60px}';
	addStyle(style);
})();
</script>




