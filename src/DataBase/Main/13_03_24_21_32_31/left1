        sina.$("L_vipmail").onclick = null;
        sina.$("L_2008mail").className = "disabled";
        sina.$("L_2008mail").onclick = null;
        sina.$("L_cnmail").className = "disabled";
        sina.$("L_cnmail").onclick = null;
      }else{
        sina.$("L_freemail").className = "";
        sina.$("L_freemail").onclick = function(){LM.LoginSelect("freemail");return false;};
        sina.$("L_vipmail").className = "";
        sina.$("L_vipmail").onclick = function(){LM.LoginSelect("vipmail");return false;};
        sina.$("L_2008mail").className = "";
        sina.$("L_2008mail").onclick = function(){LM.LoginSelect("2008mail");return false;};
        sina.$("L_cnmail").className = "";
        sina.$("L_cnmail").onclick = function(){LM.LoginSelect("cnmail");return false;};
        return;
      };
      if(username.indexOf("@sina.com") != -1){
        sina.$("L_freemail").className = "";
        sina.$("L_freemail").onclick = function(){LM.LoginSelect("freemail");return false;};
        this.LoginSelect("freemail");
      };
      if(username.indexOf("@vip.sina.com") != -1){
        sina.$("L_vipmail").className = "";
        sina.$("L_vipmail").onclick = function(){LM.LoginSelect("vipmail");return false;};
        this.LoginSelect("vipmail");
      };
      if(username.indexOf("@2008.sina.com") != -1){
        sina.$("L_2008mail").className = "";
        sina.$("L_2008mail").onclick = function(){LM.LoginSelect("2008mail");return false;};
        this.LoginSelect("2008mail");
      };
      if(username.indexOf("@sina.cn") != -1){
        sina.$("L_cnmail").className = "";
        sina.$("L_cnmail").onclick = function(){LM.LoginSelect("cnmail");return false;};
        this.LoginSelect("cnmail");
      };
    },
    LoginSelectClickOther : function(e){
      thisObj = e.target?e.target:event.srcElement;
      do{
        if(thisObj.id == "loginFldselect" || thisObj == document.showLogin.password) return;
        if(thisObj.tagName == "BODY"){this.LoginSelectClose();return;};
        thisObj = thisObj.parentNode;
      }while(thisObj.parentNode);
    },
    clickLoginSelect : function(){
      if(sina.$("loginFldselectop").style.display == "block"){
        this.LoginSelectClose();
      }else{
        sina.$("loginFldselectop").style.display = "block";
        sina.addEvent(document.body,"mousedown",LoginSelectClickOther);
      }
    },
    LoginSelectClose : function(){ //�ر���ʾ��
      sina.$("loginFldselectop").style.display = "none";
      sina.delEvent(document.body,"mousedown",LoginSelectClickOther);
    },
    openLoginType : function(){
      if(document.showLogin.username.value != "" && document.showLogin.username.value.indexOf("@") == -1 && sina.$("loginFldselectdis").innerHTML == "ѡ��ȥ��"){
        sina.$("loginFldselectop").style.display = "block";
        sina.addEvent(document.body,"mousedown",LoginSelectClickOther);
      };
      if(typeof sinaSSOController == 'undefined'){
        this.loadSSO();
      };
    },
    customLoginCallBack : function(r){ //��¼�ص�
      if(r.result === true){
        this.loginWin.location.href = this.loginUrl;
      }else{
        //��¼ʧ��
        document.showLogin.password.value = "";
        document.showLogin.retcode.value = r.errno;
        document.showLogin.target = 'loginWin';
        document.showLogin.submit();
      };
      document.showLogin.password.value = "";
      this.loginWin.focus();
    },
    ssoLogin : function(){ //ͨ��SSO��¼
      this.loginWin = window.open('http://news.sina.com.cn/iframe/login.html','loginWin');

      if(this.sso === false){
        this.waitLogin = true; //�ȴ����
        return false;
      }else if(this.sso == null){ //δ���أ�ִ�м��صȴ�
        this.waitLogin = true;
        this.loadSSO();
        return false;
      };
      var username = document.showLogin.username.value;
      var password = document.showLogin.password.value;
      var loginType = document.showLogin.entry.value;

      if(username.indexOf("@") == -1 && loginType == "vipmail"){
        username += "@vip.sina.com";
      };

      sinaSSOController.customLoginCallBack = function(r){
        LM.customLoginCallBack(r);
      };

      sinaSSOController.from = "referer:www_index";
      sinaSSOController.entry = loginType;
      sinaSSOController.login(username,password);
    },
    ssoOnLoad : function(){ //sso.js�������
