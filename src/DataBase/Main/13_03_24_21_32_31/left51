    <script type="text/javascript">

    //滚动头条
    //滚动效果
    var header_demo = document.getElementById("header_demo");
    var header_demo1 = document.getElementById("h1");
    var header_demo2 = document.getElementById("h2");
    var speed=30;    //数值越大滚动速度越慢
     header_demo2.innerHTML = header_demo1.innerHTML;

    function Marquee(){
        //如果内容宽度比容器可视宽度小,不滚动,也不显示第二部分
        if(header_demo1.clientWidth > header_demo.clientWidth)
        {
            if(header_demo2.offsetWidth-header_demo.scrollLeft<=0)
                 header_demo.scrollLeft-=header_demo1.offsetWidth
            else{
                 header_demo.scrollLeft++
             }
         }
        else
        {
             header_demo2.style.display = "none";
         }
     }
    var MyMar = setInterval(Marquee,speed);
     header_demo.onmouseover = function(){clearInterval(MyMar)}
     header_demo.onmouseout = function(){MyMar = setInterval(Marquee,speed)}
    </script>

    <script type="text/javascript">

    //滚动头条
    //滚动效果
    var header_demo3 = document.getElementById("header_demo1");
    var header_demo4 = document.getElementById("h3");
    var header_demo5 = document.getElementById("h4");
    var speed1=30;    //数值越大滚动速度越慢
     header_demo5.innerHTML = header_demo4.innerHTML;

    function Marquee1(){
        //如果内容宽度比容器可视宽度小,不滚动,也不显示第二部分
        if(header_demo4.clientWidth > header_demo3.clientWidth)
        {
            if(header_demo5.offsetWidth-header_demo3.scrollLeft<=0)
                 header_demo3.scrollLeft-=header_demo4.offsetWidth
            else{
                 header_demo3.scrollLeft++
             }
         }
        else
        {
             header_demo5.style.display = "none";
         }
     }
    var MyMar1 = setInterval(Marquee1,speed1);
     header_demo3.onmouseover = function(){clearInterval(MyMar1)}
     header_demo3.onmouseout = function(){MyMar1 = setInterval(Marquee1,speed1)}
    </script>

