		tid = o.viewId;
	}
	switch (o.funBox){
		case '2':
			t = $$($(tid),'.funBox .reply')[0];
			break;
		case '3':
			t = $$($(tid),'.funBox .comt')[0];
			break;
		case '1':
			t = $$($(tid),'.funBox .relay')[0];
			break;
		case 'none':
			t=null;
			break;
		default:
			break;
	}
	if(UI.isElement(t)){
		MI.app({
			TalkListRich : function(){
				setTimeout(function(){
					UI.fireEvent(t,'click');
				},200);
			}
		});
	}
},200);

