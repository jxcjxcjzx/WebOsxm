<script type="text/javascript">
function back_to_top(){
  $('html,body').animate({scrollTop: '0px'}, 100);
}
$(function(){
  $(window).scroll(function(){ 
    if($(document).scrollTop() >= 200){
              $(".gotop").css("display", "block");
    }else{
              $(".gotop").css("display", "none");
          }
  });   
});

</script>

</div>


<!--左右浮动广告-->


<script type="text/javascript">
$(function(){
    $(".ad img").click(function(){
        $(this).parent().hide();
    });
});
function showAd(time){
        setTimeout(function(){
            $('#ad2').show();},time);
        setTimeout(function(){
            $('#ad3').show();},time);
}

function hideAd(time){
    setTimeout(function(){
    $('#ad2').hide();},time);

    setTimeout(function(){
    $('#ad3').hide();},time);
}
function scrollAd(){
    var offset = $(window).height() - $('#ad2').height() + $(document).scrollTop();
    $('#ad2').animate({top:offset},{duration:800,queue:false});

    var offset2 = $(window).height() - $('#ad3').height() + $(document).scrollTop();
    $('#ad3').animate({top:offset2},{duration:800,queue:false});
}


showAd(1000); //页面加载完毕多久后显示广告
hideAd(100000000000); //页面加载完毕多久后关闭广告
scrollAd();
$(window).scroll(scrollAd);
</script>
<!--左右浮动广告 end-->


<!--幻灯片-->
<script type="text/javascript">
//关键函数：通过控制i ，来显示不通的幻灯片
function showImg(i){
	$("#img a")
		.eq(i).stop(true,true).fadeIn(800)
		.siblings("a").fadeOut(800);
		
	 var showTitle=$("#img a").eq(i).attr('title');
	 if(showTitle.length>24){showTitle=showTitle.substr(0,24)+"...";}
	 var showHref=$("#img a").eq(i).attr('href');
	 var showAlt=$("#img img").eq(i).attr('alt');
	 var showText="<a href='"+showHref+"' title='" + showAlt + "' target='_blank'>"+showTitle+"</a>";
	 
	 $("#showtext").empty().append(showText);
	 $("#btn span")
		.eq(i).addClass("hov")
		.siblings().removeClass("hov");
}

$(document).ready(function(){
    showImg(0);
	
	 var index = 0;
	 $("#btn span").mouseover(function(){
		index  =  $("#btn span").index(this);
		showImg(index);
	});	
	 var lenght=$("#img a").length;
	 var time=3200;
	 
	 //滑入 停止动画，滑出开始动画.
	 $('#frame').hover(
	 	function(){
			  if(MyTime){
				 clearInterval(MyTime);
			  }
		 },function(){
			  	MyTime = setInterval(function(){
			    showImg(index)
				index++;
				if(index==lenght){index=0;}
			  } , time);
	 });
	 //自动开始
	 var MyTime = setInterval(function(){
		showImg(index)
		index++;
		if(index==lenght){index=0;}
	 } , time);
})

new Marquee(["txtBoxCon"],1,0.1,300,180,20,2000,2000,1,3);
</script>


