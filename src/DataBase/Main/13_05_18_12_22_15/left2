    returnHzUrl : function (index, url) {
        var hz = '';
        if(!!index) { hz += this.urlHz + index  + '&cpp=1000648&url='; } else { hz = '';};
        if(!url) { url = ''; };
        return  hz + url; 
    },

    // IE Hack
    isIE : document.all,

    init : function() {
        var _this = this;
        // init dom
        _this.dom = document.getElementById('MYYOUKU');
        _this.domShowMore = document.getElementById('MYYOUKU-ShowMore');
        _this.domRefresh = document.getElementById('MYYOUKU-Refresh');
        _this.domExtand = document.getElementById('MYYOUKU-Extand');
        _this.domRecommend = document.getElementById('MYYOUKU-Recommend');
        _this.domRecords = document.getElementById('MYYOUKU-Records');
        _this.domRECURL = document.getElementById('MYYOUKU-RECURL');
        _this.domCOMURL = document.getElementById('MYYOUKU-COMURL');
        _this.domCollect = document.getElementById('MYYOUKU-Collect');
        _this.domSubscribe = document.getElementById('MYYOUKU-Subscribe');
        _this.domCaption = document.getElementById('MYYOUKU-Caption');
        _this.domTabs = document.getElementById('MYYOUKU-Tabs');
        // get cookies
        _this.cookieYsuid = Nova.Cookie.get('__ysuid');
        _this.cookieUid = Nova.Cookie.get('_l_lgi');
        _this.cookieYktk = Nova.Cookie.get('yktk') == null?0:1;

        _this.initTabs();
        // logon
        if(_this.cookieYktk) {
            _this.getJson(_this.getRecommendJson);
            //_this.getJson(_this.getSubJson);
            _this.timerData = setInterval(function(){
                if(!_this.lockRecommend) {
                    try {
                        _this.initRecommendDom(_this.dataRecommend, _this.domRecommend, 0 , _this.SHOWNUMBER);
                        _this.showContent(_this.MAXHEIGHT);
                    } catch(e){}
                    clearInterval(_this.timerData);
                    _this.lockData = false;
                }
            }, _this.timeSpan);
        } else {
            if(_this.cookieYsuid.substring(12,13) <= _this.RECOMMENDCHANCE) {
                _this.getJson(_this.getAnonymousRecommendJson);
                _this.timerAnonymous = setInterval(function(){
                    if(!_this.lockAnonymousRecommend) {
                        _this.showContent(_this.ANONYMOUSMAXHEIGHT);
                        clearInterval(_this.timerAnonymous);
                    }
                }, _this.timeSpan);
           }
        }
        _this.bind();
    },

    // Bind
    bind : function() {
        var _this = this;
        this.domRefresh.onclick = function() {
            _this.changeMore(this);
        }       
    },

    bindDelete : function(obj) {
        var lists = obj.getElementsByTagName('UL');
        for(var i = 0; i < lists.length; i++) {
            if(lists[i].className.indexOf('v') >=0 ) {
                lists[i].onmouseover = function() {
                    var cl = this.getElementsByTagName('LI');
                    for(var j = 0; j < cl.length; j++) {
                        if(cl[j].className.indexOf('v_close') >= 0) {
                            cl[j].style.display = 'block';
                        }
                    }
                }
                lists[i].onmouseout = function() {
                    var cl = this.getElementsByTagName('LI');
                    for(var j = 0; j < cl.length; j++) {
                        if(cl[j].className.indexOf('v_close') >= 0) {
                            cl[j].style.display = 'none';
                        }
                    }
                }
            }
        }
    },

    show : function(type) {
        var _this = this;
        var theData = [];
        var theJsonUrl = '';
        var theLock = '';

        if(type == 0) {
            this.domRefresh.style.display = 'block';
            this.domShowMore.style.display = 'none';
            this.flagRecommend = true;
        } else if( type == 1 ) {
            this.domRefresh.style.display = 'none';
            this.domShowMore.href = _this.returnHzUrl(hznum, 'http://i.youku.com/u/home?type=watch_timeline');
            this.domShowMore.target = '_blank';
            this.domShowMore.style.display = 'block';
            if(!!this.flagRecords) {
               return false; 
            } else {
                this.flagRecords = true;
            }
            var hznum = '';
            if(this.cookieYktk) {
                hznum = _this.hzCode.hzRecordsAll;
            } else {
                hznum = _this.hzCode.hzAnonymousAll;
            }

            this.getJson(this.getRecordJson);
            this.timerRecords = setInterval(function(){
                if(_this.timerRecords) {
                    var shownum = 0;
                    if(_this.cookieYktk) {
                        shownum = _this.SHOWNUMBER;     
                    } else {
                        shownum = _this.ANONYMOUSRECOMMENDNUMBER - 1;     
                    }
                    _this.initRecordsDom(_this.dataRecords, 0, shownum);
                    _this.flagRecords = true;
                    clearInterval(_this.timerRecords);
                }
            }, _this.timeSpan);            
        } else if( type == 2) {
            this.domRefresh.style.display = 'none';
            if(this.cookieYktk) {
                this.domShowMore.href = _this.returnHzUrl(_this.hzCode.hzCollectAll, 'http://i.youku.com/u/favorite');
                this.domShowMore.target = '_blank';
                this.domShowMore.style.display = 'block';
            } else {
                this.domShowMore.style.display = 'none';
            }
            if(!!this.flagCollect) {
               return false; 
            } else {
                this.flagCollect = true;
            }
            this.getJson(this.getCollectJson);
            this.timerCollect = setInterval(function(){
                if(!_this.lockCollect) {
                    try {
                        _this.initCollectDom(_this.dataCollect, 0, _this.SHOWNUMBER);
                        _this.flagCollect = true;
                    } catch(e){}
                    clearInterval(_this.timerCollect);
                }
            }, _this.timeSpan);
        } else if( type == 3) {
            this.domRefresh.style.display = 'none';
            if(this.cookieYktk) {
                this.domShowMore.href = _this.returnHzUrl(_this.hzCode.hzSubscribeAll, 'http://i.youku.com/u/home?ut=3');
                this.domShowMore.target = '_blank';
                this.domShowMore.style.display = 'block';
            } else {
                this.domShowMore.style.display = 'none';
            }
            if(!!this.flagSubscribe) {
                return false;
            } else {
                this.flagSubscribe = true;
            }
            this.getJson(this.getSubJson);
            this.timerSubscribe = setInterval(function(){
                if(!_this.lockSubscribe) {
                    try {
                        _this.initSubscribeDom(_this.dataSubscribe, 0, _this.SHOWNUMBER);
                        _this.flagSubscribe = true;
                    } catch(e){}
                    clearInterval(_this.timerSubscribe);
                }
            }, _this.timeSpan);
        } else if( type == 4 && !this.flagPick) {
            this.domRefresh.style.display = 'none';
            if(this.cookieYktk) {
                this.domShowMore.href = _this.returnHzUrl(_this.hzCode.hzPickAll, 'http://i.youku.com/u/home?type=showfriends_timeline');
                this.domShowMore.target = '_blank';
                this.domShowMore.style.display = 'block';
            } else {
                this.domShowMore.style.display = 'none';
            }
            this.flagPick = true;
        }
    },

    // Get Data
    getRecordsData : function(data) {
        if(!data || data.playtag.length == 0 && data.playtag_show.length == 0) {
            if(this.CURRENTRECORDSTATUS < this.TRYTIMES) {
                this.tryRequest(this.getRecordJson);
                this.CURRENTRECORDSTATUS++;
            }
            else {
                this.initData(data.playtag, this.dataRecords);
                this.lockRecords = false;
            }
        }
        else {
            this.initData(data.playtag, this.dataRecords);
            this.TOKEN = data.token;
            this.lockRecords = false;
        }
    },

    getRecommendData : function(data) {
        if(typeof data == 'undefined') { return false; }
        if(data.length < this.SHOWNUMBER) {
            if(this.CURRENTRECOMMENDSTATUS < this.TRYJSONTIMES) {
                this.tryRequest(this.getRecommendJson);
                this.CURRENTRECOMMENDSTATUS++;
            }
            else {
                this.hideDom(this.dom);     
                return false;
            }
        }
        else {
            this.initData(data, this.dataRecommend);
            this.lockRecommend = false;
        }
    },

    getSubscribeData : function(data) {
        if(typeof data == 'undefined') { return false; }
        if(!data.results) {
            if(this.CURRENTCOLLECTSTATUS < this.TRYTIMES) {
                this.tryRequest(this.getCollectJson);
                this.CURRENTCOLLECTSTATUS++;
            } else {
                data.results = [];
                this.initData(data.results, this.dataSubscribe);
                this.lockSubscribe = false;
                return false;
            }
        } else {
            this.initData(data.results, this.dataSubscribe);
            this.initUserInfo(data.user.name, data.user.avatar);
            this.lockSubscribe = false;
        }
    },
    
    getCollectData : function(data) {
        if(typeof data == 'undefined') { return false; }
        if(!data.results) {
            if(this.CURRENTCOLLECTSTATUS < this.TRYTIMES) {
                this.tryRequest(this.getCollectJson);
                this.CURRENTCOLLECTSTATUS++;
            }
            else {
                data.results = [];
                this.initData(data.results, this.dataCollect);
                this.lockCollect = false;
                return false;
            }
        }
        else {
            this.initData(data.results, this.dataCollect);
            this.lockCollect = false;
        }
    },

    getAnonymousRecommendData : function(data) {
        var len = data.length;
        if(len > this.ANONYMOUSRECOMMENDNUMBER) { 
            this.initData(data, this.dataAnonymous);
            this.initAnonymousRecommendDom(this.dataAnonymous);
        }
        else {
            if(this.GETANONYMOUSDATANUMBER < this.TRYTIMES) {
                this.getJson(this.getAnonymousComJson + this.randomChar(10));   
                this.GETANONYMOUSDATANUMBER++;
            }
        }
    },

    // 3 Times
    tryRequest : function(url) {
       this.getJson(url); 
    },

    // Init Data
    initData : function(data, o, num) {
        var dataLen = 0;
        if(!!data.length) { var dataLen = data.length; }
        if(!!num && num < dataLen ) { len = this.MAXSHOWNUMBER; } else { len = dataLen; }
        for(var i = 0; i < len; i++) { o.push(data[i]); };
    },

    // Check Data
    checkShowData : function(data) {
        for(var i = 0; i < data.length; i++) {
            if(data[i].showcategory == '电影') {
                if(parseInt(data[i].percent) < 95) {
                    this.dataShowInsert.push(data[i]);     
                }       
            } 
            else {
                this.dataShowInsert.push(data[i]);     
            }
        }
    },

    // Hide Dom
    hideDom : function(dom) {
       dom.style.display = 'none'; 
    },

    // Init Dom
    initTabs : function() {
        var str = '';
        if(this.cookieYktk) {
            str += '<ul>';
            str += '<li class="my-tab current"><a id="MYYOUKU-COMURL" href="http://hz.youku.com/red/click.php?tp=1&cp=4008497&cpp=1000648&url=http://i.youku.com/u/home/" target="_blank">为我推荐</a></li>';
            str += '<li class="my-tab"><a id="MYYOUKU-RECURL" href="http://hz.youku.com/red/click.php?tp=1&cp=4008510&cpp=1000648&url=http://i.youku.com/u/home?type=watch_timeline" target="_blank">观看记录</a></li>';
            str += '<li class="my-tab"><a href="http://hz.youku.com/red/click.php?tp=1&cp=4008516&cpp=1000648&url=http://i.youku.com/u/favorite" target="_blank">我的收藏</a></li>';
            str += '<li class="my-tab"><a href="http://hz.youku.com/red/click.php?tp=1&cp=4008521&cpp=1000648&url=http://i.youku.com/u/home?ut=3" target="_blank">我的订阅</a></li>';
            str += '<li class="my-tab tmptab"><a href="http://hz.youku.com/red/click.php?tp=1&cp=4008527&cpp=1000648&url=http://i.youku.com/u/home?type=showfriends_timeline" target="_blank">我的追剧</a></li>';
            str += '</ul>'; 
        } else {
            str += '<ul>';
            str += '<li class="my-tab current"><a id="MYYOUKU-COMURL" href="http://hz.youku.com/red/click.php?tp=1&cp=4008532&cpp=1000648&url=http://i.youku.com/u/home/" target="_blank">为我推荐</a></li>';
            str += '<li class="my-tab"><a id="MYYOUKU-RECURL" href="http://hz.youku.com/red/click.php?tp=1&cp=4008539&cpp=1000648&url=http://i.youku.com/u/home?type=watch_timeline" target="_blank">观看记录</a></li>';
            str += '<li class="my-tab"><a href="http://hz.youku.com/red/click.php?tp=1&cp=4008545&cpp=1000648&url=http://www.youku.com/user/login/?cburl=' + encodeURI('http://www.youku.com') + '" target="_blank">我的收藏</a></li>';
            str += '<li class="my-tab"><a href="http://hz.youku.com/red/click.php?tp=1&cp=4008546&cpp=1000648&url=http://www.youku.com/user/login/?cburl=' + encodeURI('http://www.youku.com') + '" target="_blank">我的订阅</a></li>';
            str += '<li class="my-tab tmptab"><a href="http://hz.youku.com/red/click.php?tp=1&cp=4008547&cpp=1000648&url=http://i.youku.com/u/home?type=showfriends_timeline" target="_blank">我的追剧</a></li>';
            str += '</ul>';            
        }
        this.domTabs.innerHTML = str;
        var mytabs = MYYOUKUTOOL.Dom.getElementsByClassName("my-container");
        new MyYoukuTab({root:mytabs[0], trigger:"mouseover", currentClass:"current"});
    },

    initRecommendDom : function(data, dom, start, end) {
        var _this = this;
        var str = '';
        var hzcount = 0;
        var lastThumb = '';
        var folderid = '';
        var tModule = 1;
        var showCount = 0;
        var update = '';
        // For IE
        var imglistid = [];
        var imglistsrc = [];

        for(var i = start ; i < end ; i++) {
            var index = 0;
            var stat = '';
            var vtitle = _this.removeHTMLTag(data[i].title);
            var hzThumb = '';
            var hzTitle = '';
            if(_this.cookieYktk) {
               hzcount = i%_this.SHOWNUMBER;
               lastCode = _this.hzCode.hzRecommend[_this.SHOWNUMBER];
               hzCode = _this.hzCode.hzRecommend[hzcount];
            } 
            else {
                tModule = 3
                _this.cookieUid = '';
                lastCode = _this.hzCode.hzAnonymousMore;
                hzCode = _this.hzCode.hzAnonymous[i];
            }
            if(!!data[i].ver) {
                var logdatastr = '{"uid":"' + _this.cookieUid  + '","cookie_id":"' + _this.cookieYsuid + '","apptype":"1","page":"8","module":"' + tModule + '","dvid":"' + data[i].id + '","abver":"' + data[i].ver + '","dma":"' + data[i].dma + '","pos":"' + i + '","ord":"0","dct":"' + data[i].dct + '"}';
                logdatastr = encodeURIComponent(logdatastr);
                str += '<ul class="v" logdata="' + logdatastr + '">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(hzCode, data[i].videolink) + '" target="video" title="' + vtitle + '" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>'; 
                if(_this.isIE){
                    imglistid.push(data[i].videoid);
                    str += '<li class="v_thumb" _src="' + data[i].thumburl + '" id="' + data[i].videoid + '"></li>';
                } else {
                    str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" title="' + vtitle + '" alt="' + vtitle + '" /></li>';
                }
                if(data[i].ishd) {
                    str += '<li class="v_ishd"><span class="ico__HD" title="高清"></span></li>';
                }
                else if(data[i].issd) {
                    str += '<li class="v_ishd"><span class="ico__SD" title="超清"></span></li>';
                }
                str += '<li class="v_menu" id="PlayListFlag_' + data[i].videoid + '" style="display: none; "><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '<li class="v_title"><a href="' + _this.returnHzUrl(hzCode ,data[i].videolink) + '" target="video" title="' + vtitle + '" onclick="MYYOUKU.submitVideoCommend(this)">' + data[i].title + '</a></li>';
                str += '<li class="v_stat"><span class="ico__statplay" title="播放"></span><span class="num">' + this.formatnum(data[i].total_vv) + '</span> <span title="评论" class="ico__statcomment"></span><span class="num">' + this.formatnum(data[i].total_comment) + '</span></li>';
                str += '</ul>';
                if(i+1 == end) {
                    var jumpUrl = '';
                    var lastLogdatastr = encodeURIComponent('{"abver":"' + data[i].ver + '","dma":"' + data[i].dma + '","ord":"0","dct":"' + data[i].dct + '"}');
                    if(_this.cookieYktk) {
                        jumpUrl = _this.returnHzUrl(lastCode, uReUrl);
                    } else {
                        jumpUrl = _this.returnHzUrl(lastCode, 'http://www.youku.com/user/login/?cburl=' + encodeURI('http://www.youku.com'));      
                    }
                    var uReUrl = _this.urlUser + '?videoid=' + data[i+1].videoid + '&logdata=' + lastLogdatastr;
                    str += '<ul class="p">';
                    str += '<li class="p_link"><a title="查看更多" href="' + jumpUrl + '" target="_blank" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>';
                    if(_this.isIE){
                        str += '<li class="p_thumb" id="' + data[i+1].videoid + '" _src="' + data[i+1].thumburl + '"><img alt="查看更多"></li>';
                        imglistid.push(data[i+1].videoid);
                    } else {
                        str += '<li class="p_thumb"><img src="' + data[i+1].thumburl + '" title="' + vtitle + '" alt="' + vtitle + '" /></li>';
                    }
                    str += '<li class="p_status"><span class="status">查看更多</span><span class="bg"></span></li>';
                    str += '</ul>';
                }

            } else {
                str += '<ul class="p">';
                str += '<li class="p_link"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzShowThumb[i] , _this.urlVideo + data[i].vidEncoded) + '?firsttime=' + data[i].sec + folderid +'"></a></li>';
                str += '<li class="p_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '"></li>';	
                if(!!data[i].nextid || !!data[i].watchStage) { 
                    if(!!data[i].updateStage) {
                       update = data[i].updateStage;
                    } 
                    else {
                       update = data[i].watchStage;
                    }
                    str += '<li class="p_status"><span class="status">' + update + '</span><span class="bg"></span></li>';
                } else { 
                    str += '<li class="p_status"><span class="status">' + data[i].watchStage + '</span></li>';
                }
                str += '<li class="p_ishd">' + data[i].definition + '</li>';                
                str += '<li class="p_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzShowTitle[i] , _this.urlVideo + data[i].vidEncoded)  + '?firsttime=' + data[i].sec + folderid +'">' + data[i].showname + '</a></li>';	
                switch (parseInt(data[i].percent)){
                    case 100 :
                        str += '<li class="p_record"><span>已在' + data[i].devicename + '上看完</span></li>';
                        break;
                    case 0 :
                        if(data[i].nextid) {
                            str += '<li class="p_record"><span>已看过' + data[i].watchStage + '</span></li>';
                        }
                        else {
                            str += '<li class="p_record"><span>已在' + data[i].devicename + '上看过</span></li>';
                        }
                        break;
                    default :
                        var tmpStr = '';
                        if(!!data[i].watchStage) {
                            tmpStr = '已看到' + data[i].watchStage + '的' + data[i].percent;
                        }
                        else {
                            tmpStr = '已看到' + data[i].percent;
                        }
                        str += '<li class="p_record"><span>' + tmpStr + '</span></li>';
                }
                str += '</ul>';
                showCount++;
            }
        } 
        dom.innerHTML = str;
        if(_this.isIE){
            _this.getImages(imglistid);
        }
        _this.lockRecommend = false; 
        _this.lockAnonymousRecommend = false; 
    },

    initUserInfo : function(name, avatar) {
        name =  this.removeHTMLTag(name);
        var len = name.length;
        if(len < 8 && this.GetChineseCount(name) <= 5) {
            sname = name;
        } 
        else {
            if(this.GetChineseCount(name) > 5) {
                sname = name.substring(0,5) + '... ';     
            }
            else {
                sname = name.substring(0,8) + '... ';     
            }
        }
        //this.domCaption.innerHTML = '<h3 class="title title_my"><a href="' + this.urlHz + this.hzCode.hzUsername + '&cpp=1000648&url=' + this.urlUser + '" target="_blank">' + sname + '的个人中心</a></h3><div class="clear"></div>';
    },

    initSubscribeDom : function(data, start, end) {
        var _this = this;
        var len = data.length;
        var str = '';
        var shownum = 0;    

        if(!len) {
            str += '<div class="my-null""><span>暂无订阅更新, 去看看精彩的<a href="' + _this.returnHzUrl(_this.hzCode.hzSubscribeNull,'http://i.youku.com/u/official/')+ '" target="_blank">视频官网</a></span></div>';
        } 
        else {
            if(len < end) { end = len; }
            for(var i = start; i < end; i++) {
                var hznum = '';
                var vtitle = _this.removeHTMLTag(data[i].title);
                var folderid = '';
                if(data[i].folderid != 0) {
                    folderid = '&f=' + data[i].folderid;
                }
                str += '<ul class="v">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(_this.hzCode.hzSubscribe, _this.urlVideo + data[i].videoid + '.html') + '" target="video" title="' +vtitle + '"></a></li>';
                str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '" replace="false"></li>';
                str += '<li class="v_ishd">' + data[i].definition + '</li>';                
                str += '<li class="v_menu" style="display: none;"><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '<li class="v_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzSubscribe, _this.urlVideo + data[i].videoid + '.html') + '">' + vtitle + '</a></li>';
                str += '<li class="v_username">' + data[i].usercate + ' <a href="' + _this.returnHzUrl(_this.hzCode.hzSubscribeOwnerName, data[i].userlink) + '" target="_blank" title="' + _this.removeHTMLTag(data[i].username) + '">' + _this.removeHTMLTag(data[i].username) + '</a></li>';
                //str += '<li class="v_date">' + _this.shortDate(data[i].createtime) + ' 上传</li>';
                str += '</ul>';
                if(_this.cookieYktk) {
                    shownum = _this.SHOWNUMBER;
                } else {
                    shownum = _this.ANONYMOUSRECOMMENDNUMBER;
                }
                if(i+1 == shownum && !!data[shownum]) {
                    var uReUrl = _this.urlUser + '?videoid=' + data[shownum].videoid;
                    str += _this.getLastVideo(data[shownum].videoid, uReUrl, _this.hzCode.hzSubscribeMore ,  data[shownum].thumburl, vtitle);
                }
            }
        }
        _this.domSubscribe.innerHTML = str;
        _this.lockSubscribe = false;
    },
    
    initCollectDom : function(data, start, end) {
        var _this = this;
        var len = data.length;
        var str = '';
        var shownum = 0;        

        if(!len) {
            str += '<div class="my-null"><span>暂无收藏, 去看看<a href="' + _this.returnHzUrl(_this.hzCode.hzCollectNull,'http://i.youku.com/u/home') + '" target="_blank">更多精彩内容</a></span></div>';
        } 
        else {
            if(len < end) { end = len; }
            for(var i = start; i < end; i++) {
                var hznum = '';
                var vtitle = _this.removeHTMLTag(data[i].title);
                var folderid = '';
                if(data[i].folderid != 0) {
                    folderid = '&f=' + data[i].folderid;
                }
                str += '<ul class="v">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(_this.hzCode.hzCollect, _this.urlVideo + data[i].videoid + '.html') + '" target="video" title="' +vtitle + '"></a></li>';
                str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '" replace="false"></li>';
                str += '<li class="v_ishd">' + data[i].definition + '</li>';                
                str += '<li class="v_menu" style="display: none;"><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '<li class="v_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzCollect, _this.urlVideo + data[i].videoid + '.html') + '">' + vtitle + '</a></li>';
                //str += '<li class="v_username">' + data[i].usercate + ' <a href="' + _this.returnHzUrl(_this.hzCode.hzCollectOwnerName[i], data[i].userlink) + '" target="_blank" title="' + _this.removeHTMLTag(data[i].username) + '">' + _this.removeHTMLTag(data[i].username) + '</a></li>';
                str += '<li class="v_date">' + _this.shortDate(data[i].favtime) + ' 收藏</li>';
                str += '</ul>';
                if(_this.cookieYktk) {
                    shownum = _this.SHOWNUMBER;
                } else {
                    shownum = _this.ANONYMOUSRECOMMENDNUMBER;
                }
                if(i+1 == shownum && !!data[shownum]) {
                    var uReUrl = _this.urlUser + '?videoid=' + data[shownum].videoid;
                    str += _this.getLastVideo(data[shownum].videoid, uReUrl, _this.hzCode.hzCollectMore,  data[shownum].thumburl, vtitle);
                }
            }
        }
        _this.domCollect.innerHTML = str;
        _this.lockCollect = false;
    },

    initRecordsDom : function(data, start, end) {
        var _this = this;
        var len = data.length;
        var str = '';
        var shownum = 0;
        var hznull = 0;

        if(this.cookieYktk) {
            hznull = this.hzCode.hzRecordsNull;
        } else {
            hznull = this.hzCode.hzAnonymousNull;
        }

        if(len == 0 ) {
            str += '<div class="my-null"><span>暂无观看记录, 去看看<a href="' + _this.returnHzUrl(hznull, 'http://i.youku.com/u/home') + '" target="_blank">更多精彩内容<a></span></div>';
        }      
        else {
            if(len < end) { end = len; }
            for(var i = start; i < end; i++) {
                var hznum = '';
                var hzcode = '';
                var vtitle = _this.removeHTMLTag(data[i].title);
                if(_this.cookieYktk) {
                    hzcode = this.hzCode.hzRecords; 
                    if(!!_this.hzCode.hzRecordsDel) {
                       hznum = _this.hzCode.hzRecordsDel; 
                    }
                } else {
                    hzcode = this.hzCode.hzAnonymousRecords; 
                    if(!!_this.hzCode.hzAnonymousDel) {
                       hznum = _this.hzCode.hzAnonymousDel; 
                    }
                }
                var folderid = '';
                if(data[i].folderid != 0) {
                    folderid = '&f=' + data[i].folderid;
                }
                str += '<ul class="v">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(hzcode, _this.urlVideo + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) + '" target="video" title="' +vtitle + '"></a></li>';
                str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '" replace="false"></li>';
                str += '<li class="v_ishd">' + data[i].definition + '</li>';                
                str += '<li class="v_menu" style="display: none;"><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '<li class="v_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(hzcode, _this.urlVideo + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) + '">' + vtitle + '</a></li>';
                if(parseInt(data[i].percent) === 100) {
                    str += '<li class="v_record"><span>已看完</span></li>';
                } else if(parseInt(data[i].percent) === 0) {
                    str += '<li class="v_record"><span>已看过</span></li>';
                } else {
                    str += '<li class="v_record"><span>已看到' + data[i].percent + '</span></li>'
                }
                str += '<li class="v_close"><a href="javascript:;" onclick="MYYOUKU.deleteList(\'' + _this.TOKEN + '\',\'' + data[i].videoid + '\',\'' + data[i].folderid + '\',\'' + data[i].showid + '\',\'' + data[i].videoid  + '\',\'MYYOUKU-Records\',\'' + hznum + '\');">删除</a></li>';
                str += '</ul>';
                if(_this.cookieYktk) {
                    shownum = _this.SHOWNUMBER;
                    morehznum = _this.hzCode.hzRecordsMore;
                } else {
                    shownum = _this.ANONYMOUSRECOMMENDNUMBER - 1;
                    morehznum = _this.hzCode.hzAnonymousMore;
                }
                if(i+1 == shownum && !!data[shownum]) {
                    var uReUrl = _this.urlUser + '?videoid=' + data[shownum].videoid;
                    str += _this.getLastVideo(data[shownum].videoid, uReUrl, morehznum,  data[shownum].thumburl, vtitle);
                }
            }
        }
        _this.domRecords.innerHTML = str;
        _this.bindDelete(_this.domRecords);
    },

    initAnonymousRecommendDom : function(data) {
        //this.domCaption.innerHTML = '<h3 class="title title_anonymous"><a href="' + this.urlHz + '4007101&cpp=1000648&url=' + this.urlUser + '" target="_blank">个人中心</a></h3>'; 
        this.initRecommendDom(this.dataAnonymous, this.domRecommend, 0, this.ANONYMOUSRECOMMENDNUMBER);
        //this.initLoginDom();
    },

    // More Conetnt
    changeMore : function(o) {
        var _this = this;
        if(_this.MORECOUNT == 1) {
            o.innerHTML = '更多推荐';
            o.className = 'showmore';
            if(_this.cookieYktk) {
                _this.initRecommendDom(_this.dataRecommend, _this.domRecommend, _this.SHOWNUMBER, _this.SHOWNUMBER*2);
                _this.requestImg(_this.returnHzUrl('4007092'));
            } else {
                _this.initRecommendDom(_this.dataAnonymous, _this.domRecommend, _this.ANONYMOUSRECOMMENDNUMBER, _this.ANONYMOUSRECOMMENDNUMBER*2);
                _this.requestImg(_this.returnHzUrl('4007103'));
            }
            _this.MORECOUNT++;
        } 
        else if(_this.MORECOUNT == 2) {
            if(_this.cookieYktk) {
                o.href= _this.returnHzUrl('4006771', _this.urlUser);
                _this.returnHzUrl('4006771', _this.urlUser);
            } else {
                o.href= _this.returnHzUrl('4007102', _this.urlUser);
                _this.returnHzUrl('4006771', _this.urlUser);
            }
            o.target= '_blank';
        } 
        else {
            if(_this.cookieYktk) {
                _this.CURRENTINDEX = _this.SHOWNUMBER * _this.MORECOUNT;
                _this.initRecommendDom(_this.dataRecommend, _this.domRecommend, _this.CURRENTINDEX, _this.SHOWNUMBER + _this.CURRENTINDEX);
                _this.requestImg(_this.returnHzUrl('4007092'));
            } else {
                _this.CURRENTINDEX = _this.ANONYMOUSRECOMMENDNUMBER * _this.MORECOUNT;
                _this.initRecommendDom(_this.dataAnonymous, _this.domRecommend, _this.CURRENTINDEX, _this.ANONYMOUSRECOMMENDNUMBER + _this.CURRENTINDEX);
                _this.requestImg(_this.returnHzUrl('4007103'));
            }
            _this.MORECOUNT++;
        }
        return false;
    },

    getLastVideo : function(vid, reUrl, lastHz, thumburl, vtitle) {
        var str = ''
        str += '<ul class="p">';
        str += '<li class="p_link"><a title="查看更多" href="' + this.returnHzUrl(lastHz ,reUrl) + '" target="_blank" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>';
        str += '<li class="p_thumb"><img src="' + thumburl + '" title="' + vtitle + '" alt="' + vtitle + '" /></li>';
        str += '<li class="p_status"><span class="status">查看更多</span><span class="bg"></span></li>';
        str += '</ul>';  
        return str;
    },

    showContent: function(height) {
        var _this = this;
        _this.dom.style.display = 'block';
        var slidingTimer = null;
        var b=10, c=height, d=5, t=0;
        function easeOut(t,b,c,d) {
            return -c *(t/=d)*(t-2) + b;
        };
        function Sliding(){
            _this.dom.style.height = easeOut(t,b,c,d) + 'px';
            if(t<d){
                t++;
                slidingTimer = setTimeout(Sliding, 1);
            }
            else{
                window.clearInterval(slidingTimer);
            }
        };
        Sliding();  
    },

    // For IE
    getImages : function(imglistid) {
        var _this = this;
        var doms = [];
        var imgs = [];
        var index= 0;
        for(var i = 0 ; i < imglistid.length; i++) {
            var thumb = document.getElementById(imglistid[i]);
            doms.push(thumb);
            imgs.push(thumb.getAttribute('_src'));
        }
        var timer = setInterval(function(){
            try {
                var img = new Image();
                doms[index].appendChild(img);
                img.src = imgs[index];
                if(index >= _this.SHOWNUMBER) {
                    clearInterval(timer);
                } else {
                    index++;
                }
            } catch(e) {
            }
        },5);
    },

    // Delete Records
    deleteList : function(tk, vid, fid, shid, lild, list, charset) {
        var hznull = '';
        var e = window.event || arguments.callee.caller.arguments[0];
        var obj = e.target || e.srcElement;
        if(!tk || !vid || !lild) return;
        if(fid == undefined) fid = 0;
        if(shid == undefined) shid = 0;
        var delUrl = "http://yus.navi.youku.com/playlog/delete.json?token="+tk+"&v="+vid+"&fid="+fid+"&shid="+shid+"&"+Math.random();
        var listObj = document.getElementById(list);
        var listUL = listObj.getElementsByTagName('UL');
        var itemObj = obj.parentNode.parentNode;
        if(this.cookieYktk) {
            hznull = this.hzCode.hzRecordsNull;
        } else {
            hznull = this.hzCode.hzAnonymousNull;
        }
        if(listUL.length == 1) {
            if(list == 'MYYOUKU-Records') {
                var str = '<div class="my-null"><span>暂无观看记录, 去看看<a href="' + this.returnHzUrl(hznull,'http://i.youku.com/u/home') + '" target="_blank">更多精彩内容<a></span></div>';
            }
            listObj.innerHTML = str; 
        }
        else {
            listObj.removeChild(itemObj);
        }
     
        var img = new Image();
        img.src = delUrl;    
        if(charset) {
            addScript("http://hz.youku.com/red/click.php?tp=1&cp="+charset+"&cpp=1000648");
        };
        function addScript(src){
            var doc = document;
            var g = doc.createElement("script");
            g.type = "text/javascript";
            g.src = src;
            doc.getElementsByTagName('head')[0].appendChild(g);
        }
    },

    // Request Images SRC
    requestImg : function(url) {
        var imgReq = new Image();
        imgReq.src = url;   
        imgReq = null;
    },

    submitVideoCommend : function(o){
        var req = o.parentNode.parentNode.getAttribute('logdata');
        req = decodeURIComponent(req);
        var d = eval('('+req+')');
        if(d) {
            var logstr = 'uid=' + d.uid  + '&cookie_id=' + d.cookie_id + '&apptype=1&page=8&module=' + d.module + '&dvid=' + d.dvid + '&abver=' + d.abver + '&dma=' + d.dma + '&pos=' + d.pos + '&ord=0&dct=0';
            var url = 'http://e.stat.youku.com/personalization/log?';
            var imgReq = new Image();
            imgReq.src = url + logstr;  
            imgReq = null;
        }   
    },

    // Get Random String
    randomStr : function(l)  {
        var x="0123456789qwertyuioplkjhgfdsazxcvbnm";
        var tmp="";
        var timestamp = new Date().getTime();
        for(var i=0;i<l;i++)  {
            tmp += x.charAt(Math.ceil(Math.random()*100000000)%x.length);
        }
        return timestamp+tmp;
    },

    // Format Number
    formatnum : function(n){
        n  =  n+''; 
        if(n.indexOf(',')>0){n = n.replace(/,/gi,'') + '';};  
        var re=/(-?\d+)(\d{3})/;
        while(re.test(n)){n = n.replace(re,'$1,$2')}
        return n;  
    },

    GetChineseCount : function(str){ 
        var chinieseCount=0;
        var badChar ="ABCDEFGHIJKLMNOPQRSTUVWXYZ"; 
        badChar += "abcdefghijklmnopqrstuvwxyz"; 
        badChar += "0123456789"; 
        badChar += " "+"　";//半角与全角空格 
        badChar += "`~!@#$%^&()-_=+]\\|:;\"\\'<,>?/";//不包含*或.的英文符号 
        if(""==str){ 
            return false; 
        } 
        for(var i=0;i<=str.length;i++)
        { 
            var c = str.charAt(i);//字符串str中的字符 
            if(badChar.indexOf(c) > -1){ 
            }
            else{
                chinieseCount++;
            } 
        } 
        return chinieseCount;
    },

    // Format Time Second to Minute
    secondTofMinute : function(t) {
        var m = parseInt(t/60); 
        var s = parseInt(t%60); 
        if(m < 10) {m = '0' + m;} else {m = m.toString();}
        if(s < 10) {s = '0' + s;} else {s = s.toString();}
        return m + ':' +s;
    },

    //Short Date 
	shortDate: function(tp) {
        var str = tp;
        var new_tp = str.replace(/:/g,'-');
        new_tp = new_tp.replace(/ /g,'-');
        var arr = new_tp.split('-');
        var datum = new Date(Date.UTC(arr[0],arr[1]-1,arr[2],arr[3]-8,arr[4],arr[5]));
        tp = datum.getTime()/1000;
		var d, now = new Date(), delta;
		tp = parseInt(tp);
		if(!tp || isNaN(tp)) return '';
		d = new Date(tp*1000);
		delta = Math.round((now - d) / 1000);
		
		var yyyy = d.getFullYear();
		var mm = d.getMonth() + 1; 		 mm = this.fillzero(mm);  
		var dd = d.getDate();			 dd = this.fillzero(dd);
		var HH = d.getHours();			 HH = this.fillzero(HH);
		var MM = d.getMinutes();		 MM = this.fillzero(MM);
		
		var t0 = new Date(now.getFullYear(), now.getMonth(), now.getDate())/1000;
		var t1 = d.getTime()/1000;
		
		//1分钟内
		if(delta < 60){ return '刚刚'; } 
		//1小时内
		else if(delta < 3600){ return Math.floor(delta/60) +'分钟前'; }
		//当天内
		else if(t1 > t0){ return  Math.floor(delta/3600)+'小时前'; }
		//昨天
		else if(t0 - t1 < 24*3600){ return '昨天 ' + HH + ':' + MM; }
		//前天
		else if(t0 - t1 < 48*3600){ return '前天 ' + HH + ':' + MM; }
		//一周内
		else if(delta < 7*24*3600){ return Math.floor(delta/(24*3600)) + '天前';
		}else{
			//本年内 || 去年但是当前是1月份
			if(yyyy == now.getFullYear() || (now.getFullYear() - yyyy == 1 && now.getMonth()==0)){ return mm + '-' + dd + ' ' + HH + ':' + MM;	 }
			//全年但当前是2月份及以后
			else{ return yyyy + '-' + mm + '-' + dd; }
		}
		return delta;
	},

	fillzero: function(n){
		if(n<10){ return '0' + n; }
		return '' + n;
	},

    // Login
    mylogin : function (hzcode) {
        MYYOUKU.requestImg(MYYOUKU.returnHzUrl(hzcode));
        login(MYYOUKU.refresh);
    },

    refresh : function () {
        MYYOUKU.requestImg(MYYOUKU.returnHzUrl('4007350'));
        window.location.reload();
    },

    // Filter HTML Tag
    removeHTMLTag : function (str) {
        return str.replace(/</g,"&lt;").replace(/>/g,"&gt;").replace(/"/g,"&quot;");
    },

    // Get Json Data
    getJson : function(url){
        var _this = this;
        var _script = document.createElement("script");
        _script.type = "text/javascript";
        _script.src = url + _this.randomStr(10);
        if (!/*@cc_on!@*/0) {
            _script.onload = _this.callBackScript;
        } else {
            _script.onreadystatechange = function () { 
                if (_script.readyState == 'loaded' || _script.readyState == 'complete') { 
                    _this.callBackScript();
                } 
            }
        }
        document.getElementsByTagName("head")[0].appendChild(_script);
    },

    callBackScript : function(){
        if(this.parentNode){
            this.parentNode.removeChild(this);
        }
    }
}


window.nova_init_hook_myyouku = function() {
    MYYOUKU.init();
}



</script>
</head>

