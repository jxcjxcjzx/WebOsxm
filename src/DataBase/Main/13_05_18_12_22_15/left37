		UI.hide($('btnWrap'));
		UI.addClass(funBox,'isBlock');
		UI.hide($$(".opt_l_c1")[0]);
		UI.hide($$(".opt_l_c2")[0]);
	}
}
if(blackDel){
	blackDel.onclick=function(){
		var obj = {u:'mycos',name:'mycos',sex:'它',target:this};
		obj.call = function(){
			blackDel.innerHTML='[取消拉黑]';
			UI.addClass(blackDel,'blackDel');
