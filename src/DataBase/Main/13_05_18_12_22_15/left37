			document.location.href = 'http://t.qq.com' + href + '?dd=1';
		}
	}
}});
if(black){
	black.onclick=function(){
		var obj = {u:'mycos',name:'mycos',sex:'它',target:this};
		if(!this.innerHTML.hasString('取消')){
			obj.call = blackCall;
		}
		MI.black(obj);
	}
	function blackCall(){
		black.innerHTML='<b></b>拉黑名单';
		UI.removeClass(black,'blackDel');
		if($('followBtn')&&$('followBtn').style.display=='none'){
		followCall();
