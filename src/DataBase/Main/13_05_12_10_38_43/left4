    $( "#srch1" ).keydown( function( event ) {
        if( event.keyCode == 13 )
        {
            var val = $( this ).val();
            val
                ? window.open( 'http://so.csdn.net/Default.aspx?q=' + encodeURIComponent( val ) )
                : alert( '请输入搜索内容' );
        }
    } );
});
