  <div class="link_r"><img src="http://bbs.fwxgx.com//images/link_r.jpg" /></div>
</div>
<script>
  $(function() {
    var scrtime;
    $("#link_demo").hover(function() {
      clearInterval(scrtime);

    }, function() {
      scrtime = setInterval(function() {
        var $ul = $("#link_demo ul");
        var liHeight = $ul.find("li:last").height();
        $ul.animate({
          marginTop: liHeight + 30 + "px"
        }, 1000, function() {
          $ul.find("li:last").prependTo($ul)
          $ul.find("li:first").hide();
          $ul.css({
            marginTop: 0
          });
          $ul.find("li:first").fadeIn(1000);
        });
      }, 10000);
    }).trigger("mouseleave");
  });
</script>
<script>
  $(document).ready(function(){
      $.get(
              "/users/get_new_reviews_of_homepage",
              function(backmsg){
                  $("#con ul.navlistsub").html(backmsg)
              }
      )
  })
  function backCallfirsttoattention(msg){
    if(msg>0)
    {
        alert('关注成功');
        window.location.reload();
    }
    else
    {
        alert('关注失败！！');
    }
}
</script>
