  $(document).ready(function(){
      $.ajax({
          type: "GET",
          url: "/users/info_of_index_top",
          success:function(backmsg){
              $("#index_top_of_user_info").html(backmsg);
              IS_LOGIN = $("#current_blog_user_login").val();
              IS_OPEN_BLOG = $("#current_is_open_blog").val();
