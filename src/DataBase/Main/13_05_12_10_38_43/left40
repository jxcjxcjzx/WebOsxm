<script>
Do(function() {
$('#buzz-songs').find('li')
   .hover(function(){
     $(this).toggleClass('item-hover');
   })
   .click(function(e) {
     var el = $(this);
     if ($.contains(el.find('.pic')[0], e.target) || e.target.tagName === 'A') {
       return;
     }
     el.find('.artist-song-play').trigger('click');
   })
});
</script>






