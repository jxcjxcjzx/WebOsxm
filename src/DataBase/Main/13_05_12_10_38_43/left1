                        <!-- mod27 -->
                        <div class="mod-27 mod-02">
                            <div class="tit02">
                                <h3><a href="http://history.sina.com.cn/news/list/2.shtml" target="_blank">��ʷר��</a></h3>
                            </div>
                            <div class="mod27-cont" data-sudaclick="blk_history_zl" blkclick="auto_nav" blktitle="��ʷר��">
