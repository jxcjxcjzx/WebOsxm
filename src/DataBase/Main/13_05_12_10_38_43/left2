    initRecordsDom : function(data) {
        var _this = this;
        var len = data.length;
        var str = '';

        if(len == 0 ) {
            str += '<div class="recordnull" id="watchlist"><span>没有播放记录</span></div>';
        }      
        else {
            for(var i = 0; i < len; i++) {
                var hznum = '';
                var vtitle = _this.removeHTMLTag(data[i].title);
                if(!!_this.hzCode.hzRecordsDel[i]) {
                   hznum = _this.hzCode.hzRecordsDel[i]; 
                }
                var folderid = '';
                if(data[i].folderid != 0) {
                    folderid = '&f=' + data[i].folderid;
                }

                str += '<ul class="playrecord clearfix" itemindex="' + i + '">';
                str += '<li class="rec_thumb">';
                str += '<ul class="v vsh">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(_this.hzCode.hzRecordsThumb[i], _this.urlVideo + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) + '" target="video" title="' +vtitle + '"></a></li>';
                str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '" replace="false"></li>';
                str += '<li class="v_ishd">' + data[i].definition + '</li>';                
                str += '<li class="v_menu" style="display: none;"><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '</ul>';
                str += '</li>';
                str += '<li class="rec_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(_this.hzCode.hzRecordsTitle[i], _this.urlVideo + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) + '">' + vtitle + '</a></li>';
                if(parseInt(data[i].percent) === 100) {
                    str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看完</span></li>';
                }
                else if(parseInt(data[i].percent) === 0) {
                    str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看过</span></li>';
                    str += '<li class="rec_continue"><a target="_blank" href="' + _this.returnHzUrl(_this.hzCode.hzRecordsGo[i], _this.urlVideo +  data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) +'">继续看</a>';
                }
                else {
                    str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看到' + data[i].percent + '</span></li>'
                    str += '<li class="rec_continue"><a target="_blank" href="' + _this.returnHzUrl(_this.hzCode.hzRecordsGo[i], _this.urlVideo +  data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) +'">继续看</a>';
                    if(data[i].nextid && data[i].next !== 0) {
                        str += '<span>|</span><a target="_blank" href="' + _this.returnHzUrl(_this.hzCode.hzRecordsNext[i], 'http://v.youku.com/v_nextstage/id_' + data[i].showid + '.html?p=' + data[i].stage + '&v=' + data[i].pk_video) + '">下一集</a>'
                    }
                    str += '</li>';
                }
                str += '<li class="rec_close"><a href="javascript:;" onclick="MYYOUKU.deleteList(\'' + _this.TOKEN + '\',\'' + data[i].videoid + '\',\'' + data[i].folderid + '\',\'' + data[i].showid + '\',\'' + data[i].videoid  + '\',\'MYYOUKU-Records\',\'' + hznum + '\');"><em title="删除"></em></a></li>';
                str += '</ul>';
            }
        }
        _this.domRecords.innerHTML = str;
        _this.bindDelete(_this.domRecords);
    },

    initAnonymousRecommendDom : function(data) {
        this.initShowVideoDom(this.dataAnonymous, 0 ,this.ANONYMOUSRECOMMENDNUMBER);
