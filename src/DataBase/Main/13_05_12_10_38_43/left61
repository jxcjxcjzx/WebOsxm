<script type="text/javascript">
$("#select_div li").each(function(){
	$(this).click(function(){
		window.location = "http://emag.bundpic.com/index.php?index=" + $(this).attr("title");
	}).mouseover(function(){
		$(this).siblings().removeClass('selected');
		$(this).addClass('selected');
	});
});
$("#select_input").val($("#select_div li:first").html());
function showpic(){
	var cata_v=$("#cata_v").val();
	$(".catalog_div").hide();
	$("#catalog_div_"+cata_v).show();
}
