<script>

/* 图片列表滚动*/
var $ = jQuery;
function t1(){


	$(".left_but").click(function(){
								   
			 ul_left=$("#js_hd ul").css("left");		    
			 ul_left=ul_left.replace(/px/,"");
			 ul_left=parseInt(ul_left)+705;
				
			
				
				
				if(ul_left==-705){
				
				 $(".fy a").eq(0).attr("id"," ");
				$(".fy a").eq(1).attr("id","cur");
				  $(".fy a").eq(2).attr("id"," ");
				 }
				
				if(ul_left==-0){
				
				 $(".fy a").eq(0).attr("id","cur");
				$(".fy a").eq(1).attr("id","");
				  $(".fy a").eq(2).attr("id"," ");
				 }
				
				
				
			 if(ul_left==705){
				
				 return false;
				 
				 }else{
			$("#js_hd ul").animate({left:ul_left+"px"});
			}
			return false;
								   
			        			   });
	
$(".right_but").click(function(){
								   
			 ul_left=$("#js_hd ul").css("left");		    
			 ul_left=ul_left.replace(/px/,"");
			 ul_left=ul_left-705;
			  
			 if(ul_left==-705){
				
				 $(".fy a").eq(0).attr("id"," ");
				$(".fy a").eq(1).attr("id","cur");
				  $(".fy a").eq(2).attr("id"," ");
				 }
				 
				 if(ul_left==-1410){
				 $(".fy a").eq(0).attr("id"," ");
				$(".fy a").eq(1).attr("id","");
				  $(".fy a").eq(2).attr("id","cur");
				 
				 }
				 
			 if(ul_left==-2115){
				
				 return false;
				 
				 }else{
			$("#js_hd ul").animate({left:ul_left+"px"});
			}
			return false;
								   
			        			   });


$(".fy a").click(function(){
						
			t_cls_n=$(this).attr("class");			
			t_cls_n=t_cls_n.replace(/a_fy_/,"");
			
			
			
			if(t_cls_n==0){
				$("#js_hd ul").animate({left:0+"px"});
				
				}else if(t_cls_n==1){
				
				$("#js_hd ul").animate({left:-705+"px"});
				
				}else if(t_cls_n==2){
				
				$("#js_hd ul").animate({left:-1410+"px"});
				
				}
			
			for(k=0;k<3;k++){
			if(t_cls_n==k){
			$(".fy a").eq(k).attr("id","cur");
				}else{
			$(".fy a").eq(k).attr("id"," ");
						}
			}
			return false;  
							 });

	}
t1();
</script>
