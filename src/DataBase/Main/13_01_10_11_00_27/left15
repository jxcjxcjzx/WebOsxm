    <TD><INPUT type=radio value=3 name=choice> 休闲娱乐类</TD></TR></TBODY></TABLE>
<SCRIPT language=JavaScript>
function result(form)
{
	var id = "";
	var choice = "";
	
	for(var i = 1; i < form.elements.length; i++)
	{
		if(form.elements[i].checked)
		{
			choice = form.elements[i].value;
			break;
		}
	}
	
	id = form.id.value;
	line = "http://search.eastday.com/scripts/vote.dll?Voting?id=" + id + "&choice=" + choice;
	window.open(line, "result",
		"toolbar=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width=450,height=380");
}
function showresult(form)
{
	var id = "";
	
	id = form.id.value;
	line = "http://search.eastday.com/scripts/vote.dll?ShowResult?id=" + id;
	window.open(line, "result",
		"toolbar=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width=450,height=380");
}	
</SCRIPT>

