        artistsPlayer.ready(function(artistsPlayer, hasPlayer){
            isPlayerLoaded = true;
        });
        $(document).delegate('.artist-song-play', 'click', function(e) {
            e.preventDefault();
            var el = $(this);
            var sid = el.data('sid');
            if(sid){
                isPlayerLoaded && artistsPlayer.play(sid);
            }
        });
