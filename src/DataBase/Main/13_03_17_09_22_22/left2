<script>

(function(){
	var Cookie = {
		set: function(name, value, option){
			var str = name + '=' + escape(value);  
			if(option){
				if(option.expires){
					var date = new Date();
					var ms = option.expires*24*3600*1000;
					date.setTime(date.getTime() + ms);
					str += '; expires=' + date.toGMTString();
				} 
				if(option.path)   str += '; path='  + path;
				if(option.domain) str += '; domain' + domain;
				if(option.secure) str += '; true';
			}
			document.cookie = str;
		},
		get: function(name){
			var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
			if(arr != null) return unescape(arr[2]);
			return null;
		},
		delCookie : function(name){
			var date = new Date();
    		date.setTime(date.getTime() - 10000);
    		document.cookie = name + "=a; expires=" + date.toGMTString();	
		}
	}
	var userAgent = window.navigator.userAgent;
	var cookiename = 'yc_index_bg';
	var handle = document.getElementById('bg_close');	
	var css = document.getElementById('bg_css');
	var grah = document.getElementById('bg_grah');
	var cookie = Cookie.get(cookiename);
	if(userAgent.match(/Android/i) || userAgent.indexOf('iPod') !== -1 || userAgent.indexOf('iPad') !== -1 || userAgent.indexOf('iPhone') !== -1){
		css.disabled = true;
		css.parentNode.removeChild(css);
		grah.parentNode.removeChild(grah);
		Cookie.set(cookiename, 'hide', {expires: 1});

} else {


	if(!cookie){
	
		grah.style.display = 'block';
		css.disabled = false;	
	
	}else{
		css.disabled = true;
		css.parentNode.removeChild(css);	
	}
	handle.onclick = function(){ 
		css.disabled = true;
		css.parentNode.removeChild(css);
		grah.parentNode.removeChild(grah);
		Cookie.set(cookiename, 'hide', {expires: 1});
	}
}
})();

</script>




</div>



