BS.pubsub.fire("spreadgun", "http://digg.com/");
});
$(function () { $("#footer-copyright-year").html(new Date().getFullYear()) });
function submitDailyDiggEmail(){
var email = $.trim($("#daily-digg-email-input").val());
var
el = $(this),
url = '/api/dailydiggemail.json',
diggDailyEmailProvider = new BS.util.xhr.dataProvider({
url : url,
type : 'POST',
data : {
email : email,
_xsrf : BS.util.cookies.get("_xsrf")
},
success : function (response) {
$("#daily-digg-email-form").hide();
$("#daily-digg-email-form-thanks").show();
BS.pubsub.fire("daily_digg_email_submit_success");
},
error : function (error){
console.log("error", error);
$("#daily-digg-email-error").html("There was a problem submitting your email. Please try again.").show();
}
});
diggDailyEmailProvider.request();
return false;
}
$("#daily-digg-email-input").on("keyup", function(event){
if (event.keyCode === 13) {
submitDailyDiggEmail();
} else {
$("#daily-digg-email-error").html("&nbsp;");
}
});
$(function () {
$("#session-forms").load("/session/forms");
});
$("#daily-digg-email-submit-btn").on("click", function(event){
submitDailyDiggEmail();
});
// Load the SDK Asynchronously
(function(d){
var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
js = d.createElement('script'); js.id = id; js.async = true;
js.src = "//connect.facebook.net/en_US/all.js";
d.getElementsByTagName('head')[0].appendChild(js);
}(document));
}(this, this.jQuery));
</script>

<script type="text/javascript">
(function (window, document, $, _, BS) {
BS.extend(BS, { features : {"polling_scores": false, "saved_stories": true, "homepage_new_stories": true, "signin": true, "homepage_popular_stories": true, "readlater": true, "submit_link": true, "homepage_new_stories_polling": false, "digg": true} });
$(document).on("click", ".story-link", BS.storyactions.redirectStoryLink);
/*
$(".btn-story-action-container-digg")
.on("mouseenter", ".story-score", function (event) {
var hideScoreDetails = function (event) {
$(".btn-story-action-container-digg").removeClass("show-score-details");
$(document).off("click", hideScoreDetails);
};
event.preventDefault();
event.stopPropagation();
BS.storyactions.showScoreDetails.call(this, event);
$(document).on("click", ".story-container", hideScoreDetails);
return false;
});
*/

$(".btn-story-action-digg")
.on("click", ".btn-story-action-off", BS.userdata.authorized(BS.storyactions.submitDigg, "Digg"))
.on("click", ".btn-story-action-on", BS.storyactions.deleteDigg);

var
storyEls = $(".story-container"),
contentIDs = _.map(storyEls, function (storyEl) {
return $(storyEl).data('contentId');
}),
topStoriesScoresUpdater = BS.story_scores.factory('top_stories');
if (contentIDs.length) {
topStoriesScoresUpdater.register(contentIDs);
}
BS.util.pubsub.on('score_updates', BS.pubsub.channels.scores_update, function (data) {
$(function () { BS.storyactions.renderStoryScores(data); });
});
}(this, this.document, this.jQuery, this._, this.BS));
</script>


<script type="text/javascript">
var _sf_async_config={uid:30608,domain:"digg.com"};
(function(){
function loadChartbeat() {
window._sf_endpt=(new Date()).getTime();
var e = document.createElement('script');
e.setAttribute('language', 'javascript');
e.setAttribute('type', 'text/javascript');
e.setAttribute('src',
(("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://static.chartbeat.com/") +
"js/chartbeat_pub.js");
document.body.appendChild(e);
}
var oldonload = window.onload;
window.onload = (typeof window.onload != 'function') ?
loadChartbeat : function() { oldonload(); loadChartbeat(); };
})();
</script>

