<script>
function renderPanGuo(id){
  var currUrl = encodeURIComponent(top.location.href);
  var _id = 63;
  var _url = 'http://a.pongo.com.cn/Job/GetAdForCSDNCallBack%3Fid%3D'+_id+'%26Url%3D'+encodeURIComponent(currUrl);
  $.ajax({
      url:unescape(_url),
      dataType:"jsonp",
      jsonp:"CallBack",
      scriptCharset:"utf-8",
      success:function (data) {
          $("#"+id+" ul").html("");
          var domStr="";
          for(var i=0,len=data.length;i<len;i++){
            var item = data[i]; 
            var src= item.src;
            var url= item.url;
            var title= item.title||"";
            domStr +='<li><a onclick="LogClickCount(this,107);" href="'+url+'" title="'+title+'" target="blank" ><img src="'+src+'"/></a></li>';
          }
          $(domStr).appendTo($("#"+id+" ul"));
      }
  });
}
renderPanGuo('scrollad');
</script>
