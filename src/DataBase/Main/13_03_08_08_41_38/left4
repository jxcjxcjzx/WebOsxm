<script type="text/javascript" src="http://p5.img.cctvpic.com/10/HOMEPAGE/2012/index14.js" charset="utf-8"></script>

<script type="text/javascript">
function lianghuiShow(){
	$(".right_date").click(function(){
	  $("#lianghui > li").each(function(i){
			i=i+1;
			var li=$("#lianghui > li").length;
			if($("#lh"+i).attr("class")=="cur"){
				o=i+1;	
				li2=li-1;
				var cl=$("#lh"+o).attr("data");
				if(cl==0){
				}
				else{
					o=i+1;
					var ad=$("#lh"+o+" .dd").val();
					if(ad=="cc")
					{
						$(".right_date").css({cursor:"default"});
					}
					else
					{
						 $(".right_date").css({"cursor":"pointer"});
					}
					if(i==li){
						$("#lh"+i).css({"height":"0"}).hide();
						i=0;
						$("#lianghui > li").attr("class","");
						y=i+1;
						var src=$("#lh"+i).attr("name");//切left图
						$(".left_date").find("img").attr("src",src);
						k=i+2;
						if(k>li){
							$(".right_date").hide();
						}else{
							$(".right_date").show();
							var src2=$("#lh"+k).attr("name");//切right图
							$(".right_date").find("img").attr("src",src2);
						}
						//$("#lh"+y).animate({height:"490px"}).show();
						$("#lh"+y+":not(:animated)").animate({height:"490px"}).show();
						$.get($('#lh'+y).attr('lang'),function(data){
					if (data!=''){
						$('#lh'+y).html(data);
					}
				});
						$("#lh"+y).attr("class","cur");
						return false;
					}else{
						$("#lianghui > li").attr("class","");
						$("#lh"+i).css({"height":"0"}).hide();
						y=i+1;
						//$("#lh"+y).animate({height:"490px"}).show();
						$("#lh"+y+":not(:animated)").animate({height:"490px"}).show();
						$.get($('#lh'+y).attr('lang'),function(data){
					if (data!=''){
						$('#lh'+y).html(data);
					}
				});
						var src=$("#lh"+i).attr("name");//切left图
						$(".left_date").show();
						$(".left_date").find("img").attr("src",src);
						k=i+2;
						if(k>li){
							$(".right_date").hide();
						}else{
							var src2=$("#lh"+k).attr("name");//切right图
							$(".right_date").find("img").attr("src",src2);
						}
						$("#lh"+y).attr("class","cur");
						return false;
					}
				}
			};
		});
	});
	$(".left_date").click(function(){
		$(".right_date").css({"cursor":"pointer"});
	  $("#lianghui > li").each(function(i){
			i=i+1;
			var li=$("#lianghui > li").length;
			if($("#lh"+i).attr("class")=="cur")
			{
				k=i-1;
				if(k==1)
				{
					
					$("#lh"+i).attr("class","");
					$("#lh"+i).animate({height:"0"}).hide();
					
					$("#lh"+k).attr("class","cur");
					//$("#lh"+k).animate({height:"490px"}).show();
					$("#lh"+k+":not(:animated)").animate({height:"490px"}).show();
					$.get($('#lh'+k).attr('lang'),function(data){
					if (data!=''){
						$('#lh'+k).html(data);
					}
				});
					$(".left_date").hide();
					var src2=$("#lh"+i).attr("name");//切right图
					$(".right_date").find("img").attr("src",src2);
				}
				else
				{
					$("#lh"+i).attr("class","").css({"height":"0"}).hide();
					$("#lh"+k).attr("class","cur");
					//$("#lh"+k).animate({height:"490px"}).show();
					$("#lh"+k+":not(:animated)").animate({height:"490px"}).show();
					$.get($('#lh'+k).attr('lang'),function(data){
					if (data!=''){
						$('#lh'+k).html(data);
					}
				});
					z=i-2;
					var src=$("#lh"+z).attr("name");//切right图
					$(".left_date").find("img").attr("src",src);
					var src2=$("#lh"+i).attr("name");//切right图
					$(".right_date").find("img").attr("src",src2);
					$(".right_date").show();
				}
			};
		});
  });
}

new lianghuiShow();
</script>

<script>

/* 图片列表滚动*/
var $ = jQuery;
var currr = 0;
function t2(){



	$(".right_but").click(function(){
								   
				
			if(currr<2){
				currr += 1;
				for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
			}
			else if(currr>=2)
				{   currr=2;
					return false;}
						   });
	
$(".left_but").click(function(){
	

			if(currr>0)
				{ 
					currr -= 1;
					for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
					}
				else if(currr<=0)
				{   currr==0;
					return false;}
								   
	 });


$(".fy a").click(function(){

			var index = $(this).index();
			currr =index;
			 for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
			
				 });

	}
t2();
</script>
