var $soufun=function(id) {
   return document.getElementById(id);
}
function moreBoxto(numcss1020){
	for (j=0; j<10; j++){
	    if ($soufun('moreBox' + j)) {
	        $soufun('moreBox' + j).style.display = 'none';

	 }
	}
	$soufun('moreBox' + numcss1020).style.display = 'block';
}
function hidden_moreBoxto(numcss1020) {
    timeout = function() { $soufun("moreBox" + numcss1020).style.display = "none"; };
}
function show_moreBoxto(numcss1020){
	window.clearTimeout(timeout);
	$soufun("moreBox" + numcss1020).style.display = "";
}
