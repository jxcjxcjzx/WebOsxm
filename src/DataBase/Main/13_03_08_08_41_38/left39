//wrapper
(function(){
	var _close = NTES("#spWrapperColse");
	var _head = NTES("#spWrapperHead");
	var _wrapper = NTES("#spWrapper");
	var _body = NTES('body');
	_wrapper.addCss("sp-wrapper");
	_body.addCss("sp-wrapper-body");
	_head.style.display = "block";
	_close.addEvent("click",function() {
		_body.removeCss("sp-wrapper-body");
		_wrapper.removeCss("sp-wrapper");
		_head.style.display = "none";
	});
})();
</script>
