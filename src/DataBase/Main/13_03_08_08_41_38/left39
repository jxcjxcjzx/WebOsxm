<script type="text/javascript">
    //<![CDATA[
    (function(){
        /*
                  */
        var form = $("#passportLogin"),input = $("#passportLogin [name=username]")[0],pass = $("#passportLogin [name=password]")[0];
        var login = new PassportSuggest();
        login.init(form,input,pass,"#passportUsernameList");
        var login_true = $("#login_true"), login_false = $("#login_false"), autologin_tiparea = $('#autologin_tiparea');
        var showAndHide = function(boolean){
            if(boolean){
                login_true.addCss('display:block');
                login_false.addCss('display:none');
            }else{
                login_true.addCss('display:none');
                login_false.addCss('display:block');
            }
        }
        /*��¼�ж�*/
        var Config = {
            currentBg:"#4472AE",
            currentColor:"#ffffff",
            normalBg:"#ffffff",
            normalColor:"#2b2b2b",
            userinput_c_color:"#2b2b2b",
            userinput_n_color:"#bbbbbb",
            userinput_d_text:"name@example.com",
            requestUrl:"http://bbs.163.com/bbs/personalInfo.jsp?username="
        }
        var pinfo = NTES.cookie.get("P_INFO"),
                sinfo = NTES.cookie.get("S_INFO"),
                user,nickname,isLogined,
                passport = pinfo.split("|")[0],
                index = passport.indexOf("@163.com");
        if (index == -1){
            user = passport;
        }
        else {
            user = passport.substring(0, index);
        }
        var url = Config.requestUrl + user;
        if(pinfo && sinfo || pinfo.split("|")[2] == "1"){
            NTES.ajax.importJs(url,function(){
                personalInfo.nickname ? nickname = personalInfo.nickname : nickname = user;
                /*
                 var str = "";
                 var code = nickname.match(/&#(\d+);/g);
                 if (code == null) { return; }
                 for (var i = 0; i < code.length; i++){
                 str = str + String.fromCharCode(code[i].replace(/[&#;]/g, ''));
                 }
                 nickname = str;
                 */
                showAndHide(true);
                document.getElementById("nicknameLink").innerHTML = '<a href="http://bbs.163.com/' + user + '" class="fB">' + nickname + '</a>';
            });
        }else{
            if(pinfo && user != null){
                input.value = user;
                //input.style.color = Config.userinput_n_color;
            }else{
                input.style.color = Config.userinput_n_color;
                input.value = Config.userinput_d_text;
            }
            showAndHide(false);
            /* ���ֵ�¼��ʾ */
            $("#saveloginArea").addEvent('mouseover', function(){
                autologin_tiparea.addCss('display:block');
            }).addEvent('mouseout', function(){
                        autologin_tiparea.addCss('display:none');
                    })
        }
    })();
