<script charset="utf-8" id="seajsnode" src="http://i2.sinaimg.cn/jslib/modules2/seajs/1.3.0/sea.js"></script>
<script src="http://news.sina.com.cn/js/modules2/config/lib/config.1.1.x.js"></script>
<style type="text/css">
      #scrollToTop{position:fixed;_position:absolute;bottom: 40px;left:50%;margin-left: 500px;display:block;width:50px;height:50px;cursor:pointer;background: url("http://i3.sinaimg.cn/dy/deco/2013/0226/top.png");_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true,sizingMethod=crop, src='http://i3.sinaimg.cn/dy/deco/2013/0226/top.png');_background:none;}
      #scrollToTop:hover{background: url("http://i2.sinaimg.cn/dy/deco/2013/0226/top_h.png");_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true,sizingMethod=crop, src='http://i2.sinaimg.cn/dy/deco/2013/0226/top_h.png');_background:none;}
  </style>
  <a id="scrollToTop" suda-uatrack="key=blog_index_up_to_top&value=top" href="javascript:void(0);" style="visibility: hidden;"></a>
  <script type="text/javascript">
      seajs.use("$",function($){
          $(document).ready(function(){
              function scrollTo(posY,time){
                  var currentTop=$(window).scrollTop();
                  var dist=currentTop-posY;
                  var timeSlice=50;
                  var step=time/timeSlice;
                  var avgDist=-dist/step;
                  var timer= setInterval(function(){
                      window.scrollBy(0,avgDist);
                      if($(window).scrollTop()==posY){
                          clearInterval(timer);
                      }
                  },timeSlice);
              }

              var isIE6 = !!(navigator.appVersion.indexOf("MSIE 6") != -1);
              var $window=$(window);
              var scrollToTop=$("#scrollToTop");
              scrollToTop.css({
                  visibility:'hidden'
              }).click(function(){
                          if(isIE6){
                              scrollTo(0,500);
                          }else{
                              $("html,body").animate({
                                  scrollTop:0
                              },500,'swing');
                          }
                      });
              function backToTop(){
                  var currentTop=$(window).scrollTop();
                  var ie6Top=currentTop+$(window).height()-100;
                  if(currentTop<$(window).height()/2){
                      scrollToTop.css({
                          visibility:'hidden'
                      });
                  }else{
                      scrollToTop.css({
                          visibility:'visible'
                      });
                  }
                  if(isIE6){
                      scrollToTop.css({
                          top:ie6Top
                      });
                  }
              }
              function adjustPos(){
                  if($window.width()>1030){
                      scrollToTop.css({
                          marginLeft:500
                      });
                  }else{
                      scrollToTop.css({
                          marginLeft:425
                      });
                  }
              }
              adjustPos();
              $(window).scroll(backToTop).resize(backToTop);
              $window.resize(adjustPos);
          });
      });
  </script>
<!--返回顶部 end-->

