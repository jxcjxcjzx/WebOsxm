function addPv(url){
    nElem = "<iframe style='display:none' src='" + url + "'></iframe>";
    $('body').append(nElem);
}

window.onscroll = function () {
    var winScroll = document.documentElement.scrollTop || document.body.scrollTop;
    if(winScroll >= 700){
		$('#goTop').show();
	}else{
		$('#goTop').hide();
	}
}

 </script>
