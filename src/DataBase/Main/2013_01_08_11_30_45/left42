    <script type="text/javascript">
        function setFocus1(i)
        {
            selectLayer1(i);
        }
        function selectLayer1(i)                                                       
        {
            switch(i)
            {
                case 1:
                    document.getElementById("focusPic1").style.display="block";
                    document.getElementById("focusPic2").style.display="none";
                    document.getElementById("focusPic3").style.display="none";
                break;
                case 2:
                    document.getElementById("focusPic1").style.display="none";
                    document.getElementById("focusPic2").style.display="block";
                    document.getElementById("focusPic3").style.display="none";
                break;
                case 3:
                    document.getElementById("focusPic1").style.display="none";
                    document.getElementById("focusPic2").style.display="none";
                    document.getElementById("focusPic3").style.display="block";
                break;
                case 4:
                    document.getElementById("focusPic4").style.display="block";
                    document.getElementById("focusPic5").style.display="none";
                    document.getElementById("focusPic6").style.display="none";
                    document.getElementById("focusPic7").style.display="none";
                    document.getElementById("focusPic8").style.display="none";
                    document.getElementById("focusPic9").style.display="none";
                    document.getElementById("focusPic10").style.display="none";
                break;
                case 5:
                    document.getElementById("focusPic4").style.display="none";
                    document.getElementById("focusPic5").style.display="block";
                    document.getElementById("focusPic6").style.display="none";
                    document.getElementById("focusPic7").style.display="none";
                    document.getElementById("focusPic8").style.display="none";
                    document.getElementById("focusPic9").style.display="none";
                    document.getElementById("focusPic10").style.display="none";
                break;
                case 6:
                    document.getElementById("focusPic4").style.display="none";
                    document.getElementById("focusPic5").style.display="none";
                    document.getElementById("focusPic6").style.display="block";
                    document.getElementById("focusPic7").style.display="none";
                    document.getElementById("focusPic8").style.display="none";
                    document.getElementById("focusPic9").style.display="none";
                    document.getElementById("focusPic10").style.display="none";
                break;
                case 7:
                    document.getElementById("focusPic4").style.display="none";
                    document.getElementById("focusPic5").style.display="none";
                    document.getElementById("focusPic6").style.display="none";
                    document.getElementById("focusPic7").style.display="block";
                    document.getElementById("focusPic8").style.display="none";
                    document.getElementById("focusPic9").style.display="none";
                    document.getElementById("focusPic10").style.display="none";
                break;
                case 8:
                    document.getElementById("focusPic4").style.display="none";
                    document.getElementById("focusPic5").style.display="none";
                    document.getElementById("focusPic6").style.display="none";
                    document.getElementById("focusPic7").style.display="none";
                    document.getElementById("focusPic8").style.display="block";
                    document.getElementById("focusPic9").style.display="none";
                    document.getElementById("focusPic10").style.display="none";
                break;
                case 9:
                    document.getElementById("focusPic4").style.display="none";
                    document.getElementById("focusPic5").style.display="none";
                    document.getElementById("focusPic6").style.display="none";
                    document.getElementById("focusPic7").style.display="none";
                    document.getElementById("focusPic8").style.display="none";
                    document.getElementById("focusPic9").style.display="block";
                    document.getElementById("focusPic10").style.display="none";
                break;
                case 10:
                    document.getElementById("focusPic4").style.display="none";
                    document.getElementById("focusPic5").style.display="none";
                    document.getElementById("focusPic6").style.display="none";
                    document.getElementById("focusPic7").style.display="none";
                    document.getElementById("focusPic8").style.display="none";
                    document.getElementById("focusPic9").style.display="none";
                    document.getElementById("focusPic10").style.display="block";
                break;
            }
        }
    </script>


