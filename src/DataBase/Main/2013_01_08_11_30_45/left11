                                    dj.module.largeHeadline = dj.module.largeHeadline.update();
                  }
                            
                    }
            });  

            }, 129*1000);

            setTimeout(function() {

            dj.module.topLeftModule_updater = new dj.util.PeriodicalUpdater({success: "topLeftModule"},
                '/public/page/NA_WSJ_PUB:0_0_WP_top_left_2unit.html', {   
                    method: 'get',
                    frequency: 65,
                    decay: 2,
                    evalScripts: true,
                    onUpdate: function() {
                        
                  if ((typeof dj.module.topLeftModule === "object") && (typeof dj.module.topLeftModule.update === "function")) {
                                    dj.module.topLeftModule = dj.module.topLeftModule.update();
                  }
                            
                    }
            });  

            }, 65*1000);

            setTimeout(function() {

            dj.module.bucketBelowTabbedModule_updater = new dj.util.PeriodicalUpdater({success: "bucketBelowTabbedModule"},
                '/public/page/NA_WSJ_PUB:0_0_WP_4unit_below_tabbedModule.html', {   
                    method: 'get',
                    frequency: 99,
                    decay: 2,
                    evalScripts: true,
                    onUpdate: function() {
                        
                  if ((typeof dj.module.bucketBelowTabbedModule === "object") && (typeof dj.module.bucketBelowTabbedModule.update === "function")) {
                                    dj.module.bucketBelowTabbedModule = dj.module.bucketBelowTabbedModule.update();
                  }
                            
                    }
            });  

            }, 99*1000);

            setTimeout(function() {

            dj.module.liveVideoPlayer_updater = new dj.util.PeriodicalUpdater({success: "liveVideoPlayer"},
                '/public/page/NA_WSJ_PUB:0_0_WH_0001_public_LiveVideo.html', {   
                    method: 'get',
                    frequency: 319,
                    decay: 2,
                    evalScripts: true,
                    onUpdate: function() {
                        
                                try{
                                    dj.util.flash.template.scan();;
                                }catch(e) {console.info("JSUpdate: %p", e);}
                        
                            
                    }
            });  

            }, 319*1000);

            setTimeout(function() {

            dj.module.main_content_updater = new dj.util.PeriodicalUpdater({success: "main_content"},
                '/public/page/NA_WSJ_PUB:0_0_WH_0001_public_MainContent.html', {   
                    method: 'get',
                    frequency: 129,
                    decay: 2,
                    evalScripts: true,
                    onUpdate: function() {
                        
                                try{
                                    dj.widget.ad.AdManager.refreshAd('whatsnews_ad');
                                }catch(e) {console.info("JSUpdate: %p", e);}
                        
                            
                                try{
                                    dj.module.refreshTooltips(dj.widget.Tooltip);
                                }catch(e) {console.info("JSUpdate: %p", e);}
                        
                            
                                try{
                                    dj.module.newsTicker.init();;
                                }catch(e) {console.info("JSUpdate: %p", e);}
                        
                            
                                try{
                                    dj.util.flash.template.scan();;
                                }catch(e) {console.info("JSUpdate: %p", e);}
                        
                            
                                try{
                                    dj.util.date.timestamp.scanWithinContainer('#main_content');;
                                }catch(e) {console.info("JSUpdate: %p", e);}
                        
                            
                    }
            });  

            }, 129*1000);

//------------------------------
console.info("[ end jsexec ]");
console.groupEnd();

}

