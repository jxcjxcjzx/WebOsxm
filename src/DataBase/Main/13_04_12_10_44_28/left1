                function kwCount(el){
                  var kwValue = decodeURI(el.innerText || el.textContent);
                  var urlValue = decodeURI(el.getAttribute("href", 2));
                  (new Image()).src = "http://mark.sina.com.cn/v2/hot.php?keyword="+kwValue+"&hwfrom=TopGoogleBar&tag=1&url="+urlValue;
                };
