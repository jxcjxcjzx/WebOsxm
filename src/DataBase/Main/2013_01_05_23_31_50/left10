<script type="text/javascript"> 
jQuery(function($){
	//搜索 
	$('#wd').focus( 
		function () { 
			$(this).removeClass("blur"); 
			$("#button").addClass("blur"); 
		} 
		); 
		$('#wd').blur( 
		function () { 
			if($(this).val()==''){ 
			$(this).addClass("blur"); 
		}else{ 
			$(this).removeClass("blur"); 
		} 
			$("#button").removeClass("blur"); 
		} 
	); 
//首页回顾日历 
		buildCal(month,year); 
		$('#b-news').hover( 
		function () { 
		$('.calWrapper').slideDown(100); 
		}, 
		function () { 
		$('.calWrapper').slideUp(100); 
		} 
		); 
		document.getElementById("selected_date").innerHTML=year+'-'+month+'-'+date;
		$('#selected_date').css({'background' : 'url(http://www.chinabyte.com/w/pic/730/calendar'+todayDate.getDate()+'.gif) right 1px no-repeat #fff'});
//纪录中国 
		$('.bar2').mosaic({
			animation	:	'slide'
		});
//滑动块 
		boxs('#ph-nbpc',0,"mousemove"); 
		boxs('#ph-shang',0,"mousemove"); 
		boxs('#m-yqlj',0,"mousemove"); 
		$('#updata').css('margin-bottom','20px'); 
		$("#m-zt .list6 li:first").css("border-left", "solid 1px #ECECEC"); 
		$("#m-zt .list6 li:last").css("border-right", "solid 1px #ECECEC"); 
		boxs('#m-ios',0,"mousemove"); 
		boxs('#m-android',0,"mousemove"); 
})	
</script>
