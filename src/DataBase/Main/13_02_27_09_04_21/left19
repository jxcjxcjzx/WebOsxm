    var _bdhmProtocol = (("https:" == document.location.protocol) ? " https://" : " http://");
    document.write(unescape("%3Cscript src='" + _bdhmProtocol + "hm.baidu.com/h.js%3Fff885e3822119b5c09ae344516007471' type='text/javascript'%3E%3C/script%3E"));
</script>
