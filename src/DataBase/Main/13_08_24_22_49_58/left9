var searchUrl = '';
var keywords = document.getElementById("mytxtdafdfasdf").value;
var integer = /^\+?[1-9][0-9]*$/;
var key = keywords.replace(/[^\d]*/ig,'').length;
if(keywords.indexOf('鏉�) != -1 && (key+1)==keywords.length){
alert('璇疯緭鍏ュ叧閿瘝');
