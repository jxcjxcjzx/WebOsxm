}
function getOnkeyDown(e){
var ev = e;
ev = ev || event;
if(ev.keyCode == 13){
submitSearch();
}
}
function submitSearch(){
var sp_id = arguments.length==0?"&type="+search_type+"&sid=0000&pid=0000":"&type="+search_type+"&sid=0000&pid=0003";
//var searchUrl = 'http://so.cntv.cn/search.php?qtext=';
