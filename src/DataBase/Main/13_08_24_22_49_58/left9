return false;
}
if(keywords.replaceAll(' ','') == ''){
alert('璇疯緭鍏ュ叧閿瘝');
setTextFocus;
return false;
}
var type = "-1";
//searchUrl = searchUrl + encodeURIComponent(keywords) + sp_id;
