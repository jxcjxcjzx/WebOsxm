 function goKey() {
	if (document.getElementById('key').value=='请输入关键词') {
		document.getElementById('key').value=''
	}
	window.open("http://ditu.iask.com/#type=0&pars=0010,"+document.getElementById('key').value+",,,,,,,");
	return false;
}
		/* ditu end */
		/* shishiren begin */
		function iask_ck(){
			if(document.f.key.value == ""){
				window.open("http://iask.sina.com.cn/");
			}
			return true;
		}
		function ask(){
			if (document.f.key.value =='请输入关键词') {
				document.f.key.value ='';
			}
			if(document.f.key.value == ""){
				window.open("http://iask.sina.com.cn/");
				return false;
			}else{
				window.open("http://iask.sina.com.cn/question/ask_new_2.php?title=" + document.f.key.value);
				return false;
			}
		}
		/* zhishiren end */
		/* ishare begin */
		function checkishare(){
			if(document.f.key.value == ""){
				window.open("http://ishare.iask.sina.com.cn/");
				return false;
			}else{
				return;
			}
		}
		/* ishare end */
