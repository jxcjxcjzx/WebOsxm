<!-- 统一注册登录 begin -->
<script type="text/javascript">
var sina = {
  $ : function(objName){if(document.getElementById){return eval('document.getElementById("'+objName+'")')}else{return eval('document.all.'+objName)}},
  isIE : navigator.appVersion.indexOf("MSIE")!=-1?true:false,
  addEvent : function(obj,eventType,func){if(obj.attachEvent){obj.attachEvent("on" + eventType,func);}else{obj.addEventListener(eventType,func,false)}},
  delEvent : function(obj,eventType,func){
    if(obj.detachEvent){obj.detachEvent("on" + eventType,func)}else{obj.removeEventListener(eventType,func,false)}
  },
  readCookie : function(l){var i="",I=l+"=";if(document.cookie.length>0){offset=document.cookie.indexOf(I);if(offset!=-1){offset+=I.length;end=document.cookie.indexOf(";",offset);if(end==-1)end=document.cookie.length;i=unescape(document.cookie.substring(offset,end))}};return i},
  writeCookie : function(O,o,l,I){var i="",c="";if(l!=null){i=new Date((new Date).getTime()+l*3600000);i="; expires="+i.toGMTString()};if(I!=null){c=";domain="+I};document.cookie=O+"="+escape(o)+i+c}
};
var loginManager = (function(){
  function getScript(url,dispose,charset){var scriptNode=document.createElement("script");scriptNode.type="text/javascript";if(charset){scriptNode.charset=charset};scriptNode.onreadystatechange=scriptNode.onload=function(){if(!this.readyState||this.readyState=="loaded"||this.readyState=="complete"){if(dispose){dispose()};scriptNode.onreadystatechange=scriptNode.onload=null;scriptNode.parentNode.removeChild(scriptNode)}};scriptNode.src=url;var h=document.getElementsByTagName("head")[0];h.insertBefore(scriptNode,h.firstChild)};
  var LM = {
    sso : null, //sso是否已经加载 null未加载,false加载中,true加载完成
    chkLogin : function(){ //登录
      var username = document.showLogin.username.value;
      var password = document.showLogin.password.value;
      var loginType = document.showLogin.entry.value;
      if(username.length < 1 || username.length > 64){
        alert("请正确填写用户名!");
        document.showLogin.username.focus();
        return false;
      }
      if(password.length < 1 || password.length > 30){
        alert("请正确填写密码!");
        document.showLogin.password.focus();
        return false;
      }
      if(loginType == "null"){
        alert("您还没有选择要登录到哪里，请您选择去向！");
        sina.$("loginFldselectop").style.display = "block";
        sina.addEvent(document.body,"mousedown",LoginSelectClickOther);
        return false;
      }

      this.ssoLogin();

      GB_SUDA._S_uaTrack('30','lt_' + loginType);
      return false;
    },
    chkLoginType : function(username){　//选择登录去向
      username = username.toLowerCase();
      if(username.indexOf("@") != -1){
        sina.$("L_freemail").className = "disabled";
        sina.$("L_freemail").onclick = null;
        sina.$("L_vipmail").className = "disabled";
        sina.$("L_vipmail").onclick = null;
        sina.$("L_2008mail").className = "disabled";
        sina.$("L_2008mail").onclick = null;
        sina.$("L_cnmail").className = "disabled";
        sina.$("L_cnmail").onclick = null;
      }else{
        sina.$("L_freemail").className = "";
        sina.$("L_freemail").onclick = function(){LM.LoginSelect("freemail");return false;};
        sina.$("L_vipmail").className = "";
        sina.$("L_vipmail").onclick = function(){LM.LoginSelect("vipmail");return false;};
        sina.$("L_2008mail").className = "";
        sina.$("L_2008mail").onclick = function(){LM.LoginSelect("2008mail");return false;};
        sina.$("L_cnmail").className = "";
        sina.$("L_cnmail").onclick = function(){LM.LoginSelect("cnmail");return false;};
        return;
      };
      if(username.indexOf("@sina.com") != -1){
        sina.$("L_freemail").className = "";
        sina.$("L_freemail").onclick = function(){LM.LoginSelect("freemail");return false;};
        this.LoginSelect("freemail");
      };
      if(username.indexOf("@vip.sina.com") != -1){
        sina.$("L_vipmail").className = "";
        sina.$("L_vipmail").onclick = function(){LM.LoginSelect("vipmail");return false;};
        this.LoginSelect("vipmail");
      };
      if(username.indexOf("@2008.sina.com") != -1){
        sina.$("L_2008mail").className = "";
        sina.$("L_2008mail").onclick = function(){LM.LoginSelect("2008mail");return false;};
        this.LoginSelect("2008mail");
      };
      if(username.indexOf("@sina.cn") != -1){
        sina.$("L_cnmail").className = "";
        sina.$("L_cnmail").onclick = function(){LM.LoginSelect("cnmail");return false;};
        this.LoginSelect("cnmail");
      };
    },
    LoginSelectClickOther : function(e){
      thisObj = e.target?e.target:event.srcElement;
      do{
        if(thisObj.id == "loginFldselect" || thisObj == document.showLogin.password) return;
        if(thisObj.tagName == "BODY"){this.LoginSelectClose();return;};
        thisObj = thisObj.parentNode;
      }while(thisObj.parentNode);
    },
    clickLoginSelect : function(){
      if(sina.$("loginFldselectop").style.display == "block"){
        this.LoginSelectClose();
      }else{
        sina.$("loginFldselectop").style.display = "block";
        sina.addEvent(document.body,"mousedown",LoginSelectClickOther);
      }
    },
    LoginSelectClose : function(){ //关闭提示层
      sina.$("loginFldselectop").style.display = "none";
      sina.delEvent(document.body,"mousedown",LoginSelectClickOther);
    },
    openLoginType : function(){
      if(document.showLogin.username.value != "" && document.showLogin.username.value.indexOf("@") == -1 && sina.$("loginFldselectdis").innerHTML == "选择去向"){
        sina.$("loginFldselectop").style.display = "block";
        sina.addEvent(document.body,"mousedown",LoginSelectClickOther);
      };
      if(typeof sinaSSOController == 'undefined'){
        this.loadSSO();
      };
    },
    customLoginCallBack : function(r){ //登录回调
      if(r.result === true){
        this.loginWin.location.href = this.loginUrl;
      }else{
        //登录失败
        document.showLogin.password.value = "";
        document.showLogin.retcode.value = r.errno;
        document.showLogin.target = 'loginWin';
        document.showLogin.submit();
      };
      document.showLogin.password.value = "";
      this.loginWin.focus();
    },
    ssoLogin : function(){ //通过SSO登录
      this.loginWin = window.open('http://news.sina.com.cn/iframe/login.html','loginWin');

      if(this.sso === false){
        this.waitLogin = true; //等待加载
        return false;
      }else if(this.sso == null){ //未加载，执行加载等待
        this.waitLogin = true;
        this.loadSSO();
        return false;
      };
      var username = document.showLogin.username.value;
      var password = document.showLogin.password.value;
      var loginType = document.showLogin.entry.value;

      if(username.indexOf("@") == -1 && loginType == "vipmail"){
        username += "@vip.sina.com";
      };

      sinaSSOController.customLoginCallBack = function(r){
        LM.customLoginCallBack(r);
      };

      sinaSSOController.from = "referer:www_index";
      sinaSSOController.entry = loginType;
      sinaSSOController.login(username,password);
    },
    ssoOnLoad : function(){ //sso.js加载完成
      this.sso = true;
      if(this.waitLogin){
        this.waitLogin = false;
        this.ssoLogin();
      }
    },
    loadSSO : function(){ //加载sso文件
      this.sso = false;
      getScript('http://i.sso.sina.com.cn/js/ssologin.js',function(){LM.ssoOnLoad()});
    },
    LoginSelect : function(title){
      var titleName = "选择去向";
      switch(title){
        case "cnmail":
          document.showLogin.entry.value = "cnmail";
          this.loginUrl = 'http://mail.sina.com.cn/';
          titleName = "CN邮箱";
          break;
        case "freemail":
          document.showLogin.entry.value = "freemail";
          this.loginUrl = 'http://mail.sina.com.cn/';
          titleName = "免费邮箱";
          break;
        case "vipmail":
          document.showLogin.entry.value = "vipmail";
          this.loginUrl = 'http://mail.sina.com.cn/';
          titleName = "VIP邮箱";
          break;

        case "2008mail":
          document.showLogin.entry.value = "2008mail";
          this.loginUrl = 'http://mail.sina.com.cn/';
          titleName = "2008邮箱";
          break;
        case "blog":
          document.showLogin.entry.value = "blog";
          this.loginUrl = 'http://blog.sina.com.cn/';
          titleName = "博客";
          break;
        case "vblog":
          document.showLogin.entry.value = "vblog";
          this.loginUrl = 'http://video.sina.com.cn/';
          titleName = "视频";
          break;
        case "miniblog":
          document.showLogin.entry.value = "miniblog";
          this.loginUrl = 'http://weibo.com/';
          titleName = "微博";
          break;
        case "bar":
          document.showLogin.entry.value = "tieba";
          this.loginUrl = 'http://blog.sina.com.cn/';
          titleName = "贴吧";
          break;
        case "bbs":
          document.showLogin.entry.value = "forum";
          this.loginUrl = 'http://blog.sina.com.cn/';
          titleName = "论坛";
          break;
        case "login":
          document.showLogin.entry.value = "sso";
          this.loginUrl = 'http://login.sina.com.cn/';
          titleName = "通行证";
          break;
      };
      sina.$("loginFldselectdis").innerHTML = titleName;
      sina.writeCookie("loginType",title,2160,"sina.com.cn;path=/");
      if(document.showLogin.username.value != "" && document.showLogin.password.value != ""){
        document.showLogin.onsubmit();
      };
      this.LoginSelectClose();
    }
  };

  sinaSSOConfig = {};
  sinaSSOConfig.customLoginCallBack = function(r){
    LM.customLoginCallBack(r);
  };

  var LoginSelectClickOther = function(e){
    LM.LoginSelectClickOther(e);
  };
  return LM;
})();
//]]>
</script>
