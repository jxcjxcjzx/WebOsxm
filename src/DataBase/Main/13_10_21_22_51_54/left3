function t2(){
	$(".right_but").click(function(){
								   
				
			if(currr<2){
				currr += 1;
				for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
			}
			else if(currr>=2)
				{   currr=2;
					return false;}
						   });
	
$(".left_but").click(function(){
	

			if(currr>0)
				{ 
					currr -= 1;
					for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
					}
				else if(currr<=0)
				{   currr==0;
					return false;}
								   
	 });


