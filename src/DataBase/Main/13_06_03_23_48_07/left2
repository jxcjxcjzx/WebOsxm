	(function(){
		var list = document.getElementById('mango-logo-list').getElementsByTagName('li'),
			len = list.length,
			current = null,
			index = 0;
		function show() {
			if (current) current.style.display = 'none';
			list[index].style.display = 'block';
			current = list[index];
			if (index >= len -1) {
				index = 0;
			} else {
				index++;
			}
			setTimeout(show, 5000);
		}
		show();
	})();
