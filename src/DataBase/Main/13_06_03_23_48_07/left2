<script type="text/javascript">

(function(){
var topRecommend = function(){
	var userAgent = window.navigator.userAgent;
	if(userAgent.match(/Android/i) || userAgent.indexOf('iPod') !== -1 || userAgent.indexOf('iPad') !== -1 || userAgent.indexOf('iPhone') !== -1){
		var cookie = Nova.Cookie.get("_top_banner_");
		if(cookie == 1) return false; 
		var dom = document.getElementById('topbanner');
		var cssPlus = '';
		if(window.screen.width < 768){
			cssPlus = "x_topbanner_l";
		}
		if(userAgent.match(/Android/i)){
			var aString = '<div class="x_topbanner '+cssPlus+'"><div class="banner_skip"><div class="banner_panel"><i class="ico__info"></i><strong>推荐使用<a href="http://mobile.youku.com/index/wireless" target="_blank">优酷客户端</a></strong><br><span>下载视频、离线观看、跨屏追剧，</span><a href="http://mobile.youku.com/index/wireless" target="_blank">立即安装</a></div><a title="关闭" class="btn__cls"></a></div></div>';	
		}else if(userAgent.indexOf("iPad") !== -1){	
			var aString = '<div class="x_topbanner '+cssPlus+'"><div class="banner_skip"><div class="banner_panel"><i class="ico__info"></i><strong>推荐使用<a href="http://mobile.youku.com/index/wireless" target="_blank">优酷客户端</a></strong><br><span>下载视频、离线观看、跨屏追剧，</span><a href="http://mobile.youku.com/index/wireless" target="_blank">立即安装</a></div><a title="关闭" class="btn__cls"></a></div></div>';	
		}
		else{
			var mUrl = "http://m.youku.com";
			//{if !empty($show)}mUrl += "&type=show";{/if}
			var aString = '<div class="x_topbanner '+cssPlus+'"><div class="banner_skip"><div class="banner_panel"><i class="ico__info"></i><strong>手机浏览时建议您：</strong><br><span>安装<a href="http://mobile.youku.com/index/wireless" target="_blank">优酷客户端</a>，或访问<a href="'+mUrl+'" onclick="Nova.Cookie.set(\'_from_x_\',0,-1);">手机优酷网</a></span></div><a title="关闭" class="btn__cls"></a></div></div>';		
		}
		dom.innerHTML = aString;
		dom.style.display = "block";
		var close = $$(".x_topbanner .btn__cls")[0];
		if(close){
			close.onclick = function(){
				dom.style.display = "none";
				Nova.Cookie.set("_top_banner_",1,1);
			}
		}
	}
};
topRecommend();
})();

</script>




</div>



