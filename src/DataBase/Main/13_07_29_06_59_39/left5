 $(function(){
	$("#scrollBtn li a").hover(function(){
		$(this).css('opacity', 1);
	},function(){
		$(this).css('opacity', 0.5);
	})
	
        //控制收藏提示显示
        $('#tShare').mouseover(function(){
                $(this).attr('title','收藏本站');
        });

         //微信鼠标滑过变底色
        $('.weixin').hover(function(){
              $(this).css('background', 'url(http://img.cankaoxiaoxi.com/templates/cankaoxiaoxi/css/images/bg_red.png)');
        },function(){
              $(this).css('background', 'url(http://img.cankaoxiaoxi.com/templates/cankaoxiaoxi/css/images/wx_bg.png)');
        });
        $('.wx_close').click(function(){
              $('.weixin').hide();
        });
})
 
function addPv(url){
    nElem = "<iframe style='display:none' src='" + url + "'></iframe>";
    $('body').append(nElem);
}

window.onscroll = function () {
    var winScroll = document.documentElement.scrollTop || document.body.scrollTop;
    if(winScroll >= 700){
		$('#goTop').show();
	}else{
		$('#goTop').hide();
	}
}

 </script>
 <div id="scrollBtn" class="scrollBtn">
     <ul class="ov">
        <li><a title="参考消息" id="tShare"  class="tShare" href="http://www.cankaoxiaoxi.com/" mce_href="http://www.cankaoxiaoxi.com/" onClick="javascript:addPv('http://www.cankaoxiaoxi.com?favorite=1');this.title='参考消息';window.external.addFavorite('http://www.cankaoxiaoxi.com/','参考消息');return false;" target="_self" rel="sidebar"></a></li>
        <li style="display:none;" id="goTop"><a title="返回顶部" onClick="window.scrollTo(0, 0);return false"  class="goTop" href="#"></a></li>
    </ul>
</div>
 <!--收藏本站 end-->
