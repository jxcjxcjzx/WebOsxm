		<script language="javascript" type="text/javascript">    
			var ADTime = 15;    
			var intervalid;    
			intervalid = setInterval("fun()", 1000);    
			var currentUrl = ""+window.location;
			var currentTime = new Date().getTime();
			if(currentUrl.indexOf("/welcomeAd.jspa")>=0){
				currentUrl= "http://forum.eet-cn.com";    
			}
			var separator = "?";
			if(currentUrl.indexOf("?")>=0){
				separator = "&";
			}
			if(currentUrl.indexOf("#")>=0){
				var tmpUrl = currentUrl.substring(0,currentUrl.indexOf("#"));
				var aimto = currentUrl.substring(currentUrl.indexOf("#"));
				currentUrl = tmpUrl + separator + "jumpto=view_welcomead_forum_" + currentTime + aimto;
			}else{
				currentUrl = currentUrl + separator + "jumpto=view_welcomead_forum_" + currentTime;
			}
			function fun() {
				
				if (ADTime == 0) {    
					window.location.href = currentUrl;
					clearInterval(intervalid);    
				}    
				var admes = document.getElementById("ADmes");
				if(admes != null){
					document.getElementById("ADmes").innerHTML = ADTime;    
					ADTime--; 
				}
			}
			
			/**
			 * For tracking click action.
			 */
			function trackJump(code){
				var trackUrl = '/images/blank.gif?jumpto='+code+'-'+ new Date().getTime();
				(new Image()).src=trackUrl;
			}
		</script>
