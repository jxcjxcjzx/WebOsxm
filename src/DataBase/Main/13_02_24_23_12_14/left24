	var MIT = MIT || {};
	MIT.clicktrack = function () {
	    'use strict';
	    return {
	        init: function () {
	            var a, b, e, g, h, f, c, d, i;
	            e = document.getElementsByTagName("a");
	            g = e.length;
	            for (a = 0; a < g; a += 1) MIT.clicktrack.addclick(e[a]);
	            document.getElementById("quicksearch").onsubmit = function () {
	                h = document.getElementById("searchBox");
	                (f = h.value) || (f = "[blank]");
	                d = document.getElementsByName("engine");
	                i = d.length;
	                for (b = 0; b < i; b += 1) if (d[b].checked) c = d[b].id;
	                c || (c = "[blank]");
	                _gaq.push(["_trackEvent", "Search", c, f]);
	                return !0
	            }
	        },
	        addclick: function (a) {
	            a.onclick = function () {
	                return MIT.clicktrack.sendclick(this.href)
	            }
	        },
	        sendclick: function (a) {
	            a && (0 <= a.indexOf("mit.edu/news") ? _gaq.push(["_trackEvent", "Link", "MIT News", a]) : 0 <= a.indexOf("mit.edu") || 0 <= a.indexOf("mitadmissions") ? _gaq.push(["_trackEvent", "Link", "MIT", a]) : _gaq.push(["_trackEvent", "Link", "External", a]));
	            return !0
	        }
	    }
	}();
	MIT.clicktrack.init();
</script> 
