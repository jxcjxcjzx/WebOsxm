<script language="javascript" type="text/javascript">
/*=================���ID==================*/
function getObject(objectId) {
    if(document.getElementById && document.getElementById(objectId)) {
	// W3C DOM
	return document.getElementById(objectId);
    } else if (document.all && document.all(objectId)) {
	// MSIE 4 DOM
	return document.all(objectId);
    } else if (document.layers && document.layers[objectId]) {
	// NN 4 DOM.. note: this won't find nested layers
	return document.layers[objectId];
    } else {
	return false;
    }
}
var sec_key_new="";

function tooltip_findPosX(obj) {
	var curleft = 0;
	if (obj.offsetParent) {
		while (obj.offsetParent) {
			curleft += obj.offsetLeft
			obj = obj.offsetParent;
		}
	}
	else if (obj.x) curleft += obj.x;
	return curleft;
}

function tooltip_findPosY(obj) {
	var curtop = 0;
	if (obj.offsetParent) {
		while (obj.offsetParent) {
			curtop += obj.offsetTop
			obj = obj.offsetParent;
		}
	}
	else if (obj.y) curtop += obj.y;
	return curtop;
}

function show_adm_menu(obj_id_e,o2,MX,MY)
{

	var f = document.getElementById(obj_id_e);

	var x = tooltip_findPosX(o2);
	var y = tooltip_findPosY(o2);

	f.style.left = x + MX;
	f.style.top = y + MY;
	f.style.display = "block";

	return false;
}
function chang_link(obj_id_e)
{
	var h = document.getElementById(obj_id_e);
	h.style.display = "none";

}
function CheckKeywords()
{
	if(searchform.q.value.length==0)
	{
		alert("�����������ؼ���!");
		searchform.q.focus();
		return false;
	}
	return true;
}

