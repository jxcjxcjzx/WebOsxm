function ISL_GoDown_1_2(){clearInterval(MoveTimeObj_1);if(MoveLock_1)return;clearInterval(AutoPlayObj_1_2);MoveLock_1=true;MoveWay_1="right";ISL_ScrDown_1_2();MoveTimeObj_1=setInterval('ISL_ScrDown_1_2()',Speed_1)}
