<script language="javascript" type="text/javascript">
/** show the express box **/
function openexpress(){
 var ckepop = document.getElementById("gssharesnspop");
 ckepop.style.left="0px";
}
/** hidden the express box **/
function closeexpress(){
 var ckepop = document.getElementById("gssharesnspop");
 ckepop.style.left="-240px";
}

if(document.getElementById("gssharesnspop").style.position == "fixed") {
	var boxHeight = document.getElementById("gssharesnspop").offsetHeight;
	var _oldResize = window.onresize;
	window.onresize = function () {
		if (document.documentElement.clientHeight > boxHeight){
			document.getElementById("gssharesnspop").style.top=(document.documentElement.clientHeight-boxHeight)/2 + "px";
		} else {
			document.getElementById("gssharesnspop").style.top="0px";
		}
		if(_oldResize) {
			_oldResize();
		}
	};

	if (document.documentElement.clientHeight > boxHeight){
		document.getElementById("gssharesnspop").style.top=(document.documentElement.clientHeight-boxHeight)/2 + "px";
	} else {
		document.getElementById("gssharesnspop").style.top="0px";
	}
}
</script><div id="wrapper" class="homeContainer">
