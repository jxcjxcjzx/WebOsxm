															$(".scorebox .score .scoreNote .scoreBlock ul li").attr("class","");
															$(this).attr("class","active");
														});
														$(".scorebox .score .scoreNote .scoreBlock ul li").mouseleave(function(){
															$(".scorebox .score .scoreNote .scoreBlock ul li").attr("class","");
