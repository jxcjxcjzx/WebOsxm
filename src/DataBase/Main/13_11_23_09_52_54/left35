    function chcolor(){
        $('.txt-focus').css('color',($(this).value=='请输入关键词')?'#999':'#000');
    }
    

    function checkserAgent(){
        var userAgentInfo=navigator.userAgent;
        if((userAgentInfo.match(/Android/i))){return true;}
        if((userAgentInfo.match(/iPhone/i))){return true;}
        if((userAgentInfo.match(/SymbianOS/i))){return true;}
        if((userAgentInfo.match(/Windows Phone/i))){return true;}
        if((userAgentInfo.match(/iPad/i))){return false;}
        if((userAgentInfo.match(/iPod/i))){return true;}
        if((userAgentInfo.match(/MQQBrowser/i))){return true;}
        return false;
    }
    if(checkserAgent()){
         //window.location.href="http://m.cankaoxiaoxi.com";
    }


function getParameter(paraStr, url)
{
    var result = "";
    //获取URL中全部参数列表数据
    var str = "&" + url.split("?")[1];
    var paraName = paraStr + "=";
    //判断要获取的参数是否存在
    if(str.indexOf("&"+paraName)!=-1)
    {
        //如果要获取的参数到结尾是否还包含“&”
        if(str.substring(str.indexOf(paraName),str.length).indexOf("&")!=-1)
        {
            //得到要获取的参数到结尾的字符串
            var TmpStr=str.substring(str.indexOf(paraName),str.length);
            //截取从参数开始到最近的“&”出现位置间的字符
            result=TmpStr.substr(TmpStr.indexOf(paraName),TmpStr.indexOf("&")-TmpStr.indexOf(paraName));  
        }
        else
        {  
            result=str.substring(str.indexOf(paraName),str.length);  
        }
    }  
    else

    {  
        result="无此参数";  
    }  
    return (result.replace("&",""));  
}

var r = getParameter("fr",window.location.href);
if(!(r=="fr=mb") && checkserAgent())  window.location.href="http://m.cankaoxiaoxi.com";

//幻灯片
window.onload = function()
{
    var oDiv = document.getElementById("slide");
    var oUl = oDiv.getElementsByTagName("ul")[0];
    var oOl = oDiv.getElementsByTagName("ol")[0];
    var oLi = oOl.getElementsByTagName("li");
    var oImgLi = oUl.getElementsByTagName("li");
    var oPrev = document.getElementById("btn_prev");
    var oNext = document.getElementById("btn_next");
    
    oUl.style.width = oImgLi.length * oImgLi[0].offsetWidth + "px";
    var timer = null;
    
    timer = setInterval(slide, 3000);
    
    oDiv.onmouseover = function()
    {
        clearInterval(timer);
        $('.blk_jdt_nextbtn').show();
        $('.blk_jdt_prebtn').show();
    };
    oDiv.onmouseout = function()
    {
        timer = setInterval(slide, 3000);
        $('.blk_jdt_nextbtn').hide();
        $('.blk_jdt_prebtn').hide();
    };
    
    for (var i=0; i<oLi.length; i++)
    {
        oLi[i].index = i;
        oLi[i].onmouseover = function()
        {
            oUl.style.left = -this.index*280 + "px";
                        var j = 0;
            for (j=0; j< oLi.length; j++)
            {
                oLi[j].className = ""; 
            }
            oLi[this.index].className = "on";
                        clearInterval(timer);
        }
    }
    
    oNext.onclick = function()
    {
        slide();
        return false;
    }
    
    oPrev.onclick = function()
    {
        var left = oUl.offsetLeft;
        var index = Math.ceil(-left/280);
        var num = Math.ceil(oUl.offsetWidth/280);
        if (index == 0)
        {
            index = num - 1;
        }else
        {
            index -= 1;
        }
        oUl.style.left = -index*280 + "px";
        for (i=0; i<oLi.length; i++)

        {
            oLi[i].className = ""; 
        }
        oLi[index].className = "on"; 
        return false;
    }

    function slide()
    {
        var i = 0;
        var left = oUl.offsetLeft;
        var index = Math.ceil(-left/280);
        var num = Math.ceil(oUl.offsetWidth/280);
        if (index >= (num - 1))
        {
            index = 0;
        }else
        {
            index += 1;
        }
        oUl.style.left = -index*280 + "px";
        for (i=0; i<oLi.length; i++)
        {
            oLi[i].className = ""; 
        }
        oLi[index].className = "on"; 
    }

    //军事幻灯片
    var oDiv2 = document.getElementById("slide2");
    var oUl2 = oDiv2.getElementsByTagName("ul")[0];
    var oOl2 = oDiv2.getElementsByTagName("ol")[0];
    var oLi2 = oOl2.getElementsByTagName("li");
    var oImgLi2 = oUl2.getElementsByTagName("li");
    var oPrev2 = document.getElementById("btn_prev2");
    var oNext2 = document.getElementById("btn_next2");
    
    oUl2.style.width = oImgLi2.length * oImgLi2[0].offsetWidth + "px";
    var timer2 = null;
    
    timer2 = setInterval(slide2, 3000);
    
    oDiv2.onmouseover = function()
    {
        clearInterval(timer2);
        $(oPrev2).show();
        $(oNext2).show();
    };
    oDiv2.onmouseout = function()
    {
        timer2 = setInterval(slide2, 3000);
        $(oPrev2).hide();
        $(oNext2).hide();
    };
    
    for (var i=0; i<oLi2.length; i++)
    {
        oLi2[i].index = i;
        oLi2[i].onmouseover = function()
        {
            oUl2.style.left = -this.index*270 + "px";
                        var j = 0;
            for (j=0; j< oLi2.length; j++)
            {
                oLi2[j].className = ""; 
            }
            oLi2[this.index].className = "on";
            clearInterval(timer2);
        }
    }
    
    oNext2.onclick = function()
    {
        slide2();
        return false;
    }
    
    oPrev2.onclick = function()
    {
        var left = oUl2.offsetLeft;
        var index = Math.ceil(-left/270);
        var num = Math.ceil(oUl2.offsetWidth/270);
        if (index == 0)
        {
            index = num - 1;
        }else
        {
            index -= 1;
        }
        oUl2.style.left = -index*280 + "px";
        for (i=0; i<oLi2.length; i++)

        {
            oLi2[i].className = ""; 
        }
        oLi2[index].className = "on"; 
        return false;
    }

    function slide2()
    {
        var i = 0;
        var left = oUl2.offsetLeft;
        var index = Math.ceil(-left/270);
        var num = Math.ceil(oUl2.offsetWidth/270);
        if (index >= (num - 1))
        {
            index = 0;
        }else
        {
            index += 1;
        }
        oUl2.style.left = -index*270 + "px";
        for (i=0; i<oLi2.length; i++)
        {
            oLi2[i].className = ""; 
        }
        oLi2[index].className = "on"; 
    }

    //屏幕宽确定返回顶部位置
    if(document.body.clientWidth <=1090){
        $('.scrollBtn').css('margin-left',"420px");
    }
    $(window).resize(function(){
         if(document.body.clientWidth <=1090){
            $('.scrollBtn').css('margin-left',"420px");
         }else{
            $('.scrollBtn').css('margin-left',"485px");
         }
    });  

}

</script>
