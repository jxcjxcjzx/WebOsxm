<div name="m_pos" id="m_87756">
	



<script type="text/javascript" src="http://r3.ykimg.com/05100000527C7F6A6714C031D2070351?150851.js"></script>

<script>
document.observe("dom:loaded", function() {
  function slideInit(el) {
    var $base = $(el);
    var $pages = $base.select('.yk-slide-pages')[0];
    var pageClass = $base.getAttribute('pageSel') ||'yk-slide-page'; 
    var $page = $pages.select("."+pageClass);
    var num = $page.length;
    var cur = 0;
    if($page.length == 0) return;
    var pageWidth = $page[0].getWidth() + parseInt($page[0].getStyle('margin-right') || 0, 10)+ parseInt($page[0].getStyle('margin-left') || 0, 10);;

    var addon = "";


    var perPage = +$base.getAttribute('perPage');
    var leave = num % perPage;
    if (num % perPage !== 0) {
    	for(var i = leave; i < prePage; ++i){
          $page[$page.length-q].insert({after:'<div class="' + pageClass + ' empty"></div>'});
          num++;
        }
        $page=$base.select('.'+pageClass);
    }
    var step = pageWidth * perPage;
    
    for (i = 0; i < perPage; ++i) {
        $page[$page.length -1].insert({
            after:$page[perPage - i - 1].clone(true).addClassName('fake')
        });
    }

    for (i = 0, s = num - perPage ; i < perPage ; ++i){
        $page[0].insert({
            before:$page[s+i].clone(true)
              .addClassName('fake')
        });
    }

    //random set from position
    var random = $base.getAttribute('random');
    if (random) {
        random = Math.ceil(Math.random() * num / perPage);
        $base.setAttribute('from', random);
    }

    //set *from* position
    var from = $base.getAttribute('from');
    if (from) {
        cur = from - 1;
        $pages.style.left = -(cur+1) * step + "px";
    }else{
        $pages.style.left = - step + "px";
    }

    //loop Events
    var loop = $base.getAttribute('loop'),
        timer;
    if (loop) {
        if (loop == 0) loop = 5;
        timer = setTimeout(next, loop *= 1000);
    }
    
    //bind Event;
    var prevBtn = $base.select('.yk-slide-btnprev')[0];
    var nextBtn = $base.select('.yk-slide-btnnext')[0];
    var $num     = $base.select('.yk-slide-num')[0];
    
    if($num){
        $num.innerHTML = ((cur + num ) % num )/ perPage + 1 + "/"+num/perPage;
    }
    prevBtn && prevBtn.observe('click', function () {
        next(-1);
    });
    nextBtn && nextBtn.observe('click', function () {
        next(1);
    });
    //$base.select('img').each(function(el, i){
    //	el.setAttribute('src',el.getAttribute('_src'));
   // })
   $base.select('img').each(function(el, i){
    	var src;
    	if(src=el.getAttribute('_src')){
    		el.setAttribute('src',src);
    	}
    })
    if (!$base.ani) {
        $base.ani = new Ani($pages, {
            direction: 1,
            autoStart: false,
                "time": 1000,
            onstart: function (el) {
                this.left = parseInt(el.getStyle('left') || 0);
                this.dis = this.direction*step;
                this.end = this.left - this.dis;
            },
            onevery: function (el, percent) {
                el.style.left = this.left - percent * this.dis + 'px';
            },
            onstop: function (el) {

                cur += this.direction * perPage;
                var total = num/perPage;
                if($num){
                    $num.innerHTML = ((cur + num ) % num )/ perPage + 1 + "/"+num/perPage;
                }
                if (cur >= num) {
                    el.style.left = - step + 'px';
                    cur = 0;
                }else if(cur <0){
                    cur = num - perPage;
                    el.style.left = - num * pageWidth +'px';
                } else {
                    el.style.left = this.end + 'px';
                }
            }
        });
    }

    function next(direction) {
    	if(timer)clearTimeout(timer);
        var dis = direction * step;
        direction = direction || 1;
        if ($base.ani.running) {
            if (cur + perPage == num) {
                $base.ani.stop();
            } else {
                $base.ani.direction = direction;
                cur += direction * perPage;
                $base.ani.dis += dis;
                $base.ani.end -= dis;
            }
        } else {
            $base.ani.direction = direction;
            $base.ani.start();
        }

        if (loop) {
            timer = setTimeout(next, loop);
        }
    }
  };
  $$(".yk-slide-index").each(slideInit);
  window["__ad__slide"]=slideInit;
});

(function(){

var userAgent = window.navigator.userAgent;
	if(userAgent.match(/Android/i) || userAgent.indexOf('iPod') !== -1 || userAgent.indexOf('iPad') !== -1 || userAgent.indexOf('iPhone') !== -1){
	    document.getElementById('m_87594').innerHTML = '<a style="display:block;width:640px;height:110px;margin:0 auto;" href="http://hz.youku.com/red/click.php?tp=1&cp=4008714&cpp=1000749&url=http://m.youku.com/webapp/dl?app=youku&source=webqr" target="_blank"><img src="http://res.mfs.ykimg.com/0510000051C931FE6714C049470E4C25" width="640" height="110" /></a>';
	    //document.getElementById('m_86823').innerHTML = '<a style="display:block;width:320px;height:240px;margin:0 auto;" href="http://hz.youku.com/red/click.php?tp=1&amp;cp=4008713&amp;cpp=1000749&amp;url=http://v.youku.com/v_show/id_XNjE5Njc2MDk2.html?firsttime=120?app=youku&amp;source=webqr" target="_blank"><img src="http://static.atm.youku.com/Youku2013/201310/1011/21942/310-205.jpg" width="320" height="240"></a>';
	    //document.getElementById('m_86823').style.overflow = "hidden";
	}

})();
</script>




</div>



<div name="m_pos" id="m_93820">
	



<script language="javascript" type="text/javascript">
(function() {
	var tmpShowId	= 0;
	var divId		= 'ab_101556';
	var adShowIds	= [];
	var oAdShows	= $$('div[_showid]');
	var url			= 'http://html.atm.youku.com/html?p=101556&k=';

	//创建存放返回结果的div
	var oDiv		= $(divId);
	if (!oDiv) {
		oContainerDiv	= document.createElement('div');
		oContainerDiv.style.display	= 'none';
		document.body.appendChild(oContainerDiv);

		oDiv	= document.createElement('div');
		oDiv.id	= divId;
		oContainerDiv.appendChild(oDiv);
	}

	oAdShows.each(function(oShow) {
		tmpShowId	= parseInt(oShow.readAttribute('_showid'));
		if (!isNaN(tmpShowId) && (adShowIds.indexOf(tmpShowId) == -1)) {
			adShowIds.push(tmpShowId);
		}
	});
	url	+= '&s=' + adShowIds.join(',');
	Nova.addScript(url);

	//判断Nova.addScript()是否加载完成的定时器
	var count	= 0;
	var intv	= window.setInterval(callback, 500);
	function callback() {
		if (count >= 20) {return window.clearInterval(intv);}
		++count;

		var oDiv	= $(divId);
		if (!oDiv || oDiv.innerHTML.strip().empty()) {return;}
		try {
			var mapping	= {};
			var objJson	= oDiv.innerHTML.evalJSON();
			for (var i = 0; i < objJson.shows.length; ++i) {
				mapping['show_' + objJson.shows[i].id]	= {logo_url:objJson.shows[i].logo_url, logo_click_url:objJson.shows[i].logo_click_url};
			}
			oAdShows.each(function(oShow) {
				oShow	= $(oShow);
				tmpShowId	= parseInt(oShow.readAttribute('_showid'));
				if (isNaN(tmpShowId)) {return;}

				tmpShowId	= 'show_' + tmpShowId;				
				if (typeof mapping[tmpShowId] === 'undefined') {return;}

				var oAdCont;
				var adlogo	= '<a href="' + mapping[tmpShowId].logo_click_url + '" target="_blank" class="v-meta-adLogo" style="background:url(' + mapping[tmpShowId].logo_url + ') no-repeat right 0; ;_background:none;_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src=' + mapping[tmpShowId].logo_url + ');">&nbsp;</a>';
				if (oShow.down('.vb')) {	//for show4.tpl
					oAdCont	= oShow.down('.v-meta-tagrb');
				} else if (oShow.down('.va')) {	//for show5.tpl
					oAdCont	= oShow.down('.v-meta-neck');
				}
				if (oAdCont) {oAdCont.insert(adlogo);}
			});
		} catch (e) {
		} finally {
			window.clearInterval(intv);
		}
	};
})();


(function(){
var blockiPadAD = function(){
	var userAgent = window.navigator.userAgent;
	if(userAgent.indexOf('iPad') !== -1){
		document.getElementById('m_87596').style.display = 'none';
	}
};
blockiPadAD();
})();
</script>




</div>



