.feedback_top {position: fixed; bottom: 100px; _position: absolute; _top: expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight - 100)); right: 50px; _right:50px; /*left: 50%; margin-left: 500px;*/ } 
.feedback_base {background-position: 0 0;}
.feedback_base, .feedback_top .gotop {width: 50px; height: 50px; text-indent: -9999em; display: block; margin-top: 3px; font-size: 0; line-height: 0; background-position: 0 0; }
.feedback_base, .feedback_top .gotop {background-image: url(/static/images/feedback_top.png); background-repeat: no-repeat; }
.feedback_top .gotop {background-position: 0 -52px;display: none;}
