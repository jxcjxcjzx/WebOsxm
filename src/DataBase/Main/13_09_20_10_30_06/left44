function followCall(isFollow){
	UI.toggle('followTxt');
	UI.toggle('followBtn');
	$('followBtn').className='addAttention';
	$('unfollowBtn').className='delAttention';
	var btn=$('sFollowBtn');
		if(isFollow==1){
		UI.show(btn);
		MI.Bos('btnFollowHis');
		MI.feedBack({name:'guestpage',iPos:51,iBak1:3,sBak1:MI.guest.uin,id:1191});
	}
	else {
		UI.hide(btn);
		MI.Bos('btnUnFollowHis');
		MI.feedBack({name:'guestpage',iPos:51,iBak1:8,sBak1:MI.guest.uin,id:1191});
		UI.hide($('followBtn').parentNode);
		UI.removeClass(btn,'foLight');
	}
		var note = $('addNote'),noteBtn = $$(note,'a')[0];
	if(isFollow==1)
	{
		UI.show(note);
		UI.hide($('followBtn').parentNode);
		showSameStar();
	}
	else
	{
		UI.hide(note);
		UI.A(noteBtn,'note','');
		noteBtn.innerHTML = '添加备注';
		UI.show($('followBtn').parentNode);
	}
}
