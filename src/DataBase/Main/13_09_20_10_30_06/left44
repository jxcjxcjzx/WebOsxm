setTimeout(function(){
	var o = UI.parseUrl(),t,tid = 'talkList';
	if (o.pgv_ref && o.pgv_ref.slice(0,3) == 'im.'){
		o.funBox = '3';
		if (o.viewId && !$(o.viewId)){
			MI.Bos('btnQQ2HomeNoTalk');
		}
	}
	if (o.viewId){
		tid = o.viewId;
	}
	switch (o.funBox){
		case '2':
			t = $$($(tid),'.funBox .reply')[0];
			break;
		case '3':
			t = $$($(tid),'.funBox .comt')[0];
			break;
		case '1':
			t = $$($(tid),'.funBox .relay')[0];
			break;
		case 'none':
			t=null;
			break;
		default:
			break;
	}
	if(UI.isElement(t)){
		MI.app({
			TalkListRich : function(){
				setTimeout(function(){
					UI.fireEvent(t,'click');
				},200);
			}
		});
	}
},200);
