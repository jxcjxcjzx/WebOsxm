                        <div class="mod-13 mod-02">
                            <div class="tit02 clearfix">
                                <h3><a target="_blank" href="http://hangye.sina.net/">行业动态</a></h3>
                            </div>
                            <script type="text/javascript">
                                 var dupDataResPDPS = [];
                                 function pushResPDSRes(arr, pos) {
                                     if (arr) {
