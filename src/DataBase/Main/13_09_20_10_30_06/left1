/*二维码*/
.side-btns-2wm{display:none;}
.side-btns-2wm {bottom: 280px; _top:expression(documentElement.scrollTop + documentElement.clientHeight-400); left: 50%;margin: 0 0 0 505px;position: fixed;_position:absolute;width: 110px;z-index: 0; /* display:none; */}
.side-btns-2wm a{ display:block; width:110px; height:147px; background:url(http://i3.sinaimg.cn/dy/main/ewm/2/wap_sina.png) no-repeat}
.side-btns-2wm a:hover{ background-position:-110px 0}
.side-btns-2wm-resize{margin-left: 360px; display:none;}
