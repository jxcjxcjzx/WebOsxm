<script type="text/javascript">
        function setPanel1(i)
        {
            selectLayer2(i);
        }
        function selectLayer2(i)                                                       
        {
            switch(i)
            {
                case 1:
                    document.getElementById("focusPanel1").style.display="block";
                    document.getElementById("focusPanel2").style.display="none";
                    document.getElementById("focusPanel3").style.display="none";
                break;
                case 2:
                    document.getElementById("focusPanel1").style.display="none";
                    document.getElementById("focusPanel2").style.display="block";
                    document.getElementById("focusPanel3").style.display="none";
                break;
                case 3:
                    document.getElementById("focusPanel1").style.display="none";
                    document.getElementById("focusPanel2").style.display="none";
                    document.getElementById("focusPanel3").style.display="block";
                break;
            }
        }
</script>

<script type="text/javascript">
var fs2_fsgnrname = "";
var fs2_xxshname = "";

//all friend
function fs2_viewAllfriend()
{
	var fsgnrname = "fsg_nr";
	if (fs2_fsgnrname != "")
	{
		fsgnrname = fs2_fsgnrname;
	}

	var xxshname = "xx_sh";
	if (fs2_xxshname != "")
	{
		xxshname = fs2_xxshname;
	}

	if($(fsgnrname).style.display == "block")
	{
		$(fsgnrname).style.display = "none";
		$(xxshname).innerHTML = '<img src="/images/category/xx_xx1.gif" class="cp" onmouseover="this.src=\'/images/category/xx_xx2.gif\';" onmouseout="this.src=\'/images/category/xx_xx1.gif\';" />';
	}
	else
	{
		$(fsgnrname).style.display = "block";
		$(xxshname).innerHTML = '<img src="/images/category/xx_xs1.gif" class="cp" onmouseover="this.src=\'/images/category/xx_xs2.gif\';" onmouseout="this.src=\'/images/category/xx_xs1.gif\';" />';

	}
}

