                for (var i = 0, il = arrWeb1330.length; i < il; i++) {
                    if (arrWeb1330[i].area.substr(local_index, 1) == "1") {
                        sp[0].innerHTML = '<img width="103" height="68"  border="0" alt="' + arrWeb1330[i].title + '"  src="' + arrWeb1330[i].picpath + '"/>';
                        sp[1].innerHTML = WSCSubstr(arrWeb1330[i].title, 0, 16);
