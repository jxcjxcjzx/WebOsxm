				var jcpp = document.getElementById("jcpp");
				var jcppParent = jcpp.parentNode;
				jcppParent.removeChild(jcpp);
				var lipic = '<div style="height:8px;overflow:hidden; width:100%;"></div><span class="ylblktp"></span><div style="height:105px;" class="ylblkct"><div class="PartLBtnAds"><div style="width:140px;height:105px;margin:0 auto;overflow:hidden;"><a href="'+arrWeb1[0].url+'" target="_blank"><img src="'+arrWeb1[0].pic+'" style="width:140px; height:105px;border:0 none;" /></a></div></div></div><span class="ylblkbt"></span><div style="height:8px;overflow:hidden; width:100%;"></div><span class="ylblktp"></span><div style="height:105px;" class="ylblkct"><div class="PartLBtnAds"><div style="width:140px;height:105px;margin:0 auto;overflow:hidden;"><a href="'+arrWeb2[0].url+'" target="_blank"><img src="'+arrWeb2[0].pic+'" style="width:140px; height:105px;border:0 none;" /></a></div></div></div><span class="ylblkbt"></span><div style="height:8px;overflow:hidden; width:100%;"></div><span class="ylblktp"></span><div style="height:105px;" class="ylblkct"><div class="PartLBtnAds"><div style="width:140px;height:105px;margin:0 auto;overflow:hidden;"><a href="'+arrWeb3[0].url+'" target="_blank"><img src="'+arrWeb3[0].pic+'" style="width:140px; height:105px;border:0 none;" /></a></div></div></div><span class="ylblkbt"></span>';
				jcppParent.innerHTML += lipic;
			}

function setZtDefault0224(o,b,index){
					 for(var i=0;i<o.length;i++){
	    				var itemobj=o[i];					
						if(itemobj.type){
							if(itemobj.type=="S"){
							      for(var j=0;j<b.length;j++){
									  var obj=b[i];
									  if(obj.area){
									  		if(obj.area.substr(index,1)=="1"){
										       if(obj.picpath&&obj.url){
												    itemobj.url=obj.url;
													itemobj.pic=obj.picpath;
													if(itemobj.imurl){
														var img=document.createElement("img");
														img.src =itemobj.imurl;
		  												img.onload = img.onreadystatechange = img.onerror = function () {
														img = null;
														};
													};
													b.splice(j,1);
													break;
												  };										 	  
								        	   };
									  }
								  }						
							};												
						};		             
                  };
				};		
	}

});

