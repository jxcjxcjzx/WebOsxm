                    'titlelen': 22
                });

if(typeof arrWeb1220 != "undefined"){ var arrPad=webShow({'resid':'1','webs':arrWeb1220,'area':local_index,'rearrange':true,'cols':2,'rows':5,'titlelen':24,'action':'data'}); if(typeof arrPad.grid!="object"){arrPad.grid=new Array();}; var numArr2=[0,2,4,6,8,1,3,5,7,9]; var li=document.getElementsByName("res1332");for(var i=0,j=0;i<li.length;i++){j=numArr2[i];if(li[j].getAttribute("webtype")=="S"){while(arrPad.grid.length>0&&(web=arrPad.grid.shift())){if(typeof web.title!='undefined'){li[j].href=web.url;li[j].innerHTML=WSCSubstr(web.title,0,22);break;};};};}; };

            }
        });
    }
});
</script>

