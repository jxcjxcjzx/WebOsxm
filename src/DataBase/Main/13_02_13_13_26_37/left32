			function getCbCookie(name) //取cb cookies函数 
			{ 
				var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)")); 
				if (arr != null) return arr[2].toString(); 
				return null; 
			} 
			var winduid = parseInt("");
			if(!isNaN(winduid)){ 
				showLogin(); 
			}else{ 
				showNoLogin(); 
			} 
			
			function showLogin(){ 
				var str = ""; 
				str +="<form id=\"form1\" name=\"form1\" method=\"post\" action=\"\">"; 
				str +="<div class=\"yjdlbox\"><p class=\"username\"><strong><span><a href=\"http://blog.chinabyte.com/user_index.php\" class=\"red\" target=\"_blank\">";//
				var nick = (getCookie(findNickCookie) != '') ? getCookie(findNickCookie) : "";
				str += nick+"</a></span>,欢迎回来!</strong> <a href=\"login.php?action=quit\" target=\"_self\" class=\"red\">[退出]</a></p><dl><dt>您可以:</dt><dd class=\"black\"><a href=\"http://blog.chinabyte.com/u//"; 
				str += "\">进入个人博客首页</a></dd><dd class=\"black\"><a href=\"http://blog.chinabyte.com/user_index.php\">博客管理</a></dd><dd><a href=\"http://passport.chinabyte.com\" class=\"red\">修改密码</a></dd></dl><p class=\"phone\">编辑电话:<strong>010-82605321-540</strong></p></div></form>"; 
				document.write(str); 
			} 
			
			function showNoLogin(){ 
				var str = ""; 
				str +="<form id=\"form1\" name=\"form1\" method=\"post\" action=\"http://passport.chinabyte.com/cas/login\" target=\"_self\">"; 
				
				str +="<input type=\"hidden\" name=\"renew\" value=\"true\" ><input type=\"hidden\" name=\"service\" value=\"http://blog.chinabyte.com/cb_transfer.php?goto=aHR0cDovL2Jsb2cuY2hpbmFieXRlLmNvbS8=&dn=1\" ><input type=\"hidden\" name=\"furl\" value=\"http://blog.chinabyte.com/cb_transfer.php?goto=aHR0cDovL2Jsb2cuY2hpbmFieXRlLmNvbS8=&dn=1\" >"; 
				str +="<div class=\"userlogin\"><input type=\"hidden\" name=\"siteid\" value=\"2\"/><input type=\"hidden\" name=\"productid\" value=\"2\"/><dl class=\"userin\"><dt>用户名</dt><dd><input type=\"text\" tabindex=\"1\" name=\"username\" id=\"username\" class=\"suerinb\"/></dd></dl><dl class=\"userin\"><dt>密　码</dt><dd><input type=\"password\" tabindex=\"2\" name=\"password\" id=\"password\" class=\"suerinb\"/></dd></dl><dl class=\"suerpass\"><dt><input type=\"checkbox\" class=\"rembmm\"/>记住密码</dt><dd><input type=\"submit\" class=\"suerinsb\" tabindex=\"3\" value=\"登 陆\" name=\"button\"/></dd></dl><div class=\"userzc\"><dl><dt><strong><a href=\"http://passport.chinabyte.com/register.jsp?auto=1&returnurl=http://blog.chinabyte.com\">注册新用户</a></strong></dt><dd><a href=\"http://passport.chinabyte.com/getpass_name.jsp\" class=\"red\">忘记密码</a></dd></dl><p>编辑电话:<strong>010-82605321-540</strong></p></div></div></form>"; 
