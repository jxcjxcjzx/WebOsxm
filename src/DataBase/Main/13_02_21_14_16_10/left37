<script>
jQuery(".coverpeople img").each(function(){
        $(this).mouseover(function(event){
                $(".coverpeople div img").attr('src', $(this).attr("src"));
                $(".coverpeople div a").attr('href', $(this).parent().attr("href"));
                
                x=event.clientX ;
                y=event.clientY + $(window).scrollTop();
                $(".coverpeople div").css("left", x).css("top", y).show();
        }).mouseout(function(){
                $(".coverpeople div").hide();
        });
        $(".coverpeople div").mouseover(function(){
                $(this).show();
        }).mouseout(function(){
                $(this).hide();
        });
});
