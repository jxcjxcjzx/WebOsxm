 <div class="btn"><a href="javascript:void(0);" id="img_p_next"><img src="images/p_next.png" /></a><a href="javascript:void(0);" id="img_p_pre"><img src="images/p_pre.png" /></a><span id="img_index">1</span>/<span id="img_count"></span></div>
 <script type="text/javascript">
var t1 = n1 = 0, count1=$("#play_img a").size();
$("#img_count").text(count1);
$(function(){
	$("#play_img a:not(:first)").hide();
	$("#img_p_next").click(function() {
		var i1 = $("#img_index").text() - 1;
		n1 = i1;
		if (i1 >= count1) i1=0;
		$("#play_img a").hide();
		$("#play_img a").eq(i1).show();
		//$("#play_list span").filter(":visible").fadeOut(500).parent().children().eq(i).fadeIn(1000);
		if (n1>=count1-1) n1=-1;
		$("#img_index").text(n1+2);
	});
	t1 = setInterval("showAuto1()", 3000);
	
	$("#img_p_pre").click(function(){
		var i2 = $("#img_index").text() - 1;
		i3=i2;
		if (i2<=0) {
			i2=count1-1;
			i3=count1;
		}
		$("#play_img a").hide();
		$("#play_img a").eq(i2).show();
		$("#img_index").text(i3);
	});
})
function showAuto1()
{
	$("#img_p_next").trigger('click');
}
</script>
