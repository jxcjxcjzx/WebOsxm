var loginStatus = (function(){
  var $ = function(id){return document.getElementById(id)};
  var onDiv = $('logonStatus'), outDiv = $('logoutStatus'), uDiv = $('userInfoBox'), unreadMail = $('mailinfo'), gList = $('logonFldselectop'), mailLink = $('linkTo_mail'), mailLinkLine = mailLink.parentNode.nextSibling;
  var HTML_ENTITIES = {
      '&': '&amp;',
      '>': '&gt;',
      '<': '&lt;',
      '"': '&quot;',
      "'": '&#39;'
  },
  HTML_ESCAPE_TOKENS = {
    '&lt;': '<',
    '&gt;': '>',
    '&quot;': '"',
    '&nbsp;': ' ',
    '&amp;' : '&'
  };
  if(mailLinkLine.nodeType != 1){
    mailLinkLine = mailLinkLine.nextSibling;
  };
  function chkLogonGuideClose(e){
    thisObj = e.target || e.srcElement;
    do{
      if(thisObj.id == "logonFldselect") return;
      if(thisObj.tagName == "BODY"){statusObj.guide.close();return;};
      thisObj = thisObj.parentNode;
    }while(thisObj.parentNode);
  };
  function leftStr(str, n){
    var s = str.replace(/\*/g, " ");
    s = s.slice(0, n).replace(/\*\*/g, " ").replace(/\*/g, "").length;
    return str.slice(0, s);
  }
  function leftB(str, len){
    var sb = [];
    var n = 0;
    var l = 1;
    for(var i = 0, ln = str.length; i <= len; i++){
      if(i >= ln) break;
//      l = str.charCodeAt(i) < 128 ? 1 : 2;
      if(n + l > len){
        sb.push("...");
        break;
      }
      n += l;
      sb.push(str.charAt(i));
    }
    return sb.join("");
  }
  function lengthB(str){
    var n = 0;
    for(var i = 0, len = str.length; i < len; i++){
      n += str.charCodeAt(i) < 128 ? 1 : 2;
    }
    return n;
  }
  function encodeHTML(s) {
    return s && s.replace(/[&\"\'><]/g, function(c) {
      return HTML_ENTITIES[c];
    });
  }
  function decodeHTML(s) {
    return s && s.replace(/&[a-z]+;/gi, function($0) {
      return HTML_ESCAPE_TOKENS[$0] || $0;
    }).replace(/&#(\d{2}|\d{4});/ig, function($0, $1) { return String.fromCharCode($1); });
  }
  var statusObj = {
    userInfo : null,
    showUserInfo : function(){
      outDiv.style.display = 'none';
      onDiv.style.display = 'block';
      var nick = decodeHTML(this.userInfo.nick);
      nick = nick.length > 6 ? encodeHTML(leftB(nick, 6)) : encodeHTML(nick);
      uDiv.innerHTML = nick;
      $('linkTo_video').href = 'http://you.video.sina.com.cn/m/' + this.userInfo.uid;
      $('linkTo_blog').href = 'http://blog.sina.com.cn/u/' + this.userInfo.uid;
      mailLink.parentNode.style.display = 'block';
      mailLinkLine.style.display = 'block';
      switch(this.userInfo.ag){
        case '2':
        case '9'://com
          mailLink.innerHTML = '�������';
          break;
        case '3':
          mailLink.innerHTML = 'VIP����';
          break;
        case '5':
        case '10':
          mailLink.innerHTML = 'UC����';
          break;
        case '7':
          mailLink.innerHTML = '2008����';
          break;
        case '8':
          mailLink.innerHTML = 'CN����';
          break;
        default:
          mailLink.parentNode.style.display = 'none';
          mailLinkLine.style.display = 'none';
      };
      //<!-- �������䵽������ START-->
      jsLoader({
        url : 'http://service.mail.sina.com.cn/mailproxy/mail.php?s=1&timeStamp=' + new Date().getTime(),
        callback : function(){
          try{//��ֹtimeout��abort
            if(sinamailinfo && sinamailinfo.result && sinamailinfo.unreadmail>=0 && sinamailinfo.url && sinamailinfo.email){
              var uread = (sinamailinfo.unreadmail-0)>99 ? '99+' : sinamailinfo.unreadmail;

              unreadMail.innerHTML = '(<a href="' + sinamailinfo.url + '" target="_blank">' + uread + '</a>)';
            }
          }catch(ex){
          }
        }
      });
      //<!-- �������䵽������ END-->
    },
    showLoginForm : function(){
      outDiv.style.display = 'block';
      onDiv.style.display = 'none';
      unreadMail.innerHTML = "";
    },
    check : function(){
      var userInfo = sinaSSOManager.getSinaCookie();

      if(userInfo === this.userInfo && userInfo === null){
        return;
      }else if(userInfo && this.userInfo && this.userInfo.nick === userInfo.nick){
        return;
      }else if(userInfo){
        this.userInfo = userInfo;
        this.showUserInfo();
      }else{
        this.userInfo = userInfo;
        this.showLoginForm();
      };
    },
    guide : {
      status : 'close',
      open : function(){
        if(this.status == 'open'){this.close();return};
        gList.style.display = 'block';
        sina.addEvent(document.body,"mousedown",chkLogonGuideClose);
        this.status = 'open';
      },
      close : function(){
        gList.style.display = 'none';
        sina.delEvent(document.body,"mousedown",chkLogonGuideClose);
        this.status = 'close';
      }
    }
  };
  setInterval(function(){statusObj.check()},1000);
  return statusObj;
})();
loginStatus.check();
