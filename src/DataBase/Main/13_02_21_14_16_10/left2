<script type="text/javascript">
(function(){
	var Shibada = {
		css		: null
		,dom	: null
		,js		: null
		,handle	: null
		,cookie	: null
		,header	: null
		,mrbAdBox : null
		,cssRule: ''
		,init	: function() {
			this.css = document.createElement('STYLE');
			this.dom = document.getElementById('indexad_bg');
			this.handle = document.getElementById('indexad_bg_close');
			//this.mrbAdBox = document.getElementById('mrbAdBox');
			this.cssRule = document.getElementById('indexad_bg_cssRule');
			this.header = document.getElementsByTagName("head")[0];
			//this.bind();
			
				var userAgent = window.navigator.userAgent;
	if(userAgent.match(/Android/i) || userAgent.indexOf('iPod') !== -1 || userAgent.indexOf('iPad') !== -1 || userAgent.indexOf('iPhone') !== -1){
} else {
			this.bind();

}
			
		}
		,bind	: function() {
			var _this = this;
			this.show();
			this.handle.onclick = function() {
				_this.hidde();
			};
		}
		,hidde	:function() {
			this.dom.style.display = 'none';
			try{//IE
				this.css.styleSheet.cssText = '';
			}catch(e){//FF Chrome
				this.css.innerHTML = '';
			}
			//this.mrbAdBox.style.display = 'none';
			//Nova.Cookie.set('d_g_cookie','0',30,'/','.youku.com');
		}
		,show	: function() {
		        this.dom.style.display = 'block';
			this.css.type = 'text/css';

			try{//IE
				this.css.styleSheet.cssText = this.cssRule.innerText;

			}catch(e){//FF Chrome
				this.css.innerHTML = this.cssRule.innerHTML;
			}
			this.header.appendChild(this.css);
		}
	};


	var democraticCookie = Nova.Cookie.get('d_mg_cookie');
	if( democraticCookie == null || democraticCookie == 1 ) {

		if( !democraticCookie ) {
			Nova.Cookie.set('d_mg_cookie','1',30,'/','.youku.com');
		}
		else {
 			//if(navigator.userAgent.indexOf('Firefox') >= 0) {
				//return false;

			//} else {
				Shibada.init();
			//}
		}

	}

})();




