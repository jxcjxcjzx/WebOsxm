        */

        if (
            navigator.userAgent.match(/iPad/i) ||
            navigator.userAgent.match(/iPhone/i) ||
            navigator.userAgent.match(/iPod/i)
        ) {
            ua_info.is_ios = true;
        } else {
            ua_info.is_ios = false;
        }

        if (navigator.userAgent.match(/android/i)) { ua_info.is_android = true; }
        else { ua_info.is_android = false; }

        if (navigator.userAgent.match(/playbook/i)) {
            ua_info.is_playbook = true;
        } else {
            ua_info.is_playbook = false;
        }

        if (navigator.userAgent.match(/android 2\.1/i) ) {
            ua_info.is_android_old_flash = true;
