			$("#list_new_c_2").append("<iframe marginheight=\"0\" marginwidth=\"0\" frameborder=\"0\" width=\"318\" height=\"120\" scrolling=\"no\" src=\"http://fujian.people.com.cn/GB/336279/index.html\" ></iframe>");
			 }			 
		else if(city_id == "230000")
		{
			$("#list_new_1").after("<h3 onmouseover=\"list_tab_new(2)\" id=\"list_new_2\" class=\"dfnormal\"><a href=\"http://ah.people.com.cn/\">安徽</a></h3>");
