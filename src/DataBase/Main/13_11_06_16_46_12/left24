jQuery(document).ready(function(){
	jQuery(".newsmap_list li:not(:first)").css("display","none");
		var B=jQuery(".newsmap_list li:last");
		var C=jQuery(".newsmap_list li:first");
		setInterval(function(){
			if(B.is(":visible")){
				C.fadeIn(500).addClass("in");
				B.hide(400)
			}else{
				jQuery(".newsmap_list li:visible").addClass("in");
				jQuery(".newsmap_list li.in").next().fadeIn(500);
				jQuery("li.in").hide(400).removeClass("in")
			}
		},3000)
});
</script>
