function tmplfun(o) {var p=[],print=function(){p.push.apply(p,arguments);};p.push('<div id="',o.id,'_w" token="',o.token,'" class="blog_item"'); if( o.tv && o.tv==2 ){ p.push(' tv="2" tvs=\'',o.tvs,'\''); } p.push('>'); if( o.videos&&o.videos[0].miniPicUrl){ p.push('<div class="blog_pic"><a target="_blank" href="',o.msgUrl,'',o.id,'"><img src="',o.videos[0].miniPicUrl,'"><span class="ico_vedio"></span></a></div>'); }else if( o.img&&o.img.length > 0){ p.push('<div class="blog_pic" style="height:',o.ph,'"><a target="_blank" href="',o.jumpUrl ? o.jumpUrl :(o.msgUrl+o.id),'">'); if( o.tv && o.tv == 2){ p.push('<span class="ico_spread"></span>'); } p.push(''); for(var i=0,len=o.img.length;i<len;i++){p.push('<img src="',o.img[i].url,'/',o.img[i].type == 2 ? 460 : 220,'">');}p.push('</a></div>'); }p.push('<div class="blog_user clear"><a target="_blank" title="',o.nick,'(@',o.chName||o.name,')" href="http://t.qq.com/',o.name,'" class="user_pic"><img src="',o.pic,'"></a><a target="_blank" title="',o.nick,'(@',o.chName||o.name,')" href="http://t.qq.com/',o.name,'" class="user_name '); if(o.miniUserName){ p.push('mini_user_name'); }p.push('">',o.bkname||o.nick,'</a>',o.icon,''); if( !o.isme){ if( o.isfollow){ p.push('<span class="add_attention added_state"><span class="ico_added"></span>已收听</span>'); }else{ p.push('<a edata="',o.name,'" href="#" class="add_attention add_state js_fol">收听</a>'); }} p.push('</div><div class="blog_info"><p>',o.content||"",'</p><a target="_blank" href="http://t.qq.com/p/t/',o.id,'" class="blog_time">',o.time,'</a>'); if(o.chsrc || o.hotSpot) {p.push('<p class="hot_channel">'); if(o.chsrc) {p.push('<a href="/i/',o.chsrc.categoryId,'?pgv_ref=web.c.watrefall.src" title="来自“',o.chsrc.categoryName,'”微频道"><span class="ico_bookmark"></span>',o.chsrc.categoryName,'</a>'); } p.push(''); if(o.hotSpot) p.push('<a target="_blank" href="',o.hotSpot.spotUrl,'?pgv_ref=web.c.hotspot" title="微热点：',_encodeHtml( o.hotSpot.spotName),'"><span class="ico_hdot"></span>',o.hotSpot.spotName,'</a></p>'); } p.push(''); if(false && o.commodity && o.commodity.UrlInfos[0].Price > 0) {p.push('<div class="book_shop"><a title="点击购买:',o.commodity.Name,'" target="_blank" href="',o.commodity.UrlInfos[0].Url,'&pgv_ref=c_waterfall2"><i class="trolley"></i><span>',o.commodity.UrlInfos[0].From,'(',o.commodity.UrlInfos[0].Price,'元');if( o.commodity.UrlInfos[0].Discount > 0) {p.push(', ',o.commodity.UrlInfos[0].Discount,'折');}p.push(')</span></a></div>'); } p.push('</div><div class="blog_option"><a href="#" class="option_item"><span title="收藏" class="ico_collect js_fav"></span></a><span class="split"></span><a class="option_item js_comt">评论</a><span class="split"></span><a class="option_item js_relay">转播 <em class="num">',o.counts[0],'</em></a></div></div>');return p.join('');}	function _encodeHtml(str) {
        return (str + '').replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/\x60/g, '&#96;').replace(/\x27/g, '&#39;').replace(/\x22/g, '&quot;');
    }
	function formatData( o, k){
		o.msgUrl   = o.jumpUrl ? o.jumpUrl : 'http://c.t.qq.com/p/33/';
		o.isme     = account == o.name;
		o.isfollow = k.relationMap[ o.name];
		o.chsrc    = k.categoryMap[ o.id];
		o.token    = k.token[ o.id];
		o.hotSpot  = k.hotSpots[o.id] && k.hotSpots[o.id].status == 0 ? k.hotSpots[o.id] : 0;
		o.miniUserName = !o.icon ? false : ( o.icon.match(/title/g).length >= 2 ? true : false );
        if(k.commodity && k.commodity[ o.id])
        {
            o.commodity = k.commodity[ o.id];
        }
        var len = o.img ? o.img.length : 0,ph = 0;
        for( var i = 0; i < len; i++){
        	ph += parseInt(o.img[i]["height"]*220/o.img[i]["width"]);
        }
        o.ph =  isNaN(ph) ? "" :(ph + ( len - 1) * 1 + "px");
	}

	
	for( var i = 0; i < 3; i++){
		var o = messageInfo.info.talk[i];
		if( !o)
			break;
		formatData( o, messageInfo.info);
		document.getElementById( 'col' + parseInt(i +1)).innerHTML = tmplfun( o);
	}
	

