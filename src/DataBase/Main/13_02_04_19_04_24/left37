(function (window, document, $, _, BS) {
BS.extend(BS, { features : {"polling_scores": false, "saved_stories": true, "homepage_new_stories": true, "signin": true, "homepage_popular_stories": true, "readlater": true, "submit_link": true, "homepage_new_stories_polling": false, "digg": true} });
$(document).on("click", ".story-link", BS.storyactions.redirectStoryLink);
/*
