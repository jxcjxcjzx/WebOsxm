				<script>
				function yeeyan_search(){
					var url='http://www.yeeyan.org/search/articles';
					var keys = document.getElementById('keys').value;
					window.location.href=url+'?q='+encodeURI(keys);
				}
				function bindEnter(event) {
					if (event.keyCode == 13) {
						yeeyan_search();
						event.returnValue = false;
					}
				}
				</script>
				<input name="q" type="text" class="text" value="" style="width:200px;margin-right:0;float:left"  id="keys"  onkeydown="bindEnter(event)" />
				<input type="button" onclick="yeeyan_search();"   class="search_btn" value="" style="float:left;width:35px;height:32px;border:0;margin-top:10px;cursor:pointer;background:url(http://cdn.yeeyan.com/img/ac/search_ico.gif) no-repeat #fff 7px 6px" />
