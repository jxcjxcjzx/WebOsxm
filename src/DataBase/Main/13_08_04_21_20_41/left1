lassName.indexOf(sugitemovercn)&&removeClass(ele,sugitemovercn);!!me.cache.curitem&&removeClass(me.cache.curitem,sugitemovercn)});sugdlt.add(sugtype,"mousedown",function(e){me.evtclick(e.el)});addEvent(ipt,"focus",function(){!me.cache.listStatus&&me.getTimeOut(me.show,"show")});if(ipt.onpropertychange){ipt.onpropertychange=function(){me.sendRequest()}}else{addEvent(ipt,"input",function(){me.sendRequest()})}addEvent(ipt,"blur",function(e){me.getTimeOut(me.hidden,"hidden")});addEvent(ipt,"keydown",function(e){me.keydownEvt(e)});addEvent(ipt,"keyup",function(e){var e=e||w.event;if(e.keyCode==38||e.keyCode==40||e.keyCode==27){return false}me.sendRequest()})},getTimeOut:function(fun,type){var me=this,timelayer=me.cache["timeLayer_"+type];!!timelayer&&clearTimeout(timelayer);me.cache["timeLayer_"+type]=setTimeout(function(){fun.apply(me,arguments)},200)},sendRequest:function(){var me=this,val=me.getInputVal();me.setDefault();if(val!==""){me.getSugData(globalConfig.requestURL,"key="+encodeURIComponent(val));me.cache.listType='sug';}else{if(!!me.cache.hotList){me.sugWrap.innerHTML=me.cache.hotList;me.cache.curVal="";me.curMaxLen=me.sugWrap.getElementsByTagName("DIV").length-2;me.innerShowORHidden(true)}else{me.getHotData(globalConfig.requestURL,"",true)}me.cache.listStatus=false;me.cache.listType='hot';}},setDefault:function(){var me=this;me.cache.curitem=null;me.cache.curindex=-1},show:function(){var me=this;if(!me.sugWrap){me.sugWrap=me.createSugList()}else{me.innerShowORHidden(true)}},hidden:function(){this.innerShowORHidden(false);var val=this.getInputVal();if(val!==""&&val!=="请输入关键字"){this.cache.listStatus=true}},innerShowORHidden:function(flag){var me=this;if(this.controlFunction()){!!flag?(me.sugWrap.style.display="block",me.cache.keyDownOff=false):(me.sugWrap.style.display="none",me.cache.keyDownOff=true)}else{me.sugWrap.style.display="none";document.hform_10.Refer.value = 'sina_index';me.cache.keyDownOff=true}},getInputVal:function(){return trim(this.input.value)},evtclick:function(t){var tmpstr=t.innerHTML;var key=encodeURIComponent(encodeURIComponent(tmpstr));this.input.value=tmpstr;if(this.cache.curVal!=undefined&&tmpstr!=trim(this.cache.curVal)){if(this.cache.listType==='sug'){document.hform_10.Refer.value='sina_sug';}else{document.hform_10.Refer.value = 'sina_hot_sug';}}else{document.hform_10.Refer.value = 'sina_index';}document.hform_10.setAttribute("action",globalConfig.searchURL.replace("@@key@@",key));document.hform_10.submit()},keydownEvt:function(){var me=this;var e=arguments[0]||w.event;if(!!me.cache.keyDownOff){return false}switch(e.keyCode){case 38:var idx,curMaxLen=me.curMaxLen||me.maxLen;me.cache.curindex===-1?idx=curMaxLen-1:idx=me.cache.curindex-1;me.select(idx,true);break;case 40:var idx,curMaxLen=me.curMaxLen||me.maxLen;if(me.cache.curindex===-1){idx=0}else{if(me.cache.curindex===curMaxLen-1){idx=-1}else{idx=me.cache.curindex+1}}me.select(idx,true);break;case 13:break;default:break}},select:function(obj,valchange){var me=this,ele,index,sugitemovercn=sugProFix+"mover",curitem=me.cache.curitem;typeof(obj)==="object"?(function(){ele=obj;index=+ele.getAttribute("index")})():(function(){if(obj===-1){ele=null;index=-1}else{ele=byAttr(me.sugWrap,"index",obj+"")[0];index=+obj}})();!!curitem&&removeClass(curitem,sugitemovercn);if(!!ele){addClass(ele,sugitemovercn);me.cache.curitem=ele;me.cache.curindex=index;if(!!valchange){me.input.value=ele.innerHTML}}else{me.input.value=me.cache.curVal;me.cache.curitem=null;me.cache.curindex=-1}setTimeout(function(){me.setCursorPos(me.input.value.length)},-1)},setCursorPos:function(pos){var ipt=this.input;if(ipt.setSelectionRange){ipt.setSelectionRange(pos,pos);ipt.focus()}else{var range=ipt.createTextRange();range.collapse(true);range.moveEnd("character",pos);range.moveStart("character",pos);range.select()}},createSugList:function(){var me=this,ipt=me.input,uniqueKey=$.util.getUniqueKey();if(!ipt){throw"input is undefined!"}me.sugWrap=document.createElement("DIV");me.sugWrap.className=sugWrapcn;me.innerShowORHidden(false);ipt.value="";if(!!me.cache.hotList){me.sugWrap.innerHTML=me.cache.hotList}else{me.getHotData(globalConfig.requestURL,"",true)}me.wrap.appendChild(me.sugWrap);setInterval(function(){me.getHotData(globalConfig.requestURL,"",false)},globalConfig.interTime);return me.sugWrap},getHotData:function(url,param,first){var me=this;var onDataSuccess=function(msg){if(msg.code==100000&&!!msg.data.length){var data=msg.data;first?(me.cache.hotList=me.sugWrap.innerHTML=me.itemConsturctor(data,0),me.innerShowORHidden(true)):me.cache.hotList=me.itemConsturctor(data,0,true)}else{me.innerShowORHidden(false)}};jsonp2(url,param,onDataSuccess)},getSugData:function(url,param){var me=this;var onDataSuccess=function(msg){if(msg.code==100000&&!!msg.data.length){if(me.getInputVal()!==""){var data=msg.data;me.sugWrap.innerHTML=me.itemConsturctor(data,1);me.innerShowORHidden(true);me.cache.listStatus=false}}else{me.innerShowORHidden(false)}};jsonp2(url,param,onDataSuccess)},getRandomKey:function(){var me=this;var ipt=me.input;var getRangeRandom=function(min,max){return Math.floor(min+Math.random()*(max-min))};var onDataSuccess=function(msg){if(msg.code==100000&&!!msg.data.length){var data=msg.data,len=data.length,rdm=getRangeRandom(0,len-1);w.___homeSugVal___=ipt.value="\u5927\u5BB6\u6B63\u5728\u641C\uFF1A"+data[rdm];me.cache.keyDownOff=true}else{ipt.value="\u8BF7\u8F93\u5165\u5173\u952E\u5B57\u000D\u000A"}};jsonp2(globalConfig.requestURL,"",onDataSuccess)},itemConsturctor:function(data,type,mode){var me=this,data=data,tmpstr=[],proto="word",tip="",more="";l=data.length>me.maxLen?me.maxLen:data.length;if(!mode){me.curMaxLen=l;me.cache.curVal=me.input.value}type==0?(proto="word",tip='<div class="top-suggest-tip">\u5927\u5BB6\u6B63\u5728\u641C\uFF1A\u000D\u000A</div>',more='<div class="top-suggest-more clearfix"><a href="http://s.weibo.com/top/summary?Refer=sina_sug" target="_blank" class="top-suggest-hotAll">\u5B8C\u6574\u70ED\u641C\u699C&gt;&gt;</a><a href="http://s.weibo.com/?Refer=sina_sug" target="_blank" class="top-suggest-toHomePage">\u8FDB\u5165\u641C\u7D22\u9996\u9875&gt;&gt;</a></div>'):(proto="suggestion",tip="",more="");tmpstr.push(tip);for(var i=0;i<l;i++){tmpstr.push('<div class="');tmpstr.push(sugItemcn);tmpstr.push('" index="');tmpstr.push(i);tmpstr.push('" action-type="');tmpstr.push(sugtype);tmpstr.push('">');tmpstr.push(data[i]);tmpstr.push("</div>")}tmpstr.push(more);return tmpstr.join("")}};return cSuggest});window.search_suggest=new SHM.home.suggest({input:document.SearchEcho.SerchKey,maxLen:10,onoff:"on",controlFunction:function(){if(document.SearchEcho.SerchType.value==="微博"){return true}else{return false}}})}});

            function tSearchUatrack(val) {
                try{
                    _S_uaTrack('index_new_search', val);
                }catch(e){}
            }
            function formSubmit(form,isWb,key){
                var isFF = /firefox/.test(navigator.userAgent.toLowerCase());
                if(isFF){
                    if(!isWb) {
                        setTimeout(function(){
                            form.submit();
                        },0);
                    } else {
                        setTimeout(function(){ 
                            !!key ? form.setAttribute('action', 'http://s.weibo.com/weibo/@@key@@'.replace('@@key@@',key)) : 
                            form.setAttribute('action', 'http://s.weibo.com/weibo/');
                            form.submit();
                        },0);
                    }
                } else {
                    if(!isWb) {
                        form.submit();  
                    } else {
                        !!key ? form.setAttribute('action', 'http://s.weibo.com/weibo/@@key@@'.replace('@@key@@',key)) : 
                        form.setAttribute('action', 'http://s.weibo.com/weibo/');
                        form.submit();
                    }
                }
            }
            function SearchSubmit(){
              var key = document.SearchEcho.SerchKey.value.replace(/(^[\s\u3000]*)|([\s\u3000]*$)/g, "");
              var ipt = document.SearchEcho.SerchKey;
              if(key == "请输关键词" || key == "请输入关键字" || key === "")
              {
                 ipt.value = "";
                 ipt.className = 'inp-txt inp-txt-click';
                 setTimeout(function(){ipt.className='inp-txt'},500);
                 ipt.focus();
                 return false;
              } else if(key === window.___homeSugVal___) {
                key = key.replace("\u5927\u5BB6\u6B63\u5728\u641C\uFF1A","");
                document.hform_10.Refer.value = 'sina_direct_index';
              }
              switch(document.SearchEcho.SerchType.value){
                case "新闻" :
                  document.hform_02.q.value = key;
                  tSearchUatrack('search_click_news');
                  formSubmit(document.hform_02);
                  break;
                case "视频" :
                  document.hform_03.k.value = key;
                  tSearchUatrack('search_click_video');
                  formSubmit(document.hform_03);
                  break;
                case "图片" :
                  document.hform_05.q.value = key;
                  tSearchUatrack('search_click_pic');
                  formSubmit(document.hform_05);
                  break;
                case "博客" :
                  document.hform_08.q.value = key;
                  tSearchUatrack('search_click_blog');
                  formSubmit(document.hform_08);
                  break;
                case "微博" :
                  var ReferEle = document.hform_10.Refer;
                  if(search_suggest.cache.curVal!=undefined && key != search_suggest.cache.curVal.replace(/(^[\s\u3000]*)|([\s\u3000]*$)/g, "") && search_suggest.sugWrap.style.display === 'block') {
                   if(search_suggest.cache.listType==='sug') {
                        ReferEle.value = 'sina_sug';
                    } else {
                        ReferEle.value = 'sina_hot_sug';
                    }
                  } else if(ReferEle.value != 'sina_direct_index') {
                   ReferEle.value = 'sina_index';
                  }
                  key = encodeURIComponent(encodeURIComponent(key));
                  tSearchUatrack('search_click_weibo');
                  formSubmit(document.hform_10,true,key);
                  break;
                case "知识人" :
                  document.hform_09.key.value = key;
                  tSearchUatrack('search_click_knowledge');
                  formSubmit(document.hform_09);  
                  break;
                default : //网页
                  break;
              }
              return false;
            }

            jsLoader({
                name: 'shm',
                callback: function() {
                    SHM.register('home.custEvent.firstpage.fire', function($) {
                        $.evt.custEvent.fire($, 'firstPageEnd');
                    });

                    //测试猜你喜欢的登录
                }
            });           
        </script>

