                             return document.getElementById(id) || null;
                           }

                           function attachEvent(obj, evt, func, eObj) {
                               eObj = !eObj ? obj : eObj;
                               if(obj.addEventListener) {
                                   obj.addEventListener(evt , func, false);
                               } else if(eObj.attachEvent) {
                                   obj.attachEvent('on' + evt, func);
                               }
                           }
