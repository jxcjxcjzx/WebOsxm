<!--加载跨栏 begin-->
<span id="CoupletMediaWrap"></span>
<script language="javascript" type="text/javascript">
//<![CDATA[
var CoupletMediaData=new Array();

/*--投放列表 begin--*/

//乐居跨栏广告 begin
try {
    if((new Date).getDay() === 4) {
        var _CoupletMediaOpenWin = CoupletMediaOpenWin;
        CoupletMediaOpenWin = function (rad, nad, divid) {
            lejuMedia.then(function (data) {
                leju.couplet(CoupletMediaData, data, "2013-2-14-9", "2013-12-31-9");
                _CoupletMediaOpenWin();
                CoupletMediaOpenWin = _CoupletMediaOpenWin;
                _CoupletMediaOpenWin = null;
            }, function () {
                _CoupletMediaOpenWin();
                CoupletMediaOpenWin = _CoupletMediaOpenWin;
                _CoupletMediaOpenWin = null;
            });
        };
    }
} catch (e) {}
//乐居跨栏广告 end

<!--ADS:begin=PDPS000000006450:{169F8B1D-CF56-4F7A-A401-B5C6716D66E3}-->

<!--B5C6716D66E3--> 
CoupletMediaData.push([
