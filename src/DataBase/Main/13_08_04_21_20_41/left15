        newNode.setAttribute('async', true);
        existingNode.parentNode.insertBefore(newNode, existingNode);

        var testNode = global.document.createElement('script');
        testNode.setAttribute('type', 'text/javascript');
        testNode.setAttribute('src', 'http://erebor.douban.com/isloaded.js?t=1375622402.92&uid=' + userId + '&bid=' + encodeURIComponent(browserId));
        testNode.setAttribute('async', true);
