<script type="text/javascript">

function SideBarToggle() {
		if ('block' == $('#sidebar')[0].style.display || ''==$('#sidebar')[0].style.display) {
			$('#sidebar')[0].style.display = 'none';$('#dicts')[0].style.width = '730px';$('#sidebarToggleImg')[0].src = 'resource/left_onck2.gif';
			$('#sidebarToggleImg')[0].alt = '显示导航栏';
		} else {
			$('#sidebar')[0].style.display = 'block';$('#dicts')[0].style.width = '730px';$('#sidebarToggleImg')[0].src = 'resource/left_onck.gif';
			$('#sidebarToggleImg')[0].alt = '关闭导航栏';
		}
};

function DictFold(id){
if($('#dictc_'+id)[0].style.display=='none'){
 $('#dictc_'+id)[0].style.display=''
 $('#dicttoggle_'+id)[0].src='resource/von.gif';
}else{
 $('#dictc_'+id)[0].style.display='none';
 $('#dicttoggle_'+id)[0].src='resource/von2.gif'
}
};


var dictLangList = ['ec', 'lovedict','bbs','lib','ajax'];

function DictLangSwitch(dictLang) {
	if (0 == dictLangList.length) {
		return;
	}

	var dictLangConfig = '';
	if (!dictLang) {
		var dictLangFound = false;
		for(var ldli=0; ldli<dictLangList.length; ldli++) {
			if (dictLangConfig == dictLangList[ldli]) {
				dictLangFound = true;
				break;
			}
		}
		if (!dictLangFound) dictLangConfig = dictLangList[0];
	} else {
		dictLangConfig = dictLang;
		if (!dictLangConfig) dictLangConfig = dictLangList[0];
	}
	
	$("#"+dictLangConfig)[0].className = 'v_';
	
	$('#dictlangc_'+dictLangConfig)[0].style.display = 'block';
	for(var ldi=0; ldi<dictLangList.length; ldi++) {
		if (dictLangConfig != dictLangList[ldi]) {
			$("#"+dictLangList[ldi])[0].className = '';
			$('#dictlangc_'+dictLangList[ldi])[0].style.display = 'none';
		}
	}
};


</script>

