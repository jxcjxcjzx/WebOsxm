if(keywords.indexOf('条') != -1 && (key+1)==keywords.length){
alert('请输入关键词');
setTextFocus;
return false;
}
if(keywords.replaceAll(' ','') == ''){
alert('请输入关键词');
setTextFocus;
return false;
}
var type = "-1";
//searchUrl = searchUrl + encodeURIComponent(keywords) + sp_id;
