    <script type="text/javascript">
	var nick = 'mycos',
		account = 'mycos',
		gender = '它';
		function addQQ(){
			var qq;
			//B设置为不允许的情况
    			qq = 0;
			isaddQQ = 0;
        			//只有用户设置为允许加为QQ好友时，才发送qq号
			MI.Bos('btnGuestQQ');
			var nick = 'mycos',
			account = 'mycos',
			mynick = '',
			myaccount = '',
			myqq = '',
			gender = '它';
			MI.addQQ(nick,qq,account,mynick,myaccount,gender,myqq,isaddQQ);
		}
	//出向他推荐朋友浮层，然后私信给他
	function recommentToGuest(){
		MI.Bos('btn_recommentToGuest');
		var data = {
			dialogTitle : _('向{0}推荐你收听的人',nick),
			title : '<h3 class="pop_chs_top">' + _('选择要推荐给{0}的用户<span class="text_gray">(最多100人，推荐将以私信的方式发给对方)</span>',gender) + '</h3>',
			confirmTxt :  _('发送推荐'),
			cancelTxt :  _('取消'),
			memberMax : 100,
			confirmCall : function(itemSelect){
				if (itemSelect){
					var data = itemSelect;
					if (data.result == 0 && data.info){
						var mem = [];
						UI.each(data.info.member,function(o,i){
							mem.push(o);
						});
						MI.ajax({
							url : MI.url.recommendAdd,
							type : 'get',
							data : {id:MI.user.account,srv:'p2p',toacc:account,friendaccs:mem.join(',')},
							success : function(data){
									MI.dialog.hide();
									setTimeout(function(){MI.tip(_('发送成功！'));},100);
							}
						});
					}
				}
				return false;
			}
		};
		MI.app({
			ItemSelect : function(){
				MI.ItemSelectDialog(data);
				MI.Bos('btnRecommendTo_user');
			}
		});
	}
    </script>
<script type="text/javascript">
function sendBirthGift() { // 生日礼物
	var author  = MI.guest.user;
	if(MI.user.account != author) {
		MI.Bos("btnGuestBirth");
		MI.app({
			TalkBoxRich:function(){
				MI.sendGiftShow({
					userAccount:author,
					nick:MI.guest.nickName,
					giftType:1,
					successCall:function(){
						MI.Bos("guestBirthSuccess");
					}
				})
			}
		})
	}
	return false;
}
</script>








