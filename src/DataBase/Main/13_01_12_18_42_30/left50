    function path()
    {
      var args = arguments,
          result = []
          ;
      for(var i = 0; i < args.length; i++)
          result.push(args[i].replace('@', 'http://yiivon.com/wp-content/plugins/syntax-highlighter-with-add-button-in-editor/scripts/'));
      return result
    };
    SyntaxHighlighter.autoloader.apply(null, path(
