                                 var dupDataResPDPS = [];
                                 function pushResPDSRes(arr, pos) {
                                     if (arr) {
                                         for (var i = 0, len = arr.length; i < len; i++) {
                                             if (arr[i]) {
                                                 arr[i].pos = pos;
                                                 dupDataResPDPS.push(arr[i]);
                                             }
                                         }
                                     }
                                 }
