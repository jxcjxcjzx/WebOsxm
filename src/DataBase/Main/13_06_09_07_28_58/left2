                var folderid = '';
                if(data[i].folderid != 0) {
                    folderid = '&f=' + data[i].folderid;
                }
                str += '<ul class="v">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(_this.hzCode.hzCollect, _this.urlVideo + data[i].videoid + '.html') + '" target="video" title="' +vtitle + '"></a></li>';
                str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '" replace="false"></li>';
