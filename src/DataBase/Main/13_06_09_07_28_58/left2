                }
                if(i+1 == shownum && !!data[shownum]) {
                    var uReUrl = 'http://i.youku.com/u/favorite';
                    str += _this.getLastVideo(data[shownum].videoid, uReUrl, _this.hzCode.hzCollectMore,  data[shownum].thumburl, vtitle);
                }
            }
        }
        _this.domCollect.innerHTML = str;
        _this.lockCollect = false;
    },

    initRecordsDom : function(data, start, end) {
        var _this = this;
        var len = data.length;
        var str = '';
        var shownum = 0;
        var hznull = 0;

        if(this.cookieYktk) {
            hznull = this.hzCode.hzRecordsNull;
        } else {
            hznull = this.hzCode.hzAnonymousNull;
        }

        if(len == 0 ) {
            str += '<div class="my-null"><span>暂无观看记录, 去看看<a href="' + _this.returnHzUrl(hznull, 'http://i.youku.com/u/home') + '" target="_blank">更多精彩内容<a></span></div>';
        }      
        else {
            if(len < end) { end = len; }
            for(var i = start; i < end; i++) {
                var hznum = '';
                var hzcode = '';
                var vtitle = _this.removeHTMLTag(data[i].title);
                if(_this.cookieYktk) {
                    hzcode = this.hzCode.hzRecords; 
                    if(!!_this.hzCode.hzRecordsDel) {
                       hznum = _this.hzCode.hzRecordsDel; 
                    }
                } else {
                    hzcode = this.hzCode.hzAnonymousRecords; 
                    if(!!_this.hzCode.hzAnonymousDel) {
                       hznum = _this.hzCode.hzAnonymousDel; 
                    }
                }
                var folderid = '';
                if(data[i].folderid != 0) {
                    folderid = '&f=' + data[i].folderid;
                }
                str += '<ul class="v">';
                str += '<li class="v_link"><a href="' + _this.returnHzUrl(hzcode, _this.urlVideo + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) + '" target="video" title="' +vtitle + '"></a></li>';
                str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" alt="' + vtitle + '" replace="false"></li>';
                str += '<li class="v_ishd">' + data[i].definition + '</li>';                
                str += '<li class="v_menu" style="display: none;"><span class="ico__listadd" title="添加到点播单"></span></li>';
                str += '<li class="v_time"><span class="num">' + _this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>';
                str += '<li class="v_title"><a target="_blank" title="' + vtitle + '" href="' + _this.returnHzUrl(hzcode, _this.urlVideo + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid) + '">' + vtitle + '</a></li>';
                if(parseInt(data[i].percent) === 100) {
                    str += '<li class="v_record"><span>已看完</span></li>';
                } else if(parseInt(data[i].percent) === 0) {
                    str += '<li class="v_record"><span>已看过</span></li>';
                } else {
                    str += '<li class="v_record"><span>已看到' + data[i].percent + '</span></li>'
                }
                str += '<li class="v_close"><a href="javascript:;" onclick="MYYOUKU.deleteList(\'' + _this.TOKEN + '\',\'' + data[i].videoid + '\',\'' + data[i].folderid + '\',\'' + data[i].showid + '\',\'' + data[i].videoid  + '\',\'MYYOUKU-Records\',\'' + hznum + '\');">删除</a></li>';
                str += '</ul>';
                if(_this.cookieYktk) {
                    shownum = _this.SHOWNUMBER;
                    morehznum = _this.hzCode.hzRecordsMore;
                } else {
                    shownum = _this.ANONYMOUSRECOMMENDNUMBER;
                    morehznum = _this.hzCode.hzAnonymousMore;
                }
                if(i+1 == shownum && !!data[shownum]) {
                    var uReUrl = 'http://i.youku.com/u/home?type=watch_timeline';
                    str += _this.getLastVideo(data[shownum].videoid, uReUrl, morehznum,  data[shownum].thumburl, vtitle);
                }
            }
        }
        _this.domRecords.innerHTML = str;
        _this.bindDelete(_this.domRecords);
    },

    initAnonymousRecommendDom : function(data) {
        //this.domCaption.innerHTML = '<h3 class="title title_anonymous"><a href="' + this.urlHz + '4007101&cpp=1000648&url=' + this.urlUser + '" target="_blank">个人中心</a></h3>'; 
        this.initRecommendDom(this.dataAnonymous, this.domRecommend, 0, this.ANONYMOUSRECOMMENDNUMBER);
        //this.initLoginDom();
    },

    // More Conetnt
    changeMore : function(o) {
        var _this = this;
        if(_this.MORECOUNT == 1) {
            o.innerHTML = '更多推荐';
            o.className = 'showmore';
            if(_this.cookieYktk) {
                _this.initRecommendDom(_this.dataRecommend, _this.domRecommend, _this.SHOWNUMBER, _this.SHOWNUMBER*2);
                _this.requestImg(_this.returnHzUrl('4007092'));
            } else {
                _this.initRecommendDom(_this.dataAnonymous, _this.domRecommend, _this.ANONYMOUSRECOMMENDNUMBER, _this.ANONYMOUSRECOMMENDNUMBER*2);
                _this.requestImg(_this.returnHzUrl('4007103'));
            }
            _this.MORECOUNT++;
        } 
        else if(_this.MORECOUNT == 2) {
            if(_this.cookieYktk) {
                o.href= _this.returnHzUrl('4006771', _this.urlUser);
                _this.returnHzUrl('4006771', _this.urlUser);
            } else {
                o.href= _this.returnHzUrl('4007102', _this.urlUser);
                _this.returnHzUrl('4006771', _this.urlUser);
            }
            o.target= '_blank';
        } 
        else {
            if(_this.cookieYktk) {
                _this.CURRENTINDEX = _this.SHOWNUMBER * _this.MORECOUNT;
                _this.initRecommendDom(_this.dataRecommend, _this.domRecommend, _this.CURRENTINDEX, _this.SHOWNUMBER + _this.CURRENTINDEX);
                _this.requestImg(_this.returnHzUrl('4007092'));
            } else {
                _this.CURRENTINDEX = _this.ANONYMOUSRECOMMENDNUMBER * _this.MORECOUNT;
                _this.initRecommendDom(_this.dataAnonymous, _this.domRecommend, _this.CURRENTINDEX, _this.ANONYMOUSRECOMMENDNUMBER + _this.CURRENTINDEX);
                _this.requestImg(_this.returnHzUrl('4007103'));
            }
            _this.MORECOUNT++;
        }
        return false;
    },

    getLastVideo : function(vid, reUrl, lastHz, thumburl, vtitle) {
        var str = ''
        str += '<ul class="p">';
        str += '<li class="p_link"><a title="查看更多" href="' + this.returnHzUrl(lastHz ,reUrl) + '" target="_blank" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>';
        str += '<li class="p_thumb"><img src="' + thumburl + '" title="' + vtitle + '" alt="' + vtitle + '" /></li>';
        str += '<li class="p_status"><span class="status">查看更多</span><span class="bg"></span></li>';
        str += '</ul>';  
        return str;
    },

    showContent: function(height) {
        var _this = this;
        _this.dom.style.display = 'block';
        var slidingTimer = null;
        var b=10, c=height, d=5, t=0;
        function easeOut(t,b,c,d) {
            return -c *(t/=d)*(t-2) + b;
        };
        function Sliding(){
            _this.dom.style.height = easeOut(t,b,c,d) + 'px';
            if(t<d){
                t++;
                slidingTimer = setTimeout(Sliding, 1);
            }
            else{
                window.clearInterval(slidingTimer);
            }
        };
        Sliding();  
    },

    // For IE
    getImages : function(imglistid) {
        var _this = this;
        var doms = [];
        var imgs = [];
        var index= 0;
        for(var i = 0 ; i < imglistid.length; i++) {
            var thumb = document.getElementById(imglistid[i]);
            doms.push(thumb);
            imgs.push(thumb.getAttribute('_src'));
        }
        var timer = setInterval(function(){
            try {
                var img = new Image();
                doms[index].appendChild(img);
                img.src = imgs[index];
                if(index >= _this.SHOWNUMBER) {
                    clearInterval(timer);
                } else {
                    index++;
                }
            } catch(e) {
            }
        },5);
    },

    // Delete Records
    deleteList : function(tk, vid, fid, shid, lild, list, charset) {
        var hznull = '';
        var e = window.event || arguments.callee.caller.arguments[0];
        var obj = e.target || e.srcElement;
        if(!tk || !vid || !lild) return;
        if(fid == undefined) fid = 0;
        if(shid == undefined) shid = 0;
        var delUrl = "http://yus.navi.youku.com/playlog/delete.json?token="+tk+"&v="+vid+"&fid="+fid+"&shid="+shid+"&"+Math.random();
        var listObj = document.getElementById(list);
        var listUL = listObj.getElementsByTagName('UL');
        var itemObj = obj.parentNode.parentNode;
        if(this.cookieYktk) {
            hznull = this.hzCode.hzRecordsNull;
        } else {
            hznull = this.hzCode.hzAnonymousNull;
        }
        if(listUL.length == 1) {
            if(list == 'MYYOUKU-Records') {
                var str = '<div class="my-null"><span>暂无观看记录, 去看看<a href="' + this.returnHzUrl(hznull,'http://i.youku.com/u/home') + '" target="_blank">更多精彩内容<a></span></div>';
            }
            listObj.innerHTML = str; 
        }
        else {
            listObj.removeChild(itemObj);
        }
     
        var img = new Image();
        img.src = delUrl;    
        if(charset) {
            addScript("http://hz.youku.com/red/click.php?tp=1&cp="+charset+"&cpp=1000648");
        };
        function addScript(src){
            var doc = document;
            var g = doc.createElement("script");
            g.type = "text/javascript";
            g.src = src;
            doc.getElementsByTagName('head')[0].appendChild(g);
        }
    },

    // Request Images SRC
    requestImg : function(url) {
        var imgReq = new Image();
        imgReq.src = url;   
        imgReq = null;
    },

    submitVideoCommend : function(o){
        var req = o.parentNode.parentNode.getAttribute('logdata');
        req = decodeURIComponent(req);
        var d = eval('('+req+')');
        if(d) {
            var logstr = 'uid=' + d.uid  + '&cookie_id=' + d.cookie_id + '&apptype=1&page=8&module=' + d.module + '&dvid=' + d.dvid + '&abver=' + d.abver + '&dma=' + d.dma + '&pos=' + d.pos + '&ord=0&dct=0';
            var url = 'http://e.stat.youku.com/personalization/log?';
            var imgReq = new Image();
            imgReq.src = url + logstr;  
            imgReq = null;
        }   
    },

    // Get Random String
    randomStr : function(l)  {
        var x="0123456789qwertyuioplkjhgfdsazxcvbnm";
        var tmp="";
        var timestamp = new Date().getTime();
        for(var i=0;i<l;i++)  {
            tmp += x.charAt(Math.ceil(Math.random()*100000000)%x.length);
        }
        return timestamp+tmp;
    },

    // Format Number
    formatnum : function(n){
        n  =  n+''; 
        if(n.indexOf(',')>0){n = n.replace(/,/gi,'') + '';};  
        var re=/(-?\d+)(\d{3})/;
        while(re.test(n)){n = n.replace(re,'$1,$2')}
        return n;  
    },

    GetChineseCount : function(str){ 
        var chinieseCount=0;
        var badChar ="ABCDEFGHIJKLMNOPQRSTUVWXYZ"; 
        badChar += "abcdefghijklmnopqrstuvwxyz"; 
        badChar += "0123456789"; 
        badChar += " "+"　";//半角与全角空格 
        badChar += "`~!@#$%^&()-_=+]\\|:;\"\\'<,>?/";//不包含*或.的英文符号 
        if(""==str){ 
            return false; 
        } 
        for(var i=0;i<=str.length;i++)
        { 
            var c = str.charAt(i);//字符串str中的字符 
            if(badChar.indexOf(c) > -1){ 
            }
            else{
                chinieseCount++;
            } 
        } 
        return chinieseCount;
    },

    // Format Time Second to Minute
    secondTofMinute : function(t) {
        var m = parseInt(t/60); 
        var s = parseInt(t%60); 
        if(m < 10) {m = '0' + m;} else {m = m.toString();}
        if(s < 10) {s = '0' + s;} else {s = s.toString();}
        return m + ':' +s;
    },

    //Short Date 
	shortDate: function(tp) {
        var str = tp;
        var new_tp = str.replace(/:/g,'-');
        new_tp = new_tp.replace(/ /g,'-');
        var arr = new_tp.split('-');
        var datum = new Date(Date.UTC(arr[0],arr[1]-1,arr[2],arr[3]-8,arr[4],arr[5]));
        tp = datum.getTime()/1000;
		var d, now = new Date(), delta;
		tp = parseInt(tp);
		if(!tp || isNaN(tp)) return '';
		d = new Date(tp*1000);
		delta = Math.round((now - d) / 1000);
		
		var yyyy = d.getFullYear();
		var mm = d.getMonth() + 1; 		 mm = this.fillzero(mm);  
		var dd = d.getDate();			 dd = this.fillzero(dd);
		var HH = d.getHours();			 HH = this.fillzero(HH);
		var MM = d.getMinutes();		 MM = this.fillzero(MM);
		
		var t0 = new Date(now.getFullYear(), now.getMonth(), now.getDate())/1000;
		var t1 = d.getTime()/1000;
		
		//1分钟内
		if(delta < 60){ return '刚刚'; } 
		//1小时内
		else if(delta < 3600){ return Math.floor(delta/60) +'分钟前'; }
		//当天内
		else if(t1 > t0){ return  Math.floor(delta/3600)+'小时前'; }
		//昨天
		else if(t0 - t1 < 24*3600){ return '昨天 ' + HH + ':' + MM; }
		//前天
		else if(t0 - t1 < 48*3600){ return '前天 ' + HH + ':' + MM; }
		//一周内
		else if(delta < 7*24*3600){ return Math.floor(delta/(24*3600)) + '天前';
		}else{
			//本年内 || 去年但是当前是1月份
			if(yyyy == now.getFullYear() || (now.getFullYear() - yyyy == 1 && now.getMonth()==0)){ return mm + '-' + dd + ' ' + HH + ':' + MM;	 }
			//全年但当前是2月份及以后
			else{ return yyyy + '-' + mm + '-' + dd; }
		}
		return delta;
	},

	fillzero: function(n){
		if(n<10){ return '0' + n; }
		return '' + n;
	},

    // Login
    mylogin : function (hzcode) {
        MYYOUKU.requestImg(MYYOUKU.returnHzUrl(hzcode));
        login(MYYOUKU.refresh);
    },

    refresh : function () {
        MYYOUKU.requestImg(MYYOUKU.returnHzUrl('4007350'));
        window.location.reload();
    },

    // Filter HTML Tag
    removeHTMLTag : function (str) {
        return str.replace(/</g,"&lt;").replace(/>/g,"&gt;").replace(/"/g,"&quot;");
    },

    // Get Json Data
    getJson : function(url){
        var _this = this;
        var _script = document.createElement("script");
        _script.type = "text/javascript";
        _script.src = url + _this.randomStr(10);
        if (!/*@cc_on!@*/0) {
            _script.onload = _this.callBackScript;
        } else {
            _script.onreadystatechange = function () { 
                if (_script.readyState == 'loaded' || _script.readyState == 'complete') { 
                    _this.callBackScript();
                } 
            }
        }
        document.getElementsByTagName("head")[0].appendChild(_script);
    },

    callBackScript : function(){
        if(this.parentNode){
            this.parentNode.removeChild(this);
        }
    }
}


window.nova_init_hook_myyouku = function() {
    MYYOUKU.init();
}


</script>
</head>

