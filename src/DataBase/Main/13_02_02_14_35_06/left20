	var speed=30;
	document.getElementById("demo2").innerHTML=document.getElementById("demo1").innerHTML;
	function Marquee(){  
 		if(document.getElementById("demo1").offsetHeight-document.getElementById("demo").scrollTop<=0) {  
			document.getElementById("demo").scrollTop = 0;  
 		} else {  
  		document.getElementById("demo").scrollTop++  
 		}  
	}  
	var MyMar=setInterval(Marquee,speed);
	document.getElementById("demo").onmouseover=function() {clearInterval(MyMar);MyMar=null;}  
	document.getElementById("demo").onmouseout=function() {if(MyMar == null)MyMar=setInterval(Marquee,speed);} 
</script>

