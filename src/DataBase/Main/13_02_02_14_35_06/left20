  <link href="http://file.caing.com/images/channel/calendar/css/datePicker.css" type="text/css" rel="stylesheet">
  <script>
				function searchData(){
					   var date=document.getElementById("date1").value;
						document.location.href='http://data.caixin.com/calendar/?date='+date;
				}
				  var date = new Date(); //日期对象
				var now = "";
				now = date.getFullYear()+"-"; // 
				now = now + ((date.getMonth()+1)<10?"0":"");
				now = now + (date.getMonth()+1)+"-";//取月的时候取的是当前月-1如果想取当前月+1就可以了
				now = now + ((date.getDate()+1)<10?"0":"");
				now = now + date.getDate();
				 
				document.getElementById("date1").value=now;
				</script>
