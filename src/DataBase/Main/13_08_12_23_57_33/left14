<script type="text/javascript" src="http://r1.ykimg.com/0510000051D122C16714C0494B0C452F?2405984.js"></script>
<script>
setTimeout(function() {
    var $slideIndex = $$('.yk-slide-index');
    //console.log($slideIndex.length);
    $slideIndex.each(function(el,i){
        //console.log(el,i);
        //console.log(i);
        var $el         = $(el);
        var $pages      = $el.select('.yk-slide-pages')[0];
        if ($pages.children.length && typeof $pages.children[0] != "undefined"){
        	var mname = $pages.children[0].getAttribute('name');
        	if (typeof mname != "undefined" && mname == 'm_pos'){
        		$pages = $pages.children[0];
        	}
        }
        // var $prev       = $el.select('.yk-slide-btnprev')[0];
        var $next       = $el.select('.yk-slide-btnnext')[0];
        var pages       = $pages.children.length;
        $pages.cur = 0;
        if(!$pages.ani){
			$pages.ani = new Ani($pages,{
				amount:1,
				time: 1500,
				autoStart:false,
				onstart:function(el){
					this.left_start = parseInt(el.getStyle('left'));
					if (isNaN(this.left_start)){
						this.left_start = 0;
					}
					this.left_dis = this.amount * 330;
					this.left_end = this.left_start + this.left_dis;
				},
				onevery:function(el, percent){
					 //console.log(this.left_start 
					 //		, this._valueFn(percent) , this.left_dis,  "px")
					el.setStyle({
						left: this.left_start 
							+ this._valueFn(percent) * this.left_dis + "px"
					});
				},
				onstop:function(el){
					el.setStyle({
						left: this.left_end + "px"
					});
					//console.log("onstop");
					var $outLeft = $(el.children[0]);
					
					$outLeft.remove();
					
					$pages.insert($outLeft);
					el.setStyle({
						left:0
					})
				}
			});
		};
	var timer;
	function next(){
		clearTimeout(timer);
		if($pages.ani.running){
			return;
		}
		$pages.cur++;
		if($pages.cur >= pages - 1 ){
			$pages.cur = pages -1;
		}
		$pages.ani.amount = -1;
		$pages.ani.start();
		timer = setTimeout(next,8500);
	}
	timer = setTimeout(next,8500);
	$next.observe('click',next);
  });
}, 8000);


(function(){

var userAgent = window.navigator.userAgent;
	if(userAgent.match(/Android/i) || userAgent.indexOf('iPod') !== -1 || userAgent.indexOf('iPad') !== -1 || userAgent.indexOf('iPhone') !== -1){
	    document.getElementById('m_87594').innerHTML = '<a style="display:block;width:640px;height:110px;margin:0 auto;" href="http://hz.youku.com/red/click.php?tp=1&cp=4008714&cpp=1000749&url=http://m.youku.com/webapp/dl?app=youku&source=webqr" target="_blank"><img src="http://res.mfs.ykimg.com/0510000051C931FE6714C049470E4C25" width="640" height="110" /></a>';
	    document.getElementById('m_86823').innerHTML = '<a style="display:block;width:320px;height:240px;margin:0 auto;" href="http://hz.youku.com/red/click.php?tp=1&amp;cp=4008713&amp;cpp=1000749&amp;url=http://m.youku.com/webapp/dl?app=youku&amp;source=webqr" target="_blank"><img src="http://res.mfs.ykimg.com/0510000051C933E86714C049AD014E6A" width="320" height="240"></a>';
	
