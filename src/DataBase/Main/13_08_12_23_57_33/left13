                        <div class="mod-42 mod-13 mod-02">
                            <div class="tit02 clearfix">
                                <h3><a href="http://ke.edu.sina.com.cn/" target="_blank">新浪微课堂</a></h3>
                            </div>
                            <div class="mod42-cont" data-sudaclick="blk_edu_wkt" blkclick="auto_nav" blktitle="微课堂">
<!-- publish_helper name='新浪微课堂' p_id='30' t_id='111' d_id='3' -->
