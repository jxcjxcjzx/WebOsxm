	Pos=0;
	PosSize=316;
	$(document).ready(function(){
		$("#GoPrev").click(function(){
			if(Pos<0){
				Pos+=PosSize
				$("#hdi").animate({top:Pos+"px"},"slow");	
			}
		});
		$("#GoNext").click(function(){
			if(Pos>-632){
				Pos-=PosSize
				$("#hdi").animate({top:Pos+"px"},"slow");	
			}
		});
	});
