		}
		else{
			if(frame){
				UI.remove(frame);
				if(D){
					UI.remove(D);
				}
				frame = D = null;
			}
			openQuickLogin = 1;
			setTimeout(function(){
				MI.dialog.show({
					title:'<h3 class="DmainTit">微博登录</h3>',
					end:function(){
						openQuickLogin = 0;
						UI.cookie('reg_quick',1,0.42);					}
					,html:'<iframe src="http://mini.t.qq.com/mblogin_quick.htm" frameborder="0" scrolling="no" width="370" height="326" id="QuickLoginFrame"></iframe>',
					width : 441				});
			},500);
		}
	}
	MI.Bos('btnPortalQuickLogin',from);
}
function mb_quick_reg_call(isReg){
	MI.dialog.hide();
	if (isReg){
        document.location.reload();
	}
	else {
        document.location.href = 'http://t.qq.com/regfriend.php?reg';
	}
}
if(!MI.user.account){
	MI.app({
		Base:function(){
			                if(UI.cookie('mb_reg_quick') == 1){
                    return;
                }if (!UI.cookie('reg_quick')){
                            mb_quick_reg(document.body,'');
                          }                    //cookie:mb_reg_quick弹出之后1小时内不再弹出
                    UI.cookie('mb_reg_quick', 1, 1/24);		}
	});
}
