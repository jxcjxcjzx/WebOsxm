<script type="text/javascript" language="javascript">
function changelocationnew() {
	var listvalue;	
	var listt;
	listvalue=document.forms["iask_f_new"].stype;
	listt=listvalue.options[listvalue.selectedIndex].value;
	if(listt=="circle")
	{
		document.forms["iask_f_new"].ts.value = "bgroup";	
	}
	else if(listt=="bauthor")
	{
		document.forms["iask_f_new"].ts.value = "bauthor";
	}
	else
	{
		document.forms["iask_f_new"].ts.value = "bpost";
	}
}

function iask(fn,strName) {
	if(fn.k.value=="" || fn.k.value=="请输入查询词")
	{
		fn.k.value="请输入查询词";fn.k.focus();
		return false;
	}
	if(strName == "blog")
	{
		/*
		if(document.forms["iask_f_new"].t.selectedIndex==3)
		{
			fn.action="http://uni.sina.com.cn/c.php";
		} 
		else 
		{
			if(document.forms["iask_f_new"].t.selectedIndex==2)
			{
				fn.action="http://uni.sina.com.cn/c.php";
			} 
			else
			{
				fn.action="http://uni.sina.com.cn/c.php";
			}
		}
		*/
	} else {
		return false;
	}
	fn.submit();
	return false;
}
