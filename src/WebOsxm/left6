<script type="text/javascript" language="javascript">
// <![CDATA[
var mobileDetect = function() {
	var agent = navigator.userAgent.toLowerCase(),
	url = window.location.toString(),
	cookie = document.cookie.toString();

	redirect = (url.match(/health\//)) ? url.replace(/health\//, "health/m/") : url.replace(/health/, "health/m/");
	//alert("User agent is -" + agent + "-\nRedirect Url is -" + redirect + "\nyour cookie is -" + cookie );
	if (cookie.match(/TI_PREFS=default/) == null && cookie.match(/TI_PREFS=tablet/) == null) {
		if (url.match(/nomobile=1/)) {
			//alert("user preferred to use full site");
			document.cookie = "TI_PREFS=default";
		}

		else if (cookie.match(/TI_PREFS=phone/) !== null) {
			//alert("Mobile device confirmed redirect to mobile site");
			window.location = redirect;
		} 

		else if (agent.match(/iphone/i)  || agent.match(/ipod/i)  || agent.match(/mobile/i) || agent.match(/blackberry/i)) {
			//alert("Case if cookie is not set as phone but user agent detected as mobile device redirecting to mobile site");
			document.cookie = "TI_PREFS=phone";
			window.location = redirect;
		} else {
			//alert("not a mobile device use full site");
			document.cookie = "TI_PREFS=default";
		}
	}
}();
// ]]>  
</script>
		
