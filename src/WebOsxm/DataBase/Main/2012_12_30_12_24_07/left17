<script>

/* 图片列表滚动*/
var $ = jQuery;
var currr = 0;
function t2(){



	$(".right_but").click(function(){
								   
				
			if(currr<2){
				currr += 1;
				for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
			}
			else if(currr>=2)
				{   currr=2;
					return false;}
						   });
	
$(".left_but").click(function(){
	

			if(currr>0)
				{ 
					currr -= 1;
					for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
					}
				else if(currr<=0)
				{   currr==0;
					return false;}
								   
	 });


$(".fy a").click(function(){

			var index = $(this).index();
			currr =index;
			 for(i=0;i<3;i++)
					{
						 $(".fy a").eq(i).attr("id"," ");
						  $(".fy a").eq(currr).attr("id","cur");
						  ul_left = -currr*705;
						  $("#js_hd ul").animate({left:ul_left+"px"});
						}
			
				 });

	}
t2();


</script>
<script>

//首屏切换
function groupList(obj){
var jq = jQuery;
var obj = jq('#' + obj);
var lis = obj.find('li');
var h3s = obj.find('h3');
var len = lis.length;
h3s.mouseover(function(){
lis.each(function(key, obj){
jq(this).find('.image').css('display', 'none');
jq(this).removeClass('cur');
});
jq(this).parent().addClass('cur');
jq(this).parent().find('.image').css('display', 'block');
realSrc=$(this).parent().find(".image").find("img").attr("name");
curSrc=$(this).parent().find(".image").find("img").attr("src");
if(curSrc!=realSrc){
$(this).parent().find(".image").find("img").attr("src",realSrc);
}
});
}
groupList('group_list');

</script>

