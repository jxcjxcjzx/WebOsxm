  callback : function(){
    jsLoader({
      name : 'headwww',
      callback : function(){
        if (remote_ip_info.ret==1){
          switch (remote_ip_info.province){
            case '上海' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/0905/17.js"});
              break;
            case '四川' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/1010/22.js"});
              break;
            case '河南' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/1010/21.js"});
              break;
            case '云南' : jsLoader({url:"http://www.sina.com.cn/js/67/2010/0731/12.js"});
              break;
            case '广东' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/1010/20.js"});
        /*
              if (remote_ip_info.city=='广州' || remote_ip_info.desc=='Guangzhou')  {
                jsLoader({url:"http://www.sina.com.cn/js/67/2009/0701/5.js"});
              }
        */
              break;

            case '福建' :
              if (remote_ip_info.city=='厦门' || remote_ip_info.desc=='Xiamen' || remote_ip_info.city=='漳州' || remote_ip_info.city=='泉州') {
                jsLoader({url:"http://www.sina.com.cn/js/67/2012/0112/28.js"});
              }

                          if (remote_ip_info.city!='厦门' && remote_ip_info.desc!='Xiamen' && remote_ip_info.city!='漳州' && remote_ip_info.city!='泉州') {
                jsLoader({url:"http://www.sina.com.cn/js/67/2011/1010/19.js"});
              }

              break;
            case '浙江' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/1010/18.js"});
              break;
            case '重庆' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/1019/23.js"});
              break;
            case '湖南' : jsLoader({url:"http://www.sina.com.cn/js/67/2011/1118/24.js"});
