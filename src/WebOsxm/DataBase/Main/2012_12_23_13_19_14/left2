                var slide_01=new ScrollPic();
                slide_01.id='slide';//容器ID为必选参数
                slide_01.prevId='btn_prev';//按钮ID (上一个)
                slide_01.nextId='btn_next';//按钮ID (下一个)
                //以下皆是可选参数
                slide_01.moveNum=1;//每次滚动几个单元格 [默认1个]
                slide_01.isLoop=true;//无缝循环滚动 [默认是]
            //	slide_01.speed = 20;//速度 越小越快 [默认20]
                slide_01.autoPlay=true;
                slide_01.playTime = 5000;//自动播放时间间隔 [默认5秒]
                slide_01.dotId='slide_dot';//点列表容器ID [默认没点]
                slide_01.dotEvent='mouseover';//点切换事件 [默认click]
                //slide_01.dotType='number';//点类型 可以是数字或HTML代码 [默认空]
                slide_01.dotClass='dot_list';//点className [默认dot_list]
                slide_01.dotClassOn = 'current';//当前点 className [默认current]
                slide_01.init();
