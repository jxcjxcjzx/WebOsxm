var MYYOUKU = {
	MAXHEIGHT : 310,
	ANONYMOUSHEIGHT : 200,
	MAXNUMBER : 3,
	RECOMMENDCHANCE : 4,
	recommendData : [], //Recmmend Videos
	playListData : [],//Playlist Videos
	showListData : [], 
	payListData : [], 
	anonymousData : [],
	deleteList : [],
	getRecJson : 'http://nc.youku.com/index_cookielist/s/jsonp?callback=MYYOUKU.getRecordsData&timestamp=',
	getComJson : 'http://nc.youku.com/index_videocommend?callback=MYYOUKU.getRecommendData&pz=30&timestamp=',
	getPayJson : 'http://nc.youku.com/index/userpaylist?pn=1&pz=10&callback=MYYOUKU.getPayData&timestamp=',
	getAnonymousComJson : 'http://nc.youku.com/index_videocommend?callback=MYYOUKU.getAnonymousRecommendData&pz=30&timestamp=',
	getReommendDataNumber : 0,
	recommendNumber : 7,//recommended number	
	anonymousRecommendNumber : 4,
	payNumber : 5,
	playNumber : 4,
	showNumber : 4,
	currentStat : 0,
	moreCount : 1,
	ysuid : Nova.Cookie.get('__ysuid'),
	uid : Nova.Cookie.get('_l_lgi'),
	yktk : Nova.Cookie.get('yktk') == null?0:1,	
	token : 0,
	dom : null,
	recommendDom : null,
	payDom : null,
	showDom : null,
	playDom : null,
	showMore : null,
	tabsDOM : null,

	hzLinks : {
		hzShowDelList : '4006790',
		hzPlayDelList : '4006807',
		hzRecommendThumbList : ['4006772' ,'4006774' ,'4006776' ,'4006778' ,'4006780' ,'4006782' ,'4006784' ,'4006786' ,'4006786'],
		hzRecommendTitleList : ['4006773' ,'4006775' ,'4006777' ,'4006779' ,'4006781' ,'4006783' ,'4006785' ,'4006787' ,'4006789'],
		hzPayTitleList : ['4006823' ,'4006824' ,'4006825' ,'4006826' ,'4006827' ,'4006828' ,'4006829' ,'4006830'],
		hzShowTitList : ['4006789' ,'4006793' ,'4006797' ,'4006801'],
		hzShowGoList : ['4006791' ,'4006795' ,'4006799' ,'4006803'],
		hzShowNextList : ['4006792' ,'4006796' ,'4006800' ,'4006804'],
		hzPlayTitList : ['4006806' ,'4006810' ,'4006814' ,'4006818'],
		hzPlayGoList : ['4006808' ,'4006812' ,'4006816' ,'4006820'],
		hzPlayNextList : ['4006809' ,'4006813' ,'4006817' ,'4006821'],
		hzAnonymousThumbList : ['4007104' ,'4007106' ,'4007108' ,'4007110'],
		hzAnonymousTitleList : ['4007105' ,'4007107' ,'4007109' ,'4007111'],

		returnHzUrl : function (index,url) {
			if(url == undefined) { url = ''; };
			return 'http://hz.youku.com/red/click.php?tp=1&cp=' + index + '&cpp=1000648&url=' + url; 
		}
	},

	init : function() {
		this.dom = document.getElementById('MYYOUKU');
		this.showMore = document.getElementById('MYYOUKU-ShowMore');
		this.recommendDom = document.getElementById('MYYOUKU-Recommend');
		if(this.yktk) {
			this.payDom = document.getElementById('MYYOUKU-Pay');
			this.showDom = document.getElementById('MYYOUKU-Show');
			this.playDom = document.getElementById('MYYOUKU-Play');
			this.dom.style.height = this.MAXHEIGHT;
			this.getJson(this.getRecJson);
			this.getJson(this.getComJson);
			this.getJson(this.getPayJson);
			this.bind();
		} else {
			if(this.ysuid.substring(12,13) < this.RECOMMENDCHANCE) {
                                this.moreCount = 2;
				this.tabsDOM = document.getElementById('MYYOUKU_tabs');
				this.dom.style.height = this.ANONYMOUSHEIGHT;
				this.getJson(this.getAnonymousComJson);
				this.bind();
			}

		}
	},

	bind : function() {
		var _this = this;
		this.showMore.onclick = function() {
			_this.changeMore(this);
		}
	},

	changeMore : function(o) {
		if(this.moreCount == 1) {
			o.innerHTML = '更多推荐';
			o.className = 'showmore';
			if(this.yktk) {
				this.recommendShow(this.recommendData,this.recommendNumber*2,this.recommendNumber+this.recommendNumber*2);
			} else {
				this.recommendShow(this.anonymousData,this.anonymousRecommendNumber*2,this.anonymousRecommendNumber+this.anonymousRecommendNumber*2);
			}
			this.moreCount--;
			if(this.yktk) {
				this.requestImg(this.hzLinks.returnHzUrl('4007409'));
			} else {
				this.requestImg(this.hzLinks.returnHzUrl('4007408'));
			}
		} else if(this.moreCount == 0) {
			if(this.yktk) {
				o.href= this.hzLinks.returnHzUrl('4006771','http://i.youku.com/u/home');
			} else {
				o.href= this.hzLinks.returnHzUrl('4007102','http://i.youku.com/u/home');
			}
			o.target= '_blank';
		} else {
			if(this.yktk) {
				this.recommendShow(this.recommendData,this.recommendNumber,this.recommendNumber*2);
			} else {
				this.recommendShow(this.anonymousData,this.anonymousRecommendNumber,this.anonymousRecommendNumber*2);
			}
			this.moreCount--;
			if(this.yktk) {
				this.requestImg(this.hzLinks.returnHzUrl('4007092'));
			} else {
				this.requestImg(this.hzLinks.returnHzUrl('4007103'));
			}
		}
		PlayList.HoverVP.init("MYYOUKU");
		return false;
	},

	requestImg : function(url) {
		var imgReq = new Image();
		imgReq.src = url;	
		imgReq = null;
	},

	initRecommendDom : function(data) {
		var len = total = data.length;
		var num = this.recommendNumber;
		var pages = parseInt(total/num);
		this.recommendShow(data, 0 , num);
	},

	initAnonymousRecommendDom : function(data) {
		var len = total = data.length;
		var num = this.anonymousRecommendNumber;
		var pages = parseInt(total/num);
		this.recommendShow(data, 0 , num);
		this.initLoginDom();
	},

	initLoginDom : function() {
		var str = '';
		str += '<div class="my-login">';
		str += '<h6>登录优酷, 收看更多精彩推荐</h6>'
		str += '<div class="my-login-area">';
		str += '<a id="MYYOUKU-LoginBtn" href="javascript:;" onclick="MYYOUKU.mylogin();return false;"></a>';
		str += '<span>没有帐号, <a href="http://hz.youku.com/red/click.php?tp=1&cp=4007113&cpp=1000648&url=http://www.youku.com/user_signup/" target="_blank">立即注册</a></span>';
		str += '</div>';
		str += '</div>';
		this.tabsDOM.innerHTML = str;
	},

	recommendShow : function(data, start, end) {
		var _this = this;
		var str = '';
		var imglistid = [];
		var imglistsrc = [];
		var hzcount = 0;
		var isIE6 = document.all;
		var hzThumbType = '';
		var hzTitleType = '';
		var lastThumbType = '';
		for(var i = start ; i < end ; i++) {
			var tModule = 1;
			if(_this.yktk) {
				hzThumbType = _this.hzLinks.hzRecommendThumbList[hzcount];
				hzTitleType = _this.hzLinks.hzRecommendTitleList[hzcount];	
				lastThumbType = _this.hzLinks.hzRecommendThumbList[7];	
			} else {
				hzThumbType = _this.hzLinks.hzAnonymousThumbList[hzcount];	
				hzTitleType = _this.hzLinks.hzAnonymousTitleList[hzcount];	
				tModule = 3
				_this.uid = '';
			}
			var logdatastr = '{"uid":"' + _this.uid  + '","cookie_id":"' + _this.ysuid + '","apptype":"1","page":"8","module":"' + tModule + '","dvid":"' + data[i].id + '","abver":"' + data[i].ver + '","dma":"' + data[i].dma + '","pos":"' + i + '","ord":"0","dct":"0"}';
			logdatastr= encodeURIComponent(logdatastr)
			str += '<ul class="v" logdata="' + logdatastr + '">';
			str += '<li class="v_link"><a href="' + this.hzLinks.returnHzUrl(hzThumbType,data[i].videolink)+ '" target="video" title="' + data[i].title + '" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>';	
			if(isIE6){
				imglistid.push(data[i].videoid);
				str += '<li class="v_thumb" _src="' + data[i].thumburl + '" id="' + data[i].videoid + '"></li>';
			} else {
				str += '<li class="v_thumb"><img src="' + data[i].thumburl + '" title="' + data[i].title + '" alt="' + data[i].title + '" /></li>';
			}
			if(data[i].ishd) {
				str += '<li class="v_ishd"><span class="ico__HD" title="高清"></span></li>';
			}
			else if(data[i].issd) {
				str += '<li class="v_ishd"><span class="ico__SD" title="超清"></span></li>';
			}
			str += '<li class="v_menu" id="PlayListFlag_' + data[i].videoid + '" style="display: none; "><span class="ico__listadd" title="添加到点播单"></span></li>';
			str += '<li class="v_time"><span class="num">' + this.secondTofMinute(data[i].seconds) + '</span><span class="bg"></span></li>'
			str += '<li class="v_title"><a href="' + this.hzLinks.returnHzUrl(hzTitleType,data[i].videolink) + '" target="video" title="' + data[i].title + '" onclick="MYYOUKU.submitVideoCommend(this)">' + data[i].title + '</a></li>';
			str += '<li class="v_stat"><span class="ico__statplay" title="播放"></span><span class="num">' + this.formatnum(data[i].total_vv) + '</span> <span title="评论" class="ico__statcomment"></span><span class="num">' + this.formatnum(data[i].total_comment) + '</span></li>';
			str += '</ul>';
			if(_this.yktk) {			
				if(i+1 == end) {
					str += '<ul class="p">';
					str += '<li class="p_link"><a title="查看更多推荐" href="' + this.hzLinks.returnHzUrl(lastThumbType,"http://i.youku.com/u/home") + '" target="_blank" onclick="MYYOUKU.submitVideoCommend(this)"></a></li>';
					if(isIE6){
						str += '<li class="p_thumb" id="' + data[i+1].videoid + '" _src="' + data[i+1].thumburl + '"><img alt="查看更多推荐"></li>';
						imglistid.push(data[i+1].videoid);
					} else {
						str += '<li class="p_thumb"><img src="' + data[i+1].thumburl + '" title="' + data[i+1].title + '" alt="' + data[i+1].title + '" /></li>';
					}
					str += '<li class="p_status"><span class="status">查看更多推荐</span><span class="bg"></span></li>';
					//str += '<li class="p_ishd"><span class="ico__SD" title="超清"></span></li>';
					str += '</ul>';
				}
			}
			hzcount++;
		} 
		this.recommendDom.innerHTML = str;
		if(isIE6){
			this.getImages(imglistid);
		}
	},

	getImages : function(imglistid) {
		var doms = [];
		var imgs = [];
		var index= 0;
		for(var i = 0 ; i < imglistid.length; i++) {
			var thumb = document.getElementById(imglistid[i]);
			doms.push(thumb);
			imgs.push(thumb.getAttribute('_src'));
		}
		var timer = setInterval(function(){
			try {
				var img = new Image();
				doms[index].appendChild(img);
				img.src = imgs[index];
				if(index >= 7) {
					clearInterval(timer);
				} else {
					index++;
				
				}
			} catch(e) {

			}
		},5);
	},

	initPayDom : function(data) {
		var len = total = data.length;
		var num = this.payNumber - 1;
		var pages = Math.ceil(total/num);
		this.payShow(data, 0 , num);
	},	

	payShow : function(data, start, end) {
		var len = data.length;
		var str = '';
		if(len == 0 ) {
			str = '<div class="recordnull"><span>暂无即将到期的影片</span><span>查看 <a href="http://hz.youku.com/red/click.php?tp=1&cp=4006831&cpp=1000648&url=http://pay.youku.com/trade/index.html" target="_blank">消费记录</a></span></div>';
		}
		else{
			for(var i = 0; i < len; i++) {
				var hzTitle;
				if( i < 4) {
					hzTitle = this.hzLinks.returnHzUrl(this.hzLinks.hzPayTitleList[i]);
				} else {
					hzTitle = '';
				}
				str += '<ul class="payrecord clearfix">';	
				str += '<li><label>' + data[i].category + '</label>';
				str += '<a title="' + data[i].title + '" target="_blank" href="' + hzTitle +data[i].videourl + '">' + data[i].title+ '</a>';
				str += '<span>' + data[i].remain + '</span>';
				str += '</ul>';
			}
		}
		this.payDom.innerHTML = str;
		this.bindDelete(this.payDom);
	},

	initShowlistDom : function(data) {
		var len = total = data.length;
		var num = this.showNumber - 1;
		var pages = Math.ceil(total/num);
		this.showListShow(data, 0 , num);
	},

	showListShow : function(data, start, end) {
		var _this = this;
		var len = data.length;
		var str = '';
		if(len == 0) {
			str = '<div class="recordnull" id="showlist" style="overflow-y:hidden;"><span>您最近没有观看过剧集节目</span><a href="http://tv.youku.com" target="_blank">更多精彩剧集</a></div>';
		}
		else{
			for(var i = 0; i < len; i++) {
				var hzTitle = '';
				var hzGo = '';
				var hzNext = '';
				if( i < 4) {
					hzTitle = this.hzLinks.returnHzUrl(this.hzLinks.hzShowTitList[i]);
					hzGo = this.hzLinks.returnHzUrl(this.hzLinks.hzShowGoList[i]);
					hzNext = this.hzLinks.returnHzUrl(this.hzLinks.hzShowNextList[i]);
				}
				str += '<ul class="playrecord clearfix" itemindex="' + i + '">';	
				str += '<li class="rec_title"><a target="_blank" title="' + data[i].title + '" href="' + hzTitle + 'http://v.youku.com/v_show/id_' + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + '">' + data[i].title +'</a></li>';
				if(data[i].percent === "100%") {
					str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看完</span></li>'
				}
				else if(data[i].percent === " 0%") {
					str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看过</span></li>'
					str += '<li class="rec_continue"><a target="_blank" href="' + hzGo + 'http://v.youku.com/v_show/id_' + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + '">继续看</a>';
					if(data[i].nextid != 0) {
						str += '<span>|</span><a target="_blank" href="' + hzNext + 'http://v.youku.com/v_nextstage/id_' + data[i].showid + '.html?p=' + data[i].stage + '&v=' + data[i].pk_video + '">下一集</a>'
					}
					str += '</li>';
				}
				else {
					str += '<li class="rec_record"><span>在' + data[i].devicename + '上看到 ' + data[i].percent + '</span></li>'
					str += '<li class="rec_continue"><a target="_blank" href="' + hzGo + 'http://v.youku.com/v_show/id_' + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + '">继续看</a>';
					if(data[i].nextid != 0) {
						str += '<span>|</span><a target="_blank" href="' + hzNext + 'http://v.youku.com/v_nextstage/id_' + data[i].showid + '.html?p=' + data[i].stage + '&v=' + data[i].pk_video + '">下一集</a>'
					}
					str += '</li>';
				}
				str += '<li class="rec_close"><a href="javascript:;" onclick="MYYOUKU.deleteList(\'' + _this.token + '\',\'' + data[i].videoid + '\',\'' + data[i].folderid + '\',\'' + data[i].showid + '\',\'' + data[i].videoid  + '\',\'MYYOUKU-Show\',\'' + _this.hzLinks.hzShowDelList + '\');"><em title="删除"></em></a></li>';
				str += '</ul>';
			}
		}
		this.showDom.innerHTML = str;
		this.bindDelete(this.showDom);
	},

	initPlaylistDom : function(data) {
		var len = total = data.length;
		var num = this.playNumber - 1;
		var pages = Math.ceil(total/num);
		this.playListShow(data, 0 , num);
	},

	playListShow : function(data, start, end) {
		var _this = this;
		var len = data.length;
		var str = '';
		if(len == 0 ) {
			str = '<div class="recordnull" id="watchlist"><span>没有播放记录</span></div>';
		}
		else{
			for(var i = 0; i < len; i++) {
				var hzTitle = '';
				var hzGo = '';
				var hzNext = '';
				var folderid = '';
				if(data[i].folderid != 0) {
					folderid = '&f=' + data[i].folderid;
				}
				if( i < 4) {
					hzTitle = this.hzLinks.returnHzUrl(this.hzLinks.hzPlayTitList[i]);
					hzGo = this.hzLinks.returnHzUrl(this.hzLinks.hzPlayGoList[i]);
					hzNext = this.hzLinks.returnHzUrl(this.hzLinks.hzPlayNextList[i]);
				}
				str += '<ul class="playrecord clearfix" itemindex="' + i + '">';	
				str += '<li class="rec_title"><a target="_blank" title="' + data[i].title + '" href="' + hzTitle + 'http://v.youku.com/v_show/id_' + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid +'">' + data[i].title + '</a></li>';
				if(data[i].percent === "100%") {
					str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看完</span></li>'
				}
				else if(data[i].percent === " 0%") {
					str += '<li class="rec_record"><span>已在' + data[i].devicename + '上看过</span></li>'
					str += '<li class="rec_continue"><a target="_blank" href="' + hzGo + 'http://v.youku.com/v_show/id_' + data[i].vidEncoded + '.html?firsttime=' + data[i].sec + folderid +'">继续看</a>';
					if(data[i].nextid && data[i].next !== 0) {
						str += '<span>|</span><a target="_blank" href="' + hzNext + 'http://v.youku.com/v_nextstage/id_' + data[i].showid + '.html?p=' + data[i].stage + '&v=' + data[i].pk_video + '">下一集</a>'
					}
				}
				else {
					str += '<li class="rec_record"><span>在' + data[i].devicename + '上看到 ' + data[i].percent + '</span></li>'
					str += '<li class="rec_continue"><a target="_blank" href="' + hzGo + 'http://v.youku.com/v_show/id_' + data[i].vidEncoded + '.html?firsttime=' + data[i].sec +  folderid +'">继续看</a>';
					if(data[i].nextid && data[i].next !== 0) {
						str += '<span>|</span><a target="_blank" href="' + hzNext + 'http://v.youku.com/v_nextstage/id_' + data[i].showid + '.html?p=' + data[i].stage + '&v=' + data[i].pk_video + '">下一集</a>'
					}
					str += '</li>';
				}
				str += '<li class="rec_close"><a href="javascript:;" onclick="MYYOUKU.deleteList(\'' + _this.token + '\',\'' + data[i].videoid + '\',\'' + data[i].folderid + '\',\'' + data[i].showid + '\',\'' + data[i].videoid  + '\',\'MYYOUKU-Play\',\'' + _this.hzLinks.hzPlayDelList + '\');"><em title="删除"></em></a></li>';
				str += '</ul>';
			}
		}
		this.playDom.innerHTML = str;
		this.bindDelete(this.playDom);
	},

	getAnonymousRecommendData : function(data) {
		var len = data.length;
		if(len > this.anonymousRecommendNumber) { 
			this.initData(data,this.anonymousData);
			this.initAnonymousRecommendDom(this.anonymousData);
			this.showConetnt();
		}
		else {
			if(this.getReommendDataNumber < this.MAXNUMBER) {
				this.getJson(this.getAnonymousComJson + this.randomChar(10));	
				this.getReommendDataNumber++;
			}
		}
	},

	getRecommendData : function(data) {
		var len = data.length;
		if(len >= this.recommendNumber) { 
			this.initData(data,this.recommendData);
			this.initRecommendDom(this.recommendData);
			this.showConetnt();
		}
		else {
			if(this.getReommendDataNumber < this.MAXNUMBER) {
				this.getJson(this.getComJson + this.randomChar(10));	
				this.getReommendDataNumber++;
			}
		}
	},

	getRecordsData : function(data) {
		this.initData(data.playtag,this.playListData);
		this.initData(data.playtag_show,this.showListData);
		this.token = data.token;
		this.initShowlistDom(this.showListData);
		this.initPlaylistDom(this.playListData);
	},

	getPayData : function(data) {
		this.initData(data,this.payListData);
		this.initPayDom(this.payListData);
	},

	initData : function(data,o) {
		var ptlen = data.length; 
		for(var i = 0; i < ptlen; i++) {
			o.push(data[i]);
		};
	},

	showConetnt : function() {
		var _this = this;
		_this.dom.style.display = 'block';
		var slidingTimer = null;
		var theHeight = 0;
		if(_this.yktk) {
			theHeight = _this.MAXHEIGHT;
		} else {
			theHeight = _this.ANONYMOUSHEIGHT;
		}
		var b=10, c=theHeight, d=5, t=0;
		function Sliding(){
			_this.dom.style.height = _this.easeOut(t,b,c,d) + 'px';
			if(t<d){
				t++;
				slidingTimer = setTimeout(Sliding, 1);
			}
			else{
				window.clearInterval(slidingTimer);
			}
		};
		Sliding();	
	},

	formatnum : function(n){
		n  =  n+'';   
		if(n.indexOf(',')>0){  
			n = n.replace(/,/gi,'') + '';   
		}  
		var re=/(-?\d+)(\d{3})/; 
		while(re.test(n)){    
			n = n.replace(re,'$1,$2')    
		}
		return n;  
	},

	secondTofMinute : function(t) {
		var m = parseInt(t/60);	
		var s = parseInt(t%60);	
		if(m < 10) {
			m = '0' + m;	
		} else {
			m = m.toString();
		}
		if(s < 10) {
			s = '0' + s;	
		} else {
			s = s.toString();
		}
		return m + ':' +s;
	},

	deleteList : function(tk, vid, fid, shid, lild, list, charset) {
		var e = window.event || arguments.callee.caller.arguments[0];
		var obj = e.target || e.srcElement;
		var index = obj.parentNode.parentNode.parentNode.getAttribute('itemindex');
		if(!tk || !vid || !lild) return;
		if(fid == undefined) fid = 0;
		if(shid == undefined) shid = 0;
		var delUrl = "http://yus.navi.youku.com/playlog/delete.json?token="+tk+"&v="+vid+"&fid="+fid+"&shid="+shid+"&"+Math.random();
		var listObj = document.getElementById(list);
		var listUL = listObj.getElementsByTagName('UL');
		var itemObj = obj.parentNode.parentNode.parentNode;
		if(listUL.length == 1) {
			if(list == 'MYYOUKU-Play') {
				str = '<div class="recordnull" id="watchlist"><span>没有播放记录</span></div>';
			}
			else if(list == 'MYYOUKU-Show') {
				str = '<div class="recordnull" id="showlist" style="overflow-y:hidden;"><span>您最近没有观看过剧集节目</span><a href="http://tv.youku.com" target="_blank">更多精彩剧集</a></div>';
			}
			listObj.innerHTML = str; 
		}
		else {
			listObj.removeChild(itemObj);
		}
	 
		var img = new Image();
		img.src = delUrl;	 
		if(charset) {
			addScript("http://hz.youku.com/red/click.php?tp=1&cp="+charset+"&cpp=1000648");
		};
		function addScript(src){
			var doc = document;
			var g = doc.createElement("script");
			g.type = "text/javascript";
			g.src = src;
			doc.getElementsByTagName('head')[0].appendChild(g);
		}
	},

	submitVideoCommend : function(o){
		var req = o.parentNode.parentNode.getAttribute('logdata');
		req = decodeURIComponent(req);
		var d = eval('('+req+')');
		if(d) {
			var logstr = 'uid=' + d.uid  + '&cookie_id=' + d.cookie_id + '&apptype=1&page=8&module=' + d.module + '&dvid=' + d.dvid + '&abver=' + d.abver + '&dma=' + d.dma + '&pos=' + d.pos + '&ord=0&dct=0';
			var url = 'http://e.stat.youku.com/personalization/log?';
			var imgReq = new Image();
			imgReq.src = url + logstr;	
			imgReq = null;
		}	
	},

	bindDelete : function(obj) {
		var lists = obj.getElementsByTagName('UL');
		for(var i = 0; i < lists.length; i++) {
			if(lists[i].className.indexOf('playrecord') >=0 ) {
				lists[i].onmouseover = function() {
					var cl = this.getElementsByTagName('LI');
					for(var j = 0; j < cl.length; j++) {
						if(cl[j].className.indexOf('rec_close') >= 0) {
							cl[j].style.display = 'block';
						}
					}
				}
				lists[i].onmouseout = function() {
					var cl = this.getElementsByTagName('LI');
					for(var j = 0; j < cl.length; j++) {
						if(cl[j].className.indexOf('rec_close') >= 0) {
							cl[j].style.display = 'none';
						}
					}
				}
			}
		}
	},

	randomChar : function(l)  {
		var  x="0123456789qwertyuioplkjhgfdsazxcvbnm";
		var  tmp="";
		var timestamp = new Date().getTime();
		for(var i=0;i<l;i++)  {
			tmp  +=  x.charAt(Math.ceil(Math.random()*100000000)%x.length);
		}
		return  timestamp+tmp;
	},

	mylogin : function () {
		MYYOUKU.requestImg(MYYOUKU.hzLinks.returnHzUrl('4007112'));
		login(MYYOUKU.refresh);
	},

	refresh : function () {
		MYYOUKU.requestImg(MYYOUKU.hzLinks.returnHzUrl('4007350'));
		window.location.reload();
	},

	easeOut : function(t,b,c,d) {
		return -c *(t/=d)*(t-2) + b;
	},

	callBackScript : function(){
		if(this.parentNode){
			this.parentNode.removeChild(this);
		}
	},

	getJson : function(url){
		var _this = this;
		var _script = document.createElement("script");
		var _prob = _this.ysuid.substr(-4,1);
		_script.type = "text/javascript";
		_script.src = url + _this.randomChar(10);
		if (!/*@cc_on!@*/0) {
			_script.onload = _this.callBackScript;
		} else {
			_script.onreadystatechange = function () { 
				if (_script.readyState == 'loaded' || _script.readyState == 'complete') { 
					_this.callBackScript();
				} 
			}
		}
		document.getElementsByTagName("head")[0].appendChild(_script);
	}
}

window.nova_init_hook_myyouku = function() {
	MYYOUKU.init();
}

window.nova_init_hook_hovervp = function() { 
	PlayList.HoverVP.init("MYYOUKU");
}


</script>
