function blogrec_change(x)
{
        var d = data[x];
		var d_v=data_value[x]
        with (document.blogrec_fm.elements["t_col6"])
		{
                length = 0;
                for(i=0;i<d.length;i++)
                        options[i] = new Option(d[i],d_v[i]);
        }
		if (document.blogrec_fm.t_col5.options[x].value=="caogen")
		{
			//caogen
			document.blogrec_fm.action='http://hi.blog.sina.com.cn/run.php?project_id=473';
		}
		else if (document.blogrec_fm.t_col5.options[x].value=="style")
		{
			//style
			document.blogrec_fm.action='http://hi.blog.sina.com.cn/run.php?project_id=4195';
		}
		else if (document.blogrec_fm.t_col5.options[x].value=="collection")
		{
			//collection
			document.blogrec_fm.action='http://hi.blog.sina.com.cn/run.php?project_id=4193';
		}
		else
		{	//normal
			document.blogrec_fm.action='http://hi.blog.sina.com.cn/run.php?project_id=5009';
		}
}
</script>

