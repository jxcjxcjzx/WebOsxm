<!-- ip定向mt推广位 begin -->
<script>
(function(){

	if(!window.remote_ip_info){
		getJsData("//int.dpool.sina.com.cn/iplookup/iplookup.php?format=js", function(){		
			getJsData("//www.sina.com.cn/js/67/2012/1120/38.js",DynContent);
		});
	}
	else
	{
		getJsData("//www.sina.com.cn/js/67/2012/1120/38.js",DynContent);
	}
	function  DynContent() {
				//todo >		
				var province = remote_ip_info.province;
				var dataBlock = mtLabs;			
				var provinceLen = dataBlock.length;		

				for(var counter = 0; counter < provinceLen; counter++)  //省
				{					
					if(province === dataBlock[counter].province)   //
					{								
						var data = dataBlock[counter].data;
						var dataLen = data.length;					
						var container =  document.getElementById("sinaDynContBlockID");
						var containerVol = container.getAttribute("charsLen");
						var dynContent = "";

						var strLen = 0;  //统计容器当前容量
						var strNotEmpty = 0;
						var startPoint = 0;  //
						var curr = 0;
						var curr = Math.floor((Math.random()*dataLen ));
						startPoint = curr;
						do{
							var  item = data[curr];						
							if(!!item.lab){	  //								
								strLen += parseInt(item.len);						
								if(strLen < containerVol)  //容器可以继续容纳
								{								
									dynContent += item.lab;
									dynContent += " ";
								}
								else
								{
									break;  //
								}								
							}
							if(curr < dataLen - 1)   //保证 数组下标不会越界
							{
								curr++;
							}
							else{
								curr = 0;
							}
						}while(startPoint != curr);  //保证不会出现重复内容	
						if(dynContent){
							container.innerHTML = dynContent;
						}
						break;
					}
				}					
	}

	//get script
    function  getJsData (url,dispose){
            var scriptNode = document.createElement("script");
            scriptNode.type = "text/javascript";
            scriptNode.setAttribute('charset', 'gb2312');
            scriptNode.onreadystatechange = scriptNode.onload = function(){
                if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete"){
                    if(dispose){dispose()};
                    scriptNode.onreadystatechange = scriptNode.onload = null;
                    scriptNode.parentNode.removeChild(scriptNode);
                }
            };
            scriptNode.src = url;
            document.getElementsByTagName("head")[0].appendChild(scriptNode);
    }

})();
</script>
<!-- ip定向mt推广位 end -->

