									focusScroll_sjtp.scrollContId	= "scrPic_sjtp_01"; //内容容器ID

									focusScroll_sjtp.dotListId		= "scrDotList_sjtp_01";//点列表ID
									focusScroll_sjtp.dotClassName	= "";//点className
									focusScroll_sjtp.dotOnClassName = "on";//当前点className
									focusScroll_sjtp.listType	= "";//列表类型(number:数字，其它为空)
									focusScroll_sjtp.listEvent	= "onmouseover"; //切换事件

									focusScroll_sjtp.frameWidth	= 280;//显示框宽度
									focusScroll_sjtp.pageWidth	= 280; //翻页宽度
									focusScroll_sjtp.upright	= false; //垂直滚动
									focusScroll_sjtp.speed		= 10; //移动速度(单位毫秒，越小越快)
									focusScroll_sjtp.space		= 40; //每次移动像素(单位px，越大越快)
									focusScroll_sjtp.autoPlay	= false; //自动播放
									focusScroll_sjtp.autoPlayTime = 5; //自动播放间隔时间(秒)
									focusScroll_sjtp.circularly = true;
									focusScroll_sjtp.initialize(); //初始化
									document.getElementById('scrArrLeft_sjtp_01').onmousedown = function(){
										focusScroll_sjtp.pre();
										return false;
									}
									document.getElementById('scrArrRight_sjtp_01').onmousedown = function(){
										focusScroll_sjtp.next();
										return false;
									}

								})();
								</script>		
	<!-- left end -->
	</div>
