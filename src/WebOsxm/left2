<script>

(function(){
	var url = location.href;
	if(url.indexOf('ipad') > -1){
		document.getElementById('custom_guide').style.display = 'none';
		document.getElementById('nav_survey').style.display = 'none';
	}
})();

var fn = {
	getById : function(id){
		var elements = [];
		for (var i = 0; i < arguments.length; i++) {
			var element = arguments[i];
			if (typeof element == 'string')
				element = document.getElementById(element);
			if (arguments.length == 1){
				if(typeof(element) == 'undefined'){
				}
				return element;
			}
			elements.push(element);
    	}
		debugger;
		return elements;
	},
	getElementsByClass : function(searchClass, node, tag) {
        var classElements = [];
        if (node == null) node = document;
        if (tag == null) tag = '*';
        var els = node.getElementsByTagName(tag);
        var elsLen = els.length;
        var pattern = new RegExp("(^|\\s)"+searchClass+"(\\s|$)");
        for (i = 0, j = 0; i < elsLen; i++) {
			if (pattern.test(els[i].className)) {
				classElements[j] = els[i];
				j++;
			}
		}
		return classElements;
	},
	getElementsByAttribute : function(attribute, attributeValue, containner)
	{
		var elementArray = [];
		var matchedArray = [];
		if(containner){
			elementArray = containner.getElementsByTagName("*");
		}
		else{
			if (document.all) elementArray = document.all;
			else elementArray = document.getElementsByTagName("*");
		}
		for (var i = 0; i < elementArray.length; i++)
		{
			if (attribute == "class")
			{
				var pattern = new RegExp("(^| )" + attributeValue + "( |$)");		
				if (elementArray[i].className.match(pattern)) matchedArray.push(elementArray[i]);
			}
			else if (elementArray[i].getAttribute(attribute) == attributeValue) matchedArray.push(elementArray[i]);
		}
		return matchedArray;
	},
	insertAfter : function(node, referNode) {
		var nextNode = referNode.nextSibling;
		if(nextNode){
			referNode.parentNode.insertBefore(node, nextNode);
		}
		else{
			referNode.parentNode.appendChild(node);
		}
	},
	getBrowserInfo : function(){
		if(this.browserInfo)
			return this.browserInfo;
		this.browserInfo = {};
		var ua = navigator.userAgent.toLowerCase();
		var bs = ['msie','chrome','firefox','opera','safari'];
		var brIndex = '';
		for(var i=0,bl=bs.length;i<bl;i++){
			brIndex = ua.indexOf(bs[i]);
			if(brIndex > -1){
				var str = ua.substr(brIndex);
				this.browserInfo.browser = bs[i];
				this.browserInfo.version = str.match(/\d+/)[0];
				break;
			}
		}
		return this.browserInfo;
	},
	getCookie : function (name){
		var start = document.cookie.indexOf( name + "=" );
		var len = start + name.length + 1;
		if ( ( !start ) && ( name != document.cookie.substring( 0, name.length ) ) ) {
			return null;
		}
		if ( start == -1 ) return null;
		var end = document.cookie.indexOf( ';', len );
		if ( end == -1 ) end = document.cookie.length;
		return unescape( document.cookie.substring( len, end ) );
	},
	setCookie : function(name, value, expires, path, domain, secure) {
		var today = new Date();
		today.setTime( today.getTime() );
		if ( expires ) {
			expires = expires * 1000 * 60 * 60 * 24;
		}else{
			expires = 365 * 24 * 3600000;
		}
		var expires_date = new Date( today.getTime() + (expires) );
		document.cookie = name+'='+value +
			( ( expires ) ? ';expires='+expires_date.toGMTString() : '' ) + 
			( ( path ) ? ';path=' + path : '' ) +
			( ( domain ) ? ';domain=' + domain : '' ) +
			( ( secure ) ? ';secure' : '' );
	},
	deleteCookie : function(name, path, domain){
		if ( this.getCookie( name ) ) document.cookie = name + '=' +
				( ( path ) ? ';path=' + path : '') +
				( ( domain ) ? ';domain=' + domain : '' ) +
				';expires=Thu, 01-Jan-1970 00:00:01 GMT';
	},
	setStyle : function (el, prop, value){
		if(prop == "opacity" && !+"\v1"){
			if (!el.currentStyle || !el.currentStyle.hasLayout) el.style.zoom = 1;
			prop = "filter";
			if(!!window.XDomainRequest){
				value ="progid:DXImageTransform.Microsoft.Alpha(style=0,opacity="+value*100+")";
			}else{
				value ="alpha(opacity="+value*100+")";
			}
		}
		el.style.cssText += ';' + (prop+":"+value);
	}, 
	getStyle : function (el, style){
		if(!+"\v1"){
			style = style.replace(/\-(\w)/g, function(all, letter){
				return letter.toUpperCase();
			});
			return el.currentStyle[style];
		}else{
			return document.defaultView.getComputedStyle(el, null).getPropertyValue(style)
		}
	},
	getPosition : function (o){
		var p = {x : 0, y : 0};
		while (o.offsetParent) {
			p.x += o.offsetLeft;
			p.y += o.offsetTop;
			o = o.offsetParent;
		}
		return p;
	},
	getScrollPos : function(oDocument){
		oDocument = oDocument||document;
		var dd = oDocument.documentElement;
		var db = oDocument.body;
		return [Math.max(dd.scrollTop,db.scrollTop),Math.max(dd.scrollLeft,db.scrollLeft),Math.max(dd.scrollWidth,db.scrollWidth),Math.max(dd.scrollHeight,db.scrollHeight)]
	},
	getViewportSize: function(){
		return {
			w: (window.innerWidth) ? window.innerWidth : (document.documentElement && document.documentElement.clientWidth) ? document.documentElement.clientWidth : document.body.offsetWidth,
			h: (window.innerHeight) ? window.innerHeight : (document.documentElement && document.documentElement.clientHeight) ? document.documentElement.clientHeight : document.body.offsetHeight
		}
	},
	extend : function(o, t){
		o = o || {};
		for( var sName in t){
			o[sName] = t[sName];
		}
		return o;
	},
	trim : function (str){
		if(str.trim) return str.trim();
		return str.replace(/(^[\s]*)|($[\s]*)/g,'');
	},
	clipString : function(str, a, b){
		var s = str.replace(/([^\x00-\xff])/g, "\x00$1");
		return (s.length < b) ? str: (s.substring(a, b-3).replace(/\x00/g, '') + '...');
	},
	addEvent : function(obj, type, fn){
		if (obj.addEventListener)
			obj.addEventListener(type, fn, false);
		else if (obj.attachEvent)
		{
			obj["e"+type+fn] = fn;
			obj[type+fn] = function() { obj["e"+type+fn](window.event); }
			obj.attachEvent("on"+type, obj[type+fn]);
		}
	},	
	removeEvent : function(obj, type, fn){
		if (obj.removeEventListener)
			obj.removeEventListener(type, fn, false);
		else if (obj.detachEvent)
		{
			obj.detachEvent( "on"+type, obj[type+fn] );
			obj[type+fn] = null;
			obj["e"+type+fn] = null;
		}
	},
	getEvent : function(){
		if (document.all) {
			return window.event;
		}
		func = getEvent.caller;
		while (func != null) {
			var arg0 = func.arguments[0];
			if (arg0) {
				if ((arg0.constructor == Event || arg0.constructor == MouseEvent) || (typeof (arg0) == "object" && arg0.preventDefault && arg0.stopPropagation)) {
					return arg0;
				}
			}
			func = func.caller;
		}
		return null;
	},
	getMousePos : function(ev){
		if (ev.pageX || ev.pageY){
			return {
				x: ev.pageX,
				y: ev.pageY
			};
		}
		if (document.documentElement && document.documentElement.scrollTop) {
			return {
				x: ev.clientX + document.documentElement.scrollLeft - document.documentElement.clientLeft,
				y: ev.clientY + document.documentElement.scrollTop - document.documentElement.clientTop
			};
		}
		else if (document.body){
			return {
				x: ev.clientX + document.body.scrollLeft - document.body.clientLeft,
				y: ev.clientY + document.body.scrollTop - document.body.clientTop
			};
		}
	},
	actTo : function(obj, key, start, end, speed, endFn, u){
		if(typeof(u) == 'undefined'){u = 'px'};
		clearTimeout(obj['_extend_actPX' + key.replace(/\-\.\=/,'_') + '_timeOut']);
		if(start > end){
			speed = - Math.abs(speed);
		}else{
			speed = Math.abs(speed);
		};
		var now = start;
		var length = end - start;
		obj['_extend_actPX' + key.replace(/\-\.\=/,'_') + '_timeOut'] = setTimeout(function(){
			now += speed;
			//debugger;
			var space = end - now;
			if(start < end){
				if(space < length/3){
					speed = Math.ceil(space/3);
				};
				if(space <= 0){ //end
					obj[key] = end + u;
					if(endFn){endFn()};
					return;
				};
			}else{
				if(space > length/3){
					speed = Math.floor(space/3);
				};
				if(space >= 0){ //end
					obj[key] = end + u;
					if(endFn){endFn()};
					return;
				};
			};

			obj[key] = now + u;
			obj['_extend_actPX' + key.replace(/\-\.\=/,'_') + '_timeOut'] = setTimeout(arguments.callee,20);

		},20);
	},
	windowScrollTo: function(start, end, speed, endFn, u) {
		var obj = {}, key = '';
		if(typeof(u) == 'undefined'){u = 'px'};
		clearTimeout(obj['_extend_actPX' + key.replace(/\-\.\=/,'_') + '_timeOut']);
		if(start > end){
			speed = - Math.abs(speed);
		}else{
			speed = Math.abs(speed);
		};
		var now = start;
		var length = end - start;
		obj['_extend_actPX' + key.replace(/\-\.\=/,'_') + '_timeOut'] = setTimeout(function(){
			now += speed;
			var space = end - now;
			if(start < end){
				if(space < length/3){
					speed = Math.ceil(space/3);
				};
				if(space <= 0){ //end
					window.scrollTo(0, end);
					if(endFn){endFn()};
					return;
				};
			}else{
				if(space > length/3){
					speed = Math.floor(space/3);
				};
				if(space >= 0){ //end
					window.scrollTo(0, end);
					if(endFn){endFn()};
					return;
				};
			};

			window.scrollTo(0, now);
			obj['_extend_actPX' + key.replace(/\-\.\=/,'_') + '_timeOut'] = setTimeout(arguments.callee,20);

		},20);
	},
	opacityTo: function (element, op, time, endFn) {
		var oOp = parseFloat(fn.getStyle(element, 'opacity')),
			speed, counter = 0, loop;
		if(op != oOp){
			speed = (op - oOp) / 5;
			loop = setInterval(function () {
				if(counter == 5){
					fn.setStyle(element, 'opacity', op);
					clearInterval(loop);
					return;
				}
				fn.setStyle(element, 'opacity', oOp + speed);
				oOp += speed;
				counter ++;
			}, 20);
		}
		if(endFn) endFn(element, op, time);
	},
	hasClass: function(element, name){
		var re = new RegExp('(^| )' + name + '( |$)');
		return re.test(element.className);
	},
	removeClass: function(element, name){
		var re = new RegExp('(^| )' + name + '( |$)');
		if(name){
			element.className = element.className.replace(re, ' ').replace(/^\s+|\s+$/g, "");
		}else{
			element.className = '';
		}
	},
	addClass : function(element, name){
		if (!fn.hasClass(element, name)){
			element.className += ' ' + name;
		}
	},
	getIndex: function (element) {
		var parent = element.parentNode,
			childNodes = parent.childNodes,
			cLen = childNodes.length,
			counter = 0;
		for(var i = 0; i < cLen; i++){
			if(childNodes[i] == element){
				return counter;
				break;
			}
			counter++;
		}
		return false;
	},
	swapNode: function(node1,node2){
	   var parent = node1.parentNode;
	   var t1 = node1.nextSibling;
	   var t2 = node2.nextSibling;

	   //如果是插入到最后就用appendChild
	   if(t1) parent.insertBefore(node2,t1);
	   else parent.appendChild(node2);
	   if(t2) parent.insertBefore(node1,t2);
	   else parent.appendChild(node1);
	 },
	 subTring: function(str,n){   
		var r = /[^\x00-\xff]/g;    
		if(str.replace(r, "mm").length <= n) return str;  
		var m = Math.floor(n/2);    
		for(var i=m; i<str.length; i++) {    
		if(str.substr(0, i).replace(r, "mm").length>=n) {    
		return str.substr(0, i) +"..."; }    
		} return str;   
	}
};

var Drag = function () {
	var init = function(opts) {
		this.conf = this._initOpts(opts);
		this._initEvent(true);
	};
	init.prototype = {
		_extendOpts : function(destination, source) {
			for(property in source) {
				destination[property] = source[property];
			}
		},
		_initOpts: function(o){
			var defaultOpts = {
				objDom: document.getElementById('custom_drag'),
				handle: null,
				activated: true,
				range: [0, 800, 0, 500],
				beforeDrag: function(conf){
					//alert(1);
				},
				onDraging: function (conf) {
					//console.log(conf.x, conf.y);
				},
				afterDrag: function(conf){
					//alert(1);
				}
			};
			this._extendOpts(defaultOpts, o);
			return defaultOpts;
		},
		_initEvent: function(isAble){
			var that = this,
				dom = this.conf.handle ? this.conf.handle : this.conf.objDom,
				diffX, diffY,
				onMove, onUp, onDown,
				doc,
				obj = this.conf.objDom;
			onMove = function(e){
				var e = e || window.event,
					moveX = e.clientX - diffX, moveY = e.clientY - diffY,
					minX, maxX, minY, maxY,
					mousePos = fn.getMousePos(e);
					conf = that.conf;
				if(conf.range){
					minX = conf.range[0];
					maxX = conf.range[1];
					minY = conf.range[2];
					maxY = conf.range[3];
					moveX < minX && (moveX = minX);
					moveX > maxX && (moveX = maxX);
					moveY < minY && (moveY = minY);
					moveY > maxY && (moveY = maxY);
				}
				if(conf.activated){
					obj.style.left = moveX + 'px';
					obj.style.top =  moveY + 'px';
					if(conf.onDraging){
						conf.mouseX = mousePos.x;
						conf.mouseY = mousePos.y;
						conf.x = moveX;
						conf.y = moveY;
						conf.onDraging.call(that, conf);
					}
				}
			};
			onUp = function(e){
				var e = e || window.event,
					mousePos = fn.getMousePos(e);
				obj.style.cursor = '';
				obj.style.position = 'static';
				// fn.removeEvent(document, 'mousemove', onMove);
				// fn.removeEvent(document, 'mouseup', onUp);
				document.onmousemove = document.onmouseup = null;
				if(obj.releaseCapture){
					// fn.removeEvent(obj, "losecapture", onUp);
					obj.onlosecapture = null;
					obj.releaseCapture();
				}
				if(window.releaseEvents){
					// fn.removeEvent(window, "blur", onUp);
					window.onblur = null;
				}
				that.conf.mouseX = mousePos.x;
				that.conf.mouseY = mousePos.y;
				that.conf.afterDrag(that.conf);
			};
			onDown = function(e){
				var e = e || window.event,
					mousePos = fn.getMousePos(e);
				obj.style.position = 'absolute';
				obj.style.cursor = 'move';
				if(obj.setCapture){
					// fn.addEvent(obj, "losecapture", onUp);
					obj.onlosecapture = onUp;
					obj.setCapture();
					e.cancelBubble = true;
				}else if(window.captureEvents){
					e.stopPropagation();
					// fn.addEvent(window, "blur", onUp);
					window.onblur = onUp;
					e.preventDefault();
				}
				that.conf.mouseX = mousePos.x;
				that.conf.mouseY = mousePos.y;
				that.conf.beforeDrag(that.conf);
				diffX = e.clientX - obj.offsetLeft;
				diffY = e.clientY - obj.offsetTop;
				// fn.addEvent(document, 'mousemove', onMove);
				// fn.addEvent(document, 'mouseup', onUp);
				document.onmousemove = onMove;
				document.onmouseup = onUp;
			};
			if(!isAble){
				// fn.removeEvent(dom, 'mousedown', onDown);
				// fn.removeEvent(document, 'mousemove', onMove);
				// fn.removeEvent(document, 'mouseup', onUp);
				dom.onmousedown = null;
				document.onmousemove = null;
				document.onmouseup = null;
				if(obj.releaseCapture){
					// fn.removeEvent(obj, "losecapture", onUp);
					obj.onlosecapture = null;
					obj.releaseCapture();
				}
				if(window.releaseEvents){
					// fn.removeEvent(window, "blur", onUp);
					window.onblur = null;
				}
				return;
			}
			// fn.addEvent(dom, 'mousedown', onDown);
			dom.onmousedown = onDown;
		},
		setDragAble: function(isOn){
			isOn = isOn ? true : false;
			this._initEvent(isOn);
			this.conf.activated = isOn;
		}
	};
	return init;
}();

var Storage = (function (opts) {
	// body...
	var init = function (opts) {
		this.conf = this._extendOpts(opts);
		this.html5Mode = window.localStorage ? true : false;

	};
	init.prototype = {
		_extendOpts : function(destination, source) {
			for(property in source) {
				destination[property] = source[property];
			}
		},
		_initOpts: function(o){
			var defaultOpts = {
			};
			this._extendOpts(defaultOpts, o);
			return defaultOpts;
		},
		getStorage : function (key) {
			var ret = window.localStorage ? this._getLocalStorage(key) : fn.getCookie(key);
			return ret;
		},
		setStorage : function (key, value) {
			window.localStorage ? this._setLocalStorage(key, value) : fn.setCookie(key, value);
		},
		_getLocalStorage : function (key) {
			return localStorage[key];
		},
		_setLocalStorage : function (key, value) {
			localStorage.setItem(key, value);
		},
		_deleteLocalStorage : function (key) {
			localStorage.removeItem(key);
		}
	};
	return init;
})();

var CustomNavData = (function(){
	var init = function(opts){
		this.dataBase = {
			total: 22,
			filled: 0,
			navIndex: [],
			navItems: {}
		};
	};
	init.prototype = {
		_extendOpts : function(destination, source) {
			for(property in source) {
				destination[property] = source[property];
			}
		},
		_initOpts: function(o){
			var defaultOpts = {
			};
			this._extendOpts(defaultOpts, o);
			return defaultOpts;
		},
		getDataBase: function(){
			return this.dataBase;
		},
		getData: function(key){
			return this.dataBase[key];
		},
		setData: function(key, value){
			this.dataBase[key] = value;
		},
		getIndexData: function(){

		}
	};
	return init;
})();
</script>
