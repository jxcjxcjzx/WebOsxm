<!-- service.js -->
<script>
(function(){

try {document.domain = 'sina.com.cn';}
catch (__err) {}

var flashData;
if (!window.tmpUidev || !window.tmpUidev._flashData) {
    var tmpUidev = window.tmpUidev || {};
    window.tmpUidev = tmpUidev;
    //数据
    flashData = {
        flashObj : null,
        id : "uidevGuessInfoDataFrame",
        status : "loading",
        clear : function(){
            this.flashObj.write("");
        },
        read : function(name){
            if(this.status != "ok"){return "";};
            var nameRule = /[=,;]+/i;
            if(nameRule.test(name)){return "";};
            var dataScring = this.flashObj.read();
            if(dataScring == null){dataScring = ''};
            var reg = new RegExp("(?:^|;)" + name + "=(.*?),expires=([^;]*)","i");
            var value = dataScring.match(reg);
            if(value == null){
                value = "";
            }else if(value.length >= 3){
                var now = new Date();
                var dd = now.getTime();
                if(new Number(value[2]) > dd){
                    value = value[1];
                    value = value.replace(/\[\$\=\]/g,"=");
                    value = value.replace(/\[\$\;\]/g,";");
                    value = value.replace(/\[\$\,\]/g,",");
                }else{
                    value = "";
                };
            }else{
                value = "";
            };
            return value;
        },
        write : function(name,value,hours){
            if(this.status != "ok"){return;};
            var nameRule = /[=,;]+/i;
            if(nameRule.test(name)){return;};
            value = value.toString();
            value = value.replace(/\\/g,"\\\\");
            value = value.replace(/\&/g,"&amp;");
            value = value.replace(/\=/g,"[$$=]");
            value = value.replace(/\;/g,"[$$;]");
            value = value.replace(/\,/g,"[$$,]");
            if(typeof(hours) != "number"){hours = 24*30*3}; //默认3个月
            var now = new Date();
            var expire = new Date(now.getTime() + hours * 3600000);
            expire = ",expires=" + expire.getTime();

            var list = this.flashObj.read();
            if(list == null){list = ''};
            list = list.split(/;(?!\])/i);

            var newList = [],update = false;
            var data;
            for(var i=0;i<list.length;i++){
                data = list[i].match(/(.*?)\=(.*?),expires=(\d*)/i);
                if(data == null){continue};
                if(data.length < 4){continue};
                if(data[1] == name){
                    if(value != ""){newList.push(name + "=" + value + expire)};
                    update = true;
                }else{
                    if(new Date(new Number(data[3])) > now && data[2] != ""){
                        newList.push(list[i]);
                    };
                };
            };
            if(!update){ //添加新的
                newList.push(name + "=" + value + expire);
            };
            var newStr = "";
            for(var i=0;i<newList.length;i++){
                if(newStr != ""){newStr += ";"};
                newStr += newList[i];
            };
            this.flashObj.write(newStr);

        },
        load : function(){
            var i;
            try{
                this.flashObj = document.getElementById(this.id).contentWindow.flashData.flashObj;
                this.status = "ok";
                this.myLoaded = true;
                if (this.myLoadFn && this.myLoadFn.length > 0) {
                    for (i = 0; i < this.myLoadFn.length; i++) {
                        this.myLoadFn[i](); 
                    }
                }
                if(this.onload){this.onload()};
            }catch(e){
            }
        },
        myLoadFn: null,
        myLoad: function (fn) {
            if (flashData.myLoaded) {
                (typeof fn === 'function') && (fn());
                return;
            }
            if (!flashData.myLoadFn) {
                flashData.myLoadFn = []; 
            }
            (typeof fn === 'function') && (flashData.myLoadFn.push(fn));
        },
        myLoaded: false
    };

    //expose
    tmpUidev._flashData = flashData;

    //load flash iframe
    if (!document.getElementById(flashData.id)) {
        function loadDataIframe () {
            var body = document.getElementsByTagName('body');
            if (!body || body.length === 0) {
                setTimeout(loadDataIframe, 100); 
                return;
            }
            body = body[0];
            if (!body.children || body.children.length === 0) {
                setTimeout(loadDataIframe, 100); 
                return;
            }
            var div = document.createElement('div');
            div.style.width = '1px';
            div.style.height = '1px';
            div.style.overflow = 'hidden';
            div.style.position = 'absolute';
            div.style.left = '-2000px';
            div.innerHTML = '<iframe src="http://www.sina.com.cn/iframe/fdata/data.html" id="uidevGuessInfoDataFrame" style="height:0px;width:1px;overflow:hidden;" frameborder="0" scrolling="no"></iframe>';
            body.insertBefore(div, body.children[0]);
        };
        loadDataIframe();
    }
} else {
    flashData = window.tmpUidev._flashData; 
}

var events = {
    'dataready': flashData.myLoad 
};

window.sinaService = {
    listen: function (evt, fn) {
        events[evt](fn); 
    },

    data: {
        read: function (key) {
            return flashData.read.call(flashData, key); 
        },
        write: function (key, val) {
            flashData.write.call(flashData, key, val); 
        }
    }
};

})();

</script>
<script>
function insertCustomNav(logonav,standardHeaderChannel,defaultIndex,guideBtn,channelHead){
	var doms = {},
		customNavData = new CustomNavData(),
		sObj = new Storage(),
		sData = sinaService.data,
		initData = '',
		customing = false,
		customOpenO = false,
		customOpenN = false,
		firstGuide = false,
		narrowMode = false,
		nMode = false,
		guiding = false,
		timeout = null,
		defaultIndex = defaultIndex || '1,15,48,66,32,84,166,152,174,203,202,2',
		storageName = 'customNav_guided',
		defaultIndex = defaultIndex.split(','),
		dl = defaultIndex.length;
	for(var i = dl; i < 22; i++){
		defaultIndex.push('blank');
	}
	defaultIndex = defaultIndex.join(',');
	//console.log(sData);
	window.customNavData = customNavData;
	_initDoms();
	_initStorage(defaultIndex);
	_initEvents();

	var coverLayer2 = {
		divObj : null,
		_coverTime : null,
		_coverRe : function(){
			if(!this.wid && !this.hei){
				if(document.body.offsetHeight < document.documentElement.clientHeight){
					this.divObj.style.width = document.body.clientWidth + "px";
					this.divObj.style.height = document.documentElement.clientHeight + "px";
				}else{
					this.divObj.style.width = document.body.clientWidth + "px";
					this.divObj.style.height = document.body.clientHeight + "px";
				}
			}
		},
		isIE : navigator.appVersion.indexOf("MSIE")!=-1?true:false,
		on : function(w, h, x, y){
			var that = this;
			this.wid = w;
			this.hei = h;
			this.x = x;
			this.y = y;
			if(this.divObj == null){
				this.divObj = document.createElement("div");
				this.divObj.className = 'move_arr_bg';
				this.divObj.style.zIndex = 10000;
				this.divObj.style.left = '0px';
				this.divObj.style.top = '0px';
				this.divObj.style.overflow = 'hidden';
				this.divObj.style.position = "absolute";
				this.divObj.style.backgroundColor = "#000";
				if(this.isIE){
					var tempFrame = document.createElement("iframe");
					tempFrame.style.filter = "Alpha(Opacity=0)";
					tempFrame.frameBorder=0;
					tempFrame.scrolling="no";
					tempFrame.style.width = "100%";
					tempFrame.style.height = "100%";
					this.divObj.appendChild(tempFrame);
					this.divObj.style.filter = "Alpha(Opacity=30)";
				}else{
					this.divObj.style.opacity = 0.3;
				};
				document.body.insertBefore(this.divObj,document.getElementsByTagName('div')[0]);
			};
			this.divObj.style.width = this.divObj.style.height = '100%';
			this.divObj.style.left = this.divObj.style.top = '0px';
			if(w) this.divObj.style.width = w + 'px';
			if(h) this.divObj.style.height = h + 'px';
			if(x) this.divObj.style.left = x + 'px';
			if(y) this.divObj.style.top = y + 'px';
			if(!w && !h){
				if(document.body.offsetHeight < document.documentElement.clientHeight){
					this.divObj.style.width = document.body.clientWidth + "px";
					this.divObj.style.height = document.documentElement.clientHeight + "px";
				}else{
					this.divObj.style.width = document.body.clientWidth + "px";
					this.divObj.style.height = document.body.clientHeight + "px";
				};
			}
			this.divObj.style.display = "block";
			clearInterval(this._coverTime);
			this._coverTime = setInterval(function(){that._coverRe()},50);
		},
		off : function(){
			if(this.divObj){this.divObj.style.display = "none"};
			clearInterval(this._coverTime);
		}
		//window.coverLayer2 = coverLayer2;
	};

	if(!sData.read(storageName)){
        firstGuide = true;
		doGuide();
	}
	if(sData.read('customNav_customed')){
			fn.getById(logonav).style.display = 'none';
			doms.nav_w.style.display = 'block';
			customOpenO = customOpenN = true;
	}else{
		doms.nav_w.style.display = 'none';
		fn.getById(logonav).style.display = 'block';
		customOpenO = customOpenN = false;
	}
	doSwitchMode();

	function _initDoms () {
		_createTips();
		_createCover();
		doms = {
			nav_w : fn.getById('custom_nav_w'),
			nav : fn.getById('custom_nav'),
			nav_c: fn.getById('custom_nav_c'),
			nav_c_ul: fn.getById('custom_nav_c').getElementsByTagName('ul')[0],
			inactive_w: fn.getById('custom_inactive_w'),
			inactive_tt: fn.getById('custom_inactive_tt'),
			inactive_c: fn.getById('custom_inactive_c'),
			tips: fn.getById('custom_item_tip_w'),
			item_cover: fn.getById('custom_item_cover'),
			item_cover_delete: fn.getById('custom_item_delete'),
			btn_set: fn.getById('custom_btn_set'),
			btn_switch_w: fn.getById('custom_btn_switch_w'),
			btn_switch: fn.getById('custom_btn_switch'),
			btn_guide: fn.getById(guideBtn),
			panel: fn.getById('custom_panel'),
			btn_panel_c: fn.getById('custom_close_c'),
			btn_panel_y: fn.getById('custom_close_y'),
			btn_panel_n: fn.getById('custom_close_n'),
			btn_close: fn.getById('custom_btn_close'),
			channelHead: fn.getById(channelHead)
		};
		fixSwitch();
		_createPlus();
		setDatabase();

		if(document.body.offsetWidth <= 1024){
			narrowMode = true;
			nMode = true;
		}else{
			narrowMode = false;
			nMode = false;
		}
	}
	//storage section, undone for ie6? done!
	function _initStorage (defaultIndex) {
		var ul = doms.nav_c_ul,
			liList,
			liLen,
			//indexSaved = sObj.getStorage('customNavIndex') || '',
			indexSaved = sData.read('customNavIndex') || '',
			cI, filled = 0;
		indexSaved = indexSaved ? indexSaved : defaultIndex;
		if(indexSaved){
			_sortItems(indexSaved,true);
			_resetInactive(indexSaved);
		}
		liList = ul.getElementsByTagName("li");
		liLen = liList.length;
		//customNavData.setData('total', liLen);
		customNavData.getDataBase().navIndex = [];
		for(var i = 0; i < liLen; i ++){
			cI = liList[i].getElementsByTagName('a')[0].getAttribute('data-ci');
			customNavData.getDataBase().navIndex.push(cI ? cI : 'blank');
			if(cI) filled++;
		}
		customNavData.setData('filled', filled);
		//sObj.setStorage('customNavIndex', customNavData.getDataBase().navIndex.join(','));
		sData.write('customNavIndex', customNavData.getDataBase().navIndex.join(','), 24 * 365);
		//console.log(sData.read('customNavIndex'));
	}
	function showPanel(){
		doms.panel.style.display = 'block';
	}
	function abortCustom(){
		dragging = false;
		fn.removeClass(doms.nav_w, 'custom_nav_customing');
		doms.inactive_w.style.display = 'none';
		if(firstGuide && sData.read('customNavIndex') == defaultIndex){
			dragging = false;
			customing = false;
			showSwitch();
			fn.removeClass(doms.nav_w, 'custom_nav_customing');
			doms.inactive_w.style.display = 'none';

			doms.nav_w.style.display = 'none';
			fn.getById(logonav).style.display = 'block';
			//fn.deleteCookie('customNav_customed');
			sData.write('customNav_customed','', 24 * 365);
			customOpenO = customOpenN;
			customOpenN = false;
			if(customOpenO != customOpenN){
				try{
					_S_uaTrack("navcustom","old");
				}catch(e){}

				//console.log(customOpenN);
			}
			return;
		}
		customing = false;
		showSwitch();
		customOpenO = customOpenN;
		customOpenN = true;
		_sortItems(sData.read('customNavIndex'),true);
		if(customOpenO != customOpenN){
			try{
				_S_uaTrack("navcustom","customed");
			}catch(e){}

			//console.log(customOpenN);
		}
	}
	function resetCustom(){
		sData.write('customNavIndex',initData);
		_initStorage()
		abortCustom();
	}
	function doSwitchMode(){
		if(narrowMode){
			turnNarrowMode();
		}else{
			turnNormalMode();
		}
	}
	function turnNarrowMode(){
		doms.channelHead.style.marginBottom = '25px';
		if(fn.hasClass(doms.btn_switch_w,'custom_btn_switch_w_hanging')){
			doms.btn_switch_w.className = 'custom_btn_switch_w2';
			fn.addClass(doms.btn_switch_w,'custom_btn_switch_w_hanging');
		}else{
			doms.btn_switch_w.className = 'custom_btn_switch_w2';
		}
	}
	function turnNormalMode(){
		doms.channelHead.style.marginBottom = '0';
		if(fn.hasClass(doms.btn_switch_w,'custom_btn_switch_w_hanging')){
			doms.btn_switch_w.className = 'custom_btn_switch_w';
			fn.addClass(doms.btn_switch_w,'custom_btn_switch_w_hanging');
		}else{
			doms.btn_switch_w.className = 'custom_btn_switch_w';
		}
	}
	function _initEvents () {
		var tipsOn = false, _toHide, _toHideCover, _toHideSet, _toHideSwitch, switchMoving = false, switchShow = false,
			dragObj, dragging = false,
			dragObj2, dragging2 = false, tempSwitch = true;
			doms.btn_guide = fn.getById(guideBtn);
		fn.addEvent(window,'resize',function(){
			if(document.body.offsetWidth <= 1024){
				narrowMode = true;
			}else{
				narrowMode = false;
			}
			if(narrowMode != nMode){
				doSwitchMode();
				nMode = narrowMode;
			}
			if(!customing){
				showPlus();
			}
		});
		fn.addEvent(doms.btn_close,'click',function(e){
			var nowData = sData.read('customNavIndex');
			var e = e || window.event;
                        try{
						_S_uaTrack("navcustom","cancel");
					}catch(e){}
			if (e.preventDefault) {
				e.preventDefault();
				e.stopPropagation();
			} else {
				e.returnValue = false;
				e.cancelBubble = true;
			}
			if(nowData != initData){
				showPanel();
			}else{
				abortCustom();
			}
		});
		fn.addEvent(doms.btn_panel_c,'click',function(e){
			var e = e || window.event;
			if (e.preventDefault) {
				e.preventDefault();
				e.stopPropagation();
			} else {
				e.returnValue = false;
				e.cancelBubble = true;
			}
			doms.panel.style.display = 'none';
		});
		fn.addEvent(doms.btn_panel_n,'click',function(e){
			var e = e || window.event;
			if (e.preventDefault) {
				e.preventDefault();
				e.stopPropagation();
			} else {
				e.returnValue = false;
				e.cancelBubble = true;
			}
			doms.panel.style.display = 'none';
		});
		fn.addEvent(doms.btn_panel_y,'click',function(e){
			var e = e || window.event;
			if (e.preventDefault) {
				e.preventDefault();
				e.stopPropagation();
			} else {
				e.returnValue = false;
				e.cancelBubble = true;
			}
			resetCustom();
			doms.panel.style.display = 'none';
		});
		fn.addEvent(doms.btn_guide,'click',function(e){
			var e = e || window.event;
			var target = e.target || e.srcElement;
			if(e.stopPropagation){
				e.stopPropagation();
			}else{
				e.cancelBubble = true;
			}
			doGuide();
		});
		if(document.getElementById('custom_btn_switch2_span')){
			fn.addEvent(document.getElementById('custom_btn_switch2_span'),'click',function(e){
				var e = e || window.event;
					t = e.target || e.srcElement;
				var p = t.parentNode,
					a = p.getElementsByTagName('a')[0];
				if(e.stopPropagation){
					e.stopPropagation();
				}else{
					e.cancelBubble = true;
				}
				if(!t.nodeName){
					t = t.parentNode;
				}
				a.className = 'custom_btn_switch';
				if(fn.hasClass(p,'custom_btn_switch_w_hanging')){
					fn.removeClass(p,'custom_btn_switch_w_hanging');
				}
				p.removeChild(t);
				hideSwitch();
				sData.write('custom_switchClosed','true');
			});
		}
		fn.addEvent(fn.getById(logonav), 'mouseover', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			showSwitch();
		});
		fn.addEvent(fn.getById(logonav), 'mouseout', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			hideSwitch();
		});
		fn.addEvent(doms.btn_switch_w, 'mouseover', function (e) {
			showSwitch();
		});
		fn.addEvent(doms.btn_switch_w, 'mouseout', function (e) {
			hideSwitch();
		});
		fn.addEvent(doms.btn_switch, 'click', function (e) {
			switchMoving = false;
			switchShow = false;
			if (e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
			initData = sData.read('customNavIndex');
			doms.nav_w.style.display = 'block';
			fn.getById(logonav).style.display = 'none';
			sData.write('customNav_customed', 'true', 24 * 365);
			_sortItems(sData.read('customNavIndex'),false);

			dragging = false;
			customing = true;
			hideSwitch();

			fn.addClass(doms.nav_w, 'custom_nav_customing');
			// fn.setStyle(doms.inactive_w, 'opacity', 0);
			doms.inactive_w.style.display = 'block';
			var h = doms.inactive_w.offsetHeight;
			fn.getById('custom_inactive_w').style.overflow = 'hidden';
			fn.actTo(fn.getById('custom_inactive_w').style, 'height', 0, h, 100, function(){fn.getById('custom_inactive_w').style.overflow = 'visible'; fn.getById('custom_inactive_w').style.height = 'auto';}, 'px');
			try{
				custom_subshow.select(0);
			}catch(e){}

		});
		fn.addEvent(fn.getById('custom_btn_cancel'), 'click', function (e) {
			dragging = false;
			customing = false;
			showSwitch();
			fn.removeClass(doms.nav_w, 'custom_nav_customing');
			doms.inactive_w.style.display = 'none';

			doms.nav_w.style.display = 'none';
			fn.getById(logonav).style.display = 'block';
			//fn.deleteCookie('customNav_customed');
			sData.write('customNav_customed','', 24 * 365);
			customOpenO = customOpenN;
			customOpenN = false;
			if(customOpenO != customOpenN){
				try{
					_S_uaTrack("navcustom","old");
				}catch(e){}

				//console.log(customOpenN);
			}

		});
		fn.addEvent(fn.getById('custom_btn_default'),'click',function(e){
			var e = e || window.event;
			dragging = false;
			customing = true;
			if (e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
			_sortItems(defaultIndex,true);
			sData.write('customNavIndex',defaultIndex);
			_resetInactive(defaultIndex);
			customNavData.setData('filled', dl);
		});
		fn.addEvent(doms.nav_w,'mouseover',function(){
			if(!customing){
				showSwitch();
			}else{
				hideSwitch();
			}
		});
		fn.addEvent(doms.nav_w,'mouseout',function(){
			hideSwitch();
		});
		fn.addEvent(fn.getById('custom_btn_closeina'), 'click', function (e) {
			var e = e || window.event;
			dragging = false;
			if (e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
			fn.removeClass(doms.nav_w, 'custom_nav_customing');
			doms.inactive_w.style.display = 'none';
			if(firstGuide && sData.read('customNavIndex') == defaultIndex){
				dragging = false;
				customing = false;
				showSwitch();
				fn.removeClass(doms.nav_w, 'custom_nav_customing');
				doms.inactive_w.style.display = 'none';

				doms.nav_w.style.display = 'none';
				fn.getById(logonav).style.display = 'block';
				//fn.deleteCookie('customNav_customed');
				sData.write('customNav_customed','', 24 * 365);
				customOpenO = customOpenN;
				customOpenN = false;
				if(customOpenO != customOpenN){
					try{
						_S_uaTrack("navcustom","old");
					}catch(e){}

					//console.log(customOpenN);
				}
				return;
			}
			customing = false;
			showSwitch();
			customOpenO = customOpenN;
			customOpenN = true;
			_sortItems(sData.read('customNavIndex'),true);
			if(customOpenO != customOpenN){
				try{
					_S_uaTrack("navcustom","customed");
				}catch(e){}

				//console.log(customOpenN);
			}
		});
		fn.addEvent(doms.nav_c_ul, 'click', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			if (e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
			if(target == doms.btn_set){
				if(!customing){
					dragging = false;
					fn.addClass(doms.nav_w, 'custom_nav_customing');
					// fn.setStyle(doms.inactive_w, 'opacity', 0);
					doms.inactive_w.style.display = 'block';
					var h = doms.inactive_w.offsetHeight;
					// fn.opacityTo(doms.inactive_w, 1);
					fn.getById('custom_inactive_w').style.overflow = 'hidden';
					fn.actTo(fn.getById('custom_inactive_w').style, 'height', 0, h, 100, function(){fn.getById('custom_inactive_w').style.overflow = 'visible'; fn.getById('custom_inactive_w').style.height = 'auto';}, 'px');
					try{
						custom_subshow.select(0);
					}catch(e){}
				}
				else{
					dragging = true;
					fn.removeClass(doms.nav_w, 'custom_nav_customing');
					doms.inactive_w.style.display = 'none';
				}
				customing = !customing;
				if(customing){					
					hideSwtich();
				}else{
					hideSwitch();
				}
			}else if(target.nodeName.toLowerCase() == "a" && (!target.parentNode.getAttribute('id') || target.parentNode.getAttribute('id') != 'btn_plus')){
				if(!customing && customNavData.dataBase.navItems[target.getAttribute("data-ci")]){
					location.href = customNavData.dataBase.navItems[target.getAttribute("data-ci")].url;
				}
			}
		});
		fn.addEvent(doms.nav_c_ul, 'mouseover', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			// _toHideSet && clearTimeout(_toHideSet);
			// doms.btn_set.style.display = 'block';
			_toHideSwitch && clearTimeout(_toHideSwitch);
			if(!customing && !switchMoving && !switchShow){
				switchMoving = true;
				switchMoving = false; switchShow = true;
			}
			if(!customing) return;
			if(dragging || dragging2) return;
			if(target.tagName == 'A' && !fn.hasClass(target.parentNode, 'custom_item_blank') && !fn.hasClass(target, 'custom_btn_set')){
				_toHideCover && clearTimeout(_toHideCover);
				doms.item_cover.related = target;
				_showItemCover(target);
			}
		});
		fn.addEvent(doms.nav_c_ul, 'mouseout', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			// _toHideSet = setTimeout(function(){
			// 	doms.btn_set.style.display = 'none';
			// }, 200);
			if(!customing && switchShow){
				_toHideSwitch = setTimeout(function(){
					switchMoving = true;
					switchMoving = false; switchShow = false;
				})
			}
			if(!customing) return;
			if(target.tagName == 'A' && !fn.hasClass(target.parentNode, 'custom_item_blank') && !fn.hasClass(target, 'custom_btn_set')){
				//_toHideCover = setTimeout(function(){
					_hideItemCover();
				//}, 200);
			}
		});
		fn.addEvent(doms.item_cover, 'mouseover', function (e) {
			if(!customing) return;
			_toHideCover && clearTimeout(_toHideCover);
			_showItemCover(doms.item_cover.related)
		});
		fn.addEvent(doms.item_cover, 'mouseout', function (e) {
			if(!customing) return;
			//_toHideCover = setTimeout(function(){
				_hideItemCover();
			//}, 200);
		});
		fn.addEvent(doms.item_cover_delete, 'click', function (e) {
			if (e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
			//if exist a copy in inactive_c
			removeNav(doms.item_cover.related.getAttribute('data-ci'));
			_hideItemCover();
		});
		fn.addEvent(doms.inactive_c, 'mouseover', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement,
				ul, ulPos, wPos;
			if(!customing) return;
			if(target.tagName == 'A'){
				if(fn.hasClass(target.parentNode, 'selected')){
					_toHide && clearTimeout(_toHide);
					_showTips(target, 'toAbort');
				}
				//change to ready
				else{
					//if(!fn.hasClass(target.parentNode, 'chosen') || !dragging2) target.parentNode.className += ' chosen';
					//init drag
					if(dragging2) return;	
					ul = doms.nav_c_ul,
					ulPos = fn.getPosition(ul),
					wPos = fn.getPosition(doms.inactive_w);
					dragObj2 = new Drag({
						objDom : target,
						handle : null,
						activated : true,
						range : null,
						beforeDrag: function(conf){
							//fn.removeClass(target.parentNode, 'chosen');
							fn.addClass(target.parentNode, 'drag');
							target.lt = fn.getStyle(target, 'left');
							target.tp = fn.getStyle(target, 'top');
							target.style.zIndex = '10000';
							doms.inactive_w.style.zIndex = '1005';
							//console.log('beforeDrag');
						},
						onDraging: function (conf) {
							dragging2 = true;
							tempSwitch = false;
							_previewFromS(target, _checkPos(null, null, conf.mouseX, conf.mouseY))
						},
						afterDrag: function(conf){
							// console.log('afterDrag');
							dragging2 = false;
							_moveToFromS(target, _checkPos(null, null, conf.mouseX, conf.mouseY));
							//setTimeout(function(){dragging2 = false;}, 200);
							dragObj2.setDragAble(false);
							doms.inactive_w.style.zIndex = '15';
							target.style.zIndex = '';
						}
					});
				}
			}
		});
		fn.addEvent(doms.inactive_c, 'mouseout', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			if(!customing) return;
			if(target.tagName == 'A'){
				if(fn.hasClass(target.parentNode, 'selected')){
					_toHide = setTimeout(function(){
						_hideTips();
					}, 200);
				}
			}
		});
		fn.addEvent(doms.tips, 'mouseover', function (e) {
			if(!customing) return;
			_toHide && clearTimeout(_toHide);
		});
		fn.addEvent(doms.tips, 'mouseout', function (e) {
			if(!customing) return;
			_toHide = setTimeout(function(){
				_hideTips();
			}, 200);
		});
		//tips
		fn.addEvent(doms.tips, 'click', function (e) {
			if(!customing) return;
			if (e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
			if(doms.tips.type == 'toAbort'){
				removeNav(doms.tips.related.getAttribute('data-ci'));
				_hideTips();
			}
		});
		fn.addEvent(doms.inactive_c, 'click', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;

			if(e.stopPropagation){
				e.stopPropagation();
			}else{
				e.cancelBubble = true;
			}

			if(!customing) return;
			if(dragging2) return;
			if (!target.tagName) {
				target = target.parentNode;
			}
			if(target.tagName == 'A'){
				if(fn.hasClass(target.parentNode, 'selected')){
					if(!tempSwitch){
						tempSwitch = true;
						return;
					}
					removeNav(target.getAttribute('data-ci'));	
					_hideTips();
				}
				else{
					if(!tempSwitch){
						tempSwitch = true;
						return;
					}
					if(customNavData.getData('filled') == customNavData.getData('total')){
						alert('对不起，您的定制已满，如需继续定制，请删除不常用定制项');
						return;
					}
					addNav(target.getAttribute('data-ci'), target);
				}
			}
		});
		//init drag of nav
		fn.addEvent(doms.nav_c_ul, 'mouseover', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			if(!customing){
				doms.plus.style.display = 'block';
				return;
			}
			if(dragging || dragging2) return;
			if(target.tagName == 'A' && !fn.hasClass(target.parentNode, 'custom_item_blank') && !fn.hasClass(target, 'custom_btn_set')){
				dragObj = new Drag({
					objDom : target,
					handle : null,
					activated : true,
					range : null,
					beforeDrag: function(conf){
						target.lt = fn.getStyle(target, 'left');
						target.tp = fn.getStyle(target, 'top');
						doms.nav.style.zIndex = 1001;
						//console.log('beforeDrag');
					},
					onDraging: function (conf) {
						dragging = true;
						//console.log(conf.x, conf.y, conf.mouseX, conf.mouseY);
						_hideItemCover();
						_preview(target, _checkPos(conf.x, conf.y, conf.mouseX, conf.mouseY), false);
					},
					afterDrag: function(conf){
						_preview(target, _checkPos(conf.x, conf.y, conf.mouseX, conf.mouseY), true);
						_moveTo(target, _checkPos(conf.x, conf.y, conf.mouseX, conf.mouseY), conf.x, conf.y);
						dragging = false;
						doms.nav.style.zIndex = 1000;
					}
				});
			}
		});
		fn.addEvent(doms.plus,'mouseover',function(e){
			if(!customing){
				doms.plus.style.display = 'block';
			}else{
				doms.plus.style.display = 'none';
			}
		});
		fn.addEvent(doms.nav_c_ul, 'mouseout', function (e) {
			var e = e || window.event,
				target = e.target || e.srcElement;
			// if(!customing) return;
			doms.plus.style.display = 'none';
			if(dragging || dragging2) return;
			if(target.tagName == 'A' && !fn.hasClass(target, 'custom_item_blank') && !fn.hasClass(target, 'custom_btn_set')){
				// var li = target.parentNode;
				if(dragObj) dragObj.setDragAble(false);
				//dragObj = null;
				//_hideItemCover();
			}
		});
		fn.addEvent(doms.plus, 'click', function (e) {
			switchMoving = false;
			switchShow = false;
			if (e.preventDefault) {
				e.preventDefault();
				e.stopPropagation();
			} else {
				e.returnValue = false;
				e.cancelBubble = true;
			}
			initData = sData.read('customNavIndex');
			doms.nav_w.style.display = 'block';
			doms.plus.style.display = 'none';
			fn.getById(logonav).style.display = 'none';
			//fn.setCookie('customNav_customed', 'true');
			sData.write('customNav_customed', 'true', 24 * 365);
			_sortItems(sData.read('customNavIndex'),false);
			dragging = false;
			dragging2 = false;
			customing = true;
			hideSwitch();
			fn.addClass(doms.nav_w, 'custom_nav_customing');
			// fn.setStyle(doms.inactive_w, 'opacity', 0);
			doms.inactive_w.style.display = 'block';
			//fn.getById('custom_inactive_w').style.overflow = 'hidden';
			//fn.actTo(fn.getById('custom_inactive_w').style, 'height', 0, 832, 100, function(){fn.getById('custom_inactive_w').style.overflow = 'visible'; fn.getById('custom_inactive_w').style.height = 'auto';}, 'px');

			try{
				custom_subshow.select(0);
			}catch(e){}
		});
	}
	function _checkPos (x, y, mX, mY) {
		var ul = doms.nav_c_ul,
			ulPos = fn.getPosition(ul),
			ulw = 85 * 11,
			ulh = 38;
		mX = mX - ulPos.x;
		mY = mY - ulPos.y;
		return [Math.floor(mX/85), Math.floor(mY/38)];
	}
	function _preview (ele, pos, ed) {
		var ul = doms.nav_c_ul,
			liList = ul.getElementsByTagName('li'), liListLen = liList.length,
			index, li, thisLi;
		index = pos[0] + pos[1] * 11;
		thisLi = ele.parentNode;
		if(pos[0] < 0 || pos[1] < 0 || pos[0] > 10 || pos[1] > 1 || index > 21) {
			if(_preview.box && _preview.box.parentNode){
				ul.removeChild(_preview.box);
				liList[liListLen - 1].style.display = 'inline';
			}
			return;
		};
		li = liList[index];
		for(var i = 0; i < liListLen; i++){
			if(fn.hasClass(liList[i], 'custom_items_dragging_over')){
				fn.removeClass(liList[i], 'custom_items_dragging_over');
			}
		}
		if(!fn.hasClass(li, 'custom_items_dragging_over')){
			li.className += ' custom_items_dragging_over';
		}
		// if(thisLi != li && !fn.hasClass(li, 'custom_items_added')){
		// 	li.className += ' custom_items_added';
		// }
		//switch current li
		if(!fn.hasClass(thisLi, 'custom_items_dragging')){
			thisLi.className += ' custom_items_dragging';
		}
		if(!_preview.box){
			_preview.box = document.createElement('div');
			_preview.box.className = 'custom_items_coverred';
		}
		if(thisLi != li){
			if(ed){
				fn.swapNode(thisLi,li);
				if(fn.hasClass(li,'custom_item_blank')){
					var rIndex = _getIndex(li);
					var row = (rIndex < 11) ? 1 : 2;
					var bItem = document.createElement("li");
					var lineLast;
					//var Item = document.createElement('li');
					bItem.className = "custom_item_blank";
					bItem.innerHTML = '<a href="#"></a>';
					if(row == 1){
						lineLast = ul.getElementsByTagName('li')[11];
						ul.removeChild(li);
						ul.insertBefore(bItem,lineLast);
					}else{
						ul.removeChild(li);
						ul.appendChild(bItem);
					}
				}
				var tIndex = _getIndex(thisLi);
				var tR = tIndex < 11 ? 1 : 2;
				var nLi = ul.getElementsByTagName('li');
				if(tR == 1){
					for(var i = 0; i < 11; i++){
						if(fn.hasClass(nLi[i],'custom_item_blank') && tIndex > _getIndex(nLi[i])){
							fn.swapNode(thisLi,nLi[i]);
							break;
						}
					}
				}else{
					for(var i = 11; i < 22; i++){
						if(fn.hasClass(nLi[i],'custom_item_blank') && tIndex > _getIndex(nLi[i])){
							fn.swapNode(thisLi,nLi[i]);
							break;
						}
					}
				}
			}
		}
		else{
			if(_preview.box.parentNode){
				ul.removeChild(_preview.box);
				liList[liListLen - 1].style.display = 'inline';
			}
		}
	}
	function _previewFromS (ele, pos) {
		var index, li, ul;
		index = pos[0] + pos[1] * 11,
		ul = doms.nav_c_ul;
		if(pos[0] < 0 || pos[1] < 0 || pos[0] > 10 || pos[1] > 1 || index > 21) {
			return;
		};
		if(!_previewFromS.box){
			_previewFromS.box = document.createElement('div');
			_previewFromS.box.className = 'custom_items_coverred';
		}
		li = ul.getElementsByTagName('li')[index];
		if(_previewFromS.cur != li){
			if(_previewFromS.cur){
				_previewFromS.cur.style.display = 'inline';
			}
			li.parentNode.insertBefore(_previewFromS.box, li)
			li.style.display = 'none';
		}
		_previewFromS.cur = li;
	}
	function _moveTo (ele, pos, cX, cY) {
		var ul = doms.nav_c_ul,
			liList = ul.getElementsByTagName('li'), liListLen = liList.length;
		// console.log(pos)
		var index = pos[0] + pos[1] * 11,
			li, thisLi;
		for(var i = 0; i < liListLen; i++){
			if(fn.hasClass(liList[i], 'custom_items_dragging_over')){
				fn.removeClass(liList[i], 'custom_items_dragging_over');
			}
		}
		//if out of range, delete this item
		if(pos[0] < 0 || pos[1] < 0 || pos[0] > 10 || pos[1] > 1 || index > 21){
			removeNav(ele.getAttribute('data-ci'));
		}
		//if in range, move to it
		else{
			li = doms.nav_c.getElementsByTagName('li')[index];
			thisLi = ele.parentNode;
			//console.log(li == thisLi);
			// if(thisLi == li){
			fn.removeClass(thisLi, 'custom_items_dragging');
			ele.style.left = "auto";
			ele.style.top = "auto";
			if(_preview && _preview.box && _preview.box.parentNode){
				doms.nav_c_ul.removeChild(_preview.box);
			}
			//reset storage
			_upDateStorage();

		}
		ele.style.position = 'static';
		_upDateStorage();
		//doms.nav_c.getElementsByTagName('li')[doms.nav_c.getElementsByTagName('li').length - 1].style.display = 'block';
	}
	function _moveToFromS (ele, pos) {
		var index = pos[0] + pos[1] * 11,
			ul = doms.nav_c_ul, cI, oI, aLi, oLi;
		//if out of range, recover
		if(pos[0] < 0 || pos[1] < 0 || pos[0] > 10 || pos[1] > 1 || index > 21){
			fn.removeClass(ele.parentNode, 'drag');
			ele.style.position = 'static';
			ele.style.left = 'auto';
			ele.style.top = 'auto';
			if(_previewFromS.cur){
				ul.removeChild(_previewFromS.box);
				_previewFromS.cur.style.display = 'block';
			}
		}
		else{
			cI = ele.getAttribute('data-ci');
			aLi = document.createElement('li');
			var text = ele.innerHTML;
			aLi.innerHTML = '<a href="#" data-ci="' + cI + '" title="' + ele.innerHTML + '">' +  text + '</a>';
			if(_previewFromS.cur.getElementsByTagName('a')[0].getAttribute('data-ci')){
				oI = _previewFromS.cur.getElementsByTagName('a')[0].getAttribute('data-ci');
				replaceNav(oI, aLi);
			}else{
				replaceBlank(cI, aLi);
				customNavData.setData('filled', customNavData.getData('filled') + 1);
			}
			ul.removeChild(_previewFromS.box);
			fn.removeClass(ele.parentNode, 'drag');
			ele.style.position = 'static';
			ele.style.left = 'auto';
			ele.style.top = 'auto';
		}
		_previewFromS.cur = _previewFromS.box = null;
	}
	function _animateTo () {

	}
	function _getIndex (ele) {
		var ul = ele.parentNode,
			counter = 0,
			liList = ul.getElementsByTagName('li'),
			liListLen = liList.length;
		for (var i = 0; i < liListLen; i++) {
			if(ele == liList[i]){
				return counter;
				break;
			}
			counter++;
		};
		return false;
	}
	function _getIndexFromCI (cI){
		var ul = doms.nav_c_ul,
			liList = ul.getElementsByTagName('li'),
			liListLen = liList.length;
		for (var i = 0; i < liListLen; i++) {
			if(cI == liList[i].getElementsByTagName('a')[0].getAttribute('data-ci')){
				return i;
			}
		};
		return false;
	}
	function _createTips(){
		var objW = document.createElement('div'),
			obj = document.createElement('div'),
			insertRefer = document.body.getElementsByTagName('div')[0];
		objW.id = objW.className = 'custom_item_tip_w';
		obj.className = 'custom_item_tip';
		objW.style.display = 'none';
		objW.appendChild(obj);
		document.body.insertBefore(objW, insertRefer);
	}
	function _createCover() {
		var objW = document.createElement('div'),
			obj = document.createElement('a'),
			insertRefer = document.body.getElementsByTagName('div')[0];
		objW.id = objW.className = 'custom_item_cover';
		objW.style.display = 'none';
		obj.id = obj.className = 'custom_item_delete';
		objW.appendChild(obj);
		document.body.insertBefore(objW, insertRefer);
	}
	function fixSwitch() {
		if(!sData.read('custom_switchClosed')){
			fn.addClass(doms.btn_switch_w,'custom_btn_switch_w_hanging');
			doms.btn_switch.className = 'custom_btn_switch2';
			var span = document.createElement('span');
			span.className = 'custom_btn_switch2_span';
			span.id = 'custom_btn_switch2_span';
			doms.btn_switch_w.appendChild(span);
			doms.btn_switch_w.style.display = 'block';
		}
	}
	function hideSwitch(){
		if(customing || !fn.hasClass(doms.btn_switch_w,'custom_btn_switch_w_hanging')){
			doms.btn_switch_w.style.display = 'none';
		}
	}
	function showSwitch(){
		//if(!fn.hasClass(doms.btn_switch_w,'custom_btn_switch_w_hanging')){
			doms.btn_switch_w.style.display = 'block';
		//}
	}
	function _createPlus(){
		var span = document.createElement('span');
		fn.addClass(span,'custom_plus');
		doms.plus = span;
		document.body.insertBefore(span,document.body.getElementsByTagName('div')[0]);
	}
	function _showTips (relatedElement, type) {
		var pos = fn.getPosition(relatedElement);
		doms.tips.related = relatedElement;
		switch(type){
			case 'toAbort':
				doms.tips.type = 'toAbort';
				doms.tips.getElementsByTagName('div')[0].innerHTML = '点击取消添加';
				break;
		}
		doms.tips.style.left = pos.x + 'px';
		doms.tips.style.top = pos.y - 33 + 'px';
		doms.tips.style.display = 'block';
	}
	function _hideTips (type) {
		doms.tips.style.display = 'none';
	}
	function _showItemCover (relatedElement) {
		var pos = fn.getPosition(relatedElement);
		var relatedLi = relatedElement.parentNode;
		if(!fn.hasClass(relatedElement,'hover')){
			fn.addClass(relatedElement,'hover');
		}
		doms.item_cover.related = relatedElement;
		doms.item_cover.style.left = pos.x + 1 + 'px';
		doms.item_cover.style.top = pos.y + 'px'; 
		doms.item_cover.style.display = 'block';
	/* 	relatedElement.style.border = '1px solid #999';
		relatedElement.style.background = 'url(http://i1.sinaimg.cn/edu/deco/2012/0907/img/images/bg_xb_del_hover.png) no-repeat 3px center #fff' */
	}
	function _hideItemCover (target) {
		var rel = doms.item_cover.related;
		doms.item_cover.style.display = 'none';
		if(fn.hasClass(rel,'hover')){
			fn.removeClass(rel,'hover');
		}
		/* if(target){
		target.style.border = '1px solid #dedede';
		target.style.background = 'url(http://i1.sinaimg.cn/edu/deco/2012/0907/img/images/bg_xb_del.png) no-repeat 3px center #fff';
		} */
	}
	function _fullFilled () {
		var items = doms.nav_c.getElementsByTagName('li'),
			itemsLen = items.length,
			counter = 0;
		for(var i=0; i<itemsLen; i++){
			if(items[i].getElementsByTagName('a')[0].getAttribute('data-ci') != null){
				counter++;
			}
		}
		if(counter == itemsLen) return true;
		return false;
	}
	function _sortItems (str,boo) {
		var ul = doms.nav_c_ul,
			aLi,
			ciList = str.split(','),
			//ciListLen = ciList.length,
			ciListLen = 22,
			lastLi,
			navItem,
			nName;
		ul.innerHTML = '';
		if(boo){
			for(var i = 0; i < 11; i ++){
				aLi = document.createElement('li');
				if(ciList[i] == 'blank' || typeof(ciList[i]) == 'undefined'){
					aLi.className = 'custom_item_blank';
					aLi.innerHTML = '<a href="#"></a>';	
				}else{
					navItem = customNavData.getDataBase().navItems[ciList[i]];
					if(typeof(navItem) != 'undefined'){
						nName = navItem['nName'];
						aLi.innerHTML = '<a href="' + navItem['url'] + '" data-ci="' + ciList[i] + '">' + nName + '</a>';
					}else{
						aLi.className = 'custom_item_blank';
						aLi.innerHTML = '<a href="#"></a>';	
					}
				}
				ul.appendChild(aLi);				
			}
			if(!(ciList[11] == 'blank' || typeof(ciList[i]) == 'undefined' || typeof(customNavData.getDataBase().navItems[ciList[11]]) == 'undefined')){
				for(var i = 11; i < 22; i ++){
					aLi = document.createElement('li');
					if(ciList[i] == 'blank' || typeof(ciList[i]) == 'undefined'){
						aLi.className = 'custom_item_blank';
						aLi.innerHTML = '<a href="#"></a>';	
					}else{
						navItem = customNavData.getDataBase().navItems[ciList[i]];
						if(typeof(navItem) != 'undefined'){
							nName = navItem['nName'];
							aLi.innerHTML = '<a href="' + navItem['url'] + '" data-ci="' + ciList[i] + '">' + nName + '</a>';
						}else{
							aLi.className = 'custom_item_blank';
							aLi.innerHTML = '<a href="#"></a>';	
						}
					}
					ul.appendChild(aLi);				
				}
			}

			fn.getById('custom_nav').style.display = 'block';

			//show the plus button
			showPlus();
		}else{
			for(var i = 0; i < ciListLen; i ++){
				aLi = document.createElement('li');
				if(ciList[i] == 'blank' || typeof(ciList[i]) == 'undefined'){
					aLi.className = 'custom_item_blank';
					aLi.innerHTML = '<a href="#"></a>';	
				}else{
					navItem = customNavData.getDataBase().navItems[ciList[i]];
					if(typeof(navItem) != 'undefined'){
						nName = navItem['nName'];
						var text = nName;
						aLi.innerHTML = '<a href="#" data-ci="' + ciList[i] + '" title="' + nName + '">' + text + '</a>';
					}else{
						aLi.className = 'custom_item_blank';
						aLi.innerHTML = '<a href="#"></a>';	
					}
				}
				ul.appendChild(aLi);
			}
			fn.getById('custom_nav').style.display = 'block';
		}
	}
	function showPlus(){
		var plusRelated;
			//first line
		var lis = doms.nav_c_ul.getElementsByTagName('li');
		var lislen = lis.length;
		for(var i = 0; i < lislen; i++){
			if(fn.hasClass(lis[i],'custom_item_blank')){
				plusRelated = lis[i];
				break;
			}
		}
		if(plusRelated && sData.read('customNav_customed')){
			setTimeout(function(){
				var p = fn.getPosition(plusRelated);
				doms.plus.style.left = (p.x + 13) + 'px';
				doms.plus.style.top = (p.y - 1) + 'px';
			},200);				
		}else{
			doms.plus.style.left = '-2999px';
		}
	}
	function _updateInactive (cI, toActive) {
		var inactive_c = doms.inactive_c,
			itemList = fn.getElementsByAttribute('data-ci', cI, inactive_c),
			itemListLen = itemList.length;
		for(var i = 0; i < itemListLen; i ++){
			toActive ? fn.removeClass(itemList[i].parentNode) : fn.addClass(itemList[i].parentNode, 'selected');
		}
	}
	function _resetInactive (str) {
		var inactive_c = doms.inactive_c,
			spans = inactive_c.getElementsByTagName('span'),
			spansLen = spans.length,
			ciList = str.split(','),
			ciListLen = ciList.length,
			aItem;
		for(var i = 0; i < spansLen; i ++){
			if(fn.hasClass(spans[i], 'selected')){
				fn.removeClass(spans[i]);
			}
		}
		for(var i = 0; i < ciListLen; i ++){
			_updateInactive(ciList[i], false);
		}
	}
	function _upDateStorage () {
		var updated = [],
			liList = doms.nav_c_ul.getElementsByTagName('li'), liListLen = liList.length,
			tLi, cId;
		for(var i = 0; i < 22; i ++){
			cId = liList[i].getElementsByTagName('a')[0].getAttribute('data-ci');
			cId ? updated.push(cId) : updated.push('blank');
		}
		customNavData.getDataBase().navIndex = updated;
		//sObj.setStorage('customNavIndex', customNavData.getDataBase().navIndex.join(','));
		sData.write('customNavIndex', customNavData.getDataBase().navIndex.join(','), 24 * 365);
                firstGuide = false;
		//console.log(customNavData.getDataBase().navIndex.join(','));
		//console.log(sData.read('customNavIndex'));
	}
	function addNav (cI) {
		var ul, itemStrW, itemStr, navName, domInactive, deletedBlank;
		ul = doms.nav_c_ul;
		domInactive = fn.getElementsByAttribute('data-ci', cI, doms.inactive_c)[0];
		var text = domInactive.innerHTML;
		itemStr = '<a href="#" data-ci="' + cI + '" title="' + domInactive.innerHTML + '">' + text + '</a>';
		itemStrW = document.createElement('li');
		itemStrW.innerHTML = itemStr;
		deletedBlank = fn.getElementsByClass('custom_item_blank')[0];
		//reset storage
		// customNavData.getDataBase().navIndex[_getIndex(deletedBlank)] = cI;

		ul.replaceChild(itemStrW, deletedBlank);
		/* itemStrW.style.background = 'url(http://i2.sinaimg.cn/book/2012/0919/U8921P112DT20120919140354.png) no-repeat 0 center'; */
		_updateInactive (cI, false);
		customNavData.setData('filled', customNavData.getData('filled') + 1);
		_upDateStorage();
	}
	function replaceNav (cI, ele_li) {
		var ul = doms.nav_c_ul,
			oLi = fn.getElementsByAttribute('data-ci', cI, ul)[0].parentNode,
			nCI = ele_li.getElementsByTagName('a')[0].getAttribute('data-ci');
		//reset storage
		// customNavData.getDataBase().navIndex[_getIndex(oLi)] = nCI;

		ul.insertBefore(ele_li, oLi);
		ul.removeChild(oLi);
		_updateInactive(cI, true);
		_updateInactive(nCI, false);
		_upDateStorage();
	}
	function replaceBlank (cI, ele) {
		var ul = doms.nav_c_ul,
			previousLi, index;
		previousLi = _previewFromS.box.previousSibling;
		if(_previewFromS.box.previousSibling){
			previousLi = _previewFromS.box.previousSibling;
			if(previousLi.tagName != 'li'){
				if(previousLi.previousSibling){
					previousLi = previousLi.previousSibling;
					index = _getIndex(previousLi) + 2;
				}
				else{
					index = 0;
				}
			}
		}
		else{
			index = 0;
		}
		ul.insertBefore(ele, _previewFromS.box);
		ul.removeChild(_previewFromS.cur);

		var eIndex = _getIndex(ele);
		var eLi = ul.getElementsByTagName('li');
		var teBlank, start, max;
		start = (eIndex < 11) ? 0 : 11;
		max = start + 11;
		for(var i = start; i < max; i++){
			teBlank = eLi[i];
			if(fn.hasClass(teBlank,'custom_item_blank') && _getIndex(teBlank) < _getIndex(ele)){
				fn.swapNode(teBlank,ele);
				break;
			}
		}

		_updateInactive(cI, false);
		//reset storage
		// customNavData.getDataBase().navIndex.splice(index, 1, cI);
		// customNavData.setData('filled', customNavData.getData('filled') + 1);
		_upDateStorage();

	}
	function removeNav (cI) {
		var ul = doms.nav_c_ul,
			row,
			lineLast,
			removedItem = fn.getElementsByAttribute('data-ci', cI, doms.nav_c)[0],
			removedLi = removedItem.parentNode,
			rIndex = _getIndex(removedLi);
			//item_blank_str = '<li class="custom_item_blank"><a href="#"></a></li>',
			//index = _getIndex(removedItem.parentNode);
		var bItem = document.createElement("li");
		bItem.className = "custom_item_blank";
		bItem.innerHTML = '<a href="#"></a>';
		//ul.replaceChild(bItem,removedItem.parentNode);
		row = (rIndex < 11) ? 1 : 2;
		if(row == 1){
			lineLast = ul.getElementsByTagName('li')[11];
			ul.removeChild(removedLi);
			ul.insertBefore(bItem,lineLast);
		}else{
			ul.removeChild(removedLi);
			ul.appendChild(bItem);
		}
		//reset storage
		// customNavData.getDataBase().navIndex.splice(index, 1);
		// customNavData.getDataBase().navIndex.push('blank');

		//ul.removeChild(removedItem.parentNode);
		//ul.innerHTML += item_blank_str;
		_updateInactive(cI, true);
		customNavData.setData('filled', customNavData.getData('filled') - 1);
		_upDateStorage();
	}
	//for test
/* 	function RS () {
		localStorage.removeItem('customNavIndex');
		localStorage.removeItem('custonNav_guided');
		fn.deleteCookie('customNav_customed');
	} */

	function setDatabase(){
		var inEle = doms.inactive_c;
		var spans = inEle.getElementsByTagName('span');
		var spansLen = spans.length;
		var a,ci,nName,url,icon;
		var ret = [];
		for(var i=0; i<spansLen; i++){
			a = spans[i].getElementsByTagName('a')[0];
			ci = a.getAttribute('data-ci');
			nName = a.innerHTML;
			url = a.href;
			icon = a.getAttribute('data-icon') || '';
			ret[ci] = {
				nName	: nName,
				url		: url,
				icon	: icon
			}
		}
		customNavData.setData('navItems',ret);
	}

	//user guide

	function doGuide(){
		var steps = [],
			fragments = [],
			stepIndex = 0,
			sI = 0,
			//guided = new Storage(),
			ss,
			nstep = {
				guideStr: '<div class="custom_nav_guide0_nar" id="custom_nav_guide0"></div><div class="custom_nav_guide0_cancel_nar" id="custom_nav_guide0_cancel"></div><div class="custom_nav_guide0_continue_nar" id="custom_nav_guide0_continue"></div>',
				guidePos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 617,
					y: 20
				},
				bgStr: '<div class="custom_nav_guide0_bg_nar"></div>',
				bgPos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 0,
					y: 32
				}
			};
		guiding = true;
		doms.nav_w.style.display = 'none';
		fn.getById(logonav).style.display = 'block';
		_initStep();
		_initGuide();
		_initGuideEvent();
		showGuide(0);
		function _initStep () {
			steps.push({
				guideStr: '<div class="custom_nav_guide0" id="custom_nav_guide0"></div><div class="custom_nav_guide0_cancel" id="custom_nav_guide0_cancel"></div><div class="custom_nav_guide0_continue" id="custom_nav_guide0_continue"></div>',
				guidePos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 617,
					y: 20
				},
				bgStr: '<div class="custom_nav_guide0_bg"></div>',
				bgPos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 0,
					y: 32
				}
			});
			steps.push({
				guideStr: '<div class="custom_nav_guide1" id="custom_nav_guide1"></div><div class="custom_nav_guide1_cancel" id="custom_nav_guide1_cancel"></div><div class="custom_nav_guide1_continue" id="custom_nav_guide1_continue"></div>',
				guidePos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 157,
					y: 24
				},
				bgStr: '<div class="custom_nav_guide1_bg"></div>',
				bgPos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 0,
					y: 36
				}
			});
			steps.push({
				guideStr: '<div class="custom_nav_guide2" id="custom_nav_guide2"></div><div class="custom_nav_guide2_cancel" id="custom_nav_guide2_cancel"></div><div class="custom_nav_guide2_continue" id="custom_nav_guide2_continue"></div>',
				guidePos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 468,
					y: 260
				},
				bgStr: '<div class="custom_nav_guide2_bg"></div>',
				bgPos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 0,
					y: 36
				}
			});
			steps.push({
				guideStr: '<div class="custom_nav_guide3" id="custom_nav_guide3"></div><div class="custom_nav_guide3_cancel" id="custom_nav_guide3_cancel"></div><div class="custom_nav_guide3_continue" id="custom_nav_guide3_continue"></div>',
				guidePos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 200,
					y: 24
				},
				bgStr: '<div class="custom_nav_guide3_bg"></div>',
				bgPos: {
					relatedDom: fn.getById(standardHeaderChannel),
					x: 0,
					y: 36
				}
			});
		}
		function _initGuide () {
			var stepsLen = steps.length,
				fragmentItem;
			if(fragments.length) return;
			for(var i = 0; i < stepsLen; i ++){
				fragmentItem = {
					fGuide: null,
					fBg: null
				}
				fragmentItem.fGuide = document.createElement('div');
				fragmentItem.fGuide.className = 'guide' + i +'_w';
				fragmentItem.fBg = document.createElement('div');
				fragmentItem.fBg.className = 'guide' + i +'_bg_w';
				fragmentItem.fGuide.style.display = fragmentItem.fBg.style.display = 'none';
				fragments.push(fragmentItem);
			}
		}
		function _clearGuide (sI) {
			var cf;
			if(sI < 0) return;
			cf = fragments[sI];
			document.body.removeChild(cf.fGuide);	
			document.body.removeChild(cf.fBg);
		}
		function _initGuideEvent () {
			fn.addEvent(document, 'click', doStep);
			fn.addEvent(window,'resize',doFixGuide);
		}
		function doStep(e){
			var e = e || window.event,
				target = e.target || e.srcElement,
				tCN = target.className,
				index;
			if(target.tagName == 'DIV' && tCN.indexOf('custom_nav_guide') > -1){		
				if(e.stopPropagation){
					e.stopPropagation();
				}else{
					e.cancelBubble = true;
				}
				if(tCN.indexOf('cancel') > -1){
					hideGuide();
				}else{
					tCN = tCN.replace('custom_nav_guide', '');
					index = parseInt(tCN.match(/\d/)[0], 10);
					showGuide(index + 1);
				}
			}
		}
		function doFixGuide(){
			if(document.body.offsetWidth <= 1024){
				narrowMode = true;
			}else{
				narrowMode = false;
			}
			if(guiding){
				_clearGuide(stepIndex);
				fixGuide(stepIndex);
			}
		}
		function showGuide (sI) {
			var cf = fragments[sI],
				cs = steps[sI],
				ele_guide, ele_bg,
				relPos_guide, relPos_bg,
				fPosY, tPosY;
			if(!cs){
				hideGuide();
				switchMoving = false;
				switchShow = false;

				_sortItems(customNavData.getDataBase().navIndex.join(','),false);

				doms.nav_w.style.display = 'block';
				fn.getById(logonav).style.display = 'none';
				//fn.setCookie('customNav_customed', 'true');
				sData.write('customNav_customed', 'true', 24 * 365);

				dragging = false;
				dragging2 = false;
				customing = true;
				hideSwitch();
				fn.addClass(doms.nav_w, 'custom_nav_customing');
				// fn.setStyle(doms.inactive_w, 'opacity', 0);
				doms.inactive_w.style.display = 'block';
				var h = fn.getById('custom_inactive_w').offsetHeight;
				fn.getById('custom_inactive_w').style.overflow = 'hidden';
				fn.actTo(fn.getById('custom_inactive_w').style, 'height', 0, h, 100, function(){fn.getById('custom_inactive_w').style.overflow = 'visible'; fn.getById('custom_inactive_w').style.height = 'auto';}, 'px');
				try{
					custom_subshow.select(0);
				}catch(e){}
				return;
			}
			_clearGuide(sI - 1);
			fixGuide(sI);		

			coverLayer2.on();

			//fn.windowScrollTo(fPosY, tPosY, 30, null, '');

			stepIndex = sI;
		}
		function fixGuide(sI){
			var cf = fragments[sI],
				cs = steps[sI];
			var	relPos_guide = fn.getPosition(cs.guidePos.relatedDom),
				relPos_bg = fn.getPosition(cs.bgPos.relatedDom),
				fPosY = document.body.scrollTop + document.documentElement.scrollTop,
				tPosY = relPos_guide.y + cs.guidePos.y - 100;
			cf.fGuide.innerHTML = cs.guideStr;
			cf.fBg.innerHTML = cs.bgStr;
			cf.fGuide.style.top = relPos_guide.y + cs.guidePos.y + 'px';
			cf.fGuide.style.left = relPos_guide.x + cs.guidePos.x + 'px';

			var brows = fn.getBrowserInfo();
			if(sI == 0 && brows.browser == 'msie' && brows.version == '6'){
				cf.fGuide.style.left = relPos_guide.x + cs.guidePos.x - 30 + 'px';
			}
			cf.fBg.style.top = relPos_bg.y + cs.bgPos.y + 'px';
			cf.fBg.style.left = relPos_bg.x + cs.bgPos.x + 'px';
			document.body.insertBefore(cf.fGuide, document.body.getElementsByTagName('div')[0]);
			document.body.insertBefore(cf.fBg, document.body.getElementsByTagName('div')[0]);

			if(narrowMode && sI == 0){
				if(!fn.hasClass(cf.fGuide,'guide0_w_nar')){
					fn.addClass(cf.fGuide,'guide0_w_nar');
					fn.addClass(cf.fBg,'guide0_bg_w_nar');
				}
			}else{
				if(fn.hasClass(cf.fGuide,'guide0_w_nar')){
					fn.removeClass(cf.fGuide,'guide0_w_nar');
					fn.removeClass(cf.fBg,'guide0_bg_w_nar');
				}
			}

			cf.fGuide.style.display = cf.fBg.style.display = 'block';
		}
		function hideGuide () {
			var fPosY = document.body.scrollTop + document.documentElement.scrollTop;
			_clearGuide(stepIndex);
			coverLayer2.off();
			if(sData.read('customNav_customed')){
				doms.nav_w.style.display = 'block';
				fn.getById(logonav).style.display = 'none';
			}else{
				doms.nav_w.style.display = 'none';
				fn.getById(logonav).style.display = 'block';
			}
			window.scrollTo(0, 0);
			sData.write(storageName, 'done', 24 * 365);
			guiding = false;
			fn.removeEvent(window,'resize',doFixGuide);
			fn.removeEvent(document, 'click', doStep);
		}
	}

};

</script>
<script>
sinaService.listen('dataready', function(){
	insertCustomNav("blk_cheadNav_01","blk_cheadTopbar_01",'','custom_guide','channelHead');
});
</script>
<script type="text/javascript">
	(function(index){
		//var index = 0;
		var guide = document.getElementById('move_guide'),
			move_tipBox, move_samplePic, move_guide_try, move_guide_known, move_guide_btnBox,
			move_intoGuide_Box, move_intoGuide_sample, move_intoGuide_try, move_intoGuide_known, move_intoGuide_btnBox;
		guide.onclick = function(){
			if(move_intoGuide_Box) _showIntoGuide(guide);
			else _initIntoGuide();
			//if(move_tipBox) _showGuide(index);
			//else setTimeout(function(){_initGuide();}, 50);
		}
		function _savePics(urls){
			for(var i = 0; i < urls.length; i++){
				new Image().src = urls[i];
			}
		}
		function move_checkGuide(){
			guide.style.display = 'block';
			_savePics(['http://i3.sinaimg.cn/book/deco/2012/0723/tips0.png', 'http://i3.sinaimg.cn/book/deco/2012/0723/tips.png', 'http://i3.sinaimg.cn/book/deco/2012/0723/move_arrs02.png']);
                        if(!_getStorage('blks_book_guided') && !/\((iPhone|iPad|iPod)/i.test(navigator.userAgent)){
				_setStorage();
				_initIntoGuide();
			}
		}
		function _initIntoGuide(){
			var intoGuideBox = document.createElement('div'),
				intoSample = document.createElement('div'),
				intoBtnBox = document.createElement('div');
			intoGuideBox.className = 'move_intoguide_box';
			document.body.appendChild(intoGuideBox);
			intoSample.className = 'mobe_intoguide_sample';
			intoSample.innerHTML = '<div class="move_guide">定制功能介绍</div>';
			document.body.appendChild(intoSample);
			intoBtnBox.className = 'move_intoguide_btnBox';
			intoBtnBox.innerHTML = '<a href="javascript:void(0)" class="move_intoguide_try" onfocus="this.blur();"></a><a href="javascript:void(0)" class="move_intoguide_known" onfocus="this.blur();"></a>';
			move_intoGuide_Box = intoGuideBox;
			move_intoGuide_sample = intoSample;
			move_intoGuide_btnBox = intoBtnBox;
			document.body.appendChild(move_intoGuide_btnBox);
			move_intoGuide_btnBox.onclick = function(e){
				var evt = e || window.event,
					eTarget = evt.target || evt.srcElement,
					theClass = eTarget.className;
				switch(theClass){
					case 'move_intoguide_try':
						_hideIntoGuide();
						if(move_tipBox) _showGuide(index);
						else setTimeout(function(){_initGuide();}, 50);
						break;
					case 'move_intoguide_known':
						_hideIntoGuide();
						window.scrollTo(0, 0);
						break;
					default:
						return;
				}
			}
			_showIntoGuide(guide);
		}
		function _showIntoGuide (related) {
			var b = _getBoxData(related);
			move_HideArrs();
			coverLayer.on();
			window.scrollTo(0, 0);
			move_intoGuide_Box.style.display = move_intoGuide_sample.style.display = move_intoGuide_btnBox.style.display = 'block';
			move_intoGuide_Box.style.left = b.x + 100 + 'px';
			move_intoGuide_Box.style.top = b.y + 0 + 'px';
			move_intoGuide_sample.style.left = b.x - 20 + 'px';
			move_intoGuide_sample.style.top = b.y + 'px';
			move_intoGuide_btnBox.style.left = b.x + 135 + 'px';
			move_intoGuide_btnBox.style.top = b.y + 150 + 'px';
		}
		function _hideIntoGuide (){
			coverLayer.off();
			move_intoGuide_Box.style.display = move_intoGuide_sample.style.display = move_intoGuide_btnBox.style.display = 'none';
		}
		function _initGuide () {
			var tipBox = document.createElement('div'),
				samplePic = document.createElement('div'),
				btnBox = document.createElement('div'),
				ss;
			tipBox.className = 'move_guide_box';
			document.body.appendChild(tipBox);
			btnBox.className = 'move_guide_btnBox';
			btnBox.innerHTML = '<a href="javascript:void(0)" class="move_guide_try" onfocus="this.blur();"></a><a href="javascript:void(0)" class="move_guide_known" onfocus="this.blur();"></a>';
			document.body.appendChild(btnBox);
			btnBox.onclick = function(e){
				var evt = e || window.event,
					eTarget = evt.target || evt.srcElement,
					theClass = eTarget.className;
				switch(theClass){
					case 'move_guide_try':
						move_HideArrs();
						_hideGuide();
						window.scrollTo(0, _getBoxData(move_DomStates[index][0]).y - 5);
						for(var i = 0; i < move_PosList.length; i++){
							if(move_PosList[i] == index){
								break;
							}
						}
						move_setActIndex(i);
						move_ShowArrs(move_DomStates[index][0], i);
						break;
					case 'move_guide_known':
						_hideGuide();
						break;
					default:
						return;
				}
			}
			samplePic.className = 'move_guide_sample';
			samplePic.innerHTML = '自定义板块';
			document.body.appendChild(samplePic);
			move_tipBox = tipBox;
			move_guide_btnBox = btnBox;
			move_samplePic = samplePic;
			_showGuide(index);
		}
		function _showGuide (index) {
			var blk = move_DomStates[index][0],
				b = _getBoxData(blk);
			move_HideArrs();
			coverLayer.on();
			move_tipBox.style.display = move_samplePic.style.display = move_guide_btnBox.style.display = 'block';
			move_tipBox.style.left = b.x + 440 + 'px';
			move_tipBox.style.top = b.y + 10 + 'px';
			move_guide_btnBox.style.left = b.x + 490 + 'px';
			move_guide_btnBox.style.top = b.y + 190 + 'px';
			move_samplePic.style.left = b.x + 830 + 'px';
			move_samplePic.style.top = b.y - 5 + 'px';
			move_scrollTo(document.body.scrollTop + document.documentElement.scrollTop, b.y - 5, 300, null, '');
		}
		function _hideGuide () {
			coverLayer.off();
			move_tipBox.style.display = move_samplePic.style.display = move_guide_btnBox.style.display = 'none';
		}
		function _getBoxData (o) {
			//move_DomStates
			//move_PosList
			var b = {x: 0, y: 0, width : o.offsetWidth, height : o.offsetHeight};
			while (o.offsetParent) {
				b.x += o.offsetLeft;
				b.y += o.offsetTop;
				o = o.offsetParent;
			}
			return b;
		}
		function _getStorage (name) {
			var ret = window.localStorage ? _getLocalStorage(name) : _getCookie(name);
			return ret;
		}
		function _setStorage () {
			window.localStorage ? _setLocalStorage('blks_book_guided', 'true') : _setCookie('blks_book_guided', 'true');
		}
		function _getCookie (name) {
			var start = document.cookie.indexOf( name + "=" );
			var len = start + name.length + 1;
			if ( ( !start ) && ( name != document.cookie.substring( 0, name.length ) ) ) {
				return null;
			}
			if ( start == -1 ) return null;
			var end = document.cookie.indexOf( ';', len );
			if ( end == -1 ) end = document.cookie.length;
			return unescape( document.cookie.substring( len, end ) );
		}
		function _setCookie (name, value, expires, path, domain, secure) {
			var today = new Date();
			today.setTime( today.getTime() );
			if ( expires ) {
				expires = expires * 1000 * 60 * 60 * 24;
			}
			var expires_date = new Date( today.getTime() + (expires) );
			document.cookie = name+'='+escape( value ) +
				( ( expires ) ? ';expires='+expires_date.toGMTString() : '' ) + 
				( ( path ) ? ';path=' + path : '' ) +
				( ( domain ) ? ';domain=' + domain : '' ) +
				( ( secure ) ? ';secure' : '' );
		}
		function _getLocalStorage (name) {
			return localStorage[name];
		}
		function _setLocalStorage (name, value) {
			localStorage.setItem(name, value);
		}
		function _removeStorage () {
			localStorage.removeItem('blks_book_guided');
			window.location = window.location;
		}
		window.move_checkGuide = move_checkGuide;
		//for test
		window.__rs = _removeStorage;
	})(0);
</script>
</div>
