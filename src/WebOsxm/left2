<script type="text/javascript">
if(typeof bookSearch=="undefined"){
var bookSearch={}
}
(function(b){
var _addEvent=function (o,type,fn) {
o.attachEvent ? o.attachEvent('on'+type,fn) : o.addEventListener(type,fn,false);
};
b.search_order = '';
b.search_lib = '';
b.search_sta = '';
b.search_pub = '';
b.bindPubBtnfun = function(){
var tempBtns = document.getElementById('pub_blk').getElementsByTagName('span');
b.lastPub = tempBtns[0];
var tempBtnsLen = tempBtns.length;
for(var i=0;i<tempBtnsLen;i++){
var tempBtn = tempBtns[i];
_addEvent(tempBtn,'click',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
b.search_pub = clicktarget.getAttribute('val');
b.lastPub.className = '';
clicktarget.className='cur';
b.lastPub = clicktarget;
if (b.search_pub=='2') {
for (var i = 2; i <=16; i++) {
document.getElementById('sub01_t'+i).style.display='none';
}
}else{
for (var i = 2; i <=16; i++) {
document.getElementById('sub01_t'+i).style.display='';
}
}
});
_addEvent(tempBtn,'mouseover',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
clicktarget.className = 'cur';
});
_addEvent(tempBtn,'mouseout',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
if(clicktarget!=b.lastPub){
clicktarget.className = '';
}
});
}
}
b.bindStaBtnfun = function(){
var tempBtns = document.getElementById('sta_blk').getElementsByTagName('span');
b.lastSta = tempBtns[0];
var tempBtnsLen = tempBtns.length;
for(var i=0;i<tempBtnsLen;i++){
var tempBtn = tempBtns[i];
_addEvent(tempBtn,'click',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
b.search_sta = clicktarget.getAttribute('val');
b.lastSta.className = '';
clicktarget.className='cur';
b.lastSta = clicktarget;
});
_addEvent(tempBtn,'mouseover',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
clicktarget.className = 'cur';
});
_addEvent(tempBtn,'mouseout',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
if(clicktarget!=b.lastSta){
clicktarget.className = '';
}
});
}
}
b.bindOrderBtnfun = function(){
var tempBtns = document.getElementById('order_blk').getElementsByTagName('span');
b.lastOrder = tempBtns[0];
var tempBtnsLen = tempBtns.length;
for(var i=0;i<tempBtnsLen;i++){
var tempBtn = tempBtns[i];
_addEvent(tempBtn,'click',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
b.search_order = clicktarget.getAttribute('val');
b.lastOrder.className = '';
clicktarget.className='cur';
b.lastOrder = clicktarget;
});
_addEvent(tempBtn,'mouseover',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
clicktarget.className = 'cur';
});
_addEvent(tempBtn,'mouseout',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
if(clicktarget!=b.lastOrder){
clicktarget.className = '';
}
});
}
}
b.bindLibBtnfun = function(){
var tempBtns = document.getElementById('lib_blk').getElementsByTagName('span');
b.lastLib = tempBtns[0];
var tempBtnsLen = tempBtns.length;
for(var i=0;i<tempBtnsLen;i++){
var tempBtn = tempBtns[i];
if(tempBtn.getAttribute('val')){
_addEvent(tempBtn,'click',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
b.search_lib = clicktarget.getAttribute('val');
b.lastLib.className = '';
clicktarget.className='cur';
b.lastLib = clicktarget;
});
_addEvent(tempBtn,'mouseover',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
clicktarget.className = 'cur';
});
_addEvent(tempBtn,'mouseout',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
if(clicktarget!=b.lastLib){
clicktarget.className = '';
}
});
}
if(tempBtn.getAttribute('cate')){
_addEvent(tempBtn,'mouseover',function(e){
var clicktarget = e.srcElement ? e.srcElement:e.target;
b.search_lib = clicktarget.getAttribute('cate');
b.lastLib.className = '';
clicktarget.className='cur selected';
b.lastLib = clicktarget;
});
}
}
}
b.bindSearch= function(){
var searchBtn = document.getElementById('startSearch');
_addEvent(searchBtn,'click',function(){
window.open("http://vip.book.sina.com.cn/book_lib.php?lib="+b.search_lib+"&order="+b.search_order+"&sta="+b.search_sta+"&pub="+b.search_pub+"&dpc=1");
});
}
b.bindReChoose= function(){
var reChooseBtn = document.getElementById('reChoose');
_addEvent(reChooseBtn,'click',function(){
b.search_order='';
b.search_lib='';
b.search_sta='';
b.search_pub='';
b.lastLib.className = '';
b.lastSta.className = '';
b.lastOrder.className = '';
b.lastPub.className = '';
b.lastLib = document.getElementById('lib_blk').getElementsByTagName('span')[0];
b.lastSta = document.getElementById('sta_blk').getElementsByTagName('span')[0];
b.lastOrder = document.getElementById('order_blk').getElementsByTagName('span')[0];
b.lastPub = document.getElementById('pub_blk').getElementsByTagName('span')[0];
b.lastLib.className = 'cur';
b.lastSta.className = 'cur';
b.lastOrder.className = 'cur';
b.lastPub.className = 'cur';
});
}
b.bindPubBtnfun();
b.bindStaBtnfun();
b.bindOrderBtnfun();
b.bindLibBtnfun();
b.bindSearch();
b.bindReChoose();
b.getParam = function(name){
var r = new RegExp("\\?(?:.+&)?" + name + "=(.*?)(?:&.*)?$");
var m = window.location.toString().match(r);
return m ? m[1] : ""; //如果需要处理中文，可以用返回decodeURLComponent(m[1])
}
b.initSelected = function(btnId,valName,val){
var tempBtns = document.getElementById(btnId).getElementsByTagName('span');
var tempBtnsLen = tempBtns.length;
for(var i=0;i<tempBtnsLen;i++){
var tempBtn = tempBtns[i];
tempBtn.className='';
if (tempBtn.getAttribute('val')) {
if (tempBtn.getAttribute(valName)==val) {
tempBtn.className='cur';
}
}
if (tempBtn.getAttribute('cate')) {
if (tempBtn.getAttribute(valName)==val) {
tempBtn.className='cur selected';
SubShow_01.select(i)
}
}
}
}
b.initSearch = function(){
b.search_lib = b.getParam('cate');
b.search_pub = b.getParam('pub');
if (b.search_pub=='2') {
for (var i = 2; i <16; i++) {
document.getElementById('sub01_t'+i).style.display='none';
}
}else{
for (var i = 2; i <16; i++) {
document.getElementById('sub01_t'+i).style.display='';
}
}
b.search_sta = b.getParam('sta');
b.search_order = b.getParam('order');
if(b.search_lib){
b.initSelected('sBtns','val',b.search_lib);
b.initSelected('fBtns','cate',b.search_lib.substring(0,3));
}
if(b.search_sta){
b.initSelected('sta_blk','val',b.search_sta);
}
if(b.search_order){
b.initSelected('order_blk','val',b.search_order);
}
if(b.search_pub){
b.initSelected('pub_blk','val',b.search_pub);
}
}
b.initSearch();
})(bookSearch)
</script>

